![幣別簡稱]
#CURNM=_,_,I
HELP(CURR,{j}CURDEF,ENAME,CNAME)
@C(3,#CURM,1,V(7),$)
C(4,#CURNM,S,$)
V(E,0,CURR,{j}CURDEF)

![幣別代碼]
#CURCD=_,_,O
C(4,#CURNM,S,$)
T(A,#CURNM,CURR,{j}CURDEF,ENAME,CURCD)
</CURNM.dec>

!----- type="TIM" 上行電文TIM -----
<CURNM.tim>
#CURNM#CURCDk
</CURNM.tim>

!===========================================================
! 幣別 {j}= 台外幣 F外幣 {m}=1必須輸入 0選擇輸入
!===========================================================
!----- type="DC" 交易畫面     -----
<CURNMII.scr>
#CURNM,#CURCD
</CURNMII.scr>

!----- type="DC" 交易變數宣告 -----
<CURNMII.dec>
!0選擇輸入 1必須輸入
#CURM=A,1,S
E(0,{m})

![幣別簡稱]
#CURNM=_,_,I
C(2,#FUNCD4,S,$,$,s,s,S)
HELP(CURR,{j}CURDEF,ENAME,CNAME)
@C(3,#CURM,1,V(7),$)
C(4,#CURNM,S,$)
V(E,0,CURR,{j}CURDEF)

![幣別代碼]
#CURCD=_,_,O
C(4,#CURNM,S,$)
T(A,#CURNM,CURR,{j}CURDEF,ENAME,CURCD)
</CURNMII.dec>

!----- type="TIM" 上行電文TIM -----
<CURNMII.tim>
#CURNM#CURCD
</CURNMII.tim>

!===========================================================
! 幣別 {i}=自定 {j}=台外幣 F外幣 {m}=1必須輸入 0選擇輸入
!===========================================================
!----- type="DC" 交易畫面     -----
<CURNMP.scr>
#{i}CURNM,#{i}CURCD
</CURNMP.scr>

!----- type="DC" 交易變數宣告 -----
<CURNMP.dec>
!0選擇輸入 1必須輸入
#{i}CURM=A,1,S

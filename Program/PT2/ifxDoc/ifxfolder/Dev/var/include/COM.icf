C(4,#{i}CURNM,S,$)
T(A,#{i}CURNM,CURR,{j}CURDEF,ENAME,CURCD)
</CURNMP.dec>

!----- type="TIM" 上行電文TIM -----
<CURNMP.tim>
#{i}CURNM#{i}CURCD
</CURNMP.tim>



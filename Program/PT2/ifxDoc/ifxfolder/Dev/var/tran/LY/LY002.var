[
["[LY002]                                  LY002非RBC_表14-1_會計部年度檢查報表"],
["@年度",#RocYear],
]

!---------- AP TEXT AREA Variables ----------
#CHAIN=A,1,S
T(4,CHAIN$)
IF(#CHAIN!=1,V(P,此為連動交易，請從交易:[L9806年報]進入),$)

#NTXBUF=X,3,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
!T(1,@FUNCD,#NTXBUF,1,1)
!T(1,@TLR,#NTXBUF,2,2)
T(2,@FKEY,7)


!-----------------------------------------------------

#DT=D,8,S
D(7,3,#SYSDATE,1,0)

#DTY=N,3,S
T(1,#DT,2,3)

#RocYear=A,3,I
T(1,@RocYear,#DT,2,3)
@V(5,1,999)
IF(#RocYear>#DTY,V(P,不可大於會計年),$)

!#RocYear=A,3,I
!T(1,#DT,001,003)

</sub>




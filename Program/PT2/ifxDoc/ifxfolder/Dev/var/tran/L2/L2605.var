#FdReceiveDate=X,2,L
T(3,*)

!---------- AP TEXT AREA Variables ----------
! Help
<include src="HELPRIM.icf::HELPRIM.rtn"/>
!Status
#CDDEF0001=x,20,S
T(3,CdCode.Status)

#StatusHelpRim=X,1,S
RESET_RIM(#StatusHelpRim,XXR99)
S(XXR99,01,#CDDEF0001)
R(1,XXR99)
CALL(#HelpProc)
T(2,@StatusHelp,#HelpDesc1)

#StatusHelp=x,1024,S

#CHAIN=A,1,S
T(4,CHAIN$)


#PostingDate=D,8,S
T(4,DATE$)

!收件日期 必須輸入
#ReceiveDate=D,7,I
T(1,#PostingDate,2,7)
@A(DATE,0,#ReceiveDate)V(7)

#RPTFG=A,1,S
E(0,@FUNCIND,5)


</sub>


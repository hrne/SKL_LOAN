!---------- TXFORM = L2605 ----------
<sub type="FORM" name="L2605">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L2605,$,S)

#SCRTIL=_,_,S
T(3,[L2605] 法拍費用未銷明細查詢)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------

!筆數
#OCnt=X,3,L

!法務費用
#OFee=m,14,L

!催收法務費用
#OOverFee=m,14,L

!金額
#OFeeAmt=m,14,L



!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)


!戶號
#OOCustNo=A,7,S
!額度
#OOFacmNo=A,3,S

#OOcustNoX=X,11,S
T(2,#OOCustNo+-+#OOFacmNo)


!戶名
#OOCustName=X,100,L
!收件日
#OOReceiveDate=D,7,L
!法拍費
#OOFee=m,14,L
!催收法拍費
#OOFee1=m,14,L
!法拍費合計
#OOFeeAmt=m,14,L
!本戶累溢短收
#OOShortfallOverflow=+m,14,L
!地區別
#OOClMainType=A,1,L
!擔保品大類
#OOClMainTypeX=X,10,L
SWITCH(#OOClMainType,1,T(3,不動產),
                  2,T(3,不動產),
                  3,T(3,股票),
                  4,T(3,股票),
                  5,T(3,其他擔保品),
                  9,T(3,動產),
                  0,T(3, ))
!地區別
#OOCityCode=X,2,L
!地區別名稱
#OOCityCodeX=X,10,L
!戶況
#OOStatus=X,2,L
#OOStatusX=X,14,L
T(H,#OOStatus,#StatusHelp)

!繳息迄日
#OOPrevPayIntDate=D,7,L
!催收人員
#OOCollPsn=X,6,L
#OOName=X,10,L
#OOName1=X,18,L
T(2,#OOCollPsn+  +#OOName)

!轉催收註記
#OOOverdueCode=X,1,L



!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------

</sub>



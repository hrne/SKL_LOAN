!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[130,870];printer.width=[20,20];">
[
["[L2079]                                 展期件新舊對照查詢"],
["#FdMain+查詢條件","@(借戶戶號、年月起訖，需擇一輸入)"],
["#FdCustNo+戶號",#CustNo],
["#FdYearStart+年月起訖",#YearStart,"/",#MonthStart,"@～@",#YearEnd,"/",#MonthEnd],
["#FdOldFacmNo+原額度",#OldFacmNo],
["#FdNewFacmNo+新額度",#NewFacmNo],
]

#FdMain=X,2,L
T(3,*)
#FdCustNo=X,2,L
#FdYearStart=X,2,L
T(3,*)
#FdOldFacmNo=X,2,L
#FdNewFacmNo=X,2,L

!---------- AP Button AREA ----------
![Buttom資料查詢]
#BUF1=X,42,S
!---------- AP TEXT AREA Variables ----------
! Help
<include src="HELPRIM.icf::HELPRIM.rtn"/>
#CDDEF0001=x,26,S
T(3,CdCode.RenewCode)

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF0001)
R(1,XXR99)
CALL(#HelpProc) 
T(2,@RenewCodeHelp,#HelpDesc1) 

#RenewCodeHelp=x,1024,S

#CHAIN=A,1,S
T(4,CHAIN$)


#NTXBUF=X,34,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@CustNo,#NTXBUF,1,7)
T(1,@YearStart,#NTXBUF,8,3)
T(1,@MonthStart,#NTXBUF,11,2)
T(1,@YearEnd,#NTXBUF,13,3)
T(1,@MonthEnd,#NTXBUF,16,2)
T(1,@OldFacmNo,#NTXBUF,18,3)
T(1,@NewFacmNo,#NTXBUF,21,3)

! 戶號
#CustNo=A,7,I
C(3,#CHAIN,1,s,$)

#TimCustNo=A,7,S
C(3,#CustNo,0,$,E(0,#CustNo))
T(2,@RmkCustNo,#TimCustNo)
<include src="COM.icf::CustRmk.rtn"/>

#DATE=D,8,S
T(4,DATE$)

! 年月起
#YearStart=A,3,I
C(3,#CHAIN,1,s,$)
IF(#CustNo>0,S,$)
T(1,#DATE,2,3)
@V(2,0)
#MonthStart=A,2,I
C(3,#CHAIN,1,s,$)
IF(#CustNo>0,S,$)
E(0,1)
@V(5,1,12)

#YearMonthStart=A,5,S
T(2,#YearStart+#MonthStart)


! 年月迄
#YearEnd=A,3,I
C(3,#CHAIN,1,s,$)
IF(#CustNo>0,S,$)
E(0,#YearStart)
@V(5,#YearStart,999)

#MonthEnd=A,2,I
C(3,#CHAIN,1,s,$)
IF(#CustNo>0,S,$)
E(0,#MonthStart)
@IF(#YearStart == #YearEnd,V(5,#MonthStart,12),V(5,1,12))

#YearMonthEnd=A,5,S
T(2,#YearEnd+#MonthEnd)

! 原額度
#OldFacmNo=A,3,I
C(3,#CHAIN,1,s,$)

! 新額度
#NewFacmNo=A,3,I
C(3,#CHAIN,1,s,$)


#RPTFG=A,1,S
E(0,0)

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

</sub>


!---------- AP Button AREA ----------
![Buttom資料查詢]
#BTN1=X,15,I,ui:button;value:顧客資料查詢;enable:0;tabbable:0
#BUF1=X,42,S
ENABLE(#BTN1)

! 必輸欄位
#FdBorrowerx=X,2,L


! 永遠為必輸欄位
#FdCustId=X,2,L
T(3,*)
!---------- AP TEXT AREA Variables ----------

<include src="HELPRIM.icf::HELPRIM.rtn"/>
#CDDEF01=x,30,S
T(3,CdCode.CurrPrecision)

#CurrPrecisionHelp=x,1024,S

#CDDEF02=x,30,S
T(3,CdCode.Status)

#StatusHelp=x,1024,S

#CDDEF03=x,20,S
T(3,CdCode.GuaStatCode)

#GuaStatCodeHelp=x,1024,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF01,#CDDEF02,#CDDEF03)
R(1,XXR99)
CALL(#HelpProc)
T(2,@CurrPrecisionHelp,#HelpDesc1)
T(2,@StatusHelp,#HelpDesc2)
T(2,@GuaStatCodeHelp,#HelpDesc3)

#YNHelp=x,10,S
T(3,Y:是;N:否)

#RimTxCode=X,5,L

#CHAIN=A,1,S
T(4,CHAIN$)
T(2,@RimTxCode,#TXCD)

#NTXBUF=X,57,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@CustId,#NTXBUF,1,10)

#BUF=X,134,S
T(2,0000000+          +                                                                                                                    0)
#BUF5=X,134,S

#BIND1=X,1,S
BIND(#BTN1,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUF;ntxbuf5=type<-1:CustId<-OOCustId})

!統一編號
#CustId=X,10,I
C(3,#CHAIN,1,s,$)
@
T(3,#CustName,)
T(2,@BUF5,0000000+#CustId+                                                                                                                    0)
V(7)A(ID_UNINO,0,#CustId)

#BIND5=X,1,S
BIND(#BTN1,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUF5;ntxbuf5=type<-1:CustId<-OOCustId})


<include src="ROM.icf::L1R07.dec"/>

!調rim用參數
#RimCustId=X,10,S

#rimZL1R07=X,1,S
T(2,@RimCustId,#CustId)
RESET_RIM(#rimL1R07,L1R07)
S(L1R07,1,#RimCustId)
R(1,L1R07)


#checkL1r07=X,1,S
T(2,@Borrower,#L1r07Borrower)

!身分證找戶號
<include src="ROM.icf::L2R47.dec"/>
<include src="ROM.icf::L2R07.dec"/>
#Rim2R47CustId=X,10,S

#RimL2R47=X,1,S
T(2,@Rim2R47CustId,#CustId)
RESET_RIM(#RimL2R47,L2R47)
C(4,#CustId,s,$)
S(L2R47,1,#Rim2R47CustId)
R(1,L2R47)

#CustNo=A,7,L
T(2,#L2R47CustNo)

#TimCustNo=A,7,S
E(0,0)
C(3,#CustNo,0,$,E(0,#CustNo))
T(2,@RmkCustNo,#TimCustNo)
<include src="COM.icf::CustRmk.rtn"/>

#RimCustNo=A,7,L

#SendL2r07A=X,1,S
T(2,@RimCustId,#CustId)
E(0,@RimCustNo,0)
RESET_RIM(#SendL2r07A,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo,#END)
R(1,L2R07)

#CustName=X,100,L
T(2,#L2r07CustName)


#MRKEY=_,_,S
T(2,#L2R47CustNo)

!借戶否
#Borrower=X,1,L


!#Borrowerx=X,2,L
!T(H,#Borrower,#YNHelp) 

#RPTFG=A,1,S

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

</sub>


!---------- AP Button AREA ----------
![Buttom資料查詢]
!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>

#L2419Mrkey=x,100,S
T(2,Sample)

#L2419Note=x,100,S
T(2,不動產擔保品資料整批匯入EXCEL檔範本)

#BufL2419=X,200,S
T(2,L24190000000000000+#L2419Mrkey+#L2419Note)

#Button=x,100,S
T(2,{cmd=CHAIN; ntxcd=LC014; ntxbuf=#BufL2419})ASGN(BTN_NEW1$)
K(MODIFYBUTTON,1,範本查詢)

#SysDate=D,7,S
E(0,#SYSCDATE)

#FdFileBtn=X,2,L
T(3,*)

#FdApplNo=X,2,L
T(3,*)

#FdCustNo=X,2,L
#FdCustId=X,2,L
#FdCustName=X,2,L

#FdEvaCompany=X,2,L
T(3,*)

#FdEvaDate=X,2,L
T(3,*)

#ApplNoBtn=X,15,I,ui:button;value:核准號碼查詢;enable:1;tabbable:0;title:L2010 申請案件明細資料查詢;
#EvaCompanyBtn=X,15,I,ui:button;value:鑑定公司查詢;enable:1;tabbable:0;

! #ApplnoBuf=X,14,S
! T(2,00000019999999)

#ApplnoBuf=X,14,S
T(2, )

#BindApplNoBtn=X,1,S
BIND(#ApplNoBtn,click, {cmd=CHAIN; ntxcd=L2010; ntxbuf=#ApplnoBuf;ntxbuf5=type<-1:ApplNo<-OOApplNo})

#EvaCompanyBuf=X,4,S
T(2,2+  +0)

#BindEvaCompanyBtn=X,1,S
BIND(#EvaCompanyBtn,click, {cmd=CHAIN; ntxcd=L6073; ntxbuf=#EvaCompanyBuf;ntxbuf5=type<-1:EvaCompany<-OOInsurerCode:EvaCompanyX<-OOInsurerItem})

![一般數值 multiple:true 可上傳多個檔案]
#FileBtn=X,1,O,ui:button;value:瀏覽;enable:1;tabbable:1;file:true;multiple:false;fieldName:#FileItem;width:600

#FileItem=x,100,S

#CHAIN=A,1,S
T(4,CHAIN$)

#NTXBUF=X,100,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)

#DT1=D,8,S
T(4,DATE$)

#DT2=D,7,S
T(1,#DT1,2,7)

#RPTFG=A,1,S
E(0,0)

#FdFunction=X,2,L
T(3,*)

#FunctionHelp=X,100,S
T(3,1:上傳及檢核;2:上傳回饋檔並寫入擔保品檔;3:以系統擔保品資料產生回饋檔)

! 功能選擇
#Function=A,1,I
HELP(#FunctionHelp)
@V(H,#FunctionHelp)

#FunctionX=X,30,L
T(H,#Function,#FunctionHelp)

#HideAll=A,1,S
E(0,0)
INVOKEJS(SHOW,p1,2,8,#HideAll)
K(MODIFYBUTTON,上傳及檢查)

#Show1=A,1,S
IF(#Function==1,E(0,1),S)
INVOKEJS(SHOW,p1,2,8,#Show1)

#Show2=A,1,S
IF(#Function==2,E(0,1),S)
INVOKEJS(SHOW,p1,8,8,#Show2)

#Show3=A,1,S
IF(#Function==3,E(0,1),S)
INVOKEJS(SHOW,p1,2,5,#Show3)
K(MODIFYBUTTON,送出)

#ApplNo=A,7,I
IF(#Function==1||#Function==3,$,S)
@
V(2,0)

<include src="ROM.icf::L0Rim.dec"/>
<include src="ROM.icf::L2Rim.dec"/>
<include src="ROM.icf::L2R05.dec"/>
<include src="ROM.icf::L2R07.dec"/>
<include src="ROM.icf::L2R65.dec"/>

! 功能2,3時,需檢核是否已撥款
#SendL2R65CheckDrawdown=X,1,S
IF(#Function==2||#Function==3,$,S)
T(3,@DrawdownStatus,)
E(0,@RimApplNo,#ApplNo)
RESET_RIM(#SendL2R65CheckDrawdown,L2R65)
S(L2R65,1,#RimApplNo)
R(1,L2R65)
T(2,@DrawdownStatus,#L2r05DrawdownStatus)

#DrawdownStatus=X,1,S
IF(#Function==2||#Function==3,$,S)
IF(#DrawdownStatus=="Y",V(P,此核准號碼已撥款,不可使用此功能),s)

#RimTxCode=_,_,S
T(3,L2419)

!用核准號碼(ApplNo)調L2R05
#SendL2r05=X,1,S
IF(#Function==1||#Function==3,$,S)
E(0,@CustNo,0)
E(0,@FacmNo,0)
T(3,@CustId,)
T(3,@CustName,)
E(0,@RimFuncCode,5)
E(0,@RimCustNo,0)
E(0,@RimFacmNo,0)
E(0,@RimApplNo,#ApplNo)
E(0,@RimCaseNo,0)
RESET_RIM(#SendL2r05,L2R05)
S(L2R05,1,#RimFuncCode,#RimTxCode,#RimFKey,#RimCustNo,#RimFacmNo,#RimApplNo,#RimCaseNo)
R(1,L2R05)
E(0,@CustNo,#L2r05CustNo)
E(0,@FacmNo,#L2r05FacmNo)
T(2,@CustId,#L2r05CustId)

#SendL2r07D=X,1,S
IF(#Function==1||#Function==3,$,S)
C(3,#ApplNo,0,S,$)
T(2,@RimCustId,#L2r05CustId)
E(0,@RimCustNo,0)
RESET_RIM(#SendL2r07D,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo)
R(1,L2R07)
T(2,@CustName,#L2r07CustName)


#CustNo=A,7,L
#FacmNo=A,3,L
#CustId=X,10,L
#CustName=x,100,L

#MRKEY=_,_,S
T(2,#CustNo+-+#FacmNo)

! [鑑價公司]
#EvaCompany=X,2,I
IF(#Function==1,$,S)
T(3,09)
@
V(7)

<include src="ROM.icf::L6R10.dec"/>


#RimL6R10InsurerType=X,1,S
#RimL6R10InsurerCode=X,2,S

#SendL6R10=X,1,S
IF(#Function==1,$,S)
E(0,@RimFuncCode,5)
T(2,@RimL6R10InsurerType,2)
T(2,@RimL6R10InsurerCode,#EvaCompany)
RESET_RIM(#SendL6R10,L6R10)
S(L6R10,1,#RimFuncCode,#RimTxCode,#RimL6R10InsurerType,#RimL6R10InsurerCode)
R(1,L6R10)

#EvaCompanyX=x,100,L
T(2,#L6R10InsurerItem)

! [鑑估日期] v
#EvaDate=D,7,I
IF(#Function==1,$,S)
T(2,#DT2)
D(8)
@
V(7)
A(4,0,#EvaDate)
IF(#EvaDate>#SysDate,V(P,不得超過日曆日),$)

</sub>




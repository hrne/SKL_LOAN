E(0,@CustNo,#L2r05CustNo)
E(0,@FacmNo,#L2r05FacmNo)
T(2,@CustId,#L2r05CustId)

#SendL2r07D=X,1,S
C(3,#ApplNo,0,S,$)
T(2,@RimCustId,#L2r05CustId)
E(0,@RimCustNo,0)
RESET_RIM(#SendL2r07D,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo)
R(1,L2R07)
T(2,@CustName,#L2r07CustName)



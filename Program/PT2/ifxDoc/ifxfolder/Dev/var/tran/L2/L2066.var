----- BASIC LABEL Variables ----------
<sub type="SYS">
#KeyInq=X,1,S
K(MODIFYBUTTON,查詢)
</sub>

<sub type="SYS" name="label">

!#TXCD
!T(3,L2066)

![資料表頭(HEADER)長度]
#INQHD=n,3,S
E(0,0)ASGN(INQHD$)

![每筆資料明細(OCCURS)長度]
#INQLEN=n,3,S
E(0,40)ASGN(INQLEN$)

![多筆查詢之一個畫面有N筆資料]
#INQREC=n,2,S
E(0,20)ASGN(INQREC$)

![畫面顯示的明細間的高度]
#LOOPH=n,3,S
E(0,1)ASGN(LOOPHEIGHT$)

![每張印錄單列印的筆數]
#INQPRT=n,2,S
E(0,40)ASGN(INQPRT$)

</sub>

<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)

#button=x,100,S
T(2,{cmd=CHAIN; ntxcd=L2606; ntxbuf=1; ntxbuf5=type})ASGN(BTN_NEW1$)
K(MODIFYBUTTON,1,新增)
</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,1050];printer.width=[20,20];">
[
["[L2066]                                 帳管費明細資料查詢"],
["#FdCustNo+借戶戶號",#CustNo,#BTN1,#CustName],
["#FdFacmNo+額度編號",#FacmNo],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
#BTN1=X,15,I,ui:button;value:顧客資料查詢;enable:1;tabbable:0



! 永遠為必輸欄位
#FdCustNo=X,2,L
T(3,*)
#FdFacmNo=X,2,L

!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>

#CDDEF01=x,26,S
T(3,CdCode.CurrPrecision)
#CurrPrecisionHelp=x,1024,S

#HelpRim=X,1,S 
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF01)
R(1,XXR99)
CALL(#HelpProc)
T(2,@CurrPrecisionHelp,#HelpDesc1)


#CHAIN=A,1,S
T(4,CHAIN$)


#NTXBUF=X,10,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@CustNo,#NTXBUF,1,7)
T(1,@FacmNo,#NTXBUF,8,3)


#NTXBUF5=X,10,S
C(3,#CHAIN,1,T(4,NTXBUF5$),S)
C(4,#NTXBUF5,E(0,@RPTFG2,1),E(0,@RPTFG2,2))

#BUF1=X,42,S

#BIND1=X,1,S
BIND(#BTN1,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUF1;ntxbuf5=type<-1:CustNo<-OOCustNo})

!借戶戶號
#CustNo=A,7,I
C(3,#CHAIN,1,s,$)
@V(2,0)
T(3,#CustName,)

!調RIML1R01取CustName
#RimL1r01=X,1,S
E(0,@RimFunCd,5)
E(0,@RimCustNo,#CustNo)
RESET_RIM(#RimL1r01,L1R01)
S(L1R01,1,#RimCustId,#RimFunCd,#RimCustNo)
R(1,L1R01)


#checkL1R01=X,1,S
T(2,@CustName,#L1r01CustName)


#CustName=X,100,L




#TimCustNo=A,7,S
C(3,#CustNo,0,$,E(0,#CustNo))
T(2,@RmkCustNo,#TimCustNo)
<include src="COM.icf::CustRmk.rtn"/>

!額度編號
#FacmNo=A,3,I
C(3,#CHAIN,1,s,$)

#RPTFG=A,1,S

#RPTFG2=A,1,S
C(3,#RPTFG,0,$,S)
C(4,#NTXBUF5,T(3,1),T(3,2))

#MRKEY=_,_,S
C(3,#FacmNo,0,T(2,#CustNo),T(2,#CustNo+-+#FacmNo))

#CSYSDATE=D,7,S
E(0,#SYSDATE)

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

<include src="ROM.icf::L2Rim.dec"/>
<include src="ROM.icf::L1R01.dec"/>
</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>


!---------- TXFORM = L2066 ----------
<sub type="FORM" name="L2066">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L2066,$,S)

#SCRTIL=_,_,S
T(3,[L2066] 帳管費明細資料查詢)

#OCustNo=A,7,L

#OFacmNo=A,3,L

!修改
#FUNC2=A,1,S
E(0,2)
!刪除
#FUNC4=A,1,S
E(0,4)
!查詢
#FUNC5=A,1,S
E(0,5)
!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

#OOCustNo=A,7,S
#OOFacmNo=A,3,S
#OORvNo=X,15,L
#OOOpenAcDate=D,7,L

#OOCurrencyCode=X,3,O

#Precision=N,1,S
T(H,#OOCurrencyCode,#CurrPrecisionHelp)

#OOAcctFee=m,14.2,L

#OOAcctFee1=m,14,L
E(0,#OOAcctFee)

#OOAcctFeeX=X,22,L
X(#OOAcctFee1)
#OOSlipNote=X,60,L
#OOCls=A,1,S
#OOClsX=X,10,L
!修改
#OOBUF2=X,30,S
T(2,#FUNC2+#OOCustNo+#OOFacmNo+#OORvNo)
#OOBTN2=X,1,S
IF(#OOCls==1,BIND(grid,cancel),
BIND(grid,{ cmd=CHAIN; ntxcd=L2606; ntxbuf=#OOBUF2}))

!刪除
#OOBUF4=X,30,S
T(2,#FUNC4+#OOCustNo+#OOFacmNo+#OORvNo)
#OOBTN4=X,1,S
IF(#OOCls==1,BIND(grid,cancel),
BIND(grid,{ cmd=CHAIN; ntxcd=L2606; ntxbuf=#OOBUF4}))

!查詢
#OOBUF5=X,30,S
T(2,#FUNC5+#OOCustNo+#OOFacmNo+#OORvNo)
#OOBTN5=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L2606; ntxbuf=#OOBUF5})


!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------

</sub>



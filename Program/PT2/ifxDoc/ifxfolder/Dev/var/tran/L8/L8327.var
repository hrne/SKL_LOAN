K(NOBUTTON,CLOSEY)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)


! 刪除進入
#Deal_Delete=X,1,S
C(5,#TranKey_Tmp,4,$,S)
T(1,@Ukey,#NTXBUF,3,32)
K(MODIFYBUTTON,刪除)

! 修改進入
#Deal_Revise=X,1,S
C(5,#TranKey_Tmp,7,$,S)
T(1,@Ukey,#NTXBUF,3,32)
T(F,@TranKey,1)T(F,@Civil323Amt,1)T(F,@TotalAmt,1)T(F,@FirstPayDate,1)T(F,@Period,1)T(F,@Rate,1)T(F,@MonthPayAmt,1)T(F,@PayAccount,1)   

]
!---------- AP Button AREA ----------
![Buttom資料查詢]
! #BTN_SELECT_ALL=X,15,I,ui:button;value:查詢;enable:0;tabbable:0;title:LXXXX

#FdFuncCode=X,2,L
#FdAcDate=X,2,L
#FdCustNo=X,2,L
#FdFactor=X,2,L
#FdTotalAmt=X,2,L
#FdTotalCnt=X,2,L
!#FdMemoSeq=X,2,L
#FdRational=X,2,L
#FdEmpNoDesc=X,2,L
#FdManagerCheck=X,2,L
#FdManagerDesc=X,2,L
#FdManagerDate=X,2,L

!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>

!FuncCode2(1:新增;2:修改;4:刪除)
#CODEFuncCode2=x,20,S                  
T(3,CdCode.FuncCode2) 
!INVOKEJS(SHOW,p1,10,10,1)                 
#TranKeyHelp=x,50,S

!Factor(1:樣態1;2:樣態2;3:樣態3)
#CODEFactor=x,20,S                  
T(3,CdCode.Factor) 
#FactorHelp=x,100,S

!主管1 經辦3
#Level=A,1,S
T(4,LEVEL$)

#mkg=X,1,S
INVOKEJS(SHOW,p1,11,11,0)

!AP
#CDCODEAp=x,20,S                  
T(3,CdCode.RepaySource)

#ApHelp=x,1024,S


#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CODEFuncCode2,#CODEFactor,#CDCODEAp)
R(1,XXR99)
CALL(#HelpProc)
T(2,@TranKeyHelp,#HelpDesc1)
T(2,@FactorHelp,#HelpDesc2)
T(2,@ApHelp,#HelpDesc3)

!YesNo
#YesNoHelp=x,50,S
T(3,Y:是;N:否)

!同意不同意
#AgreeHelp=x,50,S
T(3,Y:同意;N:退回)


#CHAIN=A,1,S
T(4,CHAIN$)
C(3,#FKEY,0,$,s)
IF(#CHAIN!=1,V(P,此為連動交易，請從交易:[L8922 疑似洗錢交易合理性查詢]進入),$)

#NTXBUF=X,23,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)

#CHAIN_FuncCode=X,1,S
T(1,#NTXBUF,1,1)

#CHAIN_AcDate=X,7,S
T(1,#NTXBUF,2,7)

#CHAIN_Factor=X,2,S
T(1,#NTXBUF,9,2)

#CHAIN_CustNo=X,7,S
T(1,#NTXBUF,11,7)



#CHAINSET=X,1,S
C(3,#CHAIN,1,$,S)
T(2,@FuncCode,#CHAIN_FuncCode)
C(3,#FuncCode,1,S,$)
E(0,@AcDate,#CHAIN_AcDate)
E(0,@Factor,#CHAIN_Factor)
E(0,@CustNo,#CHAIN_CustNo)
E(0,@CustNo,#CHAIN_CustNo)



! 功能 必須輸入(1: 新增;2: 修改;4: 刪除;5: 查詢)
#FuncCode=A,1,S
T(F,@FuncCode,0)
T(F,@CustName,0)
C(3,#CHAIN,1,s,$)
HELP(#TranKeyHelp)
@V(H,#TranKeyHelp)
E(0,@FUNCIND,#FuncCode)

#FuncCodeX=X,12,L
T(H,#FuncCode,#TranKeyHelp)
C(2,#FuncCode,$,K(MODIFYBUTTON,新增),K(MODIFYBUTTON,修改),$,K(MODIFYBUTTON,刪除),K(MODIFYBUTTON,查詢))
C(3,#FuncCode,5,T(2,查詢),$)

#AGAIN=X,1,S
IF(#FuncCode==4 || #FuncCode==2,$,S)
E(0,1)ASGN(AGAIN$)

#FUNCIND=_,_,S
E(0,#FuncCode)
<include src="COM.icf::CheckAuth.rtn"/>

!FuncCode=1,2 顯示框線
#WKTF=X,1,S
IF (#FuncCode==1 || #FuncCode==2 ,T(F,@TotalAmt,1)T(F,@TotalCnt,1)T(F,@Rational,1)T(F,@EmpNoDesc,1)T(F,@ManagerDesc,1)T(F,@ManagerCheck,1)T(F,@ManagerDate,1)T(F,@ManagerCheckDate,1),
                                  T(F,@TotalAmt,0)T(F,@TotalCnt,0)T(F,@Rational,0)T(F,@EmpNoDesc,0)T(F,@ManagerDesc,0)T(F,@ManagerCheck,0)T(F,@ManagerDate,0)T(F,@ManagerCheckDate,0))

IF (#FuncCode==1 ,T(F,@AcDate,1)T(F,@CustNo,1)T(F,@Factor,1),
                  T(F,@AcDate,0)T(F,@CustNo,0)T(F,@Factor,0))
                  
IF (#FuncCode==1 ,T(F,@TotalAmt,1)T(F,@TotalCnt,1),T(F,@TotalAmt,0)T(F,@TotalCnt,0))      

IF(#Level==3 && #FuncCode!=4,T(F,@Rational,1)T(F,@EmpNoDesc,1),T(F,@Rational,0)T(F,@EmpNoDesc,0))     

IF(#Level==1 && #FuncCode!=4,T(F,@ManagerCheck,1)T(F,@ManagerCheckDesc,1),T(F,@ManagerCheck,0)T(F,@ManagerCheckDesc,0))     

IF(#FuncCode==5,T(F,#TotalAmt,0)T(F,#Rational,0)T(F,#EmpNoDesc,0)K(NOBUTTON,CLOSEY),$)

#WKPSC=X,2,S

#WKSTAR1=X,2,S
C(2,#FuncCode,S,T(3,*),S,S,S,S)

#WKSTAR2=X,2,S
C(2,#FuncCode,S,$,$,S,S,S)
C(3,#Level,3,T(3,*),T(3, ))

#WKSTAR5=X,2,S
C(2,#FuncCode,S,$,$,S,S,S)
C(3,#Level,1,T(3,*),T(3, ))


#WKNAME=X,1,S
T(2,@FdAcDate,#WKSTAR1)
T(2,@FdCustNo,#WKSTAR1)
T(2,@FdFactor,#WKSTAR1)
T(2,@FdTotalAmt,#WKSTAR1)
T(2,@FdTotalCnt,#WKSTAR1)
!T(2,@FdMemoSeq,#WKSTAR2)
T(2,@FdRational,#WKSTAR2)
T(2,@FdEmpNoDesc,#WKPSC)
T(2,@FdManagerCheck,#WKSTAR5)
T(2,@FdManagerDesc,#WKSTAR5)
T(2,@FdManagerDate,#WKPSC)


!本營業日
#DATE=D,8,S
T(4,DATE$)

!入帳日期
#AcDate=D,7,I
C(3,#CHAIN,1,C(3,#FuncCode,1,$,s),$)
C(3,#FKEY,0,$,s)
T(1,#DATE,2,7)
D(8)
@V(7)A(DATE,0,#AcDate)
V(5,0000000,#DATE)

!戶號
#CustNo=A,7,I
C(3,#CHAIN,1,C(3,#FuncCode,1,$,s),$)
C(3,#FKEY,0,$,s)
@V(7)

!額度號碼
!#FacmNo=A,3,I
!C(3,#CHAIN,1,C(3,#FuncCode,1,$,s),$)
!@V(7)

!撥款序號
!#BormNo=A,3,I
!C(3,#CHAIN,1,C(3,#FuncCode,1,$,s),$)
!@V(7)

!交易樣態
#Factor=N,2,I
C(3,#CHAIN,1,C(3,#FuncCode,1,$,s),$)
HELP(#FactorHelp)
C(3,#FKEY,0,$,s)
@T(3,@FactorX,)
V(H,#FactorHelp)

#FactorX=X,6,L
T(H,#Factor,#FactorHelp)


#RimAcDate=A,7,S
E(0,#AcDate)
#RimFactor=A,2,S
E(0,#Factor)
#RimCustNo=A,7,S
E(0,#CustNo)
!#RimFacmNo=9,3,S
!E(0,#FacmNo)
!#RimBormNo=9,3,S
!E(0,#BormNo)

#SendL8R50=X,1,S
C(3,#FuncCode,1,s,$)
E(0,@RimFuncCode,#FuncCode)
T(2,@RimTxCode,#TXCD)
RESET_RIM(#SendL8R50,L8R50)
S(L8R50,1,#RimFuncCode,#RimTxCode,#RimAcDate,#RimFactor,#RimCustNo)
R(1,L8R50)
E(0,@TotalCnt,#L8R50TotalCnt)
E(0,@TotalAmt,#L8R50TotalAmt)
T(2,@Rational,#L8R50Rational)
T(2,@EmpNoDesc,#L8R50EmpNoDesc)
T(2,@ManagerDesc,#L8R50ManagerDesc)
T(2,@ManagerCheck,#L8R50ManagerCheck)
E(0,@ManagerDate,#L8R50ManagerDate)
E(0,@ManagerCheckDate,#L8R50ManagerCheckDate)
T(2,@CustName,#L8R50CustName)
T(H,#RationalX,#Rational,#YesNoHelp)
T(H,#ManagerCheckX,#ManagerCheck,#AgreeHelp)

!視調到的資料隱藏相關欄位
#WKTFDates=A,1,S
INVOKEJS(SHOW,p1,10,11,0)
C(5,#ManagerCheck,"Y",INVOKEJS(SHOW,p1,10,10,1)INVOKEJS(SHOW,p1,11,11,0),$)
C(5,#ManagerCheck,"N",INVOKEJS(SHOW,p1,10,10,0)INVOKEJS(SHOW,p1,11,11,1),$)

#mkg2=X,1,S
!IF(#L8R50ManagerDate>0,INVOKEJS(SHOW,p1,11,11,0),$)
IF(#L8R50ManagerDate!=0 && #L8R50ManagerCheckDate!=0,INVOKEJS(SHOW,p1,11,11,1),$)
!戶名
#CustName=X,50,L

!累積金額
#TotalAmt=m,14,I
IF(#FuncCode==2 || #FuncCode==4 || #FuncCode==5,s,$)
@V(2,0)

!累積筆數
#TotalCnt=N,4,I
IF(#FuncCode==2 || #FuncCode==4 || #FuncCode==5,s,$)
@V(2,0)

!備忘錄序號
!#MemoSeq=N,2,I
!C(3,#FuncCode,4,s)
!@V(2,0)

!合理性
#Rational=X,1,I
C(3,#FuncCode,4,s)
C(3,#FuncCode,5,s)
C(3,#Level,3,$,s)
HELP(#YesNoHelp)
@T(3,@RationalX,)
V(H,#YesNoHelp)

#RationalX=X,2,L
T(H,#Rational,#YesNoHelp)

!經辦合理性說明
!#EmpNoDesc=X,2.50,I
!C(3,#FuncCode,4,s)
!C(3,#FuncCode,5,s)
!C(3,#Level,3,$,s)

!調RIM的L8R50Rational資料空白=代表經辦合理性說明須帶入預設值
#SendL8R60=X,1,S
C(3,#FuncCode,2,$,s)
C(4,#L8R50Rational,$,s)
!T(2,@RimAcDate,#AcDate)
!E(0,@RimCustNo,#CustNo)
RESET_RIM(#SendL8R60,L8R60)
S(L8R60,1,#RimAcDate,#RimCustNo)
R(1,L8R60)
E(0,@RepaySource,#L8R60RepaySource)
T(2,@RepayBank,#L8R60RepayBank)
T(2,@Description,#L8R60Description)

#RepaySource=A,2,L
#RepayBank=X,20,L
#Description=X,500,L

#RepaySourceX=X,20,L
T(H,#RepaySource,#ApHelp)
!調RIM合理性為空白代表要帶預設值
#DescX=X,200,L
C(4,#L8R50Rational,$,s)
C(3,#RepaySource,1,T(B,依據電話訪問還款資金來源為+#RepayBank+#RepaySourceX),T(B,依據電話訪問還款資金來源為+#RepaySourceX+#Description))

#EmpNoDesc=x,4.50,I
TOOLTIP(Shift+Enter換行，Enter跳一下欄位)
C(3,#FuncCode,4,s)
C(3,#FuncCode,5,s)
C(3,#Level,3,$,s)
C(4,#EmpNoDesc,C(4,#L8R50Rational,T(2,@EmpNoDesc,#DescX),$),$)

!主管覆核
#ManagerCheck=X,1,H
T(4,@Level,LEVEL$)
T(4,@CDATE,CDATE$)
C(3,#FuncCode,4,s,$)
C(3,#FuncCode,5,s,$)
C(3,#Level,1,$,s)
IF(#L8R50ManagerCheck=="Y" || #L8R50ManagerCheck=="N",$,T(2,@ManagerCheck,Y))
T(3,@AgreeHelp,Y:同意;N:退回)

HELP(#AgreeHelp)
@T(3,@ManagerCheckX,)
V(H,#AgreeHelp)
!放行狀態下 S,L 的邏輯不會跑
!所以在主管覆核這邊把該做的先處理
!各欄位的註解就留在原本的欄位上方
C(3,#Level,1,$,s)
C(5,#ManagerCheck,"N",T(3,@WKSTAR3,*),T(3,@WKSTAR3, ))
C(5,#ManagerCheck,"Y",T(3,@WKSTAR4,*),T(3,@WKSTAR4, ))
IF(#FuncCode==5,T(2,@FdManagerDesc,),T(2,@FdManagerDesc,#WKSTAR3))
T(H,@ManagerCheckX,#ManagerCheck,#AgreeHelp)
T(4,@CDATE,CDATE$)
C(5,#ManagerCheck,"Y",E(0,@ManagerDate,#CDATE)INVOKEJS(SHOW,p1,10,10,1)INVOKEJS(SHOW,p1,11,11,0),E(0,@ManagerDate,0))
!C(5,#ManagerCheck,"N",E(0,@ManagerCheckDate,#CDATE)INVOKEJS(SHOW,p1,10,10,0)INVOKEJS(SHOW,p1,11,11,1),E(0,@ManagerCheckDate,0))
C(5,#ManagerCheck,"N",E(0,@ManagerCheckDate,#CDATE)INVOKEJS(SHOW,p1,10,10,0)INVOKEJS(SHOW,p1,11,11,1),E(0,@ManagerCheckDate,#CDATE))
T(2,@MRKEY,#AcDate+-+#Factor+-+#CustNo)

#ManagerDateCopy=D,7,S

#WKSTAR3=X,2,S

#WKSTAR4=X,2,S

#ManagerCheckX=X,6,L
T(H,#ManagerCheck,#AgreeHelp)

!日曆日
#CDATE=D,8,S

!Yoko 20220525指示
!覆核日期欄位其實應該是放退回日期...
!欄位名字先維持這樣
!但機制改成 
! 1. 主管同意時，同意日期代當天，覆核日期清空=>2022/8/24珮瑜調整:此時覆核日期=同意日期
! 2. 主管退回時，同意日期清空，覆核日期代當天
!xiangwei 修改

!主管同意日期
!如果ManagerCheck為Y, 自動帶入當天並略過
!只有主管時才可以編輯
#ManagerDate=D,7,L

!主管覆核日期
!如果ManagerCheck為N, 自動帶入當天並略過
!只有主管時才可以編輯
#ManagerCheckDate=D,7,L

!主管覆核說明
#ManagerDesc=X,4.50,H
TOOLTIP(Shift+Enter換行，Enter跳一下欄位)
C(3,#FuncCode,4,s,$)
C(3,#FuncCode,5,s,$)
C(3,#Level,1,$,s)
@C(5,#ManagerCheck,N,C(4,#ManagerDesc,V(7),$),$)

!參考編號
!經辦時會跑到這裡的處理
#MRKEY=_,_,S
T(2,@MRKEY,#AcDate+-+#Factor+-+#CustNo)

#RPTFG=A,1,S
E(0,0)

<include src="ROM.icf::L0Rim.dec"/>
<include src="ROM.icf::L8R50.dec"/>
<include src="ROM.icf::L8R60.dec"/>
</sub>


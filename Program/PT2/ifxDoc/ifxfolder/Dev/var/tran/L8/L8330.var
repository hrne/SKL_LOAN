!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=2;screen.width=[250,900];printer.width=[20,20,20,20];order=1;">
[
["[L8330]                                 (451)前置調解延期繳款資料"],
[#<->#,"#FdTranKey+交易代碼",#TranKey,#TranKeyX],
[#<->#,"#FdCustId+債務人IDN",#CustId,#CustIdX],
[#<->#,"#FdSubmitKey+報送單位代號",#SubmitKey,#SubmitKeyX],
["#FdApplyDate+調解申請日",#ApplyDate],
[#<->#,"#FdCourtCode+受理調解機構代號",#CourtCode,#CourtCodeX],
["#FdDelayCode+延期繳款原因",#DelayCode,#DelayCodeX],
[#<->#,"#FdDelayYM+延期繳款年月",#DelayYM],
["#FdOutJcicTxtDate+轉JCIC文字檔日期",#OutJcicTxtDate],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
#FdTranKey=X,2,L
#FdCustId=X,2,L
#FdSubmitKey=X,2,L
#FdApplyDate=X,2,L
#FdCourtCode=X,2,L
#FdDelayCode=X,2,L
#FdDelayYM=X,2,L
#FdOutJcicTxtDate=X,2,L
!---------- 日期處理 ----------
#VIEW_SYSDATE=X,8,S
D(7,2,#SYSDATE,0,0)
T(1,#VIEW_SYSDATE,2,7)
E(0,1)ASGN(AGAIN$)
!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>
<include src="ROM.icf::L8R01.dec"/>


#CDDEF0001=x,20,S
T(3,CdCode.TranKey)
#TranKeyHelp=x,1024,S

!DelayCode
#CODEF0002=x,20,S
T(3,CdCode.DelayCode)
#DelayCodeHelp=x,2024,S

! CourtCode
#CODEF0003=x,20,S
T(3,CdCode.CourtCode)
#CourtCodeHelp=x,1024,S


#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF0001,#CODEF0002,#CODEF0003)
R(1,XXR99)
CALL(#HelpProc)
T(2,@TranKeyHelp,#HelpDesc1)
T(2,@DelayCodeHelp,#HelpDesc2)
T(2,@CourtCodeHelp,#HelpDesc3)



#CHAIN=A,1,S
T(4,CHAIN$)
C(3,#CHAIN,0,V(P,本交易為連動交易，請由L8030-Jcic451連動進入),$)
T(F,@TranKey,0)T(F,@ApplyDate,0)T(F,@CourtCode,0)T(F,@DelayCode,0)T(F,@DelayYM,0)T(F,@SubmitKey,0)
#AutoReFresh=A,1,S
C(3,#CHAIN,1,$,S)
E(0,1)
ASGN(CAUTO$)

![連動交易]
#NTXBUF=X,138,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@TranKey_Tmp,#NTXBUF,1,1)
T(1,@TranKey,#NTXBUF,2,1)

! 新增進入
#Deal_New=X,1,S
C(5,#TranKey_Tmp,1,$,S)
T(1,@CustId,#NTXBUF,3,10)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(F,@ApplyDate,1)T(F,@CourtCode,1)T(F,@DelayCode,1)T(F,@DelayYM,1)T(F,@SubmitKey,1)
K(MODIFYBUTTON,新增)
T(2,@FdApplyDate,*)
T(2,@FdCourtCode,*)
T(2,@FdDelayCode,*)
T(2,@FdDelayYM,*)
T(2,@FdSubmitKey,*)
! 異動進入
#Deal_Update=X,1,S
C(5,#TranKey_Tmp,2,$,S)
T(F,@TranKey,1)T(F,@DelayCode,1)
T(1,@Ukey,#NTXBUF,3,32)
K(MODIFYBUTTON,異動)
T(2,@FdTranKey,*)
T(2,@FdDelayCode,*)
T(3,@TranKeyHelp,C:異動;D:刪除))

! 查詢進入
#Deal_Search=X,1,S
C(5,#TranKey_Tmp,5,$,S)
T(1,@Ukey,#NTXBUF,3,32)
K(NOBUTTON,CLOSEY)

! 刪除進入
#Deal_Delete=X,1,S
C(5,#TranKey_Tmp,4,$,S)
T(1,@Ukey,#NTXBUF,3,32)
K(MODIFYBUTTON,刪除)

! 修改進入
#Deal_Revise=X,1,S
C(5,#TranKey_Tmp,7,$,S)
T(1,@Ukey,#NTXBUF,3,32)
T(F,@TranKey,1)T(F,@DelayCode,1)
K(MODIFYBUTTON,修改)
T(2,@FdTranKey,*)
T(2,@FdDelayCode,*)
T(3,@TranKeyHelp,A:新增;C:異動;D:刪除))


! 預設值
#Default=X,1,S
T(2,@SubmitKey,458)
! Rim L8R38.java
#RimJcicBankCode=X,3,S
#RimCourtCode=X,3,S
#RimL8r01=X,1,S
T(2,@RimJcicBankCode,#SubmitKey)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@SubmitKeyX,#L8r01JcicBankName)

<include src="ROM.icf::L8R47.dec"/>
#RimUkey=X,32,S
! Rim L8R47.java
#RimL8r47=X,1,S
C(5,#TranKey,A,S,$)
T(2,@RimUkey,#Ukey)
RESET_RIM(#RimL8r47,L8R47)
S(L8R47,1,#RimUkey)
R(1,L8R47)
T(2,@CustId,#L8r47CustId)
T(2,@SubmitKey,#L8r47SubmitKey)
E(0,@ApplyDate,#L8r47ApplyDate)
T(2,@CourtCode,#L8r47CourtCode)
E(0,@DelayYM,#L8r47DelayYM)
T(2,@DelayCode,#L8r47DelayCode)
T(2,@OutJcicTxtDate,#L8r47OutJcicTxtDate)
![下拉選單的值]
T(2,@TranKey,#L8r47TranKey)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(H,@CourtCodeX,#CourtCode,#CourtCodeHelp)



! 調Rim相關 結束

!---------- AP TEXT AREA Variables ----------
![交易代碼]
#TranKey_Tmp=X,1,S
#TranKey=X,1,I
C(5,#TranKey_Tmp,1,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
HELP(#TranKeyHelp)
@T(3,@TranKeyX,)
V(H,#TranKeyHelp)
C(5,#TranKey,C,K(MODIFYBUTTON,異動)T(F,@DelayCode,1)T(2,@FdDelayCode,*),
C(5,#TranKey,D,K(MODIFYBUTTON,刪除)T(F,@DelayCode,0)T(2,@FdDelayCode, ),$))

!CALL()
C(5,#TranKey_Tmp,7,K(MODIFYBUTTON,修改),s)
IF(#TranKey==A,K(MODIFYBUTTON,修改)
T(F,@TranKey,1)T(F,@DelayCode,1)T(2,@FdDelayCode,*),
IF(#TranKey==C,K(MODIFYBUTTON,修改)
T(F,@TranKey,1)T(F,@DelayCode,1)T(2,@FdDelayCode,*),
IF(#TranKey==D,K(MODIFYBUTTON,修改)
T(F,@TranKey,1),$)


#TranKeyX=X,4,L
IF(#TranKey_Tmp==1||#TranKey_Tmp==7||#TranKey_Tmp==2,$,s)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
T(H,#TranKey,#TranKeyHelp)


![債務人IDN]
#CustId=X,10,L

#RimCustId=X,10,S
<include src="ROM.icf::L8R57.dec"/>
<include src="ROM.icf::L8R59.dec"/>
T(2,@RimCustId,#CustId)
! 檢核該統編是否存在於客戶檔
#RimL8R57=X,1,S
RESET_RIM(#RimL8R57,L8R57)
S(L8R57,1,#RimCustId)
R(1,L8R57)
T(2,@CustIdX,#L8R57CustName)

#CustIdX=x,100,L

![債權金融機構代號]
#SubmitKey=X,3,I
C(5,#TranKey_Tmp,7,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
@T(3,@SubmitKeyX,)
V(7)

#SubmitKeyX=X,80,L
T(2,@RimJcicBankCode,#SubmitKey)
RESET_RIM(#SubmitKeyX,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@SubmitKeyX,#L8r01JcicBankName)


!調解申請日
#ApplyDate=D,7,I
C(5,#TranKey_Tmp,7,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
D(8)
@V(7)
A(DATE,0,#ApplyDate)

!受理調解機構代號-承審法院代碼OR郵遞區號
#CourtCode=X,3,I
C(5,#TranKey_Tmp,7,s,$)
C(5,#TranKey_Tmp,2,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
HELP(#CourtCodeHelp)
@T(3,@CourtCodeX,)
V(7)


#CourtCodeX=X,80,L
T(2,@RimCourtCode,#CourtCode)
RESET_RIM(#CourtCodeX,L8R59)
S(L8R59,1,#RimCourtCode)
R(1,L8R59)
T(2,@CourtCodeX,#L8R59CourtCodeX)

!延期繳款原因

#DelayCode=X,1,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,D,T(2,@DelayCode,#L8r47DelayCode),$)
C(5,#TranKey,D,s,$)
HELP(#DelayCodeHelp)
@T(3,@DelayCodeX,)
V(H,#DelayCodeHelp)
#DelayCodeX=X,40,L
T(H,#DelayCode,#DelayCodeHelp)

#tDelayYM=X,6,S
T(1,#tDelayYM,#ApplyDate,1,5)

!延期繳款年月
#DelayYM=A,5,I
C(5,#TranKey_Tmp,7,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
C(5,#TranKey,D,s,$)
@V(7)A(YM,1,#DelayYM)
IF(#DelayYM>0,$,V(P,延期繳款年月不可小於零))
IF(#tDelayYM<=#DelayYM,$,V(P,延期繳款年月不得小於調解申請日))



!轉JCIC文字檔日期
#OutJcicTxtDate=D,7,L

! 流水號
#Ukey=X,32,S

!刪除提醒
IF(#TranKey_Tmp == 4,ALERT(交易代碼為"A"時，刪除資料視為實際刪除),$) 


! !<儲存>
! #BUF1=X,10,S
! T(2,#CUSTID)
! #BIND1=X,1,S
! BIND(#BTN1,click, {cmd=CHAIN; ntxcd=#CHAINCD; ntxbuf=#BUF1})
#RPTFG=X,1,S
E(0,0) 
</sub>


!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=3;screen.width=[250,900];printer.width=[20,20,20,20];order=1;">
[
["[L8314]                                 (53)同意報送例外處理檔案"],
[#<->#,"#FdTranKey+交易代碼",#TranKey,#TranKeyX],
[#<->#,"#FdCustId+債務人IDN",#CustId,#CustIdX],
[#<->#,"#FdSubmitKey+報送單位代號",#SubmitKey,#SubmitKeyX],
[#<->#,"#FdRcDate+協商申請日",#RcDate],
[#<->#,"#FdMaxMainCode+最大債權金融機構代號",#MaxMainCode,#MaxMainCodeX],
[#<->#,"#FdAgreeSend+是否同意報送例外處理檔案格式",#AgreeSend,#AgreeSendX],
[#<->#,"#FdAgreeSendData1+同意補報送檔案格式資料別1",#AgreeSendData1,#AgreeSendDataX1],
[#<->#,"#FdAgreeSendData2+同意補報送檔案格式資料別2",#AgreeSendData2,#AgreeSendDataX2],
[#<->#,"#FdChangePayDate+申請變更還款方案日",#ChangePayDate],
[#<->#,"#FdOutJcicTxtDate+轉JCIC文字檔日期",#OutJcicTxtDate],
]

#FdTranKey=X,2,L
#FdCustId=X,2,L
#FdSubmitKey=X,2,L
#FdRcDate=X,2,L
#FdMaxMainCode=X,2,L
#FdAgreeSend=X,2,L
#FdAgreeSendData1=X,2,L
#FdAgreeSendData2=X,2,L
#FdChangePayDate=X,2,L
#FdOutJcicTxtDate=X,2,L

!---------- AP Button AREA ----------
![Buttom資料查詢]
!---------- 日期處理 ----------
#VIEW_SYSDATE=X,8,S
D(7,2,#SYSDATE,0,0)
T(1,#VIEW_SYSDATE,2,7)
E(0,1)ASGN(AGAIN$)
!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>
<include src="ROM.icf::L8R01.dec"/>

#CDDEF0001=x,20,S
T(3,CdCode.TranKey)
#TranKeyHelp=x,1024,S

#CDDEF0002=x,20,S
T(3,CdCode.YesNo)
#YesNoHelp=x,1024,S

! DataCode
#DataCodeHelp=x,114,S
T(3,42:回報無擔保債權金額資料;43:回報有擔保債權金額資料;61:回報協商剩餘債權金額資料)

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF0001,#CDDEF0002)
R(1,XXR99)
CALL(#HelpProc)
T(2,@TranKeyHelp,#HelpDesc1)
T(2,@YesNoHelp,#HelpDesc2)

#CHAIN=A,1,S
T(4,CHAIN$)
C(3,#CHAIN,0,V(P,本交易為連動交易，請由L8030-Jcic53連動進入),$)
T(F,@TranKey,0)
T(F,@SubmitKey,0)
T(F,@CustId,0)
T(F,@RcDate,0)
T(F,@MaxMainCode,0)
T(F,@AgreeSend,0)
T(F,@AgreeSendData1,0)
T(F,@AgreeSendData2,0)
T(F,@ChangePayDate,0)
#AutoReFresh=A,1,S
C(3,#CHAIN,1,$,S)
E(0,1)
ASGN(CAUTO$)

![連動交易]
#NTXBUF=X,138,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@TranKey_Tmp,#NTXBUF,1,1)
T(1,@TranKey,#NTXBUF,2,1)

! 新增進入
#Deal_New=X,1,S
C(5,#TranKey_Tmp,1,$,S)
T(1,@CustId,#NTXBUF,3,10)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(F,@SubmitKey,1)
T(F,@CustId,1)
T(F,@RcDate,1)
T(F,@MaxMainCode,1)
T(F,@AgreeSend,1)
T(F,@AgreeSendData1,1)
T(F,@AgreeSendData2,1)
T(F,@ChangePayDate,1)
K(MODIFYBUTTON,新增)
T(2,@FdRcDate,*)
T(2,@FdMaxMainCode,*)
T(2,@FdAgreeSend,*)
T(2,@FdAgreeSendData1,*)
T(2,@FdChangePayDate,*)

! 異動進入
#Deal_Update=X,1,S
C(5,#TranKey_Tmp,2,$,S)
T(1,@Ukey,#NTXBUF,3,32)
K(MODIFYBUTTON,異動)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(F,@AgreeSend,1)
T(F,@AgreeSendData1,1)
T(F,@AgreeSendData2,1)
T(F,@ChangePayDate,1)
T(2,@FdAgreeSend,*)
T(2,@FdAgreeSendData1,*)
T(2,@FdChangePayDate,*)

! 查詢進入
#Deal_Search=X,1,S
C(5,#TranKey_Tmp,5,$,S)
T(1,@Ukey,#NTXBUF,3,32)
K(NOBUTTON,CLOSEY)
! 刪除進入
#Deal_Delete=X,1,S
C(5,#TranKey_Tmp,4,$,S)
T(1,@Ukey,#NTXBUF,3,32)
K(MODIFYBUTTON,刪除)
E(0,@FUNCIND,4)
! 修改進入
#Deal_Revise=X,1,S
C(5,#TranKey_Tmp,7,$,S)
T(1,@Ukey,#NTXBUF,3,32)
T(F,@TranKey,1)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(F,@AgreeSend,1)
T(F,@AgreeSendData1,1)
T(F,@AgreeSendData2,1)
T(F,@ChangePayDate,1)
T(2,@FdAgreeSend,*)
T(2,@FdAgreeSendData1,*)
T(2,@FdChangePayDate,*)
K(MODIFYBUTTON,修改)
T(3,@TranKeyHelp,A:新增;C:異動))

! 預設值
#Default=X,1,S
T(2,@SubmitKey,458)

#RimJcicBankCode=X,3,S
#RimL8r01=X,1,S
T(2,@RimJcicBankCode,#SubmitKey)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@SubmitKeyX,#L8r01JcicBankName)

! [調Rim相關]
! 定義Rom欄位名稱、型態、長度
<include src="ROM.icf::L8R25.dec"/>
#RimUkey=X,32,S
#RimL8r25=X,1,S
C(5,#TranKey,A,S,$)
T(2,@RimUkey,#Ukey)
RESET_RIM(#RimL8r25,L8R25)
S(L8R25,1,#RimUkey)
R(1,L8R25)
T(2,@CustId,#L8r25CustId)
T(2,@SubmitKey,#L8r25SubmitKey)
E(0,@RcDate,#L8r25RcDate)
T(2,@MaxMainCode,#L8r25MaxMainCode)
T(2,@AgreeSend,#L8r25AgreeSend)
T(2,@AgreeSendData1,#L8r25AgreeSendData1)
T(2,@AgreeSendData2,#L8r25AgreeSendData2)
E(0,@ChangePayDate,#L8r25ChangePayDate)
E(0,@OutJcicTxtDate,#L8r25OutJcicTxtDate)
T(H,@AgreeSendX,#AgreeSend,#YesNoHelp)
#Space1=X,1,S
C(5,#TranKey,A,S,$)
T(2,@RimJcicBankCode,#MaxMainCode)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@MaxMainCodeX,#L8r01JcicBankName)
![下拉選單的值]
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
#Space=X,1,S
IF(#TranKey_Tmp == "5" || #TranKey_Tmp=="4",$,S)
T(2,@TranKey,#L8r25TranKey)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(2,@RimJcicBankCode,#MaxMainCode)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@MaxMainCodeX,#L8r01JcicBankName)
! 調Rim相關 結束

![交易代碼]
#TranKey_Tmp=X,1,S
#TranKey=X,1,I
IF(#TranKey_Tmp==7,$,s)
C(5,#TranKey_Tmp,1,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
HELP(#TranKeyHelp)
@V(H,#TranKeyHelp)

#TranKeyX=X,4,L
IF(#TranKey_Tmp==7,$,s)
C(5,#TranKey_Tmp,1,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$) 
C(5,#TranKey_Tmp,2,s,$)
T(H,#TranKey,#TranKeyHelp)

![債務人IDN]
#CustId=X,10,L
#RimCustId=X,10,S
<include src="ROM.icf::L8R57.dec"/>
T(2,@RimCustId,#CustId)
! 檢核該統編是否存在於客戶檔
#RimL8R57=X,1,S
RESET_RIM(#RimL8R57,L8R57)
S(L8R57,1,#RimCustId)
R(1,L8R57)
T(2,@CustIdX,#L8R57CustName)

#CustIdX=x,100,L

![報送單位代號]
#SubmitKey=X,3,L
#SubmitKeyX=X,80,L


![協商申請日]
#RcDate=D,7,I
C(5,#TranKey_Tmp,7,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
D(8)
@V(7)A(DATE,0,#RcDate)

![最大債權金融機構代號]
#MaxMainCode=X,3,I
C(5,#TranKey_Tmp,7,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
@T(3,@MaxMainCodeX,)
V(7)V(NL)


#MaxMainCodeX=X,80,L
T(2,@RimJcicBankCode,#MaxMainCode)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@MaxMainCodeX,#L8r01JcicBankName)

!是否同意報送例外處理檔案格式
#AgreeSend=X,1,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
HELP(#YesNoHelp)
@T(3,@AgreeSendX,)
V(H,#YesNoHelp)
C(5,#AgreeSend,Y,T(F,@AgreeSendData1,1)T(F,@AgreeSendData2,1)T(2,@FdAgreeSendData1,*),
T(2,@FdAgreeSendData1, )T(F,@AgreeSendData1,0)T(F,@AgreeSendData2,0))

#AgreeSendX=X,2,L
T(H,#AgreeSend,#YesNoHelp)
! 同意報送檔案格式資料別2
#AgreeSendData1=X,2,I
IF(#AgreeSend=="Y",$,S)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
HELP(#DataCodeHelp)
@T(3,@AgreeSendDataX1,)
V(H,#DataCodeHelp)


! 同意報送檔案格式資料別名稱2
#AgreeSendDataX1=X,40,L
C(4,#AgreeSendData1,S,$)
T(H,#AgreeSendData1,#DataCodeHelp)


! 同意報送檔案格式資料別2
#AgreeSendData2=X,2,L
IF(#AgreeSend=="Y",$,S)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#AgreeSendData1,42,T(2,@AgreeSendData2,43),$)
C(5,#AgreeSendData1,43,T(2,@AgreeSendData2,42),$)
C(5,#AgreeSendData1,61,T(2,@AgreeSendData2, ),$)
@T(3,@AgreeSendDataX2,)

! 同意報送檔案格式資料別名稱2
#AgreeSendDataX2=X,40,L
C(4,#AgreeSendData2,S,$)
T(H,#AgreeSendData2,#DataCodeHelp)



!申請變更還款方案日
#ChangePayDate=D,7,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
D(8)
@V(7)A(DATE,0,#ChangePayDate)

![轉JCIC文字檔日期]
#OutJcicTxtDate=D,7,L

! 流水號
#Ukey=X,32,S

!刪除提醒
IF(#TranKey_Tmp == 4,ALERT(交易代碼為"A"時，刪除資料視為實際刪除),$) 

! !<儲存>
! #BUF1=X,10,S
! T(2,#CUSTID)
! #BIND1=X,1,S
! BIND(#BTN1,click, {cmd=CHAIN; ntxcd=#CHAINCD; ntxbuf=#BUF1})
#RPTFG=X,1,S
E(0,0)
</sub>


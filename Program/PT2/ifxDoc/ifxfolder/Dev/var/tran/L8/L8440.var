!---------- TXFORM = L8440 ----------
<sub type="FORM" name="L8440">

#SCRTIL=_,_,S
T(3,[L8440] 消債條列JCIC報送清單)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------

#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

! 項目代號(TOTA-OCCURS)
!#ItemCode=X,6,S

!#DateX=X,8,S
!T(4,#SYSCDATE)
!T(1,@ReportDate,#DateX,2,7)

!#TranCode=X,8,S
!T(3,#OOTranCode)

!申報種類
#OOTranCode=X,100,S

!未報送筆數
#OONotRepoet=m,5,S
!ALERT(#OONotRepoet)

!本日報送筆數
#OOToDateReport=m,5,S

!傳送帶碼
#OOCtxCd=X,20,S

!按鈕處理
#OOBUF5=X,20,S
T(2,0+#OOCtxCd)


#OOTDDONE_BTN=X,5,S
BIND(grid,{cmd=CHAIN; ntxcd=L8030;text=資料; ntxbuf=#OOBUF5})


#OOChainTxCd=X,5,S

#OOBUF6=X,20,S

#OOTD_BTN=X,5,S
BIND(grid,{cmd=CHAIN; ntxcd=#OOChainTxCd;text=匯出; ntxbuf=#OOBUF6})



</sub>



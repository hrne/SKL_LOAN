!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,40];">
[
["[L8205]                                 產生疑似洗錢報表"],
["#FdType+@報表",#Type,#TypeX],
["#FdRecordDate+#DateItem",#DateStart,"~",#DateEnd],
["#FdActualRepayDate+@實際償還日期",#ActualRepayDate],
["#FdRepayDate+@預定償還日期",#RepayDate],
["#FdCustNo+@戶號",#CustNo],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
!#BUF1=X,42,S

#FdRecordDate=X,2,L
T(3,*)
#FdType=X,2,L
T(3,*)
#FdActualRepayDate=X,2,L
T(3,*)
#FdRepayDate=X,2,L
T(3,*)
#FdCustNo=X,2,L

#TypeHelp=x,200,S
T(3,1:洗錢樣態3合理性報表;2:洗錢樣態1、2合理性報表;3:洗錢樣態3未完成交易報表;4:洗錢樣態1、2未完成交易報表;5:洗錢訪談報表)

!---------- AP TEXT AREA Variables ----------
#CHAIN=A,1,S
T(4,CHAIN$)
IF(#CHAIN!=1,V(P,此為連動交易，請從交易:[L8922 疑似洗錢交易合理性查詢、L8923 疑似洗錢交易訪談記錄查詢]進入),$)

#NTXBUF=X,5,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)


! 功能 (2: 修改)
#FuncCode=A,1,S
E(0,2)
K(MODIFYBUTTON,確定)

#FUNCIND=_,_,S
E(0,#FuncCode)

!日曆日
#CDATE=D,8,S
T(4,CDATE$)

#CDATEX=D,7,S
T(1,#CDATE,2,7)
INVOKEJS(SHOW,p1,2,2,0)

!本營業日
#DATE=D,8,S
T(4,DATE$)

#Type=A,1,I
HELP(#TypeHelp)
@T(3,@TypeX,)
V(H,#TypeHelp)
C(3,#Type,5,INVOKEJS(SHOW,p1,2,2,0)INVOKEJS(SHOW,p1,3,5,1)T(2,@FdRecordDate,),INVOKEJS(SHOW,p1,2,2,1)INVOKEJS(SHOW,p1,3,5,0)T(2,@FdRecordDate,*))

#TypeX=X,20,L
T(H,#Type,#TypeHelp)

#DateItem=X,8,L
C(3,#Type,5,T(2,訪談日期),T(2,入帳日期))




#DateStart=D,7,I
T(1,#DATE,2,7)
D(8)
@
V(7)
A(DATE,0,#DateStart)
C(4,#DateStart,S,V(5,0010101,#CDATEX))

#DateEnd=D,7,I
C(4,#DateStart,S,$)
E(0,#DateStart)
D(8)
@
V(7)
A(DATE,0,#DateEnd)
V(5,#DateStart,#CDATEX)

#ActualRepayDate=D,7,I
C(3,#Type,5,$,S)
@A(DATE,1,#ActualRepayDate)
V(7)

#RepayDate=D,7,I
C(3,#Type,5,$,S)
@A(DATE,1,#RepayDate)
V(7)

#CustNo=A,7,I
C(3,#Type,5,$,S)


#RPTFG=A,1,S
E(0,0)

</sub>


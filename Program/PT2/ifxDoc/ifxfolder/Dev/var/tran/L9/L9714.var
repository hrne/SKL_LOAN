!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L9714]                                 繳息證明單"],
["#R1+@戶    號",#CustNo],
["#R2+@額    度",#FacmNo, " 0:全部 "],
["#R3+@用    途",#UsageCode,#UsageCodeX],
["#R4+@資料年月區間",#sYearMonth,"～",#eYearMonth],
["#FdMsg+@注意事項",#Msg],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
! 功能 必須輸入(1: 新增;2: 修改;4: 刪除;5: 查詢)
#FUNCIND=_,_,S
E(0,5)
<include src="COM.icf::CheckAuth.rtn"/>

! 必輸欄位
##loop{times:4,i:1}
#R{i}=X,1,L
INVOKEJS(SHOW,#R{i},0)
K(LIGHT,#R{i},TXTRED,0)
##end

! 永遠為必輸欄位
#ALWAYS_HIGHLIGHT=@,1,S
T(3,@R1,*)
T(3,@R2,)
T(3,@R3,)
T(3,@R4,*)

#FdMsg=X,1,L
#Msg=x,20.100,L
K(LIGHT,#Msg,TXTRED,0)
INVOKEJS(SHOW,p1,5,5,0)

!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>

#CDCODEUsageCode=x,20,S
T(3,CdCode.UsageCode)

#UsageCodeHelpRim=X,1,S
RESET_RIM(#UsageCodeHelpRim,XXR99)
S(XXR99,01,#CDCODEUsageCode)
R(1,XXR99)
CALL(#HelpProc)
T(2,@UsageCodeHelp,00:全部;+#HelpDesc1)

#UsageCodeHelp=x,300,S

#CHAIN=A,1,S
T(4,CHAIN$)

#INIT=X,1,S
CALL(#ALWAYS_HIGHLIGHT)

#DT1=D,8,S
T(4,DATE$)

#DT2=A,5,S
T(1,#DT1,2,5)

!戶號 必須輸入
#CustNo=A,7,I
@V(2,0)

!戶號 必須輸入
#FacmNo=A,3,I 

! 用途
#UsageCode=A,2,I 
HELP(#UsageCodeHelp)
@T(3,@UsageCodeX,)
V(H,#UsageCodeHelp)

#UsageCodeX=X,20,L
T(H,#UsageCode,#UsageCodeHelp)   


!會計日期 必須輸入，不可大於本營業日
#sYearMonth=A,5,I
T(2,#DT2)
@V(7)
V(5,0,#DT2)
A(YM,1,#sYearMonth)

#eYearMonth=A,5,I
T(2,#DT2)
@V(7)
V(5,#sYearMonth,#DT2)
A(YM,1,#eYearMonth)

<include src="ROM.icf::L1R20.dec"/>

#FormNo=x,10,S
T(3,L9714)

#RimFlag=A,1,S

#notPrintLetter=X,1,S
#nLetter=X,1,S

#L1R20Rim2=X,1,S
C(3,#CustNo,0,S,$)
RESET_RIM(#L1R20Rim2,L1R20)
E(0,@RimFlag,0)
S(L1R20,01,#CustNo,#FacmNo,#FormNo,#RimFlag)
R(1,L1R20)
T(2,@Msg,#L1R20Msg)
T(2,@nLetter,#notPrintLetter)

#ShowMsg=A,1,S
C(3,#CustNo,0,E(0,0),C(4,#Msg,E(0,0),E(0,1)))
C(5,#nLetter,Y,K(NOBUTTON,CLOSEY),K(NOBUTTON,SHOWY))
INVOKEJS(SHOW,p1,5,5,#ShowMsg)


</sub>


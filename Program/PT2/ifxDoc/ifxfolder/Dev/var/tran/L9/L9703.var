!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L9703]                                 滯繳客戶明細表"],
["#R2+@戶號",#CustNo,"-",#FacmNo],
["#R3+@滯繳條件",#UnpaidCond,#UnpaidCondx],
["#R4+@滯繳期數",#UnpaidTermSt,"~",#UnpaidTermEd],
["#R5+@滯繳日數",#UnpaidDaySt,"~",#UnpaidDayEd],
["#R6+@繳款方式",#RepayType,#RepayTypex],
["#R7+@戶別",#CustType,#CustTypex],
["#R8+@本金餘額",#PrinBalance,"元以上"],
["#R9+@會計日",#AcDate],
["#R10+@入帳日",#EntryDate],
["#R11+@最近繳款日",#PayIntDateSt,"~",#PayIntDateEd],
["#FdMsg+@注意事項",#Msg],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
! 功能 必須輸入(1: 新增;2: 修改;4: 刪除;5: 查詢)
#FUNCIND=_,_,S
E(0,5)
<include src="COM.icf::CheckAuth.rtn"/>

! 必輸欄位
##loop{times:11,i:1}
#R{i}=X,1,L
INVOKEJS(SHOW,#R{i},0)
K(LIGHT,#R{i},TXTRED,0)
##end

! 永遠為必輸欄位
#ALWAYS_HIGHLIGHT=@,1,S
T(3,@R1,*)
T(3,@R3,*)
T(3,@R6,*)
T(3,@R7,*)
T(3,@R9,*)
T(3,@R10,*)
T(3,@R11,*)

#FdMsg=X,1,L
#Msg=x,20.100,L
K(LIGHT,#Msg,TXTRED,0)
INVOKEJS(SHOW,p1,11,11,0)

! UnpaidCond=1 滯繳期數為必輸欄位,UnpaidCond=2 滯繳日數為必輸欄位
#HIGHLIGHT_AFTER_UnpaidCond=@,1,S
IF(#UnpaidCond==1,T(3,@R4,*),T(3,@R4, ))
IF(#UnpaidCond==1,INVOKEJS(SHOW,#R4,1),INVOKEJS(SHOW,#R4,0))
IF(#UnpaidCond==2,T(3,@R5,*),T(3,@R5, ))
IF(#UnpaidCond==2,INVOKEJS(SHOW,#R5,1),INVOKEJS(SHOW,#R5,0))





!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>
!UnpaidCond
#CDCODEUnpaidCond=x,20,S                  
T(3,CdCode.UnpaidCond)                  
                                      
#UnpaidCondHelp=x,50,S        

!RepayType1
#CDCODERepayType=x,20,S                  
T(3,CdCode.RepayType1)                  
                                        
#RepayType1Help=x,100,S        

!CustType
#CDCODECustType=x,20,S                  
T(3,CdCode.CustType)                  
                                         
#CustTypeHelp=x,50,S        

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDCODEUnpaidCond,#CDCODERepayType,#CDCODECustType)
R(1,XXR99)
CALL(#HelpProc)
T(2,@UnpaidCondHelp,#HelpDesc1)
T(2,@RepayType1Help,0:全部;+#HelpDesc2)
T(2,@CustTypeHelp,0:全部;+#HelpDesc3)


#CHAIN=A,1,S
T(4,CHAIN$)

#INIT=X,1,S
CALL(#ALWAYS_HIGHLIGHT)

#RimDate=D,8,S

#L4R32Rim=X,1,S
RESET_RIM(#L4R32Rim,L4R32)
E(0,@RimDate,DATE$)
S(L4R32,01,#RimDate)
R(1,L4R32)

#DT1=D,8,S
T(4,DATE$)

#DT2=D,7,S
T(1,#DT1,2,7)

#DT_YM=A,5,S
T(1,#DT1,2,5)

!會計日期 必須輸入，不可大於本營業日
!#AcctDate=D,7,I
!T(2,#DT2)
!@V(7)
!V(5,0,#DT2)
!A(DATE,0,#AcctDate)

!戶號
#CustNo=A,7,I

! 調Rim相關 開始
! 定義Rom欄位名稱、型態、長度
<include src="ROM.icf::L1R01.dec"/>

! 宣告Rim欲傳送的欄位1
! 欄位名稱即為後端在titaVo get值時的KEY
! 要注意欄為型態跟長度
#RimCustId=X,10,S

! 宣告Rim欲傳送的欄位2
#RimCustNo=A,7,S

#RimFunCd=A,1,S
E(0,5)

#RimL1r01B=X,1,S
C(3,#CustNo,0,S,$)
T(2,@RimCustId,)
E(0,@RimCustNo,#CustNo)
RESET_RIM(#RimL1r01B,L1R01)
S(L1R01,1,#RimCustId,#RimCustNo,#RimFunCd)
R(1,L1R01)

#checkL1r01B=X,1,S
C(3,#CustNo,0,S,$)

!額度
#FacmNo=A,3,I
C(3,#CustNo,0,S,$)

!滯繳條件
#UnpaidCond=A,1,I
HELP(#UnpaidCondHelp)
@T(3,@UnpaidCondx,)
V(H,#UnpaidCondHelp)
CALL(#HIGHLIGHT_AFTER_UnpaidCond)
IF(#UnpaidCond==1,INVOKEJS(SHOW,p1,4,4,0),INVOKEJS(SHOW,p1,3,3,0))
IF(#UnpaidCond==2,INVOKEJS(SHOW,p1,4,4,1),INVOKEJS(SHOW,p1,3,3,1))

#UnpaidCondx=X,8,L
T(H,#UnpaidCond,#UnpaidCondHelp)

!滯繳期數
!起
#UnpaidTermSt=A,2,I
C(3,#UnpaidCond,2,S,$)
E(0,1)
!止
#UnpaidTermEd=A,2,I
C(3,#UnpaidCond,2,S,$)
T(2,#UnpaidTermSt)
@V(5,#UnpaidTermSt,99)

!滯繳日數
!起
#UnpaidDaySt=A,3,I
C(3,#UnpaidCond,1,S,$)
E(0,1)
!止
#UnpaidDayEd=A,3,I
C(3,#UnpaidCond,1,S,$)
T(2,#UnpaidDaySt)
@V(5,#UnpaidDaySt,999)

!繳款方式
#RepayType=A,1,I
HELP(#RepayType1Help)
@T(3,@RepayTypex,)
V(H,#RepayType1Help)

#RepayTypex=X,8,L
T(H,#RepayType,#RepayType1Help)

!戶別
#CustType=A,1,I
HELP(#CustTypeHelp)
@T(3,@CustTypex,)
V(H,#CustTypeHelp)

#CustTypex=X,6,L
T(H,#CustType,#CustTypeHelp)

#PrinBalance=m,14,I

#AcDate=D,7,I
E(0,#L4R32Date1)
@A(DATE,0,#AcDate)
@V(7)
V(5,0,#DT2)

#EntryDate=D,7,I
E(0,#L4R32Date2)
@A(DATE,0,#EntryDate)
@V(7)
V(5,0,#DT2)


#PayIntDateSt=D,7,I
E(0,#L4R32Date7)
@A(DATE,0,#PayIntDateSt)
@V(7)
V(5,0,#DT2)

#PayIntDateEd=D,7,I
E(0,#L4R32Date7)
@A(DATE,0,#PayIntDateEd)
@V(7)
V(5,0,#DT2)
IF(#PayIntDateEd<#PayIntDateSt,V(P,起始日不可大於終止日),$)


<include src="ROM.icf::L1R20.dec"/>
<include src="ROM.icf::L4R32.dec"/>

#FormNo=x,10,S
T(3,L9703)

#RimFlag=A,1,S

#notPrintLetter=X,1,S
#nLetter=X,1,S

#L1R20Rim2=X,1,S
C(3,#CustNo,0,S,$)
RESET_RIM(#L1R20Rim2,L1R20)
E(0,@RimFlag,0)
S(L1R20,01,#CustNo,#FacmNo,#FormNo,#RimFlag)
R(1,L1R20)
T(2,@Msg,#L1R20Msg)
T(2,@nLetter,#notPrintLetter)

#ShowMsg=A,1,S
C(3,#CustNo,0,E(0,0),C(4,#Msg,E(0,0),E(0,1)))
C(5,#nLetter,Y,K(NOBUTTON,CLOSEY),K(NOBUTTON,SHOWY))
INVOKEJS(SHOW,p1,11,11,#ShowMsg)

</sub>


[
["[L6904]                                 日結彙計查詢"],
["#FdAcBookCode+帳冊別",#AcBookCode,#AcBookItem],
["#FdAcSubBookCode+區隔帳冊",#AcSubBookCode,#AcSubBookItem,"(空白:查詢全部)"],
["#FdBranchNo+單位別",#BranchNo],
["#FdCurrencyCode+幣別",#CurrencyCode],
["#FdAcNoCode+科子細目",#AcNoCode,"-",#AcSubCode,"-",#AcDtlCode,"@",#BTN1,#AcNoItem],
["#FdAcDate+會計日期",#AcDate],
["#FdInqType+彙計方式",#InqType,#InqTypeX]
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
#BTN1=X,15,I,ui:button;value:會計科目查詢;enable:1;tabbable:0
#BUF1=X,42,S
!ENABLE(#BTN1)ENABLE(#BTN2)ENABLE(#BTN3)

#Submit=X,4,S
K(MODIFYBUTTON,查詢)

!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>

!帳冊別
#CDCODEAcBookCode=x,20,S                  
T(3,CdCode.AcBookCode)
#ApHelp=x,1024,S

#SubBookHelp=x,1024,S
T(3,CdCode.AcSubBookCode)
#AcSubBookCodeHelp=x,1024,S

!彙計方式(0:全部彙計方式;1:彙總別;2:經辦別;3:整批批號;4:摘要代號;5:傳票批號;6:業務類別)
#CDCODEInqType=x,20,S                  
T(3,CdCode.InqType)
#InqTypeHelp=x,1024,S

! 彙總別
#CDCODESumNoType=x,20,S
T(3,CdCode.SumNo)
#SumNoTypeHelp=x,1024,S

! 彙總別
#SecNoType=x,20,S
T(3,CdCode.SecNo)
#SecNoTypeHelp=x,1024,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDCODEAcBookCode,#CDCODEInqType,#CDCODESumNoType,#SubBookHelp,#SecNoType)
R(1,XXR99)
CALL(#HelpProc)
T(2,@ApHelp,#HelpDesc1)
T(2,@InqTypeHelp,0:全部彙計方式;+#HelpDesc2)
T(2,@SumNoTypeHelp,#HelpDesc3)
T(2,@AcSubBookCodeHelp,#HelpDesc4)
T(2,@SecNoTypeHelp,#HelpDesc5)

#CHAIN=A,1,S
T(4,CHAIN$)
T(2,@RimTxCode,#TXCD)

#FdAcBookCode=X,2,L
#FdAcSubBookCode=X,2,L
#FdBranchNo=X,2,L
#FdCurrencyCode=X,2,L
#FdAcNoCode=X,2,L
#FdAcDate=X,2,L
#FdInqType=X,2,L

! 永遠為必輸欄位
#ALWAYS_HIGHLIGHT=@,1,S
T(3,@FdAcBookCode,*)
T(3,@FdAcDate,*)
T(3,@FdInqType,*)

#INIT=X,1,S
CALL(#ALWAYS_HIGHLIGHT)

#RPTFG=A,1,S
E(0,0)

!帳冊別
#AcBookCode=X,3,L
T(3,000)

#AcBookItem=X,20,L
T(3,全帳冊)

!區隔帳冊
#AcSubBookCode=X,3,I
C(3,#CHAIN,1,s,$)
HELP(#AcSubBookCodeHelp)
@T(3,@AcSubBookItem,)
C(4,#AcSubBookCode,s,V(H,#AcSubBookCodeHelp))

#AcSubBookItem=X,20,L
C(4,#AcSubBookCode,S,T(H,#AcSubBookCode,#AcSubBookCodeHelp))

!單位別
#BranchNo=A,4,L
T(2,#ACBRNO)

!幣別
#CurrencyCode=X,3,L
T(3,TWD)

#BUF1A=X,18,S

#BIND1A=X,1,S
BIND(#BTN1,click, {cmd=CHAIN; ntxcd=L6061; ntxbuf=#BUF1A;ntxbuf5=type<-1:
AcNoCode<-OOAcNoCode:
AcSubCode<-OOAcSubCode:
AcDtlCode<-OOAcDtlCode})

!科子細目
#AcNoCode=X,11,I

#AcSubCode=X,5,I
C(4,#AcNoCode,S,$)
C(4,#AcNoCode,S,$)
@C(4,#AcNoCode,S,$)

#AcDtlCode=X,2,I
C(4,#AcNoCode,S,$)
C(4,#AcNoCode,S,$)
@C(4,#AcNoCode,S,$)

#SendL6R23=X,1,S
T(3,@AcNoItem,)
C(4,#AcNoCode,S,$)
E(0,@RimFuncCode,5)
T(2,@RimAcNoCode,#AcNoCode)
T(2,@RimAcSubCode,#AcSubCode)
T(2,@RimAcDtlCode,#AcDtlCode)
RESET_RIM(#SendL6R23,L6R23)
S(L6R23,1,#RimFuncCode,#RimTxCode,#RimAcNoCode,#RimAcSubCode,#RimAcDtlCode)
R(1,L6R23)

#AcNoItem=X,80,L
T(2,#L6R23AcNoItem)
@C(4,#AcNoCode,S,$)

!會計日期 必須輸入
#DATE=D,8,S
T(4,DATE$)

#DT=D,7,S
T(1,#DATE,2,7)

#AcDate=D,7,I
T(2,#DT)
@V(7)A(4,0,#AcDate)
!IF(#AcDate > #DT,V(P,不可大於會計日),$)

!彙計方式
#InqType=A,1,I
HELP(#InqTypeHelp)
@T(3,@InqTypeX,)
V(H,#InqTypeHelp)
! C(3,#InqType,1,E(0,#RPTFG,1))

#InqTypeX=X,12,L
T(H,#InqType,#InqTypeHelp)

<include src="ROM.icf::L0Rim.dec"/>
<include src="ROM.icf::L6R23.dec"/>

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!---------- TXFORM = L6904 ----------
<sub type="FORM" name="L6904">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6904,$,S)

#SCRTIL=_,_,S
T(3,[L6904] 日結彙計)

#OTotalDCnt=m,14,L
#OTotalCCnt=m,14,L
#OTotalDAmt=m,14,L
#OTotalCAmt=m,14,L

#txt=x,30,L
C(3,#InqType,7,$,S)
T(2, 彙總傳票筆數以1筆計算)
!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)
#OOAcSubBookCode=X,3,S
!科目
#OOAcNoCode=X,11,S
!子目
#OOAcSubCode=X,5,S
!細目
#OOAcDtlCode=X,2,S
!科子細目
#OOAcNoCodeX=X,18,S
T(2,#OOAcNoCode+-+#OOAcSubCode+-+#OOAcDtlCode)

!科子細目名稱
#OOAcNoItem=X,80,S

!彙計方式為0時,此欄位為"空白"
!彙計方式為1時,此欄位為"彙總別" (單筆匯款、整批匯款、匯款轉帳、銀行扣款、支票兌現)
!彙計方式為2時,此欄位為"櫃員別"
!彙計方式為3時,此欄位為"整批批號"
!彙計方式為4時,此欄位為"摘要代號" ([0001]現金存入、[1416]ＡＴ自收)
!彙計方式為5時,此欄位為"傳票批號"
!彙計方式為6時,此欄位為"業務類別" (撥款匯款、支票繳款、債協、放款)
#OOInqData=X,6,S

#OOInqDataX=x,60,S

#OOSumNoX=X,20,S
C(2,#InqType,S,T(H,#OOInqData,#SumNoTypeHelp),T(2,#OOInqData+ +#OOInqDataX),T(2,#OOInqData),T(2,#OOInqData+ +#OOInqDataX),T(2,#OOInqData),T(H,#OOInqData,#SecNoTypeHelp),T(2,#OOInqData))

!借方筆數
#OODbCnt=X,10,L
!#OODbCntX=X,5,L
!IF(#OODbCnt==0,S,$)
!X(#OODbCnt)

!借方金額
#OODbAmt=m,14.2,S
#OODbAmt1=m,14,S
E(0,#OODbAmt)
#OODbAmtX=X,17,S
IF(#OODbAmt1==0,S,$)
X(#OODbAmt1)

!貸方筆數
#OOCrCnt=X,10,L
!#OOCrCntX=X,5,L
!IF(#OOCrCnt==0,S,$)
!X(#OOCrCnt)

!貸方金額
#OOCrAmt=m,14.2,S
#OOCrAmt1=m,14,S
E(0,#OOCrAmt)
#OOCrAmtX=X,17,S
IF(#OOCrAmt1==0,S,$)
X(#OOCrAmt1)



!明細
#OOBUF5=X,46,S
T(2,#AcBookCode+#AcSubBookCode+#BranchNo+#CurrencyCode+#OOAcNoCode+#OOAcSubCode+#OOAcDtlCode+#AcDate+#InqType+#OOInqData+0)

#OOBTN5=X,4,S
BIND(grid,{ cmd=CHAIN; ntxcd=L6905;text=明細;ntxbuf=#OOBUF5})

</sub>



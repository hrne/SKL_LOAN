!---------- TXFORM = L6909 ----------
<sub type="FORM" name="L6909">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6909,$,S)

#SCRTIL=_,_,S
T(3,[L6909] 暫收款查詢(依戶號))

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)
#Precision=N,1,S
E(0,0)
!入帳日期
#OOEntryDate=D,7,O
!會計日期
#OOAcDate=D,7,O
!暫收款額度
#OOTAVFacmNo=A,3,O
!交易別
#OODesc=X,20,O

!暫收借
#OOTAVDb=+m,14.2,O
#OOTAVDbX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#OOTAVDb,#OOTAVDbX)
P(R,#OOTAVDbX)
C(3,#OOTAVDb,0,S,$)

!暫收貸
#OOTAVCr=+m,14.2,O
#OOTAVCrX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#OOTAVCr,#OOTAVCrX)
P(R,#OOTAVCrX)
C(3,#OOTAVCr,0,S,$)

!暫收餘額
#OOTAVBal=+m,14.2,S
#OOTAVBalX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#OOTAVBal,#OOTAVBalX)
P(R,#OOTAVBalX)
! C(3,#OOTAVBal,0,S,$)

#OOAcFg=X,1,S

#OOTxNo=X,18,S
!交易明細
#OOBUF1=X,37,S
T(2,#TXCD+#CustNo+000+000+#OOAcDate)
#OOBTN1=X,1,S
C(5,#OOAcFg,Y,BIND(grid,{ cmd=CHAIN; ntxcd=L3005;text=交易明細;ntxbuf=#OOBUF1}),BIND(grid,cancel))


</sub>



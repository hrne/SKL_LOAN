[
["[L6909]                                 暫收款查詢(依戶號)"],
["#FdCustNo+戶號",#CustNo,#CustName],
["#FdFacmNo+額度編號",#FacmNo],
["#FdOverflow+全戶累溢收",#Overflow],
["#FdShortfall+全戶累短收",#Shortfall],
["#FdEntryDate+入帳日期",#EntryDateS,#EntryDateE],
["#FdSortCode+資料排序",#SortCode,#SortCodeX],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
!#BTN1=X,15,I,ui:button;value:會計科目查詢;enable:1;tabbable:0
#BUF1=X,42,S
!ENABLE(#BTN1)ENABLE(#BTN2)ENABLE(#BTN3)

#Submit=X,4,S
K(MODIFYBUTTON,查詢)

!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>


#SortCodeHELP=x,1024,S
T(3,1:入帳日期+額度;2:額度+入帳日期)

#CHAIN=A,1,S
T(4,CHAIN$)
T(2,@RimTxCode,#TXCD)

#NTXBUF=x,92,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,#NTXBUF,1,3)


! 必輸欄位
#FdCustNo=X,2,L
T(3,*)
! 其他欄位
#FdFacmNo=X,2,L
#FdOverflow=X,2,L
#FdShortfall=X,2,L
#FdEntryDate=X,2,L
#FdSortCode=X,2,L

#RPTFG=A,1,S
E(0,0)


!戶號
#CustNo=A,7,I

@V(2,0)
T(3,@CustName,)


#SendL2r07=X,1,S
T(3,@RimCustId, )
E(0,@RimCustNo,#CustNo)
RESET_RIM(#SendL2r07,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo)
R(1,L2R07)
T(2,@CustName,#L2r07CustName)


#CustName=x,100,L


#SendL3r06S=X,1,S
E(0,@RimFuncCode,3)
E(0,@RimCustNo,#CustNo)
E(0,@RimFacmNo,0)
E(0,@RimBormNo,0)
T(2,@RimCurrencyCode,)
E(0,@RimExtraRepay,0)
T(2,@RimIncludeIntFlag,)
E(0,@RimRepayTerms,0)
E(0,@RimRepayType,0)
E(0,@RimEntryDate,0)
E(0,@RimCloseBreachAmt,0)
RESET_RIM(#SendL3r06S,L3R06)
S(L3R06,01,#RimTxCode,#RimFKey,#RimFuncCode,#RimCustNo,#RimFacmNo,#RimBormNo,#RimCurrencyCode,#RimExtraRepay,#RimIncludeIntFlag,#RimRepayTerms,#RimRepayType,#RimEntryDate,#RimCloseBreachAmt)
R(1,L3R06)
E(0,@Shortfall,#L3r06Shortfall)
E(0,@Overflow,#L3r06Excessive)


!額度編號
#FacmNo=A,3,I

!全戶累溢收(rim)
#Overflow=m,14,L
!全戶累短收
#Shortfall=m,14,L

!入帳日期 本月1日~本日
#EntryDateS=D,7,I
E(0,#SYSDATE/100)
E(0,#EntryDateS*100+1)
@A(DATE,0,#EntryDateS)

#EntryDateE=D,7,I
E(0,#SYSDATE)
@
V(7)
A(DATE,0,#EntryDateE)

!資料排序:1.入帳日期+額度(預設)
!         2.額度+入帳日期
#SortCode=X,1,I
C(4,#SortCode,T(3,1),$)
HELP(#SortCodeHELP)
@T(3,@SortCodeX,)
V(H,#SortCodeHELP)

#SortCodeX=X,20,L
T(H,#SortCode,#SortCodeHELP)




<include src="ROM.icf::L0Rim.dec"/>
<include src="ROM.icf::L2Rim.dec"/>
<include src="ROM.icf::L2R07.dec"/>
<include src="ROM.icf::L3R06.dec"/>
<include src="ROM.icf::L3Rim.dec"/>

</sub>


!#PRECISION=N,1,S
!T(A,#CurrencyCode,CURR,CURDEF,ENAME,PRECISION)

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

#L6908Flag=X,1,S

! 科子細目
#OOAcNoCode=X,11,S
#OOAcSubCode=X,5,S
#OOAcDtlCode=X,2,S

! 戶號-額度編號
#OOCustNo=A,7,S
#OOFacmNo=A,3,S
#OOCustNoX=X,11,L
T(2,#OOCustNo+-+#OOFacmNo)

! 銷帳科目
#OOAcctCode=X,3,L
#OOAcctItem=x,40,L

#OOAcctCodeX=x,50,L
T(2,#OOAcctCode+ +#OOAcctItem)

! 銷帳編號
#OORvNo=x,30,L
! 起帳日期
#OOOpenAcDate=D,7,L

! 起帳金額
#OORvAmt=m,14.2,S
#OORvAmt1=m,14,S
E(0,#OORvAmt)
#OORvAmtX=X,17,L
IF(#OORvAmt1==0,S,$)
X(#OORvAmt1)

! 最後交易日
#OOLastTxDate=D,7,L
! 交易序號
#OOTxtNo=X,18,L
! 未銷餘額
#OORvBal=m,14.2,S
#OORvBal1=m,14,S
E(0,#OORvBal)
#OORvBalX=X,17,L
IF(#OORvBal1==0,S,$)
X(#OORvBal1)

! 戶號合計
#OO_SUM=m,14.2,S
#OO_SUM1=m,14,S
E(0,#OO_SUM)
#OO_SUMX=X,17,L
IF(#OO_SUM1==0,S,$)
X(#OO_SUM1)


#OOAcBookCode=X,3,L
#OOAcSubBookCode=X,3,L




!明細
#OOBUF5A=x,92,S
T(2,#AcBookCode+#OOAcSubBookCode+#BranchNo+#CurrencyCode+#OOAcNoCode+#OOAcSubCode+#OOAcDtlCode+#OOCustNo+#OOFacmNo+000+#OORvNo+#OOOpenAcDate+#OOLastTxDate+#OOAcctCode)

#OOBUF5B=x,92,S
T(2,#OOCustNo+000+0000000+0000000+#OOOpenAcDate+#SYSDATE007+0+0)
!連動L3005
#OOBUF5C=X,50,S
T(2,#TXCD+#OOCustNo+000+000+#OOLastTxDate+#OOTxtNo+9)

#OOBTN5=X,1,S
C(4,#L6908Flag,BIND(grid,cancel),
            C(5,#OOAcctCode,TAV,BIND(grid,{ cmd=CHAIN; ntxcd=L6909; ntxbuf=#OOBUF5B}),
                                C(5,#L6908Flag,Y,BIND(grid,{ cmd=CHAIN; ntxcd=L6908; ntxbuf=#OOBUF5A}),
                                                BIND(grid,{ cmd=CHAIN; ntxcd=L3005;text=交易;ntxbuf=#OOBUF5C}))))


</sub>



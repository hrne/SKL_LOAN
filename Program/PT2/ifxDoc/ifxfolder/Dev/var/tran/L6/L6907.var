!---------- AP Button AREA ----------
![Buttom資料查詢]
#BTN1=X,15,I,ui:button;value:會計科目查詢;enable:1;tabbable:0
#BUF1=X,42,S
!ENABLE(#BTN1)ENABLE(#BTN2)ENABLE(#BTN3)

#Submit=X,4,S
K(MODIFYBUTTON,查詢)
!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>
#ApHelp=x,1024,S
T(3,CdCode.AcBookCode)
#AcBookCodeHelp=x,1024,S

#SubBookHelp=x,1024,S
T(3,CdCode.AcSubBookCode)
#AcSubBookCodeHelp=x,1024,S

#AcctHelp=x,1024,S
T(3,CdCode.AcctCode)
#AcctCodeHelp=x,1024,S

S(XXR99,01,#ApHelp,#AcctHelp,#SubBookHelp)
R(1,XXR99)
CALL(#HelpProc)
T(2,#AcBookCodeHelp,#HelpDesc1)
T(2,#AcctCodeHelp,#HelpDesc2)
T(HELP,@AcctCodeHelp,#AcctCodeHelp,-,310;320;330;340;990)
T(2,#AcSubBookCodeHelp,#HelpDesc3)

!銷帳記號
#ClsFgH=x,50,S
T(3,0:未銷 ;1:已銷 ;2:全部)


#CHAIN=A,1,S
T(4,CHAIN$)
T(2,@RimTxCode,#TXCD)

#NTXBUF5=X,10,S
C(3,#CHAIN,1,T(4,NTXBUF5$),S)

#NTXBUF=X,102,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@AcBookCode,#NTXBUF,1,3)
T(1,@AcSubBookCode,#NTXBUF,4,3)
T(1,@BranchNo,#NTXBUF,7,4)
T(1,@CurrencyCode,#NTXBUF,11,3)
T(1,@AcNoCode,#NTXBUF,14,11)
T(1,@AcSubCode,#NTXBUF,25,5)
T(1,@AcDtlCode,#NTXBUF,30,2)
T(1,@CustNo,#NTXBUF,32,7)
T(1,@FacmNo,#NTXBUF,39,3)
T(1,@AcctCode,#NTXBUF,42,3)
T(1,@TmpTxCode,#NTXBUF,45,5)
T(1,@TmpFacmNo,#NTXBUF,50,3)
T(1,@TmpFacmNoX,#NTXBUF,53,50)

#INIT=X,1,S
T(3,@FdAcBookCode,*)

#RPTFG=A,1,S
E(0,0)

#RPTFG2=A,1,S
C(3,#RPTFG,0,$,S)
C(4,#NTXBUF5,T(3,1),T(3,2))

#FdAcBookCode=X,2,L
#FdAcSubBookCode=X,2,L



!帳冊別
#AcBookCode=X,3,L
T(3,000)

#AcBookCodeX=X,20,L
T(3,全帳冊)

!區隔帳冊
#AcSubBookCode=X,3,I
C(3,#CHAIN,1,s,$)
HELP(#AcSubBookCodeHelp)
@T(3,@AcSubBookItem,)
C(4,#AcSubBookCode,s,V(H,#AcSubBookCodeHelp))

#AcSubBookItem=X,20,L
C(4,#AcSubBookCode,S,T(H,#AcSubBookCode,#AcSubBookCodeHelp))

#FdBranchNo=X,2,L
! [單位別]
#BranchNo=A,4,L
C(3,#CHAIN,1,s,$)
T(2,#ACBRNO)

#FdCurrencyCode=X,2,L
! 幣別
#CurrencyCode=X,3,L
C(3,#CHAIN,1,s,$)
T(3,TWD)

#BUF1A=X,18,S

#BIND1A=X,1,S
BIND(#BTN1,click, {cmd=CHAIN; ntxcd=L6061; ntxbuf=#BUF1A;ntxbuf5=type<-1:
AcNoCode<-OOAcNoCode:
AcSubCode<-OOAcSubCode:
AcDtlCode<-OOAcDtlCode})

#FdAccountType=X,2,L
! 科子細目
#AcNoCode=X,11,I
C(3,#CHAIN,1,s,$)

#AcSubCode=X,5,I
C(3,#CHAIN,1,s,$)
C(4,#AcNoCode,S,$)
@C(4,#AcNoCode,S,$)

#AcDtlCode=X,2,I
C(3,#CHAIN,1,s,$)
C(4,#AcNoCode,S,$)
@C(4,#AcNoCode,S,$)

#SendL6R23=X,1,S
C(4,#AcNoCode,S,$)
E(0,@RimFuncCode,5)
T(2,@RimAcNoCode,#AcNoCode)
T(2,@RimAcSubCode,#AcSubCode)
T(2,@RimAcDtlCode,#AcDtlCode)
RESET_RIM(#SendL6R23,L6R23)
S(L6R23,1,#RimFuncCode,#RimTxCode,#RimAcNoCode,#RimAcSubCode,#RimAcDtlCode)
R(1,L6R23)

#AcNoItem=X,80,L
T(2,#L6R23AcNoItem)
@C(4,#AcNoCode,S,$)

#FdCustNo=X,2,L
#TEMP_CUST_NO=A,7,S
#TEMP_FACM_NO=A,3,S
! 戶號
#CustNo=A,7,I
C(3,#CHAIN,1,s,$)
@C(4,#CustNo,s,$)
V(9)E(0,@TEMP_CUST_NO,#CustNo)T(2,#TEMP_CUST_NO)


#FacmNo=A,3,I
C(3,#CHAIN,1,s,$)
C(3,#CustNo,0,S,$)
@C(4,#CustNo,s,$)
V(9)E(0,@TEMP_FACM_NO,#FacmNo)T(2,#TEMP_FACM_NO)

#TmpTxCode=X,5,S

#TmpFacmNo=A,3,S

#TmpFacmNoX=X,50,L
#TmpFacmNoXX=X,60,L
C(4,#TmpFacmNoX,$,T(2,指定額度：+#TmpFacmNoX))


#MRKEY=_,_,S
C(3,#CustNo,0,S,$)
C(3,#FacmNo,0,T(2,#CustNo),T(2,#CustNo+-+#FacmNo)) 

#FdAcctCode=X,2,L
! 銷帳科目
#AcctCode=X,3,I
C(3,#CHAIN,1,s,$)
C(4,#AcNoCode,$,S)
HELP(#AcctCodeHelp,cols:2)
@T(3,@AcctCodeX,)
C(3,#CustNo,0,$,s)
C(4,#AcctCode,V(P,科子細目、戶號、銷帳科目請擇一輸入),$)

#AcctCodeX=X,20,L
T(H,#AcctCode,#AcctCodeHelp)


#FdClsFlag=X,2,L

!銷帳記號
#ClsFlag=A,1,I
C(3,#CHAIN,1,s,$)
E(0,0)
HELP(#ClsFgH)
@T(3,@ClsFlagX,)
V(H,#ClsFgH)

#ClsFlagX=X,4,L
T(H,#ClsFlag,#ClsFgH)


#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

!最後修改人員&日期
#OOLastUpdate=x,20,S

#OOLastEmp=x,47,S

#OOGetFeeTxt=X,10,S

<include src="ROM.icf::L0Rim.dec"/>
<include src="ROM.icf::L6R23.dec"/>

</sub>


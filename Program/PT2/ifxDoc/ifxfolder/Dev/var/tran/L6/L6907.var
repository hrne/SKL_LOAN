#OH_SUB_TYPE=X,5,L
T(2,#AcSubCode)

#OH_DTL_NO=X,3,L
T(2,#AcDtlCode)

! 戶號
#OH_CUST_NO=X,7,L
T(2,#CustNo)

#OH_FACM_NO=X,3,L
T(2,#FacmNo)

! 銷帳編號

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L6041]                                 使用者資料查詢"],
["#FdBrNo+使用單位",#BrNo,#BrNoX],
["#FdTlrNo+使用者代號",#TlrNo,#TlrItem],
["#FdGroupNo+課組別",#GroupNo,#GroupNoX],
["#FdLevelFg+權限等級",#LevelFg,#LevelFgX],
]
!["@顯示方式",
!<include src="COM.icf::RPTFG.scr"/>
!],
!---------- AP Button AREA ----------

<include src="HELPRIM.icf::HELPRIM.rtn"/>

#H1=X,10,S
T(3,CDBR)

!#H2=X,10,S
!T(3,AUTHGROUP)

#BrHelp=X,1024,S
!#AuthHelp=X,1024,S

#HR=X,1,S
S(XXR99,01,#H1)
R(1,XXR99)
CALL(#HelpProc)
T(2,@BrHelp,#HelpDesc1)

#RIM_L6R20a=A,1,S
RESET_RIM(#RIM_L6R20a,L6R20)
E(0,@FunCode,9)
S(L6R20,01,#FunCode)
R(1,L6R20)

#IsAdmin=A,1,S

#AdminFg=A,1,S
E(0,#IsAdmin)
C(3,#AdminFg,1,$,T(F,@BrNo,0))

!---------- AP TEXT AREA Variables ----------

#FdBrNo=X,2,L
#FdTlrNo=X,2,L
#FdGroupNo=X,2,L
#FdLevelFg=X,2,L

#CHAIN=A,1,S
T(4,CHAIN$) 

#NTXBUF=X,12,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@BrNo,#NTXBUF,1,4)
T(1,@TlrNo,#NTXBUF,5,6)
T(1,@GroupNo,#NTXBUF,11,1)
T(1,@LevelFg,#NTXBUF,12,1)


#NTXBUF5=X,10,S
C(3,#CHAIN,1,T(4,NTXBUF5$),S)

#button=x,100,S
T(2,{cmd=CHAIN; ntxcd=L6401; ntxbuf=1; ntxbuf5=type})ASGN(BTN_NEW1$)
K(MODIFYBUTTON,查詢)
C(3,#CHAIN,1,S,K(MODIFYBUTTON,1,新增使用者))

![經辦/使用者等級]
#LevelHelp=X,100,S
T(3,0:全部;1:主管;3:經辦)

#FunCode=A,1,S
E(0,5)

!使用單位
#BrNo=X,4,I
T(2,#KINBR) 
C(3,#AdminFg,1,$,s)
!C(4,#TlrNo,$,S)
HELP(#BrHelp)
@T(3,@BrNoX,)
C(4,#BrNo,$,V(H,#BrHelp))

#BrNoX=X,80,L
T(H,#BrNo,#BrHelp)
C(4,#BrNo,T(3,@FdTlrNo,*),T(3,@FdTlrNo,))

#TXMT=A,1,S
C(3,#CHAIN,0,$,E(0,@CHAIN,0)XMT())

!#TT=X,4,I
!@
!A(TIME,HHmm,#TT)


!使用者代號
#TlrNo=X,6,I
@T(3,@TlrItem,)
C(4,#BrNo,V(7),$)
C(4,#TlrNo,$,V(8,6,6))

#RIM_L6R20=A,1,S
C(4,#TlrNo,S,$)
E(0,@FunCode,8)
RESET_RIM(#RIM_L6R20,L6R20)
S(L6R20,01,#FunCode,#BrNo,#TlrNo,#END)
R(1,L6R20)

#TlrItem=X,50,L

#GroupHelp=x,50000,S

#RimL6r44=X,1,S
C(4,#BrNo,S,$)
T(2,@L6R44BranchNo,#BrNo)
S(L6R44,1,#L6R44BranchNo)
R(1,L6R44)
T(2,@GroupHelp,#L6r44GroupHelpList)
T(H,@GroupNoX,#GroupNo,#GroupHelp)

#GroupNo=X,1,I
C(4,#BrNo,S,$)
C(4,#TlrNo,$,S)
HELP(#GroupHelp,cols:2))
@T(3,@GroupNoX,)
C(4,#GroupNo,s,V(H,#GroupHelp))

#GroupNoX=X,20,L
C(4,#GroupNo,S,$)
C(4,#TlrNo,$,S)
T(H,#GroupNo,#GroupHelp)

#LevelFg=A,1,I
C(4,#TlrNo,$,S)
HELP(#LevelHelp)
@T(3,@LevelFgX,)
V(H,#LevelHelp)

#LevelFgX=X,10,L
T(H,#LevelFg,#LevelHelp)


!<include src="COM.icf::RPTFG.dec"/>
#RPTFG=A,1,S
E(0,0)

#RPTFG2=A,1,S
C(3,#RPTFG,0,$,S)
C(4,#NTXBUF5,T(3,1),T(3,0))

#RPTFG=_,_,S           

</sub>


!---------- TXFORM = L6041 ----------
<sub type="FORM" name="L6041">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6041,$,S)

#SCRTIL=_,_,S
T(3,[L6041] 使用者資料)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

! 使用者代碼
#OTlrNo=X,6,L

! 使用者名稱
#OTlrItem=X,20,L

! 使用單位
#OBrNo=X,4,L

#OBrItem=x,40,L

! 課組別
#OGroupNo=X,1,L

#OGroupItem=x,20,L

#OLastUpdate=X,18,L
#OLastUpdateEmpNo=X,30,L

! 權限群組
!#OAuthNo=X,6,L

#OOCHAIN=A,1,S
T(4,CHAIN$)

! 查詢的按鈕
#OFunc5=A,1,S
E(0,5)

#OBUF5=X,65,S
T(2,#OFunc5+#OTlrNo)

#OBTN5=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6401;text=查詢;ntxbuf=#OBUF5})

#OBUF6=X,65,S
T(2,L6401+CODE:+#OTlrNo)

!是否有歷程 Y/N
#OHasHistory=X,1,S

#OBTN6=X,4,S

!BIND(grid,{cmd=CHAIN;ntxcd=L6933;text=歷程;ntxbuf=#OBUF6})
C(5,#OHasHistory,"Y",BIND(grid,{cmd=CHAIN;ntxcd=L6933;text=歷程;ntxbuf=#OBUF6}),BIND(grid,cancel))    

! 修改的按鈕
#OFunc2=A,1,S
E(0,2)

#OBUF2=X,65,S
T(2,#OFunc2+#OTlrNo)

! 連結時隱藏按鈕
#OBTN2=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6401;text=修改;ntxbuf=#OBUF2})
IF(#OOCHAIN==1,BIND(grid,cancel),$)

! 刪除的按鈕
#OFunc4=A,1,S
E(0,4)

#OBUF4=X,65,S
T(2,#OFunc4+#OTlrNo)

! 連結時隱藏按鈕
#OBTN4=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6401;text=刪除;ntxbuf=#OBUF4})
IF(#OOCHAIN==1,BIND(grid,cancel),$)

<include src="ROM.icf::L6R44Rim.dec"/>
<include src="ROM.icf::L6R44.dec"/>

</sub>



!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L6023]                                 收件字代碼查詢"],
["#FdLandOfficeCode+地政別代碼",#LandOfficeCode,#LandOfficeCodeX],
["#FdRecWord+收件字代碼",#RecWord],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
!#BTN1=X,15,I,ui:button;value:新增保證人關係代碼;Enable:1;tabbable:0;title:L6607 保證人關係代碼維護
!#BUF1=X,42,S


!---------- AP TEXT AREA Variables ----------
#FdLandOfficeCode=X,2,L
#FdRecWord=X,2,L


#CHAIN=A,1,S
T(4,CHAIN$) 

#NTXBUF=X,4,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@LandOfficeCode,#NTXBUF,1,2),$)

#NTXBUF5=X,10,S
C(3,#CHAIN,1,T(4,NTXBUF5$),S)
C(4,#NTXBUF5,E(0,@RPTFG2,1),E(0,@RPTFG2,2))

#LoanOfficeHelp=X,2000,S

#City=X,2,S
#CityCode=X,2,S

! 依縣市別組地政下拉選單
#RimL2r59=X,1,S
T(2,@CityCode,#City)
RESET_RIM(#RimL2r59,L2R59)
S(L2R59,1,#CityCode)
R(1,L2R59)
T(2,@LoanOfficeHelp,#L2r59Help1)


!地政別代碼
#LandOfficeCode=X,2,I
HELP(#LoanOfficeHelp)
C(3,#CHAIN,1,s,$)
@T(3,@LandOfficeCodeX,)
@C(4,#LandOfficeCode,S,V(H,#LoanOfficeHelp))

#LandOfficeCodeX=X,8,L
C(4,#LandOfficeCode,S,$)
T(H,#LandOfficeCode,#LoanOfficeHelp)


!收件字代碼
#RecWord=X,3,I
C(3,#CHAIN,1,s,$)

#RPTFG=A,1,S
E(0,0)

#RPTFG2=A,1,S
C(3,#RPTFG,0,$,S)
C(4,#NTXBUF5,T(3,1),T(3,2))

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

!<include src="COM.icf::RPTFG.dec"/>

<include src="ROM.icf::L2R59.dec"/>
</sub>


!---------- TXFORM = L6023 ----------
<sub type="FORM" name="L6023">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6023,$,S)

#SCRTIL=_,_,S
T(3,[L6023] 收件字代碼查詢)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

! 地政別代碼
#OOLandOfficeCode=X,2,L

! 收件字代碼

#OORecWord=X,3,L

! 收件字說明

#OORecWordItem=x,60,L

!最後修改人員&日期
#OOLastUpdate=x,20,S

#OOLastEmp=x,47,S



! 修改的按鈕
#OOFunc2=A,1,S
E(0,2)

#OOBUF2=X,65,S
T(2,#OOFunc2+#OOLandOfficeCode+#OORecWord)

#OOBTN2=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6203;text=修改;ntxbuf=#OOBUF2})

! 刪除的按鈕
#OOFunc4=A,1,S
E(0,4)

#OOBUF4=X,65,S
T(2,#OOFunc4+#OOLandOfficeCode+#OORecWord)

#OOBTN4=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6203;text=刪除;ntxbuf=#OOBUF4})

</sub>



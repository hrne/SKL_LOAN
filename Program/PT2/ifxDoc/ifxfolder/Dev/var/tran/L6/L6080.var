["[L6080]                                 使用者權限群組查詢                                                            "],
["#FdTlrNo+使用者代號",#TlrNo],
]
!---------- AP Button AREA ----------

#button=x,1,S
K(MODIFYBUTTON,查詢)

!---------- AP TEXT AREA Variables ----------

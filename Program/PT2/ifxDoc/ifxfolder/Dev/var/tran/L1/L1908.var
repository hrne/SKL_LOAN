<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)

#FirstIn=A,1,S
E(0,0)

#BTNADDBUF=X,11,S
T(2,1+0000000+000)

#BtnAddA=x,100,S
T(2,{cmd=CHAIN; ntxcd=L1108; ntxbuf=#BTNADDBUF; ntxbuf5=type})
ASGN(BTN_NEW1$)
K(MODIFYBUTTON,1,新增)

#BTNRPTBUF=X,14,S

#BTNRPT=x,100,S

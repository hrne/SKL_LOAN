!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L4721" layout="cols=1;screen.width=[130,870];printer.width=[20,80];order=1">
[
["[L4721]                                 整批批次產出利率變動對帳單"],
[#<->#,"#FdAdjDate+利率調整日",#sAdjDate,"~",#eAdjDate],
[#<->#,"#FdTxKind+利率種類",#TxKind,#TxKindX],
[#<->#,"#FdCustType+個金/企金",#CustType,#CustTypeX]
]

#FdAdjDate=X,2,L
T(3,*)
#FdTxKind=X,2,L
T(3,*)
#FdCustType=X,2,L
T(3,*)
! Help
<include src="HELPRIM.icf::HELPRIM.rtn"/>

![戶別]
#CDDEF01=x,26,S
T(3,CdCode.EntCode2)


#EntCodeHelp=x,1024,S 

![利率種類]
#CDDEF02=x,50,S
T(3,CdCode.TxKind)

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF01,#CDDEF02)
R(1,XXR99)
CALL(#HelpProc) 
T(2,@EntCodeHelp,#HelpDesc1)
T(2,@BaseRateHelp,#HelpDesc2)  
! T(B,@BaseRateHelp,#HelpDesc2+;98:員工利率;99:自訂利率)  


#BaseRateHelp=x,1024,S 
T(2,0:全部;+#BaseRateHelp)

#CHAIN=A,1,S
T(4,CHAIN$) 

#NTXBUF=X,14,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
! T(1,@FunctionCode,#NTXBUF,1,1)

!會計日期 必須輸入
#DATE=D,8,S
T(4,DATE$)

#DT=D,7,S
T(1,#DATE,2,7)

#sAdjDate=D,7,I
T(2,#DT)
@V(7)A(4,0,#sAdjDate)
IF(#sAdjDate > #DT,V(P,不可大於會計日),$)

#eAdjDate=D,7,I
T(2,#DT)
@V(7)A(4,0,#eAdjDate)
IF(#eAdjDate > #DT,V(P,不可大於會計日),$)
IF(#eAdjDate < #sAdjDate,V(P,不可小於會計起日),$)




#TxKind=A,1,I
HELP(#BaseRateHelp)
@V(H,#BaseRateHelp)

#TxKindX=X,16,L
T(3,)T(H,#TxKind,#BaseRateHelp)
!SWITCH(#TxKind
!,01,T(2,保單分紅利率)
!,02,T(2,郵政儲金利率)
!,98,T(2,員工利率)
!,99,T(2,自訂利率)
!,T(2,))

! 戶別
#CustType=A,1,I
E(0,1)
HELP(#EntCodeHelp)
@V(H,#EntCodeHelp)
!HELP(1:個金;2:企金（含企金自然人）)
!@V(1,1,2)     

#CustTypeX=X,30,L
T(3,)T(H,#CustType,#EntCodeHelp)
!SWITCH(#CustType,1,T(3,個金),2,T(3,企金（含企金自然人）),T(3,))


! #CHAINXMT=X,1,S
! C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)


#RPTFG=A,1,S
E(0,0)


</sub>


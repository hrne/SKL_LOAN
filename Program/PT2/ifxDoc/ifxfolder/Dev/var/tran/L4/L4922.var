!---------- TXFORM = L4922 ----------
<sub type="FORM" name="L4922">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L4922,$,S)

#SCRTIL=_,_,S
T(3,[L4922] 客戶還款方式及還款帳號查詢)


!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------

#OCustNo=A,7,L

#OCustNoX=X,50,L
T(3,)T(2,#L2r07CustName)

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

!額度編號
#OOFacmNo=A,3,S

!繳款方式
#OORepayCode=A,2,S
#OORepayCodeX=X,10,S
T(3,)
T(H,#OORepayCode,#RepayCodeHelp)

!銀行代碼
#OORepayBank=A,3,S
#OORepayBankX=X,25,S
T(3,)
T(H,#OORepayBank,#BankDeductCdHelp)
#OORepayBankXX=X,30,S
C(3,#OORepayBank,0,S,$)
T(2,#OORepayBank+-+#OORepayBankX)

!扣款帳號
#OORepayAcct=X,16,S
C(3,#OORepayCode,2,$,S)
!帳號狀態
#OOStatus=X,1,S

#OOStatusX=X,15,S
C(3,#OORepayCode,2,$,S)
T(3,)
T(H,#OOStatus,#AuthStatusCodeHelp)

!歷程
!還款帳號變更含還款方式紀錄查詢
#OOBUF1=X,34,S
T(2,#OCustNo+#OOFacmNo)

!查有歷程時才顯示歷程按鈕
#OOHasHistory=A,1,S

!無歷程時cancel解綁
!直接hide似乎不能透過再度bind蓋過去
#OOBTN1=X,1,S
C(3,#OOHasHistory,1,BIND(grid,{ cmd=CHAIN; ntxcd=L4923; ntxbuf=#OOBUF1}),BIND(grid,cancel))

</sub>



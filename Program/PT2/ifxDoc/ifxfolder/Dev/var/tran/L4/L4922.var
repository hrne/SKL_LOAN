[
["[L4922]                                 客戶還款方式及還款帳號查詢"],
["#FdMain+查詢條件","@(借款人戶號、統一編號需擇一輸入)"],
["#FdCustNo+．借款人戶號",#CustNo,#BTN2,#CustNoX],
["#FdCustId+．統一編號",#CustId,#BTN3,#CustIdX],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
#BTN2=X,15,I,ui:button;value:額度資料查詢;enable:1;tabbable:0; title:L2015 額度明細資料查詢;
#BTN3=X,15,I,ui:button;value:顧客資料查詢;enable:1;tabbable:0;title:L1001 顧客明細資料查詢;
#BUF1=X,42,S

!---------- AP TEXT AREA Variables ----------
! Help
<include src="HELPRIM.icf::HELPRIM.rtn"/>
! 授權狀態
#CDDEF01=x,50,S
T(3,CdCode.AuthStatusCode)
#AuthStatusCodeHelp=x,1024,S

!RepayCode 繳款方式
#CDDEF02=x,20,S
T(3,CdCode.RepayCode)

#RepayCodeHelp=x,1024,S


!BankDeductCd 銀行代碼
#CDDEF03=x,25,S
T(3,CdCode.BankCdAppl)

#BankDeductCdHelp=x,1024,S 


#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF01,#CDDEF02,#CDDEF03)
R(1,XXR99)
CALL(#HelpProc) 
T(2,@AuthStatusCodeHelp,#HelpDesc1)
T(2,@RepayCodeHelp,#HelpDesc2)
T(2,@BankDeductCdHelp,#HelpDesc3)

#CHAIN=A,1,S
T(4,CHAIN$)
T(2,@RimTxCode,#TXCD) 
E(0,@RimFKey,#FKEY)

#NTXBUF=X,34,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
#NTXBUF5=X,10,S
C(3,#CHAIN,1,T(4,NTXBUF5$),S)
C(4,#NTXBUF5,E(0,@RPTFG2,1),E(0,@RPTFG2,2))


#BUF2A=X,18,S
T(3,00000019999999000)

#BIND2A=X,1,S
BIND(#BTN2,click, {cmd=CHAIN; ntxcd=L2015; ntxbuf=#BUF2A;ntxbuf5=type<-1:CustNo<-OOCustNo})
#FdMain=X,2,L
T(3,*) 

![借款人戶號]
!借款人戶號, 統一編號擇一輸入
#FdCustNo=X,2,L

#CustNo=A,7,I
C(3,#CHAIN,1,T(1,#NTXBUF,8,7),$)
C(3,#CHAIN,1,s,$)

#WKBTN2=X,1,S

#BUF2B=X,18,S
C(3,#CustNo,0,T(3,00000019999999000),T(2,#CustNo+#CustNo+000))

#BIND2B=X,1,S
BIND(#BTN2,click, {cmd=CHAIN; ntxcd=L2015; ntxbuf=#BUF2B;ntxbuf5=type<-1:CustNo<-OOCustNo})
T(3,#CustNoX,)
#SendL2r07B=X,1,S
C(3,#CustNo,0,S,$)
T(3,@RimCustId, )
E(0,@RimCustNo,#CustNo)
RESET_RIM(#SendL2r07B,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo)
R(1,L2R07)

#CustNoX=x,2.50,L
C(3,#CustNo,0,S,T(2,#L2r07CustName))

#BUF3A=X,100,S

#BIND3A=X,1,S
BIND(#BTN3,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUF3A;ntxbuf5=type<-1:CustId<-OOCustId})

![統一編號]
#FdCustId=X,2,L

#CustId=X,10,I
C(3,#CHAIN,1,T(1,#NTXBUF,15,10),$)
C(3,#CHAIN,1,s,$)
C(3,#CustNo,0,$,S)
@
C(4,#CustId,V(P,需輸入戶號或統一編號),$)
V(7)

#BUF3B=X,100,S
T(2,0000000+#CustId)

#BIND3B=X,1,S
BIND(#BTN3,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUF3B;ntxbuf5=type<-1:CustId<-OOCustId})

#CheckCustId=X,1,S
C(4,#CustId,S,$)
A(ID_UNINO,0,#CustId)

#WKBTN3=X,1,S

#SendL2r07C=X,1,S
C(4,#CustId,S,$)
T(2,@RimCustId,#CustId)
E(0,@RimCustNo,0)
RESET_RIM(#SendL2r07C,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo)
R(1,L2R07)

#CustIdX=x,2.50,L
C(4,#CustId,S,T(2,#L2r07CustName))

<include src="ROM.icf::L0Rim.dec"/>
<include src="ROM.icf::L2Rim.dec"/>
<include src="ROM.icf::L2R07.dec"/>

#RPTFG2=A,1,S
C(4,#NTXBUF5,T(3,1),T(3,2))
#RPTFG=A,1,S

</sub>


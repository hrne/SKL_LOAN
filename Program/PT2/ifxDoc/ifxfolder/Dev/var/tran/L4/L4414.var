!------------ TXFORM = L4414 -----------

! 新光
<sub type="FORM" name="L444A" tota-mode="1">
#OROCDateA=X,9,O
#OTimeA=X,8,O
#OHeadRocTxdayA=D,7,O
#OCntA=m,6,O
IF(#OCntA>=1,s,$)
E(0,@L444APRT,0)
#OTotalCntA=m,6,O
#OFinishCntA=m,6,O
#OCancelCntA=m,6,O


#L444A_LOOP=X,1,S
E(0,1)ASGN(LOOP$)

! 戶號
#OOCustNoA=A,7,O
#OOCustNameA=x,20,O
! 額度
#OOFacmNoA=A,3,O
! 扣款帳號
#OORepayAcctA=A,14,O
! --------------ACH--------------
! 提回訊息
#OOCreateFlagA=X,1,O
! 提回訊息
#OOAuthStatusA=X,1,O
#OOAuthStatusAX=X,20,O
C(3,#OOAuthStatusA,0,
    C(5,#OOCreateFlagA,D,T(2,取消授權扣款),
        C(5,#OOCreateFlagA,A,T(2,成功授權),T(2,))),
T(3,)T(H,#OOAuthStatusA,#AuthStatusHelp))

!SWITCH(#OOAuthStatusA,
!0,T(2,成功新增或取消授權扣款),
!1,T(2,印鑑不符),
!2,T(2,無此帳號),
!3,T(2,委繳戶統一編號不符),
!4,T(2,已核印成功在案),
!5,T(2,原交易不存在),
!6,T(2,電子資料與授權書內容不符),
!7,T(2,帳戶已結清),
!8,T(2,印鑑不清),
!9,T(2,其他),
!A,T(2,未收到授權書),
!B,T(2,用戶號碼錯誤),
!C,T(2,靜止戶),
!D,T(2,未收到聲明書),
!E,T(2,授權書資料不全),
!F,T(2,警示戶),
!G,T(2,本帳戶不適用授權扣繳),
!H,T(2,已於他行授權扣款),
!I,T(2,該用戶已死亡),
!Z,T(2,未交易或匯入失敗資料),T(2,))
! --------------ACH--------------
![防止二次單據]
E(0,@L444BPRT,0)
E(0,@L444CPRT,0)
</sub>


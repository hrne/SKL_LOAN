<sub type="FORM" name="L444C" tota-mode="1">
#OROCDateC=X,9,O
#OTimeC=X,8,O
#OFootCreateDateC=D,7,O
#OCntC=m,6,O
IF(#OCntC>=1,s,$)
E(0,@L444CPRT,0)

#OTotalCntC=m,6,O

#OFinishCntC=m,6,O
#OCancelCntC=m,6,O

#L444C_LOOP=X,1,S
E(0,1)ASGN(LOOP$)
! 戶號
#OOCustNoC=A,7,O
! 額度
#OOFacmNoC=A,3,O
#OOCustNameC=x,20,O
! 扣款帳號
#OORepayAcctC=A,14,O

! --------------POST--------------
! 核印註記
#OOStampCodeC=A,1,O	
#OOStampCodeCX=X,12,O
T(3,)T(H,#OOStampCodeC,#StampCodeHelp)
!SWITCH(#OOStampCodeC,
!1,T(2,局帳號不符),
!2,T(2,戶名不符),
!3,T(2,身分證號不符),
!4,T(2,印鑑不符),
!9,T(2,其他),T(2,))

! 狀況代號
#OOAuthErrorCodeC=X,2,O
#OOAuthErrorCodeCX=X,13,O
T(3,)T(H,#OOAuthErrorCodeC,#AuthErrorCodeHelp)
!WITCH(#OOAuthErrorCodeC,00,T(3,00-成功),
!                       03,T(3,03-已終止代繳),
!                       06,T(3,06-凍結警示戶),
!                       07,T(3,07-支票專戶),
!                       08,T(3,08-帳號錯誤),
!                       09,T(3,09-終止戶),
!                       10,T(3,10-身分證不符),
!                       11,T(3,11-轉出戶),
!                       12,T(3,12-拒絕往來戶),
!                       13,T(3,13-無此編號),
!                       14,T(3,14-編號已存在),
!                       16,T(3,16-管制帳戶),
!                       17,T(3,17-掛失戶),
!                       18,T(3,18-異常帳戶),
!                       19,T(3,19-編號非英數),
!                       91,T(3,91-期限未扣款),
!                       98,T(3,98-其他),T(2,))

! --------------POST--------------
![防止二次單據]
E(0,@L444APRT,0)
E(0,@L444BPRT,0)

</sub>


!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[LC011]                                 交易明細查詢"],
["#FdDate+查詢條件","會計日期、交易日期，需擇一輸入"],
["#FdAc+．會計日期",#iEntdySt,"~",#iEntdyEd],
["#FdTx+．交易日期",#iTxSt,"~",#iTxEd],
["#FdBrNo+使用單位",#iBrNo],
["#FdTlrNo+經@@@@辦",#iTlrNo],
["#FdTranNo+交易代碼",#iTranNo],
["#FdStatus+交易型態",#iStatus,#iStatusX],
["#FdCustNo+戶@@@@號",#iCustNo],
]
!---------- AP Button AREA ----------

#FdDate=X,2,L
T(3,*)

#FdAc=X,2,L
#FdTx=X,2,L
#FdBrNo=X,2,L
#FdTlrNo=X,2,L
#FdTranNo=X,2,L
#FdStatus=X,2,L
T(3,*)
#FdCustNo=X,2,L

!---------- AP TEXT AREA Variables ----------
#CHAIN=A,1,S
T(4,CHAIN$)
T(2,@iTlrNo,#TLRNO)
#NTXBUF=X,3,S

#iBrNo=X,4,L
T(2,#KINBR) 

#iEntdySt=D,7,I
C(3,#iEntdySt,0,E(0,#SYSDATE),$)
D(8)
@
C(3,#iEntdySt,0,S,$)
A(DATE,0,#iEntdySt)
V(5,0010101,#SYSDATE)

#iEntdyEd=D,7,I
C(3,#iEntdySt,0,S,$)
C(3,#iEntdyEd,0,E(0,#SYSDATE),$)
D(8)
@
V(7)
A(DATE,0,#iEntdyEd)
V(5,#iEntdySt,#SYSDATE)

#iTxSt=D,7,I
C(3,#iEntdySt,0,$,S)
C(3,#iTxSt,0,E(0,#SYSCDATE),$)
D(8)
@
V(7)
A(DATE,0,#iTxSt)
V(5,0010101,#SYSCDATE)

#iTxEd=D,7,I
C(3,#iTxSt,0,S,$)
C(3,#iTxEd,0,E(0,#SYSCDATE),$)
D(8)
@
V(7)
A(DATE,0,#iTxEd)
V(5,#iTxSt,#SYSCDATE)

#iTlrNo=X,6,I


!#SendL6R17=X,1,S
!C(4,#iTlrNo,S,$)
!E(0,@RimFuncCode,5)
!T(2,@RimTxCode,#TXCD)
!T(2,@RimEmployeeNo,#iTlrNo)
!RESET_RIM(#SendL6R17,L6R17)
!S(L6R17,1,#RimFuncCode,#RimTxCode,#RimEmployeeNo)
!R(1,L6R17)


!#iTlrItem=X,40,L
!C(4,#iTlrNo,T(2,),T(2,#L6R17Fullname))


!交易代碼
#iTranNo=X,5,I

!狀態
#iStatus=A,1,I
E(0,0)
HELP(#iStatusH)
@T(3,@iStatusX,)
V(H,#iStatusH)

#iStatusX=X,20,L
T(H,#iStatus,#iStatusH)

!交易帳號
#iCustNo=A,7,I

#RPTFG=A,1,S
E(0,0)

<include src="ROM.icf::L0Rim.dec"/>
<include src="ROM.icf::L6R17Rim.dec"/>
<include src="ROM.icf::L6R17.dec"/>
</sub>


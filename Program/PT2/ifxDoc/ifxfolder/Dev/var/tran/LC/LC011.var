!---------- TXFORM = LC011 ----------
<sub type="FORM" name="LC011">

!#SCRPRT=_,_,S
!C(5,#TXFORM,LC011,$,S)

#SCRTIL=_,_,S
T(3,[LC011] 交易明細查詢)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

#CalDate=D,7,S

#CalTime=A,6,S
#CalHH=X,2,S
T(1,#CalTime,1,2)
#CalMM=X,2,S
T(1,#CalTime,3,2)
#CalSS=X,2,S
T(1,#CalTime,5,2)
#CalTimeX=X,8,S
T(2,#CalHH+:+#CalMM+:+#CalSS)

#Entdy=D,7,S
#TxNo=X,18,S
#TranNo=X,5,S
#MrKey=X,20,S
#CurName=X,3,S

#TxAmt=m,14.2,S
#TxAmt1=m,14,S
E(0,#TxAmt)
#TxAmtX=X,17,S
C(3,#TxAmt1,0,T(3,)T(3,@CurName,),X(#TxAmt1)) ! 金額為0時隱藏幣別與金額

#BrNo=X,25,S
#TlrNo=X,6,S
#SupNo=X,6,S
#FlowType=X,1,S
#FlowStep=X,1,S
#Funcind=X,1,S
#Status=A,1,S
#AcCnt=A,1,S
#Hcode=A,1,S
#FlowNo=X,18,S
#OOOrgEntdy=D,7,S
C(3,#OOOrgEntdy,0,S,$)

#TranItem=X,60,S
#TranItemX=X,86,S
T(2,#TranNo+ +#TranItem)

#TlrItem=X,20,S
#SupItem=X,20,S

#FlowMode=A,1,S

#FlowModeX=x,6,S
C(2,#FlowMode,S,T(3,待放行),T(3,待審核),T(3,待提交),S)

#TypeX=X,8,S
C(2,#FlowStep,C(5,#Funcind,4,T(3,刪除),T(3,登錄)),C(5,#Funcind,4,T(3,刪除),T(3,登錄)),T(3,放行),T(3,審核),T(3,審核放行))

#HcodeX=X,4,S
C(2,#Hcode,T(3,一般),T(3,訂正),T(3,修正),T(3,沖正),$)

#StatusX=X,8,S
C(2,#Status,T(3,正常),T(3,已訂正),T(3,已修正),T(3,已沖正),$)

#RimData=x,50,S
T(6A,#Entdy+#TxNo)

#RimData2=x,50,S
C(3,#OOOrgEntdy,0,T(2,#Entdy+#Entdy+#FlowNo),T(2,#Entdy+#OOOrgEntdy+#FlowNo))

#BTN=X,1,S
BIND(grid,{ cmd=DDCHECK; ntxcd=#TranNo; txno=#RimData;rim=LCR11})

#btn_visible2=X,1,S
C(3,#AcCnt,1,C(3,#Status,0,T(3,1),T(3,0)),T(3,0))

#BTN2=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L6901; ntxbuf=#RimData2; text=分錄; visible=#btn_visible2})

#Entdy8=A,8,S
E(0,#Entdy+19110000)

#jnlId=X,40,S
T(2,#Entdy8+#TxNo)

#BTN3=X,5,S
BIND(grid, {cmd=VIEWEJ; ntxcd=#TranNo; id=#jnlId} )

</sub> 



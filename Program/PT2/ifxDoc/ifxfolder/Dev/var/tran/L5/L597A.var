#NTXBUF=X,13,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@AcDate,#NTXBUF,1,7)
T(1,@IsMainFin,#NTXBUF,8,1)
T(1,@SearchOption,#NTXBUF,9,2)
T(1,@SearchDetail,#NTXBUF,11,1)
T(1,@Export,#NTXBUF,12,1)
T(1,@IsBtn,#NTXBUF,13,1)
! INVOKEJS(SHOW,p1,02,02,1)

! 債權Help
#HelpIsMainFin=X,31,S
!T(3,0:一般債權;1:最大債權;2:無)
T(3,Y:最大債權;N:一般債權)

! 會計日期
#AcDate=D,7,S
C(3,#CHAIN,0,$,s)
E(0,#SYSDATE)

! 債權區分
#IsMainFin=X,1,I
C(3,#IsMainFin,0,T(2,N),T(2,Y))
C(3,#CHAIN,0,$,s)
HELP(#HelpIsMainFin)
@V(H,#HelpIsMainFin)

#IsMainFinX=X,8,L
T(H,#IsMainFin,#HelpIsMainFin)

! 查詢選項Help
#HelpSearchOption=X,265,S
T(3,
!01:前日匯入;
02:未入帳;
03:待處理;
04:已入帳;
05:本月入帳;
06:放款攤分;
07:保單攤分;
08:結清退還;
09:本月放款;
10:本月保單;
11:累計未退還;
12:本日匯入未入帳
!13:撥入筆數;
!14:檢核成功;
!15:檢核失敗;
!16:放款暫收;
!17:本月放款
)
! 查詢選項Help-一般債權
#HelpSearchOption2=X,265,S
T(3,
!01:前日匯入;
02:未入帳;
03:待處理;
04:已入帳;
05:本月入帳;
!06:放款攤分;
!07:保單攤分;
!08:結清退還;
!09:本月放款;
!10:本月保單;
!11:累計未退還;
!12:本日匯入未入帳;
13:撥入筆數;
14:檢核成功;
15:檢核失敗;
16:放款暫收;
17:本月放款
)

![業務別] 01:撥款匯款(含暫收退還且非退票) 02:支票繳款 03:債協 09:放款
#SECNO=_,_,S
T(3,03)

#TxKindCode=x,20,S
T(3,CdCode.NegTxKind)
#TxKindHelp=x,1024,S

<include src="HELPRIM.icf::HELPRIM.rtn"/>

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#TxKindCode)
R(1,XXR99)
CALL(#HelpProc)
T(2,@TxKindHelp,#HelpDesc1)

#TxKindHelp1=x,1024,S
T(HELP,@TxKindHelp1,#TxKindHelp,-,9)

! 查詢選項  新增最大債權跟一般債權可輸入範圍限制
#SearchOption=X,2,I
C(3,#CHAIN,0,$,s)
IF(#IsMainFin=="N",HELP(#HelpSearchOption2,cols:2),HELP(#HelpSearchOption,cols:2))
@IF(#IsMainFin=="N",V(H,#HelpSearchOption2),V(H,#HelpSearchOption))

#SearchOptionX=X,14,L
IF(#IsMainFin=="N",T(H,#SearchOption,#HelpSearchOption2),T(H,#SearchOption,#HelpSearchOption))


#WarnMsg=X,34,S
T(2,最大債權才可選擇+#SearchOptionX)

#CheckSearchOptio=X,1,S
IF(#IsMainFin=="N",$,S)
IF(#SearchOption==06 || #SearchOption==07 || #SearchOption==08 || #SearchOption==09 || #SearchOption==10 || #SearchOption==11 || #SearchOption==12,$,S)
V(P,#WarnMsg)

! 查詢細項 Help
#HelpSearchDetail=X,41,S
T(3,
0:無;
1:債協;
2:調解;
3:更生;
4:清算
)

! 查詢細項
#SearchDetail=X,1,I
C(3,#CHAIN,0,$,s)
C(5,#IsMainFin,N,s,$)
HELP(#HelpSearchDetail)
@V(H,#HelpSearchDetail)

#SearchDetailX=X,4,L
T(H,#SearchDetail,#HelpSearchDetail)

! 是否製檔 Help
#HelpExport=X,23,S
T(3,
0:無;
1:已製檔;
2:未製檔
)

! 是否製檔
#Export=X,1,I
C(3,#CHAIN,0,$,s)
C(5,#IsMainFin,N,s,$)
HELP(#HelpExport)
@V(H,#HelpExport)

#ExportX=X,6,L
T(H,#Export,#HelpExport)

!最大債權已入帳04且細項為0,每一個細項製檔日可能不同,故不可選已製檔
#CheckExport=X,1,S
@IF(#Export==1,$,S)
IF(#SearchOption==04 && #SearchDetail==0,V(P,查詢已製檔資料時查詢細項不可為0),S)


! 是否為BTN Help
#HelpIsBtn=X,7,S
T(3,0:N;1:Y;
)

! 是否為BTN
#IsBtn=X,1,L
C(3,#CHAIN,0,$,s)
T(2,0)
!HELP(#HelpIsBtn)
!@V(H,#HelpIsBtn)

#IsBtnX=X,1,L
T(H,#IsBtn,#HelpIsBtn)

! 交易別
#TransTxKind=X,1,I
C(3,#CHAIN,0,$,s)
IF(#SearchOption=="02" || #SearchOption=="03" || #SearchOption=="12",$,S)  
HELP(#TxKindHelp1,cols:3)
@C(4,#TransTxKind,$,V(H,#TxKindHelp1))

#TransTxKindX=X,8,L
C(4,#TransTxKind,S,T(H,#TransTxKind,#TxKindHelp1))

#RPTFG=A,1,S
IF(#SearchOption=="02" || #SearchOption=="12",E(0,0),IF(#SearchOption=="03",E(0,1),IF(#SearchOption=="18",E(0,2),E(0,3))))


#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

</sub>


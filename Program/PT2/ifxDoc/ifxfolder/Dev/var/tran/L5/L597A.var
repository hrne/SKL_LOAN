! ["會計日期",#AcDate],
<sub type="DC" name="L597A" layout="cols=1;screen.width=[100,900];printer.width=[20,80];">
[
["[L597A]                                 債務協商作業-整批處理"],
["債權區分",#IsMainFin,#IsMainFinX],
["查詢選項",#SearchOption,#SearchOptionX],
["查詢細項",#SearchDetail,#SearchDetailX],
["是否製檔",#Export,#ExportX],
!["是否為按鈕",#IsBtn,#IsBtnX],
["試算交易別",#TransTxKind,#TransTxKindX],
]
#CHAIN=A,1,S
T(4,CHAIN$) 

!------------ TXFORM = L5911 -----------
<sub type="FORM" name="L5911">
#SCRTIL=_,_,S
T(3,[L5911] 撥款件貸款成數統計資料)

#LOOP=X,1,S
E(0,1)ASGN(LOOP$)

! 案件編號
#OOCreditSysNo=X,7,O
! 核准額度
#OOLineAmt=m,16,O
! 借款人戶號
#OOCustNo=X,7,O
! 額度筆數
#OOFacCount=X,3,O
! 首次撥款日
#OOFirstDrawdownDate=D,7,O
! 評估淨值
#OOTotalEvaAmt=m,16,O
! 成數
#OOPerctenage=n,10,O
! 成數區間
#OOPerctenageArea=A,1,O
! 地區別
#OORegCityCode=X,2,O
! 地區區間
#OORegCityArea=A,1,O
! 地區別名稱
#OORegCityItem=X,10,O
T(H,#OORegCityCode,#RegCityCodeHelp)
! 押租金
#OOTotalRentPrice=m,16,O
! 房專員編
#OOBusinessOfficer=X,6,O
! 房貸專員
#OOBusinessOfficerName=X,80,O
! 核決主管員編
#OOSupervisor=X,6,O
! 核決主管
#OOSupervisorName=X,80,O

</sub>


---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L5911" layout="cols=1;screen.width=[130,870];printer.width=[20,80];order=1">
[
["[L5911]                                 撥款件貸款成數統計資料產生"],
[#<->#,"#FdDateFm+年月日",#DateFm,"-",#DateTo],
]

#FdDateFm=X,2,L
T(3,*)

#CHAIN=A,1,S
T(4,CHAIN$) 

#NTXBUF=X,14,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@DateFm,#NTXBUF,1,7)
T(1,@DateTo,#NTXBUF,8,7)
INVOKEJS(SHOW,p1,02,02,1)

<include src="HELPRIM.icf::HELPRIM.rtn"/>
! 地區別下拉選單-1
#RegCityCodeRimName=x,20,S
T(3,CdCode.RegCityCode)
#RegCityCodeHelp=x,1024,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#RegCityCodeRimName)
R(1,XXR99)
CALL(#HelpProc)
T(2,@RegCityCodeHelp,#HelpDesc1)

! 截止日期
! 預設系統帳務年初
!  (產收下載檔為 LNW99GP)
#TmpFm=A,7,S
#TmpTo=A,7,S
#DateFm=D,7,I
D(8)
@A(DATE,0,#DateFm)
V(7)
E(0,@TmpFm,#DateFm)
! 預設系統帳務日期
#DateTo=D,7,I
D(8)
@A(DATE,0,#DateTo)
V(7)
E(0,@TmpTo,#DateTo)

IF(#TmpTo<#TmpFm,V(P,起日不可大於迄日),$)
#RPTFG=A,1,S

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">

#OAvailable=+m,14.2,L
#OAvailable14=+m,14,L
E(0,#OAvailable)
!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#OCaseNo=X,7,L
C(4,#CaseNo,S,T(2,#CaseNo))

#OCaseNoX=X,50,L
T(2,#CaseNoX)

#OCustNo=X,7,L

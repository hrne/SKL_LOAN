#OOCustNo=A,7,O

#OOFacmNo=A,3,O

#OOApplNo=A,7,O

#OODeadline=D,7,O

#OOCurrencyCode=X,3,O

#Precision=N,1,S
E(0,0)T(H,#OOCurrencyCode,#CurrPrecisionHelp)

!核准額度
#OOLineAmt=+m,14.2,S
#OOLineAmt14=m,14,S
E(0,#OOLineAmt)
#OOLineAmtX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#OOLineAmt,#OOLineAmtX)
P(R,#OOLineAmtX)

!累計撥款金額
#OOUtilAmt=+m,14.2,O
#OOUtilAmt14=m,14,S
E(0,#OOUtilAmt)

#OOUtilAmtX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#OOUtilAmt,#OOUtilAmtX)
P(R,#OOUtilAmtX)
!已動用餘額,循環動用還款時會減少,非循環動用還款時不會減少
#OOUtilBal=+m,14.2,O
#OOUtilBal14=m,14,S
E(0,#OOUtilBal)

#OOUtilBalX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#OOUtilBal,#OOUtilBalX)
P(R,#OOUtilBalX)

#OOLoanBal=m,14.2,O
#OOLoanBal14=m,14,S
E(0,#OOLoanBal)

#OOLoanBalX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#OOLoanBal,#OOLoanBalX)
P(R,#OOLoanBalX)

!可動用餘額 

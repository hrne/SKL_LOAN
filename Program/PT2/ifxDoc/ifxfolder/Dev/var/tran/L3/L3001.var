#OValidFacm=A,3,L

#SPC10=X,10,S

#ZRO07=A,7,S

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

!連結 L3002 撥款明細資料查詢
#OOBUF2=X,31,S
T(2,#ZRO07+#ZRO07+#SPC10+#OOApplNo)

#OOBTN2=X,1,S
IF(#OOLastBormNo>0,BIND(grid,{ cmd=CHAIN; ntxcd=L3002; ntxbuf=#OOBUF2}),BIND(grid,cancel))

#OOBUF3=X,50,S

<sub type="FORM" name="L3914">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L3914,$,S)

#SCRTIL=_,_,S
T(3,[L3914] 暫收款退還／銷帳資料查詢)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------

#OCustNo=A,7,L
E(0,#TimCustNo)

#OCustNoX=X,100,L
T(2,#TimCustNoX)


!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

!固定新台幣
#OOCurrencyCode=X,3,O
T(3,TWD)

#OOPrecision=N,1,S
E(0,0)T(H,#OOCurrencyCode,#CurrPrecisionHelp)

!入帳日期
#OOEntryDate=D,7,O
!會計日期
#OOAcDate=D,7,O
!摘要
#OODesc=x,300,O
!業務科目代號
#OOAcctCode=X,3,O
#OOAcctItem=x,30,O
T(3,)T(H,#OOAcctCode,#AcctCodeHelp)
#OOAcctCodeX=x,35,O
T(2,#OOAcctCode+-+#OOAcctItem)
!額度編號
#OOFacmNo=A,3,O
!記帳金額
#OOTxAmt=+m,14.2,O

#OOTxAmtX=X,22,O
CALL2(#ADJUSTDOT,#OOPrecision,#OOTxAmt,#OOTxAmtX)
P(R,#OOTxAmtX)
!銷帳編號
#OORvNo=x,15,O
!經辦
#OOTellerNo=x,6,O
!交易序號
#OOTxtNo=x,8,O




</sub>



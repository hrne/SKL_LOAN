R(1,XXR99)
CALL(#HelpProc) 
T(2,@CurrPrecisionHelp,#HelpDesc1)
T(2,@CurrencyCodeHelp,#HelpDesc2)

#FdMain=X,2,L
T(3,*)

#CHAIN=A,1,S
T(4,CHAIN$)
T(2,@RimTxCode,#TXCD)
E(0,@RimFKey,#FKEY)

#NTXBUF=X,100,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
C(3,#CHAIN,1,T(1,@ChainCustNo,#NTXBUF,6,7),$)

#CHAINTXCD=_,_,S
C(3,#CHAIN,1,T(1,#NTXBUF,1,5),S)

#ChainCustNo=A,7,S

#BUF1A=X,18,S
T(3,00000019999999000)

#BIND1A=X,1,S
BIND(#BTN1,click, {cmd=CHAIN; ntxcd=L2015; ntxbuf=#BUF1A;ntxbuf5=type<-1:FacmNo<-OOFacmNo:TimCustNo<-OOCustNo:CustNo<-OOCustNo:FacmNo<-OOFacmNo:CustNoX<-OOCustName})

![借戶戶號]
!借戶戶號, 統一編號或核准編號擇一輸入
#FdCustNo=X,2,L

#CustNo=X,7,I
C(3,#CHAIN,1,C(3,#ChainCustNo,0,$,E(0,#ChainCustNo)),$)
C(3,#CHAIN,1,s,$)
! INVOKEJS(SHOW,p1,1,3,1)
SHOW(#BTN1)HIDE(#BTN2,#BTN3,#BTN4)
@
HIDE(#BTN1)
T(3,#CustNoX,)

#CustNo2=A,7,S
C(4,#CustNo,S,$)
E(0,#CustNo)
T(2,@CustNo,#CustNo2)


#SendL2r07A=X,1,S
C(4,#CustNo,S,$)
T(3,@RimCustId, )
E(0,@RimCustNo,#CustNo)
RESET_RIM(#SendL2r07A,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo)
R(1,L2R07)

#CustNoX=X,100,L
C(4,#CustNo,S,T(2,#L2r07CustName))

#BUF2A=X,100,S

#BIND2A=X,1,S
BIND(#BTN2,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUF2A;ntxbuf5=type<-1:CustId<-OOCustId:CustIdX<-OOCustName})

![統一編號]
#FdCustId=X,2,L

#CustId=X,10,I
T(3,@FdApplNo, )
C(4,#CustNo,$,s)
SHOW(#BTN2)HIDE(#BTN1,#BTN3,#BTN4)
@
HIDE(#BTN2)
C(4,#CustId,S,$)
V(7)
T(3,#CustIdX,)
A(ID_UNINO,0,#CustId)

#BUF2B=X,100,S
T(2,0000000+#CustId)

#BIND2B=X,1,S
BIND(#BTN2,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUF2B;ntxbuf5=type<-1:CustId<-OOCustId:CustIdX<-OOCustName})

#CheckCustId=X,1,S
C(4,#CustId,S,$)
A(ID_UNINO,0,#CustId)


#SendL2r07B=X,1,S
C(4,#CustId,S,$)
T(2,@RimCustId,#CustId)
E(0,@RimCustNo,0)
RESET_RIM(#SendL2r07B,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo,#END)
R(1,L2R07)

#CustIdX=X,100,L
C(4,#CustId,S,T(2,#L2r07CustName))

#BUF3A=X,18,S
T(3,000000199999990)

#BIND3A=X,1,S
BIND(#BTN3,click, {cmd=CHAIN; ntxcd=L2016; ntxbuf=#BUF3A;ntxbuf5=type<-1:ApplNo<-OOApplNo:FacmNo<-OOFacmNo:ApplNoX<-OOCustName})


![核准編號]
#FdApplNo=X,2,L
C(4,#CustNo,$,S)
C(4,#CustId,$,S)
T(3,*)

#ApplNo=X,7,I
C(4,#CustNo,$,S)
C(4,#CustId,$,S)
! INVOKEJS(SHOW,p1,3,3,1)
SHOW(#BTN3)HIDE(#BTN1,#BTN2,#BTN4)
@
T(3,#ApplNoX,)
C(3,#ApplNo,0,V(P,需擇一輸入),$)
HIDE(#BTN3)

#ApplNo2=A,7,S
C(4,#ApplNo,S,$)
E(0,#ApplNo)
T(2,@ApplNo,#ApplNo2)



#HIDEALLBTN=X,1,S
C(3,#CHAIN,1,$,S)
HIDE(#BTN1,#BTN2,#BTN3)

!用核准號碼(ApplNo)調L2R05
#SendL2r05A=X,1,S
C(4,#ApplNo,S,$)
E(0,@RimFuncCode,5)
E(0,@RimCustNo,#CustNo)
E(0,@RimFacmNo,0)
E(0,@RimApplNo,#ApplNo)
E(0,@RimCaseNo,0)
RESET_RIM(#SendL2r05A,L2R05)
S(L2R05,1,#RimFuncCode,#RimTxCode,#RimFKey,#RimCustNo,#RimFacmNo,#RimApplNo,#RimCaseNo)
R(1,L2R05)
E(0,@FacmNo,#L2r05FacmNo)

#SendL2r07C=X,1,S
C(3,#ApplNo,0,S,$)
T(2,@RimCustId,#L2r05CustId)
E(0,@RimCustNo,#CustNo)
RESET_RIM(#SendL2r07C,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo,#END)
R(1,L2R07)

#ApplNoX=X,100,L
C(4,#ApplNo,S,T(2,#L2r07CustName))

#TimCustNo=A,7,S
E(0,0)
C(4,#CustNo,$,E(0,#CustNo))
C(4,#CustId,$,E(0,#L2r07CustNo))
C(4,#ApplNo,$,E(0,#L2r05CustNo))
T(2,@RmkCustNo,#TimCustNo)
<include src="COM.icf::CustRmk.rtn"/>

#BUF4A=X,50,S
T(2,0000000+#CustNo+#CustId+#ApplNo+000)

#BIND4A=X,1,S
BIND(#BTN4,click, {cmd=CHAIN; ntxcd=L3002; ntxbuf=#BUF4A;ntxbuf5=type<-1:FacmNo<-OOFacmNo:BormNo<-OOBormNo})

![額度編號]
!可不輸入
#FdFacmNo=X,2,L
C(4,#ApplNo,T(3,*)T(F,@FacmNo,1),T(3, )T(F,@FacmNo,0))

#FacmNo=A,3,I
C(3,#CHAIN,1,T(1,#NTXBUF,13,3),$)
C(3,#CHAIN,1,s,$)
C(4,#ApplNo,$,s)
SHOW(#BTN4)
@V(2,0)
HIDE(#BTN4)

!用CustNo,FacmNo調L2R05
#SendL2r05B=X,1,S
C(3,#FacmNo,0,S,$)
E(0,@RimFuncCode,5)
E(0,@RimCustNo,#TimCustNo)
E(0,@RimFacmNo,#FacmNo)
E(0,@RimApplNo,0)
E(0,@RimCaseNo,0)
RESET_RIM(#SendL2r05B,L2R05)
S(L2R05,1,#RimFuncCode,#RimTxCode,#RimFKey,#RimCustNo,#RimFacmNo,#RimApplNo,#RimCaseNo)
R(1,L2R05)

![撥款序號]
!可不輸入
#FdBormNo=X,2,L


#BormNo=A,3,I
C(3,#CHAIN,1,T(1,#NTXBUF,16,3),$)
C(3,#CHAIN,1,s,$)
C(3,#FacmNo,0,S,$)
! @V(2,0)

#SendL3r02=X,1,S
C(3,#BormNo,0,S,$)
E(0,@RimFuncCode,5)
E(0,#RimCustNo,#TimCustNo)
E(0,@RimFacmNo,#FacmNo)
E(0,@RimBormNo,#BormNo)
RESET_RIM(#SendL3r02,L3R02)
S(L3R02,1,#RimTxCode,#RimFKey,#RimFuncCode,#RimCustNo,#RimFacmNo,#RimBormNo)
R(1,L3R02)

#MRKEY=_,_,S
C(3,#FacmNo,0,T(2,#TimCustNo),T(2,#TimCustNo+-+#FacmNo))
C(3,#BormNo,0,$,T(2,#TimCustNo+-+#FacmNo+-+#BormNo))

#WKPREV=X,8,L
T(3,繳息迄日)


!上次繳息日
#PrevPayIntDate=D,7,L
E(0,#L2r07PrevPayIntDate)
C(3,#FacmNo,0,$,E(0,#L2r05PrevPayIntDate))
C(3,#BormNo,0,$,E(0,#L3r02PrevPayIntDate))

#PrevPayIntDateA=A,8,S
E(0,#PrevPayIntDate)

#PrevYyyMm=A,5,S
E(0,#PrevPayIntDate/100)

!下次繳息日
#NextPayIntDate=D,7,S
E(0,#L2r07NextPayIntDate)
C(3,#FacmNo,0,$,E(0,#L2r05NextPayIntDate))
C(3,#BormNo,0,$,E(0,#L3r02NextPayIntDate))

#NextYyyMm=A,5,S
E(0,#NextPayIntDate/100)

![幣別]
#FdCurrencyCode=X,2,L

#CurrencyCode=X,3,L
C(3,#CHAIN,1,T(1,#NTXBUF,19,3),$)
C(3,#CHAIN,1,s,$)
T(3,TWD)
C(3,#FacmNo,0,s,T(2,#L2r05CurrencyCode))
C(3,#BormNo,0,s,T(2,#L3r02CurrencyCode))

#CurrencyCodeX=X,10,S
T(3,)T(H,#CurrencyCode,#CurrencyCodeHelp)

#Precision=N,1,S
E(0,0)T(H,#CurrencyCode,#CurrPrecisionHelp)

![原指定應繳日]
!自動顯示
#FdOldSpecificDd=X,2,L

#OldSpecificDd=A,2,L
E(0,#L2r07SpecificDd)
C(3,#FacmNo,0,$,E(0,#L2r05SpecificDd))
C(3,#BormNo,0,$,E(0,#L3r02SpecificDd))


! #CheckSpecificDd=X,1,S
! C(3,#BormNo,0,C(3,#L2r05SpecificDd,0,V(P,非月繳戶，不可進行此交易),$),C(3,#L3r02SpecificDd,0,V(P,非月繳戶，不可進行此交易),$))


![新指定應繳日]
!必須輸入
#FdNewSpecificDd=X,2,L
T(3,*)

#NewSpecificDd=A,2,I
@V(2,#OldSpecificDd)V(5,1,31)


!虛擬基準日
#VirtualSpecificDate=A,8,S
E(0,1090100+#NewSpecificDd)


#VirtualDiff=A,8,S
D(7,1,#VirtualSpecificDate,#PrevPayIntDateA,0)
T(1,@VirtualDiffYY,#VirtualDiff,001,004)
T(1,@VirtualDiffMM,#VirtualDiff,005,002)
T(1,@VirtualDiffDD,#VirtualDiff,007,002)

#VirtualDiffYY=A,4,S
#VirtualDiffMM=A,2,S
#VirtualDiffDD=A,2,S

!虛擬基準月差
#VirtualDiffMons=A,8,S
C(1,#VirtualDiffDD,E(0,#VirtualDiffYY*12+#VirtualDiffMM),E(0,#VirtualDiffYY*12+#VirtualDiffMM),E(0,#VirtualDiffYY*12+#VirtualDiffMM+1))


#NewSpecificDateA=A,8,S
D(7,2,#VirtualSpecificDate,#VirtualDiffMons,0)

#WKNEXT=X,10,L
T(3,應收息迄日)

#NewSpecificDate=D,7,L
E(0,#NewSpecificDateA)
V(5,#PrevPayIntDate,#NextPayIntDate)
A(DATE,0,#NewSpecificDate)

![入帳日期]
#FdEntryDate=X,2,L
T(3,*)

#EntryDate=D,7,I
C(3,#EntryDate,0,E(0,#SYSDATE),$)
@V(7)V(5,#SYSDATE,9991231)
A(DATE,0,#EntryDate)

#FdRptFg=X,2,L
T(3,*)



<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L2073]                                 結清客戶申請個人資料控管查詢"],
["#FdIdNo+依統編或戶號",#IdNo,#IdNoX],
["#FdCustId+客戶統一編號",#CustId],
["#FdCustNo+借戶戶號",#CustNo],
]

#FdIdNo=X,2,L
T(3,*)
#FdCustId=X,2,L
#FdCustNo=X,2,L

#RimFunCd=A,1,S
#RimCustId=X,10,S
#RimCustNo=A,7,S
!---------- AP Button AREA ----------
![Buttom資料查詢]
#BUF1=X,42,S
!---------- AP TEXT AREA Variables ----------
#IdNoHelp=X,30,S
T(2,1:依統一編號;2:依戶號)

#CHAIN=A,1,S
T(4,CHAIN$)

! 交易起始時的處理
#StartUp=A,1,S
E(0,@CustNo,0000000)

!隱藏統一編號,戶號
! #WKHIDE=X,1,S
! INVOKEJS(SHOW,p1,02,03,0)

!依統編或戶號
#IdNo=A,1,I
C(2,#IdNo,E(1,1),)
HELP(#IdNoHelp)
@C(2,#IdNo,E(1,1),)
V(H,#IdNoHelp)

#IdNoX=X,10,L
T(H,#IdNo,#IdNoHelp)

!統一編號
#CustId=X,10,I
C(3,#IdNo,1,$,S)
@V(7)A(ID_UNINO,0,#CustId)

!身分證找戶號
<include src="ROM.icf::L2R47.dec"/>
#Rim2R47CustId=X,10,S

#RimL2R47=X,1,S
T(2,@Rim2R47CustId,#CustId)
RESET_RIM(#RimL2R47,L2R47)
C(4,#CustId,s,$)
S(L2R47,1,#Rim2R47CustId)
R(1,L2R47)

!戶號
#CustNo=A,7,I
C(3,#IdNo,2,$,S)


#RimL1r09=X,1,S
E(0,@RimFunCd,1)
T(2,@RimCustId,)
E(0,@RimCustNo,#CustNo)
RESET_RIM(#RimL1r09,L1R09)
S(L1R09,1,#RimFunCd,#RimCustId,#RimCustNo)
R(1,L1R09)

#TimCustNo=A,7,S
C(3,#CustNo,0,$,E(0,#CustNo))
C(4,#CustId,$,E(0,#L2R47CustNo))
T(2,@RmkCustNo,#TimCustNo)
<include src="COM.icf::CustRmk.rtn"/>


#MRKEY=_,_,S
C(2,#IdNo,S,T(2,#L2R47CustNo),T(2,#CustNo))

#RPTFG=A,1,S

<include src="ROM.icf::L1R09.dec"/>
</sub>


!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[200,1000];printer.width=[20,20];">
[
["[L291B]                                 合併額度控管額度查詢"],
["#FdApplNo+核准號碼",#ApplNo],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]



!---------- AP TEXT AREA Variables ----------
! Help
<include src="HELPRIM.icf::HELPRIM.rtn"/>
#CDDEF0001=x,26,S
T(3,CdCode.CurrPrecision)

#CurrPrecisionHelp=x,1024,S 

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF0001)
R(1,XXR99)
CALL(#HelpProc) 
T(2,@CurrPrecisionHelp,#HelpDesc1) 

!YN
#YNHelp=X,30,S
T(3,Y:是;N:否)

#CHAIN=A,1,S
T(4,CHAIN$)
T(2,@RimTxCode,#TXCD)
IF(#CHAIN!=1,V(P,此為連動交易，請從交易:[L2019合併額度控管資料查詢]進入),$)

#NTXBUF=X,18,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@ApplNo,#NTXBUF,1,7)

#NTXBUF5=X,10,S
C(3,#CHAIN,1,T(4,NTXBUF5$),S)
C(4,#NTXBUF5,E(0,@RPTFG2,1),E(0,@RPTFG2,2))


!主要/共用戶號
#FdApplNo=X,2,L
#ApplNo=A,7,L

!用核准號碼(ApplNo)調L2R05
#SendL2r05=X,1,S
C(3,#ApplNo,0,S,$)
E(0,@RimFuncCode,5)
E(0,@RimCustNo,0)
E(0,@RimFacmNo,0)
E(0,@RimApplNo,#ApplNo)
E(0,@RimCaseNo,0)
RESET_RIM(#SendL2r05,L2R05)
S(L2R05,1,#RimFuncCode,#RimTxCode,#RimFKey,#RimCustNo,#RimFacmNo,#RimApplNo,#RimCaseNo)
R(1,L2R05)

#TimCustNo=A,7,S
C(3,#ApplNo,0,$,E(0,#L2r05CustNo))
T(2,@RmkCustNo,#TimCustNo)
<include src="COM.icf::CustRmk.rtn"/>


#RPTFG=A,1,S
#RPTFG2=A,1,S

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)
<include src="ROM.icf::L2R05.dec"/>
<include src="ROM.icf::L0Rim.dec"/>
<include src="ROM.icf::L2Rim.dec"/>
</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>


!---------- TXFORM = L291B ----------
<sub type="FORM" name="L291B">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L291B,$,S)

#SCRTIL=_,_,S
T(3,[L291B] 合併額度控管額度查詢)


#OApplNo=A,7,L
E(0,#ApplNo)

#OCurrencyCode=X,3,L

#OPrecision=N,1,S
E(0,0)T(H,#OCurrencyCode,#CurrPrecisionHelp)

#OLineAmt=m,14.2,L
#OLineAmtX=X,22,L
CALL2(#ADJUSTDOT,#OPrecision,#OLineAmt,#OLineAmtX)
P(R,#OLineAmtX)


#OUtilBal=m,14.2,L
#OUtilBalX=X,22,L
CALL2(#ADJUSTDOT,#OPrecision,#OUtilBal,#OUtilBalX)
P(R,#OUtilBalX)


#OUtilAmt=m,14.2,L
#OUtilAmtX=X,22,L
CALL2(#ADJUSTDOT,#OPrecision,#OUtilAmt,#OUtilAmtX)
P(R,#OUtilAmtX)

#OAvailable=m,14.2,L
#OAvailableX=X,22,L
CALL2(#ADJUSTDOT,#OPrecision,#OAvailable,#OAvailableX)
P(R,#OAvailableX)



!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

!核准號碼
#OOApplNo=A,7,O

!戶號額度
#OOCustNo=A,7,O
#OOFacmNo=A,3,O
#OOCustFacmNo=X,11,O
T(2,#OOCustNo+-+#OOFacmNo)
!戶名
#OOCustName=x,100,O
!案件編號
#OOCaseNo=A,7,O
!動支期限
#OODeadline=D,7,O
!幣別
#OOCurrencyCode=X,3,O

#OOPrecision=N,1,S
E(0,0)T(H,#OOCurrencyCode,#CurrPrecisionHelp)
!核准額度-因目前預設為新台幣為了讓occurs金額欄位比較好看故使用14(OOUtilAmt14)欄位,如後面開放外幣則使用X(OOUtilAmtX)欄位
#OOLineAmt=m,14.2,O
#OOLineAmtX=X,22,L
CALL2(#ADJUSTDOT,#OOPrecision,#OOLineAmt,#OOLineAmtX)
P(R,#OOLineAmtX)
#OOLineAmt14=m,14,L
E(0,#OOLineAmt)
!目前餘額
#OOUtilAmt=m,14.2,O
#OOUtilAmtX=X,22,L
CALL2(#ADJUSTDOT,#OOPrecision,#OOUtilAmt,#OOUtilAmtX)
P(R,#OOUtilAmtX)
#OOUtilAmt14=m,14,L
E(0,#OOUtilAmt)

!已動用額度餘額
#OOUtilBal=m,14.2,O
#OOUtilBalX=X,22,L
CALL2(#ADJUSTDOT,#OOPrecision,#OOUtilBal,#OOUtilBalX)
P(R,#OOUtilBalX)
#OOUtilBal14=m,14,L
E(0,#OOUtilBal)


!循環動用
#OORecycleCode=X,1,O



</sub>



!---------- TXFORM = L2018 ----------
<sub type="FORM" name="L2018">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L2018,$,S)

#SCRTIL=_,_,S
T(3,[L2018] 共同借款人資料查詢)

!修改
#Func2=A,1,S
E(0,2)

!刪除
#Func4=A,1,S
E(0,4)

!查1
#Func5=A,1,S
E(0,5)

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

!戶號
#OOCustNo=A,7,O
#OOFacmNo=A,3,O
#OOCustNoX=X,11,O
T(2,#OOCustNo+-+#OOFacmNo)
!戶名
#OOCustName=X,100,O

#OOApplNo=A,7,O

#OOCurrencyCode=X,3,O

#OOPrecision=N,1,S
E(0,0)T(H,#OOCurrencyCode,#CurrPrecisionHelp)

#OOLineAmt=m,14.2,O

#OOLineAmtX=X,22,L
CALL2(#ADJUSTDOT,#OOPrecision,#OOLineAmt,#OOLineAmtX)
P(R,#OOLineAmtX)


#OOCreateDate=D,7,O

#OOCreateEmpNo=X,6,O

#OOCreateEmpName=X,10,O

#OOCreateName=X,18,O
T(2,#OOCreateEmpNo+  +#OOCreateEmpName)

#OOLastUpdate=D,7,O

#OOLastUpdateEmpName=X,10,O

#OOLastUpdateName=X,18,O
T(2,#OOLastUpdate+  +#OOLastUpdateEmpName)

#OOLastUpdateEmpNo=X,6,O


#OOMApplNo=A,7,O

#OOMainFg=X,1,O
!修改
! #OOBUF2=X,18,S
! T(2,#Func2+#OOCustNo+#OOApplNo)

! #OOBTN2=X,1,S
! BIND(grid,{ cmd=CHAIN; ntxcd=L2118; ntxbuf=#OOBUF2})

!修改
#OOBUF2=X,18,S
T(2,#Func2+#OOApplNo)

#OOBTN2=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L2118; ntxbuf=#OOBUF2})
C(5,#OOMainFg,Y,$,BIND(grid,cancel))

!刪除
#OOBUF4=X,18,S
T(2,#Func4+#OOApplNo)

#OOBTN4=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L2118; ntxbuf=#OOBUF4})
C(5,#OOMainFg,Y,$,BIND(grid,cancel))

!查詢
#OOBUF5=X,18,S
T(2,#OOCustNo+#OOApplNo)

#OOBTN5=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L291A; ntxbuf=#OOBUF5})

</sub>



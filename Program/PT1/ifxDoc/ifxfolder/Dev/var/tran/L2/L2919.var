!---------- AP Button AREA ----------
![Buttom資料查詢]
#BTN1=X,15,I,ui:button;value:顧客資料查詢;enable:1;tabbable:0


!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>
!ClTypeCode
#CDDEF01=x,20,S
T(3,CdCode.ClTypeCode)

#ClTypeCodeHelp=x,1024,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF01)
R(1,XXR99)
CALL(#HelpProc)
T(2,@ClTypeCodeHelp,#HelpDesc1)

#CHAIN=A,1,S
T(4,CHAIN$)


#BUF1=X,18,S


#BIND1=X,1,S
BIND(#BTN1,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUF1;ntxbuf5=type<-1:CustId<-OOCustId})

!統一編號
#CustId=X,10,I
@V(7)
C(4,#CustId,S,$)
A(ID_UNINO,0,#CustId)

!身分證找戶號
#Rim2R47CustId=X,10,S

#RimL2R47=X,1,S
T(2,@Rim2R47CustId,#CustId)
RESET_RIM(#RimL2R47,L2R47)
C(4,#CustId,s,$)
S(L2R47,1,#Rim2R47CustId)
R(1,L2R47)

!如都沒輸入清空跳過
#MRKEY=_,_,S
T(2,#L2R47CustNo)


#TimCustNo=A,7,S
E(0,0)
C(4,#CustId,$,E(0,#L2R47CustNo))
T(2,@RmkCustNo,#TimCustNo)
<include src="COM.icf::CustRmk.rtn"/>

!調rim用參數
#RimCustId=X,10,S

#rimL1R07=X,1,S
T(2,@RimCustId,#CustId)
RESET_RIM(#rimL1R07,L1R07)
S(L1R07,1,#RimCustId)
R(1,L1R07)

#checkL1r07=X,1,S
T(2,@Borrower,#L1r07Borrower)

!借戶否
#Borrower=X,1,L


#RPTFG=A,1,S



<include src="ROM.icf::L2R47.dec"/>
<include src="ROM.icf::L1R07.dec"/>
</sub>


C(3,#CaseNo,0,S,$)
E(0,@RimCaseNo,#CaseNo)
RESET_RIM(#SendL2r10,L2R10)
S(L2R10,1,#RimCaseNo)
R(1,L2R10)

#SendL2r07=X,1,S
C(3,#CaseNo,0,S,$)
T(2,@RimCustId, )
E(0,@RimCustNo,#L2r10CustNo)

#NTXBUF=X,11,S
C(3,#CHAIN,1,T(4,NTXBUF$),HIDE(#BTN1),S)
T(1,@CustNo,#NTXBUF,1,7)

#NTXBUF5=X,10,S
C(3,#CHAIN,1,T(4,NTXBUF5$),HIDE(#BTN1),S)
C(4,#NTXBUF5,E(0,@RPTFG2,1),E(0,@RPTFG2,2))




#BUF1=X,18,S

#BIND1=X,1,S
BIND(#BTN1,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUF1;ntxbuf5=type<-1:CustNo<-OOCustNo})


!戶號
#CustNo=A,7,I
C(3,#CHAIN,1,s,$)



#CustName=X,100,L
C(4,#CustNo,S,$)

#CaseNo=A,7,I
C(3,#CustNo,0,$,S)
@C(3,#CaseNo,0,V(P,需擇一輸入查詢條件),$)

#RimCaseNo=A,7,S
#RimCustId=X,10,S
#RimCustNo=A,7,S
#RimTxCode=X,5,S

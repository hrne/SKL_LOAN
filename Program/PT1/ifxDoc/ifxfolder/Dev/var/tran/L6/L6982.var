!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L6982]                                 火險費轉列催收作業"],
["#FdCustNo+戶號",#CustNo],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
! #BTN1=X,15,I,ui:button;value:瀏覽;enable:0;tabbable:0
! #BUF1=X,42,S

!---------- AP TEXT AREA Variables ----------

#FdCustNo=X,2,L

#CHAIN=A,1,S
T(4,CHAIN$)

#SelectCodeHelp=x,100,S
T(3,1:昨日留存;2:本日新增;3:全部;4:本日處理;5:本日刪除;6:保留;7:未處理;9:未處理-轉催)

#NTXBUF=X,15,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@SelectCode,#NTXBUF,1,1)
! T(1,#NTXBUF,@CustNo,2,7)
! T(1,#NTXBUF,@TransCollDate,9,7)

#Col=A,1,S
C(3,#CHAIN,0,s,$)
INVOKEJS(SHOW,p1,2,3,0)

#DATE=D,8,S
T(4,DATE$)

#DT=D,7,S
T(1,#DATE,2,7)

! 點選連動交易 傳9 (不同於未處理按鈕)
#SelectCode=A,1,S
C(3,#CHAIN,1,s,$)
E(0,9)
!HELP(1: 昨日留存;2: 本日新增;3: 全部;4: 本日處理;5: 本日刪除;6:保留;7: 未處理;9: 未處理-轉催)
!@V(1,1,2,3,4,5,6,7,9)

#SelectCodeX=X,16,L
T(3,)T(H,#SelectCode,#SelectCodeHelp)
!C(2,#SelectCode,T(3,),
!               T(3,昨日留存),
!               T(3,本日新增),
!               T(3,全部),
!               T(3,本日處理),
!               T(3,本日刪除),
!               T(3,保留),
!               T(3,未處理),
!               T(3,),
!               T(3,未處理-轉催),
! )


!戶號
#CustNo=A,7,I
C(3,#CHAIN,1,s,$)
INVOKEJS(SHOW,p1,2,3,1)
@
C(3,#CustNo,0,INVOKEJS(SHOW,p1,2,2,0)INVOKEJS(SHOW,p1,3,3,1),INVOKEJS(SHOW,p1,2,2,1)INVOKEJS(SHOW,p1,3,3,0))
! SHOW(#BTN1)

#MRKEY=_,_,S
C(3,#CustNo,0,S,$)
T(2,#CustNo)

! #WKBTN1=X,1,S
! C(3,#CustNo,0,HIDE(#BTN1),SHOW(#BTN1))

! 轉催收日
#TransCollDate=D,7,S
C(3,#CHAIN,1,s,$)
C(3,#CustNo,0,$,S)
T(2,#DT)
@V(7)A(DATE,0,#TransCollDate)

#RPTFG=A,1,S
C(2,#SelectCode,
S,
E(0,4),
E(0,4),
E(0,4),
E(0,5),
E(0,6),
E(0,2),
E(0,3),
S,
E(0,7),
)

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

</sub>


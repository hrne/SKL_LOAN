#OOSupItem=X,20,S
#OOTitaSupNoX=X,27,S
T(2,#OOTitaSupNo+ +#OOSupItem)
!登放日期
#OORelDy=D,7,S
!交易序號
#OORelTxseq=X,18,S
!傳票號碼
#OOSlipNo=A,6,S
!經辦
#OOTitaTlrNo=X,6,S
#OOTlrItem=X,40,S
#OOTitaTlrNoX=X,47,S
T(2,#OOTitaTlrNo+ +#OOTlrItem)
#OOTitaTxtNo=X,8,S
#OOTxtNo=A,8,S
T(2,#OOTitaTxtNo)
! 最後修改日期
#OOLastUpdate=x,20,S
! 最後修改人員
#OOLastEmp=X,6,S
#OOLastEmpName=X,40,S
#OOLastEmpX=x,47,S
T(2,#OOLastEmp+ +#OOLastEmpName)

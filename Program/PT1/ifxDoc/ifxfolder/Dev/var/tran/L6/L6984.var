T(2,#OOCustId)
#ApplNo=X,7,S
E(0,#OOApplNo)
#TwDrawdownAmt=m,14,S
#UsDrawdownAmt=m,14.2,S
E(0,#TimDrawdownAmt)

#PieceCodeSecond=X,1,S

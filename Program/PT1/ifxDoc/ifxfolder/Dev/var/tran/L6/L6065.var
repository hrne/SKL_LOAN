<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD2.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)
!HIDE(#BTN1)

!#FirstIn=A,1,S
!E(0,1)

#button=x,100,S
T(2,{cmd=CHAIN; ntxcd=L6605; ntxbuf=1; ntxbuf5=type})ASGN(BTN_NEW1$)
K(MODIFYBUTTON,1,新增逾期增減碼)
K(MODIFYBUTTON,查詢)

#OverdueSignH=x,50,S
T(3,1:新增;2:減少)

</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L6065]                                 逾期新增減少原因查詢"],
["#FdOverdueSign+逾期增減碼",#OverdueSign],
["#FdOverdueCode+增減原因代號",#OverdueCode],

]
!---------- AP Button AREA ----------
![Buttom資料查詢]
!#BTN1=X,15,I,ui:button;value:新增增減原因代號;enable:1;tabbable:0;title:L6605 逾期新增減少原因維護
!#BUF1=X,42,S

!---------- AP TEXT AREA Variables ----------

#FdOverdueSign=X,2,L
#FdOverdueCode=X,2,L
#FdRPTFG=X,2,L
T(3,*)

#CHAIN=A,1,S
T(4,CHAIN$)

#NTXBUF=X,4,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)

#RPTFG=A,1,S
E(0,0)

!逾期增減碼
#OverdueSign=A,1,I
C(3,#CHAIN,1,T(1,#NTXBUF,1,4),$)
C(3,#CHAIN,1,s,$)
HELP(#OverdueSignH)
@C(3,#OverdueSign,0,s,$)
V(H,#OverdueSignH)

!增減原因代號
#OverdueCode=A,4,I
C(3,#CHAIN,1,T(1,#NTXBUF,1,4),$)
C(3,#CHAIN,1,s,$)
C(3,#OverdueSign,0,s,$)

#BUF1=X,6,S
!C(4,#OverdueCode,HIDE(#BTN1),SHOW(#BTN1))
!T(2,1+#OverdueSign+#OverdueCode)
!E(0,@FirstIn,0)

!#BIND1=X,1,S
!BIND(#BTN1,click, {cmd=CHAIN; ntxcd=L6605; ntxbuf=#BUF1})

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

!<include src="COM.icf::RPTFG.dec"/>

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!---------- TXFORM = L6065 ----------
<sub type="FORM" name="L6065">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6065,$,S)

#SCRTIL=_,_,S
T(3,[L6065] 逾期新增減少原因)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------

#OOverdueSign=A,1,L
T(2,#OverdueSign)

#OOverdueCode=A,4,L
T(2,#OverdueCode)

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)



! 逾期增減碼(TOTA)
#OOOverdueSign=A,1,L

! 增減原因代號(TOTA)
#OOOverdueCode=A,4,L

!增減原因
#OOOverdue=X,100,L

! 增減原因說明(TOTA)
#OOOverdueItem=X,100,L

!最後修改人員&日期
#OOLastUpdate=x,20,S

#OOLastEmp=x,47,S


! 修改的按鈕
#OOFunc2=A,1,S
E(0,2)

#OOBUF2=X,106,S
T(2,#OOFunc2+#OOOverdueSign+#OOOverdueCode+#OOOverdueItem)

#OOBTN2=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6605;text=修改;ntxbuf=#OOBUF2})

! 刪除的按鈕
#OOFunc4=A,1,S
E(0,4)

#OOBUF4=X,106,S
T(2,#OOFunc4+#OOOverdueSign+#OOOverdueCode+#OOOverdueItem)

#OOBTN4=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6605;text=刪除;ntxbuf=#OOBUF4})

</sub>



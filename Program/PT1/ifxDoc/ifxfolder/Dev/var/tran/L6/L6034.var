<sub type="FORM" name="L6034">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6034,$,S)

#SCRTIL=_,_,S
T(3,[L6034] 專案放款查詢)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------

#OOYearMonth=A,5,L
#OOOLoanBal=X,20,L
#OONLoanBal=X,20,L
#OODLoanBal=X,20,L
#OORemark=X,100,L


! 修改的按鈕
#OOBUF_2=X,136,S
T(2,2+#OOYearMonth)

#OOBTN_2=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6304;text=修改;ntxbuf=#OOBUF_2}))

! 刪除的按鈕
#OOBUF_4=X,136,S
T(2,4+#OOYearMonth)

#OOBTN_4=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6304;text=刪除;ntxbuf=#OOBUF_4}))

! 查詢的按鈕
!#OOBUF_5=X,136,S
!T(2,5+#OOYearMonth)

!#OOBTN_5=X,4,S
!BIND(grid,{cmd=CHAIN;ntxcd=L6304;text=查詢;ntxbuf=#OOBUF_5})
</sub>



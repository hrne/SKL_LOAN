<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD2.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)

!#button=x,100,S
!T(2,{cmd=CHAIN; ntxcd=L6304; ntxbuf=1; ntxbuf5=type})ASGN(BTN_NEW1$)
!K(MODIFYBUTTON,1,新增專案放款)
!K(MODIFYBUTTON,查詢)

</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[180,850];printer.width=[20,20];">
[
["[L6034]                                專案放款查詢"],
["#FdYear+民國年月",#sYear,"年",#sMonth,"月","(全部0為查詢全部)"] ,
]
!---------- AP Button AREA ----------
![Buttom資料查詢]

!---------- AP TEXT AREA Variables ----------

#FdYear=X,2,L
T(3,*)

#CHAIN=A,1,S
T(4,CHAIN$)

![維護記號](1:新增;2:修改;4:刪除;5:查詢)
#FunCode=A,1,S
C(2,#CHAIN,$,s)

#DATE=D,8,S
T(4,DATE$)

#Year=A,3,S
T(1,#DATE,2,3)

#Month=A,2,S
T(1,#DATE,5,2)

#sYear=A,3,I
@V(5,0,#Year)

#sMonth=A,2,I
C(3,#sYear,0,0,#Month)
@IF(#sYear==0,V(5,0,0),V(5,1,12))

#sYearMonth=A,5,S
E(0,#sYear*100+#sMonth)


#NTXBUF=x,63,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@EffectDate,1,7)
T(1,@Remark,8,100)
T(1,@Enable,108,1)

#RPTFG=A,1,S
E(0,0)

</sub>

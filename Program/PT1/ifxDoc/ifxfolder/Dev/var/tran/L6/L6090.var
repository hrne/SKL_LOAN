#OOBTN4=X,100,S
!C(5,#OORuleCode,CodeType,C(3,#IsAdmin,1,BIND(grid,{cmd=CHAIN;ntxcd=L6891;text=刪除;ntxbuf=#OOBUF4}),BIND(grid,cancel)),BIND(grid,{cmd=CHAIN;ntxcd=L6891;text=刪除;ntxbuf=#OOBUF4})) 

<include src="COM.icf::TRCTL.tom"/>
<include src="HELPRIM.icf::HELPRIM.tom"/>
TXFORM=L6970
#OOExecDate
#OOJobCode
#OOStepId
#OOStatus
#OOStepStartTime
#OOStepEndTime
^

</sub>


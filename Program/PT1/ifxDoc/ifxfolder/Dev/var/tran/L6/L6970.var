!---------- TXFORM = L6970 ----------
<sub type="FORM" name="L6970">

#SCRTIL=_,_,S
T(3,[L6970] 夜間批次控制檔查詢)

#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

!批次執行日期
#OOExecDate=D,7,L
!批次代號
#OOJobCode=x,30,L
!批次名稱
#OOJobItem=x,50,L
T(3, )
C(4,#OOJobCode,S,T(H,#OOJobCode,#JobCodeH))
!子批次代號
#OONestJobCode=x,30,L
!子批次名稱
#OONestJobItem=x,50,L
T(3, )
C(4,#OONestJobCode,S,T(H,#OONestJobCode,#JobCodeH))
!步驟代號
#OOStepId=x,30,L
!步驟名稱
#OOStepItem=x,50,L
T(3, )
C(4,#OOStepId,S,T(H,#OOStepId,#StepCodeH))
!執行結果
#OOStatus=x,1,L
#OOStatusX=x,10,L
SWITCH(#OOStatus,S,T(3,成功),
                 F,T(3,失敗),
                 T(3,))
!啟動時間 yyyy-MM-dd hh:mm:ss
#OOStepStartTime=x,20,L
!結束時間 yyyy-MM-dd hh:mm:ss
#OOStepEndTime=x,20,L


#OOJobTxSeq=x,20,L
#OOHaveUspErrorLog=x,1,L

#OOEffectX=x,8,L
IF(#OONestJobCode=="yearlyRptFlow" || #OONestJobCode=="monthlyRptFlow" ||#OONestJobCode=="dailyRptFlow",T(3,),T(3,影響換日))

#OO_BTN_UspError=X,1,S
IF(#OOHaveUspErrorLog=="Y",BIND(grid,{ cmd=CHAIN; ntxcd=L6973;text=錯誤查詢;ntxbuf=#OOJobTxSeq}),BIND(grid,cancel))

#OOFailRerunBuf=x,1000,S
T(2,#OOJobCode+#OOJobTxSeq+#OOStatus+#OOStepId)

! 失敗重跑 會執行同批號中失敗或尚未執行的批次的按鈕
! 單支重跑 只重跑選定的這支
#OO_BTN_FailRerun=X,1,S
SWITCH(#OOStatus,
	F,BIND(grid,{ cmd=CHAIN; ntxcd=L687A;text=失敗重跑;ntxbuf=#OOFailRerunBuf}),
	S,BIND(grid,{ cmd=CHAIN; ntxcd=L687A;text=單支重跑;ntxbuf=#OOFailRerunBuf}),
	BIND(grid,cancel))

#OOAllRerunBuf=x,1000,S
T(2,#OOJobCode+#OOJobTxSeq)

! 整批重跑
#OO_BTN_AllRerun=X,1,S
SWITCH(#OOStatus,
	F,BIND(grid,{ cmd=CHAIN; ntxcd=L687A;text=整批重跑;ntxbuf=#OOAllRerunBuf}),
	S,BIND(grid,{ cmd=CHAIN; ntxcd=L687A;text=整批重跑;ntxbuf=#OOAllRerunBuf}),
	BIND(grid,cancel))

</sub>


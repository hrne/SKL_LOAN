!---------- AP Button AREA ----------
![Buttom資料查詢]
! 功能 必須輸入(1: 新增;2: 修改;4: 刪除;5: 查詢)
#FUNCIND=_,_,S
E(0,5)

#CHAIN=A,1,S
T(4,CHAIN$)

#NTXBUF=X,6,S

#HideAll=A,1,S

!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>

! 選單代碼
#CdCodeBatchResultCode=x,30,S
T(3,CdCode.BatchResultCode)
#CdCodeJobCode=x,30,S
T(3,CdCode.JobCode)
#CdCodeStepCode=x,30,S
T(3,CdCode.StepCode)

! 選單內容
#BatchResultCodeH=x,100,S
#JobCodeH=x,1000,S
#StepCodeH=x,10000,S

! 根據選單代碼取得選單內容
#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CdCodeBatchResultCode,#CdCodeJobCode,#CdCodeStepCode)
R(1,XXR99)
CALL(#HelpProc)
T(2,@BatchResultCodeH,#HelpDesc1)
T(2,@JobCodeH,#HelpDesc2)
T(2,@StepCodeH,#HelpDesc3)

! 輸入欄位
! [執行結果篩選]
#BatchResultCode=A,1,I
HELP(#BatchResultCodeH)
@T(3,@BatchResultCodeX,)
C(4,#BatchResultCode,$,V(H,#BatchResultCodeH))

! 輸出欄位
! [執行結果篩選]
#BatchResultCodeX=X,20,L
C(4,#BatchResultCode,S,T(H,#BatchResultCode,#BatchResultCodeH))

! 輸入欄位
! [執行日期區間-起日]
#InputStartDate=D,7,I
!E(0,#SYSCDATE)
C(4,#InputStartDate,E(0,#SYSCDATE),$)
@V(7)
A(4,0,#SYSCDATE)
V(5,0010101,#SYSCDATE)

! 輸入欄位
! [執行日期區間-迄日]
#InputEndDate=D,7,I
!E(0,#SYSCDATE)
C(4,#InputEndDate,E(0,#SYSCDATE),$)
@V(7)
A(4,0,#InputEndDate)
V(5,#InputStartDate,#SYSCDATE)


! 輸入欄位
! [類別]
#ErrorTypeCodeHelp=X,30,S
T(3,0:全部;1:影響換日;2:不影響換日)
#ErrorTypeCode=A,1,I
HELP(#ErrorTypeCodeHelp)
C(3,#BatchResultCode,2,$,S)
@T(3,@ErrorTypeCodeX,)
C(4,#ErrorTypeCode,$,V(H,#ErrorTypeCodeHelp))

! 輸出欄位
! [執行結果篩選]
#ErrorTypeCodeX=X,20,L
C(4,#ErrorTypeCode,S,T(H,#ErrorTypeCode,#ErrorTypeCodeHelp))

#JobTxSeq=x,20,I

#RPTFG=A,1,S
E(0,0)


! iCompensateFlag


#batch-detail=x,100000,S

#batch-print=A,1,S

!#batch-rows 總筆數
#batch-rows=X,2,S

!#batch-rows-selected 勾選之筆數
#batch-rows-selected=X,2,S

!#batch-rows-ok  傳送成功筆數
#batch-rows-ok=X,2,S

!#batch-rows-error 傳送失敗筆數
#batch-rows-error=X,2,S

!#batch-row-status 該列傳送狀態
#batch-row-status=X,50,S

!#batch-row-txtno 該列傳送序號
#batch-row-txtno=X,14,O
</sub>



<sub type="FORM" name="L6025">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6025,$,S)

#SCRTIL=_,_,S
T(3,[L6025] 地政代碼查詢)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------

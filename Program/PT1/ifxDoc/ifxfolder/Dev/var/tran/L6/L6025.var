T(3,1)ASGN(LOOP$)

#OOCityCode=X,4,L
#OOLandOfficeCode=X,2,L
#OOCityItem=x,20,L
#OOLoanItem=x,20,L

!最後修改人員&日期
#OOLastUpdate=X,20,S

#OOLastEmp=x,47,S



! 修改的按鈕
#OOFunc2=A,1,S
E(0,2)

#OOBUF2=X,65,S
T(2,#OOFunc2+#OOCityCode+#OOLandOfficeCode)

#OOBTN2=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6205;text=修改;ntxbuf=#OOBUF2})

! 刪除的按鈕
#OOFunc4=A,1,S
E(0,4)

#OOBUF4=X,65,S
T(2,#OOFunc4+#OOCityCode+#OOLandOfficeCode)

#OOBTN4=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6205;text=刪除;ntxbuf=#OOBUF4})

</sub>



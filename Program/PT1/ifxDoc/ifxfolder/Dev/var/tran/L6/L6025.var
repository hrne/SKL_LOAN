<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD2.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)
!HIDE(#BTN1)

!#FirstIn=A,1,S
!E(0,1)

#button=x,100,S
T(2,{cmd=CHAIN; ntxcd=L6205; ntxbuf=1; ntxbuf5=type})ASGN(BTN_NEW1$)
K(MODIFYBUTTON,1,新增代碼)
K(MODIFYBUTTON,查詢)

</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L6025]                                 地政代碼查詢"],
["#FdCityCode+縣市別",#CityCode,#CityCodeX],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
!#BTN1=X,15,I,ui:button;value:新增保證人關係代碼;Enable:1;tabbable:0;title:L6607 保證人關係代碼維護
!#BUF1=X,42,S


!---------- AP TEXT AREA Variables ----------
#FdCityCode=X,2,L

#CHAIN=A,1,S
T(4,CHAIN$) 

! 他項權利縣市別
#CDDEF001=x,50,S
T(3,CdCode.ClOtherRightsCityCd)

#ClOtherRightsCityHelp=x,1024,S


#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF001)
R(1,XXR99)
CALL(#HelpProc)
T(2,@ClOtherRightsCityHelp,#HelpDesc1)

#NTXBUF=X,4,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@CityCode,#NTXBUF,1,2),$)

#NTXBUF5=X,10,S
C(3,#CHAIN,1,T(4,NTXBUF5$),S)
C(4,#NTXBUF5,E(0,@RPTFG2,1),E(0,@RPTFG2,2))

<include src="ROM.icf::L6R49.dec"/>
<include src="HELPRIM.icf::HELPRIM.rtn"/>

!調RIM參數
#RimCityCode=X,2,S
#RimAreaCode=X,3,S
#RimFunCd=A,1,S

! 調RIM取縣市代碼
#RimL6r49City=X,1,S
E(0,@RimFunCd,1)
T(2,@RimCityCode,)
S(L6R49,1,#RimFunCd,#RimCityCode)
R(1,L6R49)
T(B,@L6r49ResultCity,#L6r49Result+;+#ClOtherRightsCityHelp)

! 縣市代碼的下拉選單
#L6r49ResultCity=X,50000,S

#CityCode=X,4,I
HELP(#L6r49ResultCity)
@C(4,#CityCode,S,V(H,#L6r49ResultCity))

#CityCodeX=X,20,L
C(4,#CityCode,S,$)
T(H,#CityCode,#L6r49ResultCity)

#RPTFG=A,1,S
E(0,0)

#RPTFG2=A,1,S
C(3,#RPTFG,0,$,S)
C(4,#NTXBUF5,T(3,1),T(3,2))

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

</sub>


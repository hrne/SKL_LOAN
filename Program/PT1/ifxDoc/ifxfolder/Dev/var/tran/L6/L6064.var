#b1=X,1,S
BIND(#CodeBTN,click, {cmd=CHAIN; ntxcd=L6069; ntxbuf=#DefCode;ntxbuf5=type<-1:DefType<-OOType:DefCode<-OOCode:DefItemX<-OOItem})
!BIND(#CodeBTN,click, {cmd=CHAIN; ntxcd=L6069; ntxbuf=#DefCode;ntxbuf5=type<-1:DefType<-OOType:DefCode<-OOCode})
!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>

![維護記號](1:新增;2:修改;4:刪除;5:查詢)
#FunCode=A,1,S
C(2,#CHAIN,$,s)



!業務類別
#CDCODEMenuCode=x,20,S
T(3,CdCode.MenuCode)
#DefTypeH=x,1024,S

!類別代碼
#CDCODECodeType=x,20,S
T(3,CdCode.CodeType)
#DefCodeH=x,30000,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDCODEMenuCode,#CDCODECodeType)
R(1,XXR99)
CALL(#HelpProc)
T(2,@DefTypeH,#HelpDesc1)
T(2,@DefCodeH,#HelpDesc2)

#CHAIN=A,1,S
T(4,CHAIN$)

#NTXBUF=x,63,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@DefType,#NTXBUF,1,2)
T(1,@DefCode,#NTXBUF,3,20)
T(1,@Code,#NTXBUF,23,20)
!T(1,@DefItem,#NTXBUF,43,20)
HIDE(#CodeBTN)

#WKTF=X,1,S
C(3,#CHAIN,1,T(F,@DefType,0)T(F,@DefCode,0)T(F,@DefItemX,0),$)

!業務類別
#DefType=X,2,I
C(3,#CHAIN,1,s,$)
HELP(#DefTypeH)
@T(3,@DefTypeX,)
C(4,#DefType,$,V(H,#DefTypeH))
#DefTypeX=x,20,L
C(4,#DefType,S,T(H,#DefType,#DefTypeH))


#b2buf=x,200,S
T(2,#DefType+#DefCode)
#b2=X,1,S
BIND(#CodeBTN,click, {cmd=CHAIN; ntxcd=L6069; ntxbuf=#b2buf;ntxbuf5=type<-1:DefType<-OOType:DefCode<-OOCode})


!代碼檔代號
#DefCode=x,20,I
C(3,#CHAIN,1,s,$)

!處理
#b3buf=x,200,S
T(2,#DefType+#DefCode)
#b3=X,1,S
BIND(#CodeBTN,click, {cmd=CHAIN; ntxcd=L6069; ntxbuf=#b3buf;ntxbuf5=type<-1:DefType<-OOType:DefCode<-OOCode})

#Rim6R33DefType=X,2,S

#Rim6R33DefCode=x,20,S

#SendL6R33=X,1,S
IF(#DefType !=""&& #DefCode !="",$,S)
T(2,@Rim6R33DefType,#DefType)
T(2,@Rim6R33DefCode,#DefCode)
RESET_RIM(#SendL6R33,L6R33)
S(L6R33,1,#Rim6R33DefType,#Rim6R33DefCode)
R(1,L6R33)
E(0,@RimFlag,#L6R33Flag)

#RimFlag=9,1,S
IF(#DefType !=""&& #DefCode !="",$,S)
C(3,#RimFlag,0,V(P,業務類別有誤),S)


!代碼檔名稱
#DefItemX=x,80,L
C(4,#DefCode,S,T(H,#DefCode,#DefCodeH))

!代碼
#Code=x,20,I

#CodeItem=x,50,I
!C(4,#Code,$,S)

@
IF(#DefType=="" & #DefCode=="" & #Code=="" & #CodeItem=="",V(P,業務類別、代碼類別代號、代碼、代碼說明需擇一輸入),$)

#RPTFG=A,1,S
!C(4,#DefItem,E(0,0),E(0,1))

!<include src="COM.icf::RPTFG.dec"/>
<include src="ROM.icf::L6R33.dec"/>

#TXMT=A,1,S
!C(3,#CHAIN,0,$,E(0,@CHAIN,0)XMT())
</sub>


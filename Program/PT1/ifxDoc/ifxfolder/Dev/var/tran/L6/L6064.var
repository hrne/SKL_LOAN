<sub type="FORM" name="L6064">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6064,$,S)

#SCRTIL=_,_,S
T(3,[L6064] 各類代碼檔)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

! 代碼(TOTA)
#OODefCode=x,20,L

#OODefItem=x,50,L
T(H,#OODefCode,#DefCodeH)

! 代碼(TOTA)
#OOCode=x,20,L

! 說明(TOTA)
#OOItem=x,100,L

! 業務類別(TOTA)
#OOType=A,2,L

#OOTypeX=x,50,L
T(H,#OOType,#DefTypeH)


! 啟用記號(TOTA)
#OOEnable=X,1,L

!最後修改人員&日期
#OOLastUpdate=x,20,S

#OOLastEmp=x,47,S


! 修改的按鈕
#OOFunc2=A,1,S
E(0,2)

#OOBUF2=x,41,S
T(2,#OOFunc2+#OODefCode+#OOCode)

#OOBTN2=X,4,S
BIND(grid,cancel)
! CHAIN 1 時必為隱藏
C(3,#CHAIN,1,s,$)
! DefCode為CodeType時，須為Admin才可修改
! DefCode為其他時，皆可修改
C(5,#OODefCode,CodeType,C(3,#IsAdmin,1,$,s),$) 
BIND(grid,{cmd=CHAIN;ntxcd=L6604;text=修改;ntxbuf=#OOBUF2})

! 刪除的按鈕
#OOFunc4=A,1,S
E(0,4)

#OOBUF4=x,41,S
T(2,#OOFunc4+#OODefCode+#OOCode)

#OOBTN4=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6604;text=刪除;ntxbuf=#OOBUF4})

</sub>



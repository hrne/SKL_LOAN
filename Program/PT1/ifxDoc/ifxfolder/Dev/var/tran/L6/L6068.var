<sub type="FORM" name="L6068">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6068,$,S)

#SCRTIL=_,_,S
T(3,[L6068] 報表代號對照檔)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

! 報表代號(TOTA)
#OOFormNo=X,10,L

! 報表名稱(TOTA)
#OOFormName=x,80,L

! 報表週期(TOTA)
#OOCycle=A,2,L

#OOCycleX=X,20,L
T(H,#OOCycle,#RptCycle)

! 寄送記號(TOTA)
#OOSendCode=A,1,L

#OOSendCodeX=X,40,L
T(H,#OOSendCode,#RptSendCode)

! 書面寄送(TOTA)
#OOLetter=A,1,L

! 簡訊寄送(TOTA)
#OOMessage=A,1,L

! 電子郵件寄送(TOTA)
#OOEmail=A,1,L

! 用途說明(TOTA)
#OOUsageDesc=x,80,L

! 簽核記號(TOTA)
#OOSignCode=A,1,L

#OOSignCodeX=X,20,L
T(H,#OOSignCode,#RptSignCode)

!浮水印
#OOWatermarkFlag=A,1,L

#OOWatermarkFlagX=X,20,L
T(H,#OOWatermarkFlag,#RptWaterCode)

! 啟用記號(TOTA)
#OOEnable=X,1,L

#OOEnableX=X,20,L
T(H,#OOEnable,#EnableFg)

! 機密等級(TOTA)
#OOConfidentiality=X,1,L

#OOConfidentialityX=X,20,L
T(H,#OOConfidentiality,#Confidentiality)

!敏感性資料記錄記號(TOTA)
#OOApLogFlag=X,1,L
#OOApLogFlagX=X,20,L
T(H,#OOApLogFlag,#ApLogFlag)

!課組別
#OOGroupNo=X,1,L
#OOGroupNoX=X,20,L
T(H,#OOGroupNo,#GroupNo)


!最後修改人員&日期
#OOLastUpdate=x,20,S

#OOLastEmp=x,47,S

! 修改的按鈕
#OOFunc2=A,1,S
E(0,2)

#OOBUF2=X,181,S
T(2,#OOFunc2+#OOFormNo+#OOFormName+#OOCycle+#OOSendCode+#OOLetter+#OOMessage+#OOEmail+#OOSignCode+#OOUsageDesc+#OOEnable+#OOConfidentiality+#OOApLogFlag+#OOGroupNo)

#OOBTN2=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6608;text=修改;ntxbuf=#OOBUF2})

! 刪除的按鈕
#OOFunc4=A,1,S
E(0,4)

#OOBUF4=X,180,S
T(2,#OOFunc4+#OOFormNo+#OOFormName+#OOCycle+#OOSendCode+#OOLetter+#OOMessage+#OOEmail+#OOSignCode+#OOUsageDesc+#OOEnable+#OOConfidentiality+#OOApLogFlag+#OOGroupNo)

#OOBTN4=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6608;text=刪除;ntxbuf=#OOBUF4})

</sub>



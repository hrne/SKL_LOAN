!#BUF1=X,42,S

!---------- AP TEXT AREA Variables ----------
! Help
<include src="HELPRIM.icf::HELPRIM.rtn"/>
#RptCycle=x,1000,S
T(3,CdCode.RptCycle)

#RptSendCode=x,1000,S
T(3,CdCode.RptSendCode)

#RptSignCode=x,1000,S

!---------- BASIC LABEL Variables ----------
<sub type="SYS">
</sub>

<sub type="SYS" name="label">
!#TXCD
!T(3,L7913)

![資料表頭(HEADER)長度]
#INQHD=n,3,S
E(0,131)ASGN(INQHD$)

![每筆資料明細(OCCURS)長度]
#INQLEN=n,3,S
E(0,10)ASGN(INQLEN$)

![多筆查詢之一個畫面有N筆資料]
#INQREC=n,2,S
E(0,40)ASGN(INQREC$)

![畫面顯示的明細間的高度]
#LOOPH=n,3,S
E(0,1)ASGN(LOOPHEIGHT$)

![每張印錄單列印的筆數]
#INQPRT=n,2,S
E(0,40)ASGN(INQPRT$)

#BUF0=X,20,S
T(2,1)

#button=x,100,S
T(2,{ cmd=CHAIN; ntxcd=L7113; ntxbuf=#BUF0})ASGN(BTN_NEW1$)
K(MODIFYBUTTON,1,新增案件申請留存)

</sub>

<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)

</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L7913" layout="cols=1;screen.width=[250,750];printer.width=[20,80];order=1">
[ 
["[L7913]                                 (eloan)評級資訊查詢"],
["#FdMain+查詢條件","@(借戶戶號、eLoan案件編號，需擇一輸入)"],
[#<->#,"#FdItemCode+項目",#ItemCode,#ItemCodeX],
[#<->#,"#FdCustNo+借戶戶號",#CustNo],
[#<->#,"#FdCaseNo+eLoan案件編號",#CaseNo],
]

#FdMain=X,2,L
T(3,*)

#RimTxCode=X,5,S

#CHAIN=A,1,S
T(4,CHAIN$) 
T(2,@RimTxCode,#TXCD)

#ItemCodeHelp=x,1024,S
T(3,1:ELOAN評級;2:案件申請留存)

#NTXBUF=X,31,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@CustNo,#NTXBUF,1,7)
T(1,@CaseNo,#NTXBUF,8,7)

#FdItemCode=X,2,L
!項目
#ItemCode=N,1,I
HELP(#ItemCodeHelp)
@V(H,#ItemCodeHelp)
#ItemCodeX=X,20,L
T(3,)
T(H,#ItemCode,#ItemCodeHelp)


#FdCustNo=X,2,L

!借戶戶號
#CustNo=A,7,I
C(3,#CHAIN,1,s,$)

#RimCustId=X,10,S
#RimCustNo=A,7,S

#SendL2r07=X,1,S
C(3,#CustNo,0,S,$)
T(2,@RimCustId,)
E(0,@RimCustNo,#CustNo)
RESET_RIM(#SendL2r07A,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo)
R(1,L2R07)

<include src="ROM.icf::L2R07.dec"/>

#FdCaseNo=X,2,L
C(3,#CustNo,0,T(3,*),S)


!eLoan案件編號
#CaseNo=A,7,I
C(3,#CustNo,0,$,S)
@V(2,0)

#RPTFG=A,1,S
C(3,#ItemCode,1,E(0,1),E(0,2))




#MRKEY=_,_,S
C(4,#CustNo,S,$)
T(2,#CustNo)

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!------------ TXFORM = L7913 -----------
<sub type="FORM" name="L7913">
!#SCRPRT=_,_,S
!C(5,#TXFORM,L7913,$,S)

#SCRTIL=_,_,S
T(3,[L7913] (eloan)評級資訊查詢)



!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

#OOCustNo=A,7,S

#OORspCaseNo=A,7,S

#OOModifyDate=X,15,S

#OORspTotalRatingsScore=m,5.2,S

#OORspTotalRatings=X,4,S

#OOLogNo=X,11,S

! ELOAN評級 查詢
#OOBUFA=X,50,S
T(2,5+#ItemCode+#OOLogNo)

#OOBTNA=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L7113; ntxbuf=#OOBUFA})


! 案件申請留存 修改
#OOBUFB=X,50,S
T(2,2+#ItemCode+#OOLogNo)

#OOBTNB=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L7113; ntxbuf=#OOBUFB})

! 案件申請留存 刪除
#OOBUFC=X,50,S
T(2,4+#ItemCode+#OOLogNo)

#OOBTNC=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L7113; ntxbuf=#OOBUFC})

! 案件申請留存 查詢
#OOBUFD=X,50,S
T(2,5+#ItemCode+#OOLogNo)

#OOBTND=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L7113; ntxbuf=#OOBUFD})

</sub>


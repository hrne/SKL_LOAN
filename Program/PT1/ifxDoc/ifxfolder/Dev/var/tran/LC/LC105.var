!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,800];printer.width=[20,20];">
[
["[LC105]                                 上傳附件刪除"],
["#FdTranNo+交易代號",#TranNo],
["#FdCustNo+戶@@@@號",#CustNo,"-",#FacmNo,"-",#BormNo],
["#FdMrKey+參考編號",#MrKey],
["#FdNote+說@@@@明",#Note],
]

!---------- AP Button AREA ----------
![Buttom資料查詢]
!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>


#FdTranNo=X,2,L
#FdCustNo=X,2,L
#FdMrKey=X,2,L
#FdDesc=X,2,L
#FdNote=X,2,L

#CHAIN=A,1,S
T(4,CHAIN$)
IF(#CHAIN!=1,V(P,此為連動交易，LC014請從入口交易進入),$)

! 刪除進入
#Deal_Delete=X,1,S
C(5,#TranKey_Tmp,4,$,S)
K(MODIFYBUTTON,刪除)
E(0,@FUNCIND,4)

![連動交易]
#NTXBUF=X,700,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@TranKey_Tmp,#NTXBUF,1,1)
T(1,@TranNo,#NTXBUF,2,5)
T(1,@CustNo,#NTXBUF,7,7)
T(1,@FacmNo,#NTXBUF,14,3)
T(1,@BormNo,#NTXBUF,17,3)
T(1,@MrKey,#NTXBUF,20,100)
T(1,@Note,#NTXBUF,120,100)
T(1,@FileNo,#NTXBUF,220,8)


![刪除]
#TranKey_Tmp=X,1,S

!交易代號
#TranNo=X,5,L

!戶號
#CustNo=A,7,L

#FacmNo=A,3,L

#BormNo=A,3,L

#MrKey=x,100,L

#Note=x,100,L


#MRKEY=_,_,S
C(3,#CustNo,0,S,T(2,#CustNo+-+#FacmNo+-+#BormNo))

#FileNo=X,100,L

#RPTFG=A,1,S

!刪除隱藏重新交易按鈕
#AGAIN=X,1,S
E(0,1)
ASGN(AGAIN$)


!#CHAINXMT=X,1,S
!C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

</sub>


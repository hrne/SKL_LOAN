!---------- AP Button AREA ----------
![Buttom資料查詢]

! 2021-07-28 智偉增加
#FUNCIND=_,_,S
E(0,5)
<include src="COM.icf::CheckAuth.rtn"/>

!連接L2016

#R1=X,1,L
INVOKEJS(SHOW,#R1,0)
K(LIGHT,#R1,TXTRED,0)

! 必輸欄位
##loop{times:49,i:2}
#R{i}=X,1,L
##end

! 永遠為必輸欄位
#ALWAYS_HIGHLIGHT=@,1,S
T(3,@R1,*)

!隱藏2-10行
#HIDEAPPLNO=X,1,S
INVOKEJS(SHOW,p1,2,50,0)

#APPLNO0=A,7,S
E(0,0)

! 隱藏APPLNO
##loop{times:49,i:2}
#HIDEIDALL{i}=X,1,S
HIDE(#APPLNO{i},#Btn{i})
##end
!---------- AP TEXT AREA Variables ----------
#CHAIN=A,1,S
T(4,CHAIN$)

#INIT=X,1,S
CALL(#ALWAYS_HIGHLIGHT)

#BufSerch=X,15,S


! ---------------
##loop{times:50,i:1,j:0,k:2}

#Btn{i}=X,1,S,ui:button;value:核准號碼查詢;enable:1;tabbable:0

#BindSearch{i}=X,1,S
BIND(#Btn{i},click, {cmd=CHAIN; ntxcd=L3001; ntxbuf=#BufSerch;ntxbuf5=type<-1:APPLNO{i}<-OOApplNo})

![核准號碼]
#APPLNO{i}=A,7,I
IF({i}!=1 && #APPLNO{j}==0,S,$))
@IF({i}==1,V(2,0),$)

#CheckApplNO{i}=X,1,S
IF({k}<=50,$,S)
IF(#APPLNO{i}>0,SHOW(#APPLNO{k},#Btn{k}),HIDE(#APPLNO{k},#Btn{k}))
C(3,#APPLNO{i},0,INVOKEJS(SHOW,p1,{k},50,0),INVOKEJS(SHOW,p1,2,{k},1))

##end

#RPTFG=A,1,S
E(0,0)

#PRINTRIM=X,200,S
T(6B,LCR98+1+#APPLNO1)

</sub>


!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L9711]                                 放款到期明細表及通知單"],
["#R1+@到期日",#ACCTDATE_ST,#ACCTDATE_ED],
["#R2+@戶號",#CUSTNO," ( 0:全部 )"],
["#FdMsg+@注意事項",#Msg],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
! 功能 必須輸入(1: 新增;2: 修改;4: 刪除;5: 查詢)
#FUNCIND=_,_,S
E(0,5)
<include src="COM.icf::CheckAuth.rtn"/>

! 必輸欄位
##loop{times:2,i:1}
#R{i}=X,1,L
INVOKEJS(SHOW,#R{i},0)
K(LIGHT,#R{i},TXTRED,0)
##end

! 永遠為必輸欄位
#ALWAYS_HIGHLIGHT=@,1,S
T(3,@R1,*)
T(3,@R2,*)

#FdMsg=X,1,L
#Msg=x,20.100,L
K(LIGHT,#Msg,TXTRED,0)
INVOKEJS(SHOW,p1,3,3,0)


!---------- AP TEXT AREA Variables ----------
#CHAIN=A,1,S
T(4,CHAIN$)

#INIT=X,1,S
CALL(#ALWAYS_HIGHLIGHT)

! 取得系統會計日期
#DT1=D,8,S
T(4,DATE$)
D(7,2,#DT1,4,0)

#WkDate=X,8,S
T(1,#DT1,2,5)
T(B,#WkDate+01)

#DT2=D,7,S
E(0,#WkDate)

! D(7,2,#NNN,#MMM,#DDD) 計算#NNN(起日)往後#MMM月+#DDD日之日期
! D(7,3,#NNN,#MMM,#DDD) 計算#NNN(起日)往前#MMM月+#DDD日之日期
! 迄日預設值處理
! 先找到五個月後的日期,然後把日改為1,再往回推一天,找到後四個月之月底日
#DT3=D,8,S
D(7,2,#DT1,1,0)

! 把日期改為01
#WkDate=_,_,S
T(1,#DT3,1,6)
T(B,#WkDate+01)

! 往回推一天
#DT4=D,8,S
E(0,#WkDate)
D(7,3,#DT4,0,1)


![會計日期-起日]
#ACCTDATE_ST=D,7,I
E(0,#DT2)
@V(7)
V(5,0, 9999999)
A(DATE,0,#ACCTDATE_ST)

![會計日期-迄日]
#ACCTDATE_ED=D,7,I
E(0,#DT4)
@V(7)
V(5,#ACCTDATE_ST, 9999999)
A(DATE,0,#ACCTDATE_ED)

![戶號]
#CUSTNO=A,7,I
!E(0,0)

<include src="ROM.icf::L1R20.dec"/>

#CustNo=A,7,S
E(0,#CUSTNO)

#FacmNo=A,3,S
E(0,0)

#FormNo=x,10,S
T(3,L9711)

#RimFlag=A,1,S

#L1R20Rim2=X,1,S
C(3,#CustNo,0,S,$)
RESET_RIM(#L1R20Rim2,L1R20)
E(0,@RimFlag,0)
S(L1R20,01,#CustNo,#FacmNo,#FormNo,#RimFlag)
R(1,L1R20)
T(2,@Msg,#L1R20Msg)

#ShowMsg=A,1,S
C(3,#CustNo,0,E(0,0),C(4,#Msg,E(0,0),E(0,1)))
INVOKEJS(SHOW,p1,3,3,#ShowMsg)

</sub>


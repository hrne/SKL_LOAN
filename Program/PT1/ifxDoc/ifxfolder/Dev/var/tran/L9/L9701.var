!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,80];">
[
["[L9701]                                 客戶往來交易明細表"],
["#R1+@戶號",#CustNo],
["#R2+@日期",#DateType,#DateTypeX,#BeginDate,#EndDate],
["#R3+@訂正別",#CorrectType,#CorrectCodeX],
["#R4+@報表",#ReportType,#ReportTypeX],
["#FdMsg+@注意事項",#Msg],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
! 功能 必須輸入(1: 新增;2: 修改;4: 刪除;5: 查詢)
#FUNCIND=_,_,S
E(0,5)
<include src="COM.icf::CheckAuth.rtn"/>

! 必輸欄位
##loop{times:4,i:1}
#R{i}=X,1,L
INVOKEJS(SHOW,#R{i},0)
K(LIGHT,#R{i},TXTRED,0)
##end

! 永遠為必輸欄位
#ALWAYS_HIGHLIGHT=@,1,S
T(3,@R1,*)
T(3,@R2,*)
T(3,@R3, )
T(3,@R4,*)

#FdMsg=X,1,L
#Msg=x,20.100,L
K(LIGHT,#Msg,TXTRED,0)
INVOKEJS(SHOW,p1,5,5,0)

!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>
!DateType
#CDCODEDateType=x,20,S                  
T(3,CdCode.DateType)                  
                                      
#DateTypeHelp=x,50,S

!ReportType
#CDCODEReportType=x,20,S                  
T(3,CdCode.ReportType)                  
                                         
#ReportTypeHelp=x,100,S        

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDCODEDateType,#CDCODEReportType)
R(1,XXR99)
CALL(#HelpProc)
T(2,@DateTypeHelp,#HelpDesc1)
T(B,@ReportTypeHelp,#HelpDesc2+;9:全部)

T(3,@ReportTypeHelp,1:客戶往來本息明細表（額度）;2:客戶往來本息明細表（撥款）;3:客戶往來交易明細表;9:全部)


#CorrectTypeHelp=X,100,S
T(3,0:未訂正;9:全部)

#CHAIN=A,1,S
T(4,CHAIN$)

#INIT=X,1,S
CALL(#ALWAYS_HIGHLIGHT)

#DT1=D,8,S
T(4,DATE$)

#DT2=D,7,S
T(1,#DT1,2,7)

#DT_YM=A,5,S
T(1,#DT1,2,5)

!戶號 必須輸入
#CustNo=A,7,I
@V(2,0)

! 調Rim相關 開始
! 定義Rom欄位名稱、型態、長度
<include src="ROM.icf::L1R01.dec"/>

! 宣告Rim欲傳送的欄位1
! 欄位名稱即為後端在titaVo get值時的KEY
! 要注意欄為型態跟長度
#RimCustId=X,10,S

! 宣告Rim欲傳送的欄位2
#RimCustNo=A,7,S

#RimFunCd=A,1,S
E(0,5)

#RimL1r01B=X,1,S
C(3,#CustNo,0,S,$)
T(2,@RimCustId,)
E(0,@RimCustNo,#CustNo)
RESET_RIM(#RimL1r01B,L1R01)
S(L1R01,1,#RimCustId,#RimCustNo,#RimFunCd)
R(1,L1R01)

#checkL1r01B=X,1,S
C(3,#CustNo,0,S,$)

!日期 必須輸入；1.入帳日 2.會計日
#DateType=A,1,I
HELP(#DateTypeHelp)
@T(3,@DateTypeX,)
V(H,#DateTypeHelp)

#DateTypeX=X,6,L
T(H,#DateType,#DateTypeHelp)

!日期區間 必須輸入，可修改 1.查詢起日預設值為本月1日，可修改。 2.查詢止日預設值為本營業日，可修改，不可小於查詢起日，不可大於本營業日。
#BeginDate=D,7,I
T(2,#DT_YM+01)
@V(7)A(DATE,0,#BeginDate)
V(5,0,#DT2)

#EndDate=D,7,I
T(2,#DT2)
@V(7)A(DATE,0,#EndDate)
V(5,#BeginDate,#DT2)

!訂正別
#CorrectType=A,1,I
HELP(#CorrectTypeHelp)
@T(3,@CorrectCodeX,)
V(H,#CorrectTypeHelp)

#CorrectCodeX=X,6,L
T(H,#CorrectType,#CorrectTypeHelp) 

!報表
#ReportType=A,1,I
E(0,1)
HELP(#ReportTypeHelp)
@T(3,@ReportTypeX,)
V(H,#ReportTypeHelp)

#ReportTypeX=X,30,L
T(H,#ReportType,#ReportTypeHelp)

<include src="ROM.icf::L1R20.dec"/>

#FacmNo=A,3,S
E(0,0)

#FormNo=x,10,S
T(3,L9701)

#RimFlag=A,1,S

#L1R20Rim2=X,1,S
C(3,#CustNo,0,S,$)
RESET_RIM(#L1R20Rim2,L1R20)
E(0,@RimFlag,0)
S(L1R20,01,#CustNo,#FacmNo,#FormNo,#RimFlag)
R(1,L1R20)
T(2,@Msg,#L1R20Msg)

#ShowMsg=A,1,S
C(3,#CustNo,0,E(0,0),C(4,#Msg,E(0,0),E(0,1)))
INVOKEJS(SHOW,p1,5,5,#ShowMsg)

</sub>


!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[140,850];printer.width=[20,20];">
[
["[L9740]                                 公會無自用住宅利率報送"],
["@撥款日期",#DrawDownDate],
["@續期放款利率大於",#Rate,"％"],
]
!---------- AP TEXT AREA Variables ----------

#FUNCIND=_,_,S
E(0,5)
<include src="COM.icf::CheckAuth.rtn"/>

! 取得系統會計日期
#DT1=D,8,S
T(4,DATE$)
D(7,2,#DT1,2,0)

#WkDate=X,8,S
T(1,#DT1,2,5)
T(B,#WkDate+01)

#DT2=D,7,S
E(0,#WkDate)

![撥款日]
#DrawDownDate=D,7,I
E(0,#DT2)
@V(7)
A(DATE,0,#DrawDownDate)
!V(5,#DT2, 9999999)

#Rate=n,2.2,I
@V(5,0.00,99.99)


</sub>


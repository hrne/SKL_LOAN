!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[120,850];printer.width=[20,20];">
[
["[L9729]                             歷史封存資料搬運結果明細"],
["@執行日期",#InputDate],
["@查詢種類",#InputType,#InputTypeX],
]
!---------- AP TEXT AREA Variables ----------

!連動記號
#CHAIN=A,1,S
T(4,CHAIN$)

!連動傳入參數
#NTXBUF=X,1,S
C(3,#CHAIN,1,T(4,NTXBUF$),$)
C(3,#CHAIN,1,E(0,#InputDate,#SYSDATE)T(2,#InputType,#NTXBUF)XMT(),$)
#RPTFG=A,1,S
E(0,0)

#DATE=D,8,S
T(4,DATE$)

#DT=D,7,S
T(1,#DATE,2,7)

!執行日期
#InputDate=D,7,I
C(4,#InputDate,E(0,#SYSDATE),$)
@V(7)
A(DATE,0,#InputDate)
IF(#InputDate>#DT,V(P,不可大於會計日),$)

!查詢種類
#InputTypeHelp=X,40,S
T(3,1:已結清並領取清償證明五年之交易明細)

#InputType=A,1,I
HELP(#InputTypeHelp)
@T(3,@InputTypeX,)
V(H,#InputTypeHelp)

#InputTypeX=X,40,L
T(H,#InputType,#InputTypeHelp)

</sub>


!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L9723]                                 放款有效客戶數                                                           "],
!["#FdReportDate+年月份",#ReportDate],
["#FdReportDate+@年月份",#ReportDateY,"@/@",#ReportDateM],
]
!---------- AP Button AREA ----------

#button=x,1,S
K(MODIFYBUTTON,查詢)

! 2021-07-28 智偉增加
#FUNCIND=_,_,S
E(0,5)
<include src="COM.icf::CheckAuth.rtn"/>

!---------- AP TEXT AREA Variables ----------

#FdReportDate=X,2,L
T(3,*)

!#ReportDate=D,7,I
#ReportDate=D,8,S
D(7,3,#SYSDATE,1,0)
!E(0,#SYSDATE)
!@
!V(7)
!A(DATE,0,#ReportDate)

!今天日期(系統日)
#Today=A,8,S
D(6,2,#SYSDATE,0)

!前一個月日期
#LastDay=A,8,S
D(7,3,#Today,1,0)

!本月底營業日
#MFBSDYDate=A,8,S
D(6,2,#MFBSDY,0)

!當月年份    
#ThisYear=A,3,S
T(1,#Today,2,3)
!當月月份    
#ThisMonth=A,2,S
T(1,#Today,5,2)

!上月年份    
#LastYear=A,3,S
T(1,#LastDay,2,3)
!上月月份    
#LastMonth=A,2,S
T(1,#LastDay,5,2)

#DefaultYear=A,3,S
C(3,#Today,#MFBSDYDate,E(0,#ThisYear),E(0,#LastYear))

#DefaultMonth=A,2,S
E(0,#ThisMonth)


#ReportDateY=N,3,I
E(0,#DefaultYear)
@V(2,0)
V(5,0,#DefaultYear)

#ReportDateM=A,2,I
E(0,#DefaultMonth)
@V(2,0)
IF(#ReportDateY==#DefaultYear,V(5,0,#DefaultMonth),V(5,0,12))

#RPTFG=A,1,S
E(0,0)

</sub>


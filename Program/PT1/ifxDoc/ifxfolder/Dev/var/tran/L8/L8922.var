!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L8922]                                 疑似洗錢交易合理性查詢"],
["#FdType+查詢種類",#Type,#TypeX],
["#FdAcDate+入帳日期",#AcDateStart,"~",#AcDateEnd],
["#FdFactor+交易樣態",#Factor,#FactorX],
!["#FdRPTFG+顯示方式",
!<include src="COM.icf::RPTFG.scr"/>
!],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
!#BUF1=X,42,S

#FdAcDate=X,2,L
T(3,*)
#FdFactor=X,2,L
T(3,*)
#FdType=X,2,L
T(3,*)
#FdRPTFG=X,2,L
T(3,*)

!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>

!主管1 經辦3
#Level=A,1,S
T(4,LEVEL$)

! Type
#TypeHelp=x,100,S
T(3,1:合理性;2:延遲交易)


!查詢種類
#Type=N,1,I
E(0,1)
HELP(#TypeHelp)
@T(3,@TypeX,)
V(H,#TypeHelp)

#TypeX=X,12,L
T(H,#Type,#TypeHelp)

!Type為1時顯示入帳日期
!為2時隱藏入帳日期
#HideByType=A,1,S
C(3,#Type,1,INVOKEJS(SHOW,p1,2,2,1),$)
C(3,#Type,2,INVOKEJS(SHOW,p1,2,2,0),$)

! 交易樣態(Factor)
#FactorHelpRim=X,1,S
RESET_RIM(#FactorHelpRim,XXR99)
S(XXR99,01,CdCode.Factor)
R(1,XXR99)
CALL(#HelpProc)
T(2,@FactorHelp,0:全部;+#HelpDesc1)
#FactorHelp=x,1024,S

#CHAIN=A,1,S
T(4,CHAIN$)

#NTXBUF=X,4,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)


#button1=x,100,S
T(2,{cmd=CHAIN; ntxcd=L8205; ntxbuf=1; ntxbuf5=type})ASGN(BTN_NEW2$)
K(MODIFYBUTTON,2,產生疑似洗錢報表)

#Submit=A,1,S
K(MODIFYBUTTON,查詢)


!本營業日
#DATE=D,8,S
T(4,DATE$)

#CDATE=D,8,S
T(4,CDATE$)

!上營業日
#LDATE=D,8,S
T(4,LBSDY$)

!入帳日期起止
!Type為2時清空跳過
#AcDateStart=D,7,I
C(3,#Type,2,S,$)
E(0,#LDATE)
D(8)
@V(7)A(DATE,0,#AcDateStart)
V(5,0010101,#CDATE)

#AcDateEnd=D,7,I
C(3,#Type,2,S,$)
E(0,#LDATE)
D(8)	
@V(7)A(DATE,0,#AcDateEnd)
V(5,#AcDateStart,#CDATE)

!交易樣態
#Factor=N,2,I
HELP(#FactorHelp)
@T(3,@FactorX,)
V(H,#FactorHelp)

#FactorX=X,6,L
T(H,#Factor,#FactorHelp)

#RPTFG=A,1,S
E(0,0)

!<include src="COM.icf::RPTFG.dec"/>

#TXMT=A,1,S
C(3,#CHAIN,0,$,E(0,@CHAIN,0)XMT())

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!---------- TXFORM = L8922 ----------
<sub type="FORM" name="L8922">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L8922,$,S)

#SCRTIL=_,_,S
T(3,[L8922] 疑似洗錢交易合理性查詢)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

!交易樣態(TOTA)
#OOFactor=N,2,L

!入帳日期(TOTA)
#OOAcDate=D,7,L


!戶號(TOTA)
#OOCustNo=A,7,S

!額度號碼(TOTA)
!#OOFacmNo=A,3,S

!撥款序號(TOTA)
!#OOBormNo=A,3,S

#OOCustNoX=X,7,L
T(2,#OOCustNo)

!戶名(TOTA)
#OOCustName=x,200,L

!累積金額(TOTA)
#OOTotalAmt=m,14.2,S
#OOTotalAmt1=m,14,S
E(0,#OOTotalAmt)
#OOTotalAmtX=X,17,L
IF(#OOTotalAmt1==0,S,$)
X(#OOTotalAmt1)

!累積筆數(TOTA)
#OOTotalCnt=N,4,L

!備忘錄序號(TOTA)
#OOMemoSeq=N,2,L

!經辦(TOTA)
#OOEmpNo=X,6,L

!合理性(TOTA)
#OORational=X,1,L

#OORationalX=X,2,L
C(5,#OORational,Y,T(2,是),C(5,#OORational,N,T(2,否),T(2,#OORational)))

!異動日期(TOTA)
!#OOUpdate=X,9,L
#OOUpdate=D,7,L

!經辦合理性說明(TOTA)
#OOEmpNoDesc=x,200,L

!主管覆核說明(TOTA)
#OOManagerDesc=x,100,L

!主管覆核
#OOManagerCheck=X,1,L

#OOManagerCheckX=X,4,L
C(5,#OOManagerCheck,Y,T(2,同意),C(5,#OOManagerCheck,N,T(2,退回),T(2,#OOManagerCheck)))

!主管同意時間
#OOManagerDate=D,7,L
C(3,#OOManagerDate,0,S,$)

! 修改的按鈕
#OOBUF2=X,23,S
T(2,2+#OOAcDate+#OOFactor+#OOCustNo)

#OOBTN2=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L8203;text=修改;ntxbuf=#OOBUF2})
C(3,#Level,3,IF(#OORational=="",BIND(grid,{cmd=CHAIN;ntxcd=L8203;text=登錄;ntxbuf=#OOBUF2}),$),$)
C(3,#Level,1,BIND(grid,cancel),$)



! 刪除的按鈕
#OOBUF4=X,23,S
T(2,4+#OOAcDate+#OOFactor+#OOCustNo)

#OOBTN4=X,4,S
C(3,#Level,1,BIND(grid,cancel),C(5,#OOManagerCheck,Y,BIND(grid,cancel),BIND(grid,{cmd=CHAIN;ntxcd=L8203;text=刪除;ntxbuf=#OOBUF4})))



! 歷程的按鈕

!是否有歷程 - Y/N
#OOHasHistory=X,1,S

#OOBUF5=X,32,S
T(2,1+#OOCustNo+000000+#OOAcDate+#OOFactor)

#OOBTN5=X,4,S
C(5,#OOHasHistory,"Y",BIND(grid,{cmd=CHAIN;ntxcd=L8924;text=歷程;ntxbuf=#OOBUF5}),BIND(grid,cancel))

! 查詢的按鈕
#OOBUF6=X,23,S
T(2,#OOAcDate+#OOCustNo+#OOFactor)

#OOBTN6=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L8926;text=查詢;ntxbuf=#OOBUF6})

!是否有訪談 - Y/N
#OOHasL8923=X,1,S

#TodayCDate=D,7,S
E(0,#SYSCDATE)

#OOBUFL8923=X,100,S
!T(2,#OOAcDate+#TodayCDate+#OOCustNo+1)
T(2,0010101+#TodayCDate+#OOCustNo+1)
!事後新增訪談L8204-帶入畫面[戶號]+[入帳日期]+[累積金額]
#OOBUFL8204=X,100,S
T(2,1+#TodayCDate+#OOCustNo+000000+#OOAcDate+00000000000000000000+#OOTotalAmt)


#OOBTNL8923=X,4,S
!C(5,#OOHasL8923,"Y",BIND(grid,{cmd=CHAIN;ntxcd=L8923;text=訪談;ntxbuf=#OOBUFL8923}),BIND(grid,{cmd=CHAIN;ntxcd=L8204;text=登錄;ntxbuf=#OOBUFL8204}))
C(5,#OOHasL8923,"Y",BIND(grid,{cmd=CHAIN;ntxcd=L8923;text=訪談;ntxbuf=#OOBUFL8923}),BIND(grid,cancel))
#OOBTNL8204=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L8204;text=訪談登錄;ntxbuf=#OOBUFL8204})


</sub>



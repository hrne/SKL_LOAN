<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD2.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)

!#button=x,100,S
!T(2,{cmd=CHAIN; ntxcd=L8203; ntxbuf=1; ntxbuf5=type})ASGN(BTN_NEW1$)
!K(MODIFYBUTTON,1,新增)

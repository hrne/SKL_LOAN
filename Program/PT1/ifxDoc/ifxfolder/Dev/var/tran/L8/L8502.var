!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,60];">
[
["[L8502]                                 維護AML高中低風險經辦"],
["#FdAmlHMLFg+風險種類",#AmlHMLFg,#AmlHMLFgX],
["#FdAmlHMLEmp+經辦編號",#AmlEmp,#EmpNoX],
]

!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>


#FdFunCode=X,2,L
T(3, )

#FdAmlHMLFg=X,2,L
T(3, )

#FdAmlHMLEmp=X,2,L
T(3, )

#AmlHMLFgXHelp=x,50,S
T(3,L:低風險;M:中風險;H:高風險)

#CHAIN=A,1,S
T(4,CHAIN$)
IF(#CHAIN!=1,V(P,此為連動交易，請從交易:[L8085 各類代碼檔]進入),$)

! [調Rim相關]
! 定義Rom欄位名稱、型態、長度
<include src="ROM.icf::L0Rim.dec"/>

![連動交易]
#NTXBUF=X,138,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@FunCode,#NTXBUF,1,1)
T(1,@AmlHMLFg,#NTXBUF,2,1)
T(1,@AmlHMLX,#NTXBUF,3,2)
T(1,@AmlEmp,#NTXBUF,22,100)

#AmlHMLX = X,2,L

#FunCode = X,1,L

! 新增進入
#Deal_New=X,1,S
C(5,#FunCode,1,$,S)
K(MODIFYBUTTON,新增)

! 修改進入
#Deal_Udate=X,1,S
C(5,#FunCode,2,$,S)
C(5,#FunCode,2,T(F,@AmlHMLFg,0),T(F,@AmlHMLFg,1))
K(MODIFYBUTTON,修改)

! 刪除進入
#Deal_Delete=X,1,S
C(5,#FunCode,4,$,S)
C(5,#FunCode,4,T(F,@AmlHMLFg,0),T(F,@AmlHMLFg,1))
C(5,#FunCode,4,T(F,@AmlEmp,0),T(F,@AmlEmp,1))
K(MODIFYBUTTON,刪除)



#AmlHMLFg=X,1,I
C(5,#FunCode,2,s,$)
C(5,#FunCode,4,s,$)
HELP(#AmlHMLFgXHelp) 
@V(H,#AmlHMLFgXHelp)

#AmlHMLFgX=X,10,L
T(3,)T(H,#AmlHMLFg,#AmlHMLFgXHelp)

#AmlEmp=X,6,I
C(5,#FunCode,4,s,$)
@T(3,@EmpNoX,)
V(NL)

#Space=X,1,S
<include src="ROM.icf::L5R06.dec"/>     
#RimL5R06=X,1,S
#CdEmpRimEmpNo=X,6,S
T(2,#AmlEmp)
C(4,#AmlEmp,S,$)
RESET_RIM(#CdEmpRimEmpNo,L5R06)
S(L5R06,1,#CdEmpRimEmpNo)
R(1,L5R06)
T(2,@EmpNoX,#L5R06FullName)

#EmpNoX=X,10,L
C(4,#AmlEmp,S,$)

!除功能為1新增時,其他隱藏重新交易按鈕
#AGAIN=X,1,S
 C(3,#CHAIN,1,$,S)
E(0,1)ASGN(AGAIN$)

</sub>


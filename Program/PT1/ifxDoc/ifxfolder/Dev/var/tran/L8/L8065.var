T(2,0010101+9991231+L8403+#18sp+#7sp+#3sp+#3sp+#20sp+#5sp+#7sp+#7sp+#OOMrKey)
#OOBTN18=X,1,S
BIND(grid,{cmd=CHAIN; ntxcd=L6932;text=紀錄; ntxbuf=#OOBUF18})

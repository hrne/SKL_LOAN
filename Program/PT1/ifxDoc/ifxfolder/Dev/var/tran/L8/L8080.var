[
["[L8080]                                 AML 姓名檢核查詢"],
["#FdBrNo+單位",#BrNo],
["#FdStatus+狀態",#Status,#StatusX],
["#FdAcDate1+會計日期",#AcDate1,"~",#AcDate2],

!],
!---------- AP Button AREA ----------


!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>

#FdBrNo=X,2,L
#FdStatus=X,2,L
T(3,*)
#FdAcDate1=X,2,L
T(3,*)
#FdTypeCode=X,2,L
T(3,*)
#FdCustId=X,2,L
T(3,)
#FdCustNo=X,2,L
T(3,)

!AmlCheckItem
#CDCODEAmlCheckItem=x,25,S
T(3,CdCode.AmlCheckItem)

#AmlCheckItemHelpRim=X,1,S
RESET_RIM(#AmlCheckItemHelpRim,XXR99)
S(XXR99,01,#CDCODEAmlCheckItem)
R(1,XXR99)
CALL(#HelpProc)
T(2,@AmlCheckItemHelp,9:全部;+#HelpDesc1)

#AmlCheckItemHelp=x,300,S

#CHAIN=A,1,S
!T(4,CHAIN$) 
E(0,1)

#NTXBUF=X,4,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)

!#RPTFG=A,1,S
!E(0,0)

! 單位
#BrNo=X,4,L
T(2,#KINBR)

! 狀態
#Status=X,1,I
HELP(#AmlCheckItemHelp,cols:1)
@T(3,@StatusX,)
V(H,#AmlCheckItemHelp)

#StatusX=X,50,L
T(H,#Status,#AmlCheckItemHelp)

#AcDate1=D,7,I
E(0,#SYSDATE)
@
V(7)
A(DATE,0,#AcDate1)

#AcDate2=D,7,I
E(0,#SYSDATE)
@
V(7)
IF(#AcDate2<#AcDate1,V(P,不可小於會計起日),$)
A(DATE,0,#AcDate2)


#TypeCodeHelp=x,1024,S
T(3,0:撥款匯款;1:銀扣授權;2:產出扣帳檔;3:整批入帳;9:全部)

#TypeCode=X,1,I
C(4,#TypeCode,E(0,9),$)
HELP(#TypeCodeHelp)
@T(3,@TypeCodeX,)
V(7)
V(H,#TypeCodeHelp)

#TypeCodeX=X,10,L
T(3,)T(H,#TypeCode,#TypeCodeHelp)

!客戶ID
#CustId=X,10,I
@C(4,#CustId,$,A(ID_UNINO,0,#CustId))

!戶號
#CustNo=A,7,I

#RPTFG=A,1,S
E(0,0)

!<include src="COM.icf::RPTFG.dec"/>

#TXMT=A,1,S
C(3,#CHAIN,0,$,E(0,@CHAIN,0)XMT())

#RPTFG=_,_,S

</sub>


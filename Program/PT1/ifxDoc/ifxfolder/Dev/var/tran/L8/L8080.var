!---------- TXFORM = L8080 ----------
<sub type="FORM" name="L8080">
#SCRPRT=_,_,S
C(5,#TXFORM,L8080,$,S)

#SCRTIL=_,_,S
T(3,[L8080] AML 姓名檢核)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

#oLogNo=A,8,S

#oEntdy=D,7,S

#oTransactionId=X,40,S

#oAcctNo=X,20,S

#oCaseNo=X,20,S

#oName=x,100,S

#oConfirmStatus=X,1,S

#oConfirmStatusX=X,30,S
C(2,#oConfirmStatus,T(3,非可疑/已確認名單),T(3,需審查確認),T(3,為凍結/未確認名單))

#oConfirmCode=X,1,S

#oConfirmCodeX=X,10,S
C(4,#oConfirmCode,S,$)
C(2,#oConfirmCode,S,T(3,確認正常),T(3,確認可疑),T(3,確認未確定))

#oConfirmEmpNo=X,6,S

#oConfirmTranCode=X,10,S

! 更新狀態的按鈕
#oFunc2=A,1,S
E(0,2)

#oBUF2=X,9,S
T(2,#oFunc2+#oLogNo)

#FunCode=X,1,S
E(0,2)

#LogNo=A,8,S
E(0,#oLogNo)

#oBUF2a=x,100,S
T(6A,#FunCode+#LogNo)

#oBTN2=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L8100;text=更新狀態;ntxbuf=#oBUF2})

! 人工確認的按鈕
#oFunc3=A,1,S
E(0,3)

#oBUF3=X,9,S
T(2,#oFunc3+#oLogNo)

#oBTN3=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L8100;text=人工確認;ntxbuf=#oBUF3})

! 查詢的按鈕
#oFunc5=A,1,S
E(0,5)

#oBUF5=X,9,S
T(2,#oFunc5+#oLogNo)

#oBTN5=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L8100;text=查詢;ntxbuf=#oBUF5})
			
#L8100=X,5,S
T(3,L8100)


C(5,#TranKey,D,s,$)

!法院裁定保全處分
#ClaimStatus1=X,1,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,D,T(2,@ClaimStatus1,#L8r27ClaimStatus1),$)
C(5,#TranKey,D,s,$)
HELP(#YesNoHelp)
@T(3,@ClaimStatus1X,)
V(H,#YesNoHelp)

#ClaimStatus1X=X,2,L
T(H,@ClaimStatus1X,#ClaimStatus1,#YesNoHelp)

!保全處分起始日
#SaveDate=D,7,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,D,T(2,@SaveDate,#L8r27SaveDate),$)
C(5,#TranKey,D,s,$)
@A(DATE,0,#SaveDate)

!法院裁定撤銷保全處分
#ClaimStatus2=X,1,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,D,T(2,@ClaimStatus2,#L8r27ClaimStatus2),$)
C(5,#TranKey,D,s,$)
HELP(#YesNoHelp)
@T(3,@ClaimStatus2X,)
V(H,#YesNoHelp)

#ClaimStatus2X=X,2,L
T(H,@ClaimStatus2X,#ClaimStatus2,#YesNoHelp)

!是否依更生條件履行 22
#IsImplement=X,1,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(3,#CaseStatus,3,$,s)
C(5,#TranKey,D,T(2,@IsImplement,#L8r27IsImplement),$)
C(5,#TranKey,D,s,$)
HELP(#YesNoHelp)
@T(3,@IsImplementX,)
V(H,#YesNoHelp)

#IsImplementX=X,2,L
T(H,@IsImplementX,#IsImplement,#YesNoHelp)

!保全處分撤銷日
#SaveEndDate=D,7,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,D,T(2,@SaveEndDate,#L8r27SaveEndDate),$)
C(5,#TranKey,D,s,$)
D(8)
@A(DATE,0,#SaveEndDate)





!監督人姓名 5全形
#InspectName=X,10,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,D,T(2,@InspectName,#L8r27InspectName),$)
C(5,#TranKey,D,s,$)


![轉JCIC文字檔日期]

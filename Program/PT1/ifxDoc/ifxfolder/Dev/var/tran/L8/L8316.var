!更生條件（利率） 15 
#Rate=m,2.2,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(3,#CaseStatus,3,$,s)
C(5,#TranKey,D,T(2,@Rate,#L8r27Rate),$)
C(5,#TranKey,D,s,$)
@V(2,0)
!原始債權金額 16
#OutstandAmt=M,9,I

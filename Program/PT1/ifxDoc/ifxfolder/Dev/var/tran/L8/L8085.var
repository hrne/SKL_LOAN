!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L8085" layout="cols=1;screen.width=[150,800];printer.width=[20,80];">
[
["[L8085]                           查詢AML高中低風險經辦"],
["#FdAmlHMLFg+風險種類",#AmlHMLFg,#AmlHMLFgX,"(空白查詢全部)"],
["#FdAmlHMLEmp+經辦編號",#AmlEmp,#EmpNoX],
]

!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>

#FdAmlHMLFg=X,2,L
T(3, )

#FdAmlHMLEmp=X,2,L
T(3, )

#RPTFG=A,1,S
E(0,0)

#AmlHMLFgXHelp=x,50,S
T(3,L:低風險;M:中風險;H:高風險)

! [調Rim相關]
! 定義Rom欄位名稱、型態、長度
<include src="ROM.icf::L0Rim.dec"/>

#AmlHMLFg=X,1,I
HELP(#AmlHMLFgXHelp) 
! @V(H,#AmlHMLFgXHelp)

#AmlHMLFgX=X,10,L
T(3,)T(H,#AmlHMLFg,#AmlHMLFgXHelp)

#AmlEmp=X,6,I
@T(3,@EmpNoX,)
V(NL)

#Space=X,1,S
<include src="ROM.icf::L5R06.dec"/>     
#RimL5R06=X,1,S
#CdEmpRimEmpNo=X,6,S
T(2,#AmlEmp)
C(4,#AmlEmp,S,$)
RESET_RIM(#CdEmpRimEmpNo,L5R06)
S(L5R06,1,#CdEmpRimEmpNo)
R(1,L5R06)
T(2,@EmpNoX,#L5R06FullName)

#EmpNoX=X,10,L
C(4,#AmlEmp,S,$)

</sub>


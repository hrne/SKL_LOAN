<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!---------- TXFORM = L8924 ----------
<sub type="FORM" name="L8924">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L8924,$,S)

#SCRTIL=_,_,S
T(3,[L8924] 資料變更交易)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------

!#O_ST_DT=D,7,L
!T(2,#ST_DT)
!#O_ED_DT=D,7,L
!T(2,#ED_DT)
#O_TRN_CODE=X,5,L
T(2,#TRN_CODE)
#O_CUST_NO=A,7,L
T(2,#CUST_NO)


!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

#OTranName=X,40,S
#OCustNo=A,7,S
#OFacmNo=A,3,S
#OBormNo=A,3,S
#OCustNoX=20,7,S
T(2,#OCustNo+-+#OFacmNo+-+#OBormNo)

#OMrKey=x,20,S
#OReason=x,120,S
#OReasonX=x,120,S
#OLastUpdate=x,40,S
#OLastEmp=x,30,S
!會計日期
#OTxDate=D,7,S
!交易序號
#OTxSeq=X,18,S
!明細序號
#OTxSno=A,2,S

#OCustName=X,100,S

#BufA=X,50,S
T(2,#OTxDate+#OTxSeq+#OTxSno)

#OOBtnA=A,1,O
BIND(grid,{ cmd=CHAIN; ntxcd=L8925; ntxbuf=#BufA})

</sub>



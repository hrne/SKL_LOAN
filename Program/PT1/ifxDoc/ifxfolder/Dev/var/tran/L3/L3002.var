<sub type="FORM" name="L3002">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L3002,$,S)

#SCRTIL=_,_,S
T(3,[L3002] 撥款明細資料)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#OCaseNo=X,7,L
C(3,#CaseNo,0,S,T(2,#CaseNo))

#OCaseNoX=X,50,L
T(2,#CaseNoX)

#OCustNo=X,7,L
C(3,#CustNo,0,S,T(2,#CustNo))

#OCustNoX=X,50,L
T(2,#CustNoX)

#OCustId=X,10,L
T(2,#CustId)

#OCustIdX=X,50,L
T(2,#CustIdX)

#OApplNo=X,7,L
C(3,#ApplNo,0,S,T(2,#ApplNo))

#OApplNoX=X,50,L
T(2,#ApplNoX)

#SPC10=X,10,S

#ZRO07=A,7,S
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

#OOCaseNo=A,7,O

#OOApplNo=A,7,O

#OOCustNo=A,7,O

#OOCustName=x,100,O

#OOFacmNo=A,3,O

#OOBormNo=A,3,O

#OODrawdownDate=D,7,O

#OOMaturityDate=D,7,O

#OOStatus=A,2,S

#OONewStatus=A,2,S
#OOStatusX=X,10,O
T(H,#OONewStatus,#StatusHelp)

! T(A,#OOStatus,L3DEF,BormStatusCodeDef,BormStatusCode,BormStatusCodeX)

#OOPrevIntDate=D,7,S

#WkA=X,3,S
T(1,#OOPrevIntDate,1,3)

#WkB=X,2,S
T(1,#OOPrevIntDate,4,2)

#WkC=X,2,S
T(1,#OOPrevIntDate,6,2)

#OOPrevIntDateX=X,9,O
C(3,#OOPrevIntDate,0,T(3,無),T(2,#WkA+/+#WkB+/+#WkC))

#OOStoreRate=m,2.4,O

#OOCurrencyCode=X,3,O

#Precision=N,1,S
E(0,0)T(H,#OOCurrencyCode,#CurrPrecisionHelp)

#OOLoanBal=m,14.2,S

#OOLoanBalX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#OOLoanBal,#OOLoanBalX)
P(R,#OOLoanBalX)

#OODrawdownAmt=m,14.2,S

#OODrawdownAmtX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#OODrawdownAmt,#OODrawdownAmtX)
P(R,#OODrawdownAmtX)


#OODepartment=X,15,S

![資金用途別]
#OOUsageCodeX=X,20,L

#OOBookFlag=A,1,S

#OORenewFlag=A,1,S

#OOBUF1=X,37,S
T(2,#ZRO07+#ZRO07+#SPC10+#OOApplNo+#OOFacmNo+#OOBormNo)

#OOBTN1=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L3916; ntxbuf=#OOBUF1})

#OOBUF2=X,50,S
IF(#OOBookFlag==2,T(2,0000000          +#OOApplNo+#OOFacmNo+000),T(2,0000000          +#OOApplNo+#OOFacmNo+#OOBormNo))


#OOBTN2=X,1,S
C(3,#OOBookFlag,0,BIND(grid,cancel),BIND(grid,{ cmd=CHAIN; ntxcd=L3004; ntxbuf=#OOBUF2}))

#OOBUF3=X,50,S
T(2,#OOCustNo+00000+00000+000+#OOFacmNo)


#OOBTN3=X,1,S
C(3,#OORenewFlag,2,BIND(grid,{ cmd=CHAIN; ntxcd=L2079; ntxbuf=#OOBUF3}),BIND(grid,cancel))

</sub>



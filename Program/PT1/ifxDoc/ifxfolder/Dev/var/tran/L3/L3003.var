#ORvEndDate=D,7,L
E(0,#RvEndDate)

#OCaseNo=A,7,L
E(0,#CaseNo)

#OCaseNoX=X,50,L
T(2,#CaseNoX)

#OCustId=X,10,L
T(2,#CustId)

#OCustIdX=X,50,L
T(2,#CustIdX)

#OCustNo=A,7,L

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[180,820];printer.width=[20,20];">
[
["[L3003]                                 預約撥款明細資料查詢"],
["#FdMain+查詢條件","@(預約撥款起訖日、案件編號、借戶戶號、統一編號、核准編號，需擇一輸入)"],
["#FdRvStartDate+．預約撥款起訖日",#RvStartDate,"@-@",#RvEndDate],
["#FdCaseNo+．案件編號",#CaseNo,"@@@",#CaseNoX],
["#FdCustNo+．借戶戶號",#CustNo,"@@@",#BTN2,#CustNoX],
["#FdCustId+．統一編號",#CustId,#BTN3,#CustIdX],
["#FdApplNo+．核准編號",#ApplNo,"@@@",#BTN4,#ApplNoX],
["#FdStatus+狀態",#Status,#StatusX],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
#BTN2=X,15,I,ui:button;value:額度資料查詢;enable:0;tabbable:0;title:L2015 額度明細資料查詢;
#BTN3=X,15,I,ui:button;value:顧客資料查詢;enable:1;tabbable:0;title:L1001 顧客明細資料查詢;
#BTN4=X,15,I,ui:button;value:核准號碼查詢;enable:0;tabbable:0;title:L2016 核准號碼明細資料查詢;

#BUF1=X,42,S
HIDE(#BTN2,#BTN3,#BTN4)
!---------- AP TEXT AREA Variables ----------
! Help
<include src="HELPRIM.icf::HELPRIM.rtn"/>
#CDDEF0001=x,26,S
T(3,CdCode.CurrPrecision)

#CurrPrecisionHelp=x,1024,S 

#ActFgHelp=x,1024,S
T(3,1:已撥款;2:已放行;3:未放行;4:已刪除;9:全部)

#ActFlagHelp=x,1024,S
T(3,1:未放行;2:已放行)

!戶況
#CDDEF0002=x,26,S
T(3,CdCode.Status)

#StatusHelp=x,1024,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF0001,#CDDEF0002)
R(1,XXR99)
CALL(#HelpProc) 
T(2,@CurrPrecisionHelp,#HelpDesc1) 
T(2,@StatusHelp,#HelpDesc2) 

#FdMain=X,2,L
T(3,*)

#CHAIN=A,1,S
T(4,CHAIN$) 
T(2,@RimTxCode,#TXCD)
E(0,@RimFKey,#FKEY)

#NTXBUF=X,45,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
C(3,#CHAIN,1,T(1,@ChainCaseNo,#NTXBUF,15,7),$)
C(3,#CHAIN,1,T(1,@ChainCustNo,#NTXBUF,22,7),$)
C(3,#CHAIN,1,T(1,@ChainApplNo,#NTXBUF,39,7),$)

#NTXBUF5=X,10,S
C(3,#CHAIN,1,T(4,NTXBUF5$),S)
C(4,#NTXBUF5,E(0,@RPTFG2,1),E(0,@RPTFG2,2))


#ChainCustNo=A,7,S

#ChainApplNo=A,7,S

#ChainCaseNo=A,7,S


#WkEndDate=D,8,S
D(7,2,#SYSDATE,1,0)

![預約撥款起訖日]
#FdRvStartDate=X,2,L

#RvStartDate=D,7,I
! INVOKEJS(SHOW,p1,1,5,1)
HIDE(#BTN2,#BTN3,#BTN4)
C(3,#CHAIN,1,T(1,#NTXBUF,1,7),$)
C(3,#CHAIN,1,s,$)
D(8)
@C(3,#RvStartDate,0,S,$)
A(DATE,0,#RvStartDate)
V(5,0010101,9991231)

#RvEndDate=D,7,I
! INVOKEJS(SHOW,p1,1,5,1)
HIDE(#BTN2,#BTN3,#BTN4)
C(3,#CHAIN,1,T(1,#NTXBUF,8,7),$)
C(3,#CHAIN,1,s,$)
C(3,#RvStartDate,0,S,$)
C(3,#RvEndDate,0,E(0,#WkEndDate),$)
D(8)
@V(7)
A(DATE,0,#RvEndDate)
V(5,#RvStartDate,9991231)


#WKHIDE1=X,1,S
! C(3,#RvStartDate,0,INVOKEJS(SHOW,p1,1,1,0),INVOKEJS(SHOW,p1,2,5,0))

![案件編號]
#FdCaseNo=X,2,L

#CaseNo=X,7,I
C(3,#CHAIN,1,C(3,#ChainCaseNo,0,$,E(0,#ChainCaseNo)),$)
! INVOKEJS(SHOW,p1,2,5,1)
HIDE(#BTN2,#BTN3,#BTN4)
C(3,#CHAIN,1,s,$)
C(3,#RvStartDate,0,$,S)

#CaseNo2=A,7,S
C(4,#CaseNo,S,$)
E(0,#CaseNo)
T(2,@CaseNo,#CaseNo2)
T(3,#CaseNoX,)

#SendL2r10=X,1,S
C(4,#CaseNo,S,$)
E(0,@RimCaseNo,#CaseNo)
RESET_RIM(#SendL2r10,L2R10)
S(L2R10,1,#RimCaseNo)
R(1,L2R10)

#SendL2r07A=X,1,S
C(4,#CaseNo,S,$)
T(2,@RimCustId, )
E(0,@RimCustNo,#L2r10CustNo)
RESET_RIM(#SendL2r07A,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo)
R(1,L2R07)

#WKBTN1=X,1,S
!C(3,#CaseNo,0,HIDE(#BTN1),SHOW(#BTN1))
! C(3,#CaseNo,0,INVOKEJS(SHOW,p1,2,2,0),INVOKEJS(SHOW,p1,3,5,0))

#BIND1=X,1,S
!BIND(#BTN1,click, {cmd=CHAIN; ntxcd=XW922; ntxbuf=#BUF1;ntxbuf5=type<-1:MSGTYP<-RECMSG:ENTSEQ<-RECSEQ})

#CaseNoX=X,100,L
C(4,#CaseNo,S,T(2,#L2r07CustName))

#BUF2A=X,18,S
T(3,00000009999999000)

#BIND2A=X,1,S
BIND(#BTN2,click, {cmd=CHAIN; ntxcd=L2015; ntxbuf=#BUF2A;ntxbuf5=type<-1:CustNo<-OOCustNo:CustNoX<-OOCustName})

![借戶戶號]
!借戶戶號, 統一編號或核准編號擇一輸入
#FdCustNo=X,2,L

#CustNo=X,7,I
C(3,#CHAIN,1,C(3,#ChainCustNo,0,$,E(0,#ChainCustNo)),$)
HIDE(#BTN3,#BTN4)
C(3,#CHAIN,1,s,$)
C(3,#RvStartDate,0,$,S)
C(4,#CaseNo,$,S)
! INVOKEJS(SHOW,p1,3,5,1)
SHOW(#BTN2)

#CustNo2=A,7,S
C(4,#CustNo,S,$)
E(0,#CustNo)
T(2,@CustNo,#CustNo2)
T(3,#CustNoX,)

#BUF2B=X,18,S
T(2,#CustNo+9999999000)

#BIND2B=X,1,S
BIND(#BTN2,click, {cmd=CHAIN; ntxcd=L2015; ntxbuf=#BUF2B;ntxbuf5=type<-1:CustNo<-OOCustNo:CustNoX<-OOCustName})

#SendL2r07B=X,1,S
C(4,#CustNo,S,$)
T(3,@RimCustId, )
E(0,@RimCustNo,#CustNo)
RESET_RIM(#SendL2r07B,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo)
R(1,L2R07)

#WKBTN2=X,1,S
C(4,#CustNo,HIDE(#BTN2),SHOW(#BTN2))
! C(3,#CustNo,0,INVOKEJS(SHOW,p1,3,3,0),INVOKEJS(SHOW,p1,4,5,0))

#CustNoX=X,100,L
C(4,#CustNo,S,T(2,#L2r07CustName))

#BIND2=X,1,S
!BIND(#BTN2,click, {cmd=CHAIN; ntxcd=XW922; ntxbuf=#BUF1;ntxbuf5=type<-1:MSGTYP<-RECMSG:ENTSEQ<-RECSEQ})

#BUF3A=X,100,S

#BIND3A=X,1,S
BIND(#BTN3,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUF3A;ntxbuf5=type<-1:CustId<-OOCustId:CustIdX<-OOCustName})

![統一編號]
#FdCustId=X,2,L

#CustId=X,10,I
T(3,@FdApplNo, )
C(3,#CHAIN,1,T(1,#NTXBUF,29,10),$)
C(3,#CHAIN,1,s,$)
C(3,#RvStartDate,0,$,S)
C(4,#CaseNo,$,S)
C(4,#CustNo,$,S)
! INVOKEJS(SHOW,p1,4,5,1)
SHOW(#BTN3)HIDE(#BTN2,#BTN4)
@C(4,#CustId,S,$)
V(7)
T(3,#CustIdX,)

#BUF3B=X,100,S
T(2,0000000+#CustId)

#BIND3B=X,1,S
BIND(#BTN3,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUF3B;ntxbuf5=type<-1:CustId<-OOCustId:CustIdX<-OOCustName})

#CheckCustId=X,1,S
C(4,#CustId,S,$)
A(ID_UNINO,0,#CustId)

#SendL2r07C=X,1,S
C(4,#CustId,S,$)
T(2,@RimCustId,#CustId)
E(0,@RimCustNo,0)
RESET_RIM(#SendL2r07C,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo)
R(1,L2R07)

#WKBTN3=X,1,S
C(4,#CustId,HIDE(#BTN3),SHOW(#BTN3))
! C(4,#CustId,INVOKEJS(SHOW,p1,4,4,0),INVOKEJS(SHOW,p1,5,5,0))

#CustIdX=X,100,L
C(4,#CustId,S,T(2,#L2r07CustName))

#BUF4A=X,18,S
T(3,000000199999990)

#BIND4A=X,1,S
BIND(#BTN4,click, {cmd=CHAIN; ntxcd=L2016; ntxbuf=#BUF4A;ntxbuf5=type<-1:ApplNo<-OOApplNo:ApplNoX<-OOCustName})

![核准編號]
#FdApplNo=X,2,L
C(3,#CHAIN,1,s,$)
C(3,#RvStartDate,0,$,S)
C(4,#CaseNo,$,S)
C(4,#CustNo,$,S)
C(4,#CustId,$,S)
T(3,*)

#ApplNo=X,7,I
C(3,#CHAIN,1,C(3,#ChainApplNo,0,$,E(0,#ChainApplNo)),$)
C(3,#CHAIN,1,s,$)
C(3,#RvStartDate,0,$,S)
C(4,#CaseNo,$,S)
C(4,#CustNo,$,S)
C(4,#CustId,$,S)
! INVOKEJS(SHOW,p1,5,5,1)
SHOW(#BTN4)HIDE(#BTN2,#BTN3)
@C(4,#ApplNo,V(P,需擇一輸入查詢條件),$)

#ApplNo2=A,7,S
C(4,#ApplNo,S,$)
E(0,#ApplNo)
T(2,@ApplNo,#ApplNo2)
T(3,#ApplNoX,)

!用核准號碼(ApplNo)調L2R05
#SendL2r05A=X,1,S
C(4,#ApplNo,S,$)
E(0,@RimFuncCode,5)
E(0,@RimCustNo,0)
E(0,@RimFacmNo,0)
E(0,@RimApplNo,#ApplNo)
E(0,@RimCaseNo,0)
RESET_RIM(#SendL2r05A,L2R05)
S(L2R05,1,#RimFuncCode,#RimTxCode,#RimFKey,#RimCustNo,#RimFacmNo,#RimApplNo,#RimCaseNo)
R(1,L2R05)

#SendL2r07D=X,1,S
C(4,#ApplNo,S,$)
T(2,@RimCustId,#L2r05CustId)
E(0,@RimCustNo,0)
RESET_RIM(#SendL2r07D,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo)
R(1,L2R07)


#ApplNoX=X,100,L
C(4,#ApplNo,S,T(2,#L2r07CustName))

!額度編號
#FacmNo=A,3,S
E(0,0)
C(4,#ApplNo,$,E(0,#L2r05FacmNo))

#TimCustNo=A,7,S
E(0,0)
C(4,#CaseNo,$,E(0,#L2r10CustNo))
C(4,#CustNo,$,E(0,#CustNo))
C(4,#CustId,$,E(0,#L2r07CustNo))
C(4,#ApplNo,$,E(0,#L2r05CustNo))
T(2,@RmkCustNo,#TimCustNo)
<include src="COM.icf::CustRmk.rtn"/>

#MRKEY=_,_,S
C(3,#FacmNo,0,T(2,#TimCustNo),T(2,#TimCustNo+-+#FacmNo))

!狀態
#FdStatus=X,2,L

#Status=A,1,I
C(3,#Status,0,E(0,9),$)
HELP(#ActFgHelp)
@V(H,#ActFgHelp)

#StatusX=X,20,L
T(H,#Status,#ActFgHelp)




#RPTFG=A,1,S

#RPTFG2=A,1,S
C(3,#RPTFG,0,$,S)
C(4,#NTXBUF5,T(3,1),T(3,2))

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

<include src="ROM.icf::L0Rim.dec"/>
<include src="ROM.icf::L2Rim.dec"/>
<include src="ROM.icf::L2R05.dec"/>
<include src="ROM.icf::L2R07.dec"/>
<include src="ROM.icf::L2R10.dec"/>
</sub>


![Buttom資料查詢]

!---------- AP TEXT AREA Variables ----------
! Help
<include src="HELPRIM.icf::HELPRIM.rtn"/>
!ChequeStatusCode
#CDDEF0001=x,23,S
T(3,CdCode.ChequeStatusCode)

#CDDEF0002=x,26,S
T(3,CdCode.CurrPrecision)

#ChequeStatusCodeHelpRim=X,1,S
RESET_RIM(#ChequeStatusCodeHelpRim,XXR99)
S(XXR99,01,#CDDEF0001,#CDDEF0002)
R(1,XXR99)
CALL(#HelpProc)
T(2,@ChequeStatusCodeHelp,#HelpDesc1)
T(2,@CurrPrecisionHelp,#HelpDesc2) 

#ChequeStatusCodeHelp=x,1024,S

#CurrPrecisionHelp=x,1024,S 

#FdMain=X,2,L

#CHAIN=A,1,S
T(4,CHAIN$)

#NTXBUF=X,31,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)

#FdInit=X,1,S
T(3,@FdChequeDate, )
T(3,@FdChequeAcct, )
T(3,@FdChequeNo, )

![到期日] 起迄
!到期日,支票帳號或支票號碼擇一輸入, 若無輸入則輸出全部的多餘支票
#FdChequeDate=X,2,L

#ChequeDateSt=D,7,I
@A(DATE,0,#ChequeDateSt)
C(4,#ChequeDateSt,$,E(0,@ChequeDateEd,#ChequeDateSt))

#ChequeDateEd=D,7,I
C(4,#ChequeDateSt,S,$)
@V(7)
V(5,#ChequeDateSt,9991231)
A(DATE,0,#ChequeDateEd)

![支票帳號]
!到期日,支票帳號或支票號碼擇一輸入, 若無輸入則輸出全部的多餘支票
#FdChequeAcct=X,2,L

#ChequeAcct=A,9,I
C(3,#ChequeDateSt,0,$,S)

![支票號碼]
!到期日,支票帳號或支票號碼擇一輸入, 若無輸入則輸出全部的多餘支票
#FdChequeNo=X,2,L

#ChequeNo=A,7,I
C(3,#ChequeDateSt,0,$,S)
C(3,#ChequeAcct,0,$,S)


![支票狀況]
#FdStatusCode=X,2,L

#StatusCode=X,1,I
HELP(#ChequeStatusCodeHelp)
@C(4,#StatusCode,S,$)
V(H,#ChequeStatusCodeHelp)


#StatusCodeX=X,20,L
C(4,#StatusCode,S,$)
T(H,#StatusCode,#ChequeStatusCodeHelp)

#RPTFG=A,1,S

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

</sub>


!---------- TXFORM = L3009 ----------
<sub type="FORM" name="L3009">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L3009,$,S)

#SCRTIL=_,_,S
T(3,[L3009] 支票明細資料-全部)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#OChequeDateSt=D,7,L
E(0,#ChequeDateSt)

#OChequeDateEd=D,7,L
E(0,#ChequeDateEd)

#OChequeAcct=A,9,L
E(0,#ChequeAcct)

#OChequeNo=A,7,L
E(0,#ChequeNo)

#OStatusCode=X,1,L
T(2,#StatusCode)

#OStatusCodeX=X,10,L
T(H,#OStatusCode,#ChequeStatusCodeHelp)

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)


#OOChequeDate=D,7,O

#OOChequeAcct=A,9,O

#OOChequeNo=A,7,O

#OOCurrencyCode=X,3,O

#Precision=N,1,S
E(0,0)T(H,#OOCurrencyCode,#CurrPrecisionHelp)

#OOChequeAmt=m,14.2,S

#OOChequeAmtX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#OOChequeAmt,#OOChequeAmtX)
P(R,#OOChequeAmtX)

#OOStatusCode=A,1,S

#OOStatusCodeX=X,10,O
T(3,)T(H,#OOStatusCode,#ChequeStatusCodeHelp)

#OOCustNo=A,7,O

#OOCustNoX=X,100,O

#OOReceiveDate=D,7,O
C(3,#OOReceiveDate,0,S,$)

!查詢 連結[支票內容查詢]
#OOBUF1=X,37,S
T(2,#OOChequeAcct+#OOChequeNo)

#OOBTN1=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L3943; ntxbuf=#OOBUF1})
</sub>



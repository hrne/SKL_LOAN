!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[130,870];printer.width=[20,20];">
[
["[L3072]                                 帳務備忘錄明細資料查詢"],
["#FdCustNo+借戶戶號",#CustNo,#CustName],
]

#FdCustNo=X,2,L

#RimFunCd=A,1,S
#RimCustId=X,10,S
#RimCustNo=A,7,S
!---------- AP Button AREA ----------
![Buttom資料查詢]
! #BTN1=X,15,I,ui:button;value:顧客資料查詢;enable:1;tabbable:0
!---------- AP TEXT AREA Variables ----------
#CHAIN=A,1,S
T(4,CHAIN$)
IF(#CHAIN!=1,V(P,此為連動交易，請從交易:[L3005交易明細資料查詢]進入),$)

!#OCustNo+#OOFacmNo+#OOBormNo+#OOBorxNo+#OOIsOldData
!7-3-3-4-1
#NTXBUF=X,50,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@CustNo,#NTXBUF,1,7)
T(1,@FacmNo,#NTXBUF,8,3)
T(1,@BormNo,#NTXBUF,11,3)
T(1,@BorxNo,#NTXBUF,14,4)
T(1,@IsOldData,#NTXBUF,19,1)

!1時只送這些
!CustNo 7
!FacmNo 3
!BormNo 3
!BorxNo 4
#BUFCREATE=X,17,S
T(2,1+#CustNo+#FacmNo+#BormNo+#BorxNo)

#button=x,100,S
C(3,#FacmNo,0,S,$)
T(2,{ cmd=CHAIN; ntxcd=L3702; ntxbuf=#BUFCREATE})ASGN(BTN_NEW1$)
K(MODIFYBUTTON,1,新增)

!借戶戶號
#CustNo=A,7,L

#FacmNo=A,3,L

#BormNo=A,3,L

#BorxNo=A,4,L

#IsOldData=X,1,L

#FacmNoX=X,3,L

#BormNoX=X,3,L

#BorxNoX=X,4,L

#RimL1r09=X,1,S
E(0,@RimFunCd,1)
T(2,@RimCustId,)
E(0,@RimCustNo,#CustNo)
RESET_RIM(#RimL1r09,L1R09)
S(L1R09,1,#RimFunCd,#RimCustId,#RimCustNo)
R(1,L1R09)
T(2,@CustName,#L1r09CustName)

#CustName=x,2.50,L

#RPTFG=A,1,S

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)


#MRKEY=_,_,S
C(4,#CustNo,S,$)
T(2,#CustNo)

<include src="ROM.icf::L1R09.dec"/>

</sub>


!---------- BASIC LABEL Variables ----------
<sub type="SYS">
</sub>

<sub type="SYS" name="label">
!#TXCD
!T(3,L5909)
![資料表頭(HEADER)長度]
#INQHD=n,3,S
E(0,0)ASGN(INQHD$)

![每筆資料明細(OCCURS)長度]
#INQLEN=n,3,S
E(0,40)ASGN(INQLEN$)

![多筆查詢之一個畫面有N筆資料]
#INQREC=n,2,S
E(0,40)ASGN(INQREC$)

![畫面顯示的明細間的高度]
#LOOPH=n,3,S
E(0,1)ASGN(LOOPHEIGHT$)

![每張印錄單列印的筆數]
#INQPRT=n,2,S
E(0,40)ASGN(INQPRT$)
</sub>

<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)
K(MODIFYBUTTON,查詢)
</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L5909" layout="cols=1;screen.width=[130,870];printer.width=[20,80];order=1">
[
["[L5909]                                 案件品質排行表(列印)"],
[#<->#,"#FdStartDate+截止日期",#StartDate,"~",#EndDate],
]

#FdStartDate=X,2,L
T(3,*)

#CHAIN=A,1,S
T(4,CHAIN$) 

#NTXBUF=X,11,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@YyyMmFm,#NTXBUF,1,5)
T(1,@YyyMmTo,#NTXBUF,6,5)
INVOKEJS(SHOW,p1,02,02,1)

! 截止日期
! 預設系統帳務日期
!  (產生下載工作檔 LNW18M2P)
#StartDate=D,7,I
D(8)
@A(DATE,0,#StartDate)
V(7)

#EndDate=D,7,I
D(8)
@A(DATE,0,#EndDate)
V(7)
IF(#EndDate < #StartDate ,V(P,起日不可大於迄日),$)

#RPTFG=A,1,S
</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!------------ TXFORM = L5909 -----------
<sub type="FORM" name="L5909">
#SCRTIL=_,_,S
T(3,[L5909] 案件品質排行表)

#LOOP=X,1,S
E(0,1)ASGN(LOOP$)
!房貸專員
#OOBsOfficerX=X,10,O
!承做案件餘額(含轉催收款)
#OOTotal=m,14,O
!逾4期金額
#OOOvduTotal=m,14,O
!逾放百分比
#OOPercent=n,3.2,O
</sub>

!------ 上行電文 TEXT ------
<sub type="TIM">
#StartDate
#EndDate
#END
</sub>

!------ 下行電文TOM ------
<sub type="TOM">
<include src="COM.icf::TRCTL.tom"/>
TXFORM=L5909
#OOBsOfficerX#OOTotal#OOOvduTotal#OOPercent
^
</sub>

!---------- 單據輸出組合 ----------
<sub type="PART" prompt='{{#SCRTIL}} world' name="L5909" layout="cols=1;screen.width=[130,870];order=1;">
[
["[L5909]                                 房貸專員案件品質排行表                         "],
! [#<->#,"@截止日期",#DeadLine],
]
</sub>
<sub type="SELECT">
#RPTFG=0,QUERY.GRID,L5909
#any={detail:true, header:'L5909.part', caption:'', width:1020, rowNum:20, rowList:[20,40,60]}
!房貸專員
#OOBsOfficerX=房貸專員
{width:105,align:'center'}
!承做案件餘額(含轉催收款)
#OOTotal=承做案件餘額(含轉催收款)
{width:240}
!逾4期金額
#OOOvduTotal=逾四期金額
{width:150}
#OOPercent=逾放百分比(%)
{width:130}
^
<include src="PRT.icf::INQ04.sel" map="i=L5909;cpi=15"/>
#StartDate#EndDate
%
#OOBsOfficerX#OOTotal#OOOvduTotal#OOPercent
@^
</sub>

!------------ TXFORM = L5903 -----------
<sub type="FORM" name="L5903">

#SCRTIL=_,_,S
T(3,[L5903] 檔案借閱明細資料)

#LOOP=X,1,S
E(0,1)ASGN(LOOP$)
! 戶號
#OOCustNo=A,7,O
! 額度
#OOFacmNo=A,3,O
!同押品額度
#OOFacmNoMemo=X,20,O
! 申請序號
#OOApplSeq=A,3,O
! 戶名
#OOCustName=X,100,O
! 管理人
#OOKeeperEmpName=x,20,O
! 借閱人
#OOApplEmpName=x,20,O
! 借閱日期
#OOApplDate=D,7,O
! 歸還日期
#OOReturnDate=D,7,O

#OOReturnDateX=D,7,O
C(3,#OOReturnDate,0,S,E(0,#OOReturnDate))
! 歸還人
#OOReturnEmpName=x,20,O
! 用途
#OOUsageCode=X,2,O
#OOUsageCodeX=X,10,O
T(2,)
T(H,#OOUsageCode,#UsageCodeHelp)

! 正本/影本
#OOCopyCode=X,1,O
#OOCopyCodeX=X,10,O
T(2,)
C(4,#OOCopyCode,S,$)
T(H,#OOCopyCode,#CopyCodeHelp)

! 備註
#OORemark=X,100,O
! 備註
#OOApplObj=A,1,O

! 管理人
#OOKeeperEmpNo=x,6,O
! 借閱人
#OOApplEmpNo=x,6,O
! 歸還人
#OOReturnEmpNo=x,6,O
! 

! 管理人
#OKeeperEmpNoX=x,27,O
T(2,#OOKeeperEmpName)
!T(2,#OOKeeperEmpNo+ +#OOKeeperEmpName)
! 借閱人
#OApplEmpNoX=x,27,O
T(2,#OOApplEmpName)
!IF(#OOApplEmpNo=="",T(2,#OOApplEmpName),T(2,#OOApplEmpNo+ +#OOApplEmpName))
!T(2,#OOApplEmpNo+ +#OOApplEmpName)
! 歸還人
#OReturnEmpNoX=x,27,O
T(2,#OOReturnEmpName)
!T(2,#OOReturnEmpNo+ +#OOReturnEmpName)

#OOBUF_RETURN=X,300,S
T(2,1+#OOCustNo+#OOFacmNo+#OOApplSeq+#OOKeeperEnable+2)

#OOReturnFg=X,1,S

#OOBTN_RETURN=A,2,O
C(5,#OOReturnFg,Y,BIND(grid,{ cmd=CHAIN; ntxcd=L5103; ntxbuf=#OOBUF_RETURN}),BIND(grid,cancel))


!管理人修改按鈕是否顯示
#OOEnable=X,1,S
!管理人是否是啟用中
#OOKeeperEnable=X,1,S

#OOActFgX=x,12,O

#OORejectReason=x,200,O

#OOModifyFg=X,1,S

#OODeleteFg=X,1,S

#OOEntDy=D,7,S

#OOTxNo=X,18,L

#Entdy=D,7,S
E(0,#OOEntDy)
#TxNo=X,18,S
T(2,#OOTxNo)
#RimData=x,100,S
T(6A,#Entdy+#TxNo)

#tempFG=X,1,S
IF(#OOReturnDate>0,T(2,2),T(2,1))

#OOBUF_UPDATE=X,300,S
T(2,#OOCustNo+#OOFacmNo+#OOApplSeq+#OOCustName+#OOKeeperEmpNo
   +#OOApplEmpNo+#OOApplDate+#OOReturnDate+#OOReturnEmpNo
   +#OOUsageCode+#OORemark+#tempFG+#OOCopyCode+#OOApplObj+#OOFacmNoMemo+#OOApplEmpName)

#OOBTN_UPDATE=X,1,S
C(5,#OOEnable,Y,BIND(grid,{ cmd=CHAIN; ntxcd=L5108; ntxbuf=#OOBUF_UPDATE}),BIND(grid,cancel))

#LC014Mrkey=x,100,S
T(2,#OOCustNo+-+#OOFacmNo+-+#OOApplSeq)

#LC014Text=X,10,S
!T(H,#ApplCode,#ApplCodeHelp)

#LC014Note=x,100,S
T(2,#OOCustNo+-+#OOFacmNo+-+#OOApplSeq+ 檔案借閱+#LC014Text)

#BufLC014=X,200,S
T(2,L5103+#OOCustNo+#OOFacmNo+000+#LC014Mrkey+#LC014Note)

#OOTitaActFg=X,1,S
#OOTitaActFgX=X,10,O
T(2,)
C(4,#OOTitaActFg,S,$)
T(H,#OOTitaActFg,#TitaActFgHelp)

#OOBTN2NAME=X,10,S
IF(#OOTitaActFg >= 3,T(3,訂正),T(3,刪除))

#OOBUF_Delete=X,15,S
T(2,4+#OOCustNo+#OOFacmNo+#OOApplSeq+#OOKeeperEnable)
!刪除按鈕
#BTN2=X,1,S
C(5,#OODeleteFg,Y,BIND(grid,{ cmd=CHAIN; ntxcd=L5103; ntxbuf=#OOBUF_Delete}),BIND(grid,cancel))

! C(5,#OOHCodeFlag,Y,BIND(grid,{ cmd=EC; ntxcd=#OOTxCd; txno=#RimData;rim=LCR01}),BIND(grid,cancel))

#OOBUF_Modify=X,15,S
T(2,2+#OOCustNo+#OOFacmNo+#OOApplSeq+#OOKeeperEnable)
!修改按鈕
#BTN3=X,1,S
C(5,#OOModifyFg,Y,BIND(grid,{ cmd=CHAIN; ntxcd=L5103; ntxbuf=#OOBUF_Modify}),BIND(grid,cancel))


! 05:查詢
#OOBufSearch=X,300,S
T(2,5+#OOCustNo+#OOFacmNo+#OOApplSeq+Y)

#OOBtnSearch=X,1,O
BIND(grid,{ cmd=CHAIN; ntxcd=L5103; ntxbuf=#OOBufSearch})


! 檢查是否有附件資料，再顯示這個按鈕
! Y/N
#OOHasAttachment=X,1,S

#BTN=X,1,S
C(5,#OOHasAttachment,"Y",BIND(grid,{ cmd=CHAIN; ntxcd=LC014; ntxbuf=#BufLC014}),BIND(grid,cancel))
</sub>


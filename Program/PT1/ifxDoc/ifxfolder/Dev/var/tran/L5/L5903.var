!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L5903" layout="cols=1;screen.width=[130,870];printer.width=[20,80];order=1">
[
["[L5903]                                 檔案借閱明細資料查詢"],
[#<->#,"#FdCustNo+借款人戶號",#CustNo,#BTNA," ([借款人戶號]與[借閱日期]可擇一輸入)"],
[#<->#,"#FdApplDateFrom+借閱日期",#ApplDateFrom,"~",#ApplDateTo],
[#<->#,"#FdUsageCode+用途",#UsageCode,#UsageCodeX],
[#<->#,"#FdApplCode+狀態",#ApplCode,#ApplCodeX],
]
!---------- AP TEXT AREA Variables ----------
#BTNA=X,15,I,ui:button;value:顧客資料查詢;enable:1;tabbable:0
<include src="HELPRIM.icf::HELPRIM.rtn"/>

#FdCustNo=X,2,L
#FdApplDateFrom=X,2,L
T(3, )
#FdUsageCode=X,2,L
T(3,*)
#FdApplCode=X,2,L
T(3,*)

! 用途
#UsageCodeXRimName=x,20,S
T(3,CdCode.UsageCodeX)
#UsageCodeHelp=x,1024,S

!正本/影本
#CopyCodeRimName=x,20,S
T(3,CdCode.CopyCode)
#CopyCodeHelp=x,1024,S


#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#UsageCodeXRimName,#CopyCodeRimName)
R(1,XXR99)
CALL(#HelpProc)
T(2,@UsageCodeHelp,00:全部;+#HelpDesc1)
T(2,@CopyCodeHelp,#HelpDesc2)

!狀態
#ApplHelp=X,50,S
T(3,01:未還;02:已還;09:全部)

!登放記號
#TitaActFgHelp=X,50,S
T(3,1:登錄;2:放行;3:審核;4:審核放行)

#CHAIN=A,1,S
T(4,CHAIN$) 

#NTXBUF=X,26,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@CustNo,#NTXBUF,1,7)
T(1,@ApplDateFrom,#NTXBUF,8,7)
T(1,@ApplDateTo,#NTXBUF,15,7)
T(1,@UsageCode,#NTXBUF,22,2)
T(1,@ApplCode,#NTXBUF,24,2)
INVOKEJS(SHOW,p1,02,02,1)
!<瀏覽>                                   
#BUFA=X,74,S
T(2,#CustNo)
#BINDA=X,1,S
BIND(#BTNA,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUFA;ntxbuf5=type<-1:
CustNo<-OOCustNo})

#TODAY=A,7,S
T(1,#SYSCDATE,2,7)

#PREVALUE=A,1,S
E(0,@ApplDateFrom,#TODAY)
E(0,@ApplDateTo,#TODAY)
E(0,@UsageCode,0)
E(0,@ApplCode,1)

! 借款人戶號
#CustNo=A,7,I
C(3,#CHAIN,0,$,s)

#MRKEY=_,_,S
C(3,#CustNo,0,S,T(2,#CustNo))

! 借閱日期
#ApplDateFrom=D,7,I
C(3,#CHAIN,0,$,s)
C(3,#CustNo,0,E(0,@ApplDateFrom,#TODAY),$)
!@V(7)A(DATE,0,#ApplDateFrom)
@C(3,#ApplDateFrom,0,S,$)
A(DATE,0,#ApplDateFrom)

#ApplDateCheck=X,1,S
@C(3,#CustNo,0,C(3,#ApplDateFrom,0,V(P,[借款人戶號]與[借閱日期]需擇一輸入),S),S)

#ApplDateTo=D,7,I
C(3,#CHAIN,0,$,s)
C(3,#ApplDateFrom,0,S,$)
C(3,#CustNo,0,E(0,@ApplDateTo,#TODAY),$)
@V(7)A(DATE,0,#ApplDateTo)
V(5,#ApplDateFrom,#TODAY)

! 用途
#UsageCode=A,2,I
C(3,#CHAIN,0,$,s)
HELP(#UsageCodeHelp)
@T(3,@UsageCodeX,)
V(H,#UsageCodeHelp)

#UsageCodeX=X,16,L
T(H,#UsageCode,#UsageCodeHelp)

! 狀態
#ApplCode=A,2,I
C(3,#CHAIN,0,$,s)
HELP(#ApplHelp)
@T(3,@ApplCodeX,)
V(H,#ApplHelp)

#ApplCodeX=X,4,L
T(H,#ApplCode,#ApplHelp)
                   
#RPTFG=A,1,S
E(0,0)

</sub>


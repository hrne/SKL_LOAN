!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L5915" layout="cols=1;screen.width=[130,870];printer.width=[20,80];order=1">
[
["[L5915]                                 協辦人員業績統計"],
[#<->#,"#FdYm+業績年月",#Ym],
]

#FdYm=X,2,L
T(3,*)

#CHAIN=A,1,S
T(4,CHAIN$) 

#NTXBUF=X,13,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)


! 業績計算終止日
#Ym=A,5,I
@
A(YM13,1,#Ym)


#RPTFG=A,1,S
! ******年月份
</sub>


<sub type="TIM">
#AdjDate
#TxKind
#CustType
#END
</sub>

!------ 下行電文TOM ------
<sub type="TOM">
<include src="COM.icf::TRCTL.tom"/>
TXFORM=L4721
^
<include src="HELPRIM.icf::HELPRIM.tom"/>
</sub>



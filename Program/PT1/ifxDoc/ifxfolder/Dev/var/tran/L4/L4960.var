!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L4960" layout="cols=1;screen.width=[130,870];printer.width=[20,80];order=1">
[
["[L4960]@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@火險保費資料查詢(By客戶)"],
[#<->#,"#FdSearchFlag+查詢選項",#SearchFlag,#SearchFlagX],
[#<->#,"#FdCustNo+借款人戶號",#CustNo,#CnoName],
[#<->#,"#FdCustId+統一編號",#CustId,#CidName],
]

#FdSearchFlag=X,2,L
T(3,*)
#FdCustNo=X,2,L
T(3,*)
#FdCustId=X,2,L
T(3,*)

#SearchFlagHelp=x,30,S
T(3,1:戶號;2:統一編號)

#SelfInsuCodeHelp=x,30,S
T(3,0:新保;1:自保;2:續保)

#StatusCodeHelp=x,50,S
T(3,0:正常;1:借支;2:催收;3:呆帳;4:結案)

#CHAIN=A,1,S
T(4,CHAIN$) 


#NTXBUF=X,18,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)

#PREDIS=A,1,S
INVOKEJS(SHOW,L4960_2,0)INVOKEJS(SHOW,L4960_3,0)

! 查詢選項
#SearchFlag=A,1,I
C(3,#CHAIN,1,T(1,#NTXBUF,1,1),$)
C(3,#CHAIN,1,s,$)
HELP(#SearchFlagHelp)
@V(H,#SearchFlagHelp)
!HELP(1:戶號;2:統一編號)
!@V(1,1,2)

#CC123=A,1,S
C(3,#SearchFlag,1,INVOKEJS(SHOW,L4960_2,1)INVOKEJS(SHOW,L4960_3,0),INVOKEJS(SHOW,L4960_2,0)INVOKEJS(SHOW,L4960_3,1))

#SearchFlagX=X,8,L
T(3,)T(H,#SearchFlag,#SearchFlagHelp)
!SWITCH(#SearchFlag,1,T(3,戶號),3,T(3,統一編號))

<include src="ROM.icf::L4R02.dec"/>

#RimCustId=X,10,S

#RimCustNo=A,7,S

#ExistFlag=X,1,S

#RimEntryCode=A,1,S





! 借款人戶號
#CustNo=A,7,I
C(3,#CHAIN,1,T(1,#NTXBUF,2,7),$)
C(3,#CHAIN,1,s,$)
C(3,#SearchFlag,1,$,s)
@
T(3,#CnoName,)
V(2,0)


#RimL4r02_CustNo=X,1,S
C(3,#SearchFlag,1,$,s)
E(0,@RimEntryCode,2)
E(0,@RimCustNo,#CustNo)
RESET_RIM(#RimL4r02_CustNo,L4R02)
S(L4R02,1,#RimCustId,#RimCustNo,#RimEntryCode)
R(1,L4R02)
T(2,@CnoName,#L4r02CustName)

! 借款人戶號--戶名--調RIM取得
#CnoName=X,100,L
! T(2,測試戶號1)

! 統一編號
#CustId=X,10,I
C(3,#CHAIN,1,T(1,#NTXBUF,9,10),$)
C(3,#CHAIN,1,s,$)
C(3,#SearchFlag,2,$,s)
@
T(3,#CidName,)
V(7)
A(ID_UNINO,0,#CustId)


#RimL4r02_CustId=X,1,S
C(3,#SearchFlag,2,$,s)
E(0,@RimEntryCode,1)
T(2,@RimCustId,#CustId)
RESET_RIM(#RimL4r02_CustId,L4R02)
S(L4R02,1,#RimCustId,#RimCustNo,#RimEntryCode)
R(1,L4R02)
T(2,@CidName,#L4r02CustName)
T(2,@ExistFlag,#L4r02ExistFlag)
! C(5,#ExistFlag,N,V(Q,無此客戶資料，請自行輸入),$)

! 統一編號--戶名--調RIM取得
#CidName=X,100,L
! T(2,測試戶號2)


!身分證找戶號
<include src="ROM.icf::L2R47.dec"/>
#Rim2R47CustId=X,10,S

#RimL2R47=X,1,S
C(4,#CustId,S,$)
T(2,@Rim2R47CustId,#CustId)
RESET_RIM(#RimL2R47,L2R47)
S(L2R47,1,#Rim2R47CustId)
R(1,L2R47)

#MRKEY=_,_,S
C(4,#CustId,C(3,#CustNo,0,S,T(2,#CustNo)),T(2,#L2R47CustNo)) 

#date=D,7,S
E(0,#SYSDATE)
! 系統參數-查詢
#RPTFG=A,1,S
E(0,0)
C(3,#CHAIN,1,XMT(),$)
</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!------------ TXFORM = L4960 -----------OOFireInsuPre
<sub type="FORM" name="L4960">
#SCRTIL=_,_,S
T(3,[L4960] 火險保費資料By客戶)

#LOOP=X,1,S
E(0,1)ASGN(LOOP$)

! 年月份
#OOInsuYearMonth=A,5,O

! 額度
#OOFacmNo=A,3,O

! 火險保額
#OOFireInsuCovrg=m,14,O

! 火險保費
#OOFireInsuPrem=m,14,O

! 保險起日
#OOInsuStartDate=D,7,O

! 已保險迄日
#OOInsuEndDate=D,7,O

! 入帳 = 有無會計日
#OOEntryFlag=X,1,O

! 入賬日期
#OOAcDate=D,7,O

! 轉催 = 有無轉催日
#OOTransFlag=X,1,O

#OOClCode1=A,1,L
#OOClCode2=A,2,L
#OOClNo=A,7,L
!原保險單號碼
#OOPrevInsuNo=x,16,L
!批單號碼
#OOEndoInsuNo=x,16,L
#OOSelfInsuCode=A,1,L

#OOSelfInsuCodeX=X,4,L
T(3,)T(H,#OOSelfInsuCode,#SelfInsuCodeHelp)
!SWITCH(#OOSelfInsuCode,
!1,T(2,自保),
!2,T(2,續保),
!0,T(2,新保),
!T(2,))

#OOStatusCode=A,1,O
#OOStatusCodeX=X,8,O
T(3,)T(H,#OOStatusCode,#StatusCodeHelp)
! SWITCH(#OOStatusCode,0,T(2,正常),
!                      1,T(2,借支),
!                      2,T(2,催收),
!                      3,T(2,呆帳))

!地震險保險金額
#OOEthqInsuCovrg=m,14,L

!地震險保費
#OOEthqInsuPrem=m,14,L

!備註
#OORemark=X,50,L

#OOInsuReceiptDate=D,7,S

#OOTotalPrem=m,14,S
E(0,#OOFireInsuPrem+#OOEthqInsuPrem)


#OOBtnAText=X,4,L
T(3,自保)

! 1:on 0:off
#OOBtnFlag=A,1,S

#OOBUFB_M=x,55,S
T(2,2+#OOClCode1+#OOClCode2+#OOClNo+#OOPrevInsuNo+#OOEndoInsuNo+#OOSelfInsuCode+#OOInsuYearMonth)
#OOBUFA_2=x,55,S
T(2,6+#OOClCode1+#OOClCode2+#OOClNo+#OOPrevInsuNo+#OOEndoInsuNo+#OOSelfInsuCode+#OOInsuYearMonth)
#OOBUFA_1=x,55,S
T(2,1+#OOClCode1+#OOClCode2+#OOClNo+#OOPrevInsuNo+#OOEndoInsuNo+#OOSelfInsuCode+#OOInsuYearMonth)
#OOBUFA_renew=x,55,S
T(2,6+#OOClCode1+#OOClCode2+#OOClNo+#OOPrevInsuNo+#OOEndoInsuNo+#OOSelfInsuCode+#OOInsuYearMonth)

! [維護]
#OOBTNB=A,2,O
C(3,#OOSelfInsuCode,0,BIND(grid,cancel),
BIND(grid,{ cmd=CHAIN; ntxcd=L4611; ntxbuf=#OOBUFB_M}))

#OOBTNA=A,2,O
IF(#OOInsuEndDate<#date,BIND(grid,cancel),$)
IF(#OOInsuEndDate<#date,s,$)
C(3,#OOStatusCode,1,BIND(grid,cancel),$)
C(3,#OOStatusCode,1,s,$)
C(3,#OOBtnFlag,1,$,BIND(grid,cancel))
C(3,#OOBtnFlag,1,$,s)
SWITCH(#OOSelfInsuCode,
1,
BIND(grid,cancel),
2,
BIND(grid,{ cmd=CHAIN; ntxcd=L4611; text=#OOBtnAText; ntxbuf=#OOBUFA_2}),
0,
BIND(grid,{ cmd=CHAIN; ntxcd=L4611; text=#OOBtnAText; ntxbuf=#OOBUFA_renew}),
BIND(grid,cancel))


</sub>


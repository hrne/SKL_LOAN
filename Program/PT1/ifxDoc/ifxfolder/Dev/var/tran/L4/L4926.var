!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L4926]                                 匯款轉帳檔查詢"],
["#FdSearch+查詢條件","(至少需輸入二欄)"],
["#FdReconCode+對帳類別  ",#ReconCode,#ReconCodeX],
["#FdEntryDate+入帳日期",#EntryDate],
["#FdCustNo+戶號","#CustNo],
["#FdRepayAmt+交易金額","#RepayAmt],
["#FdTraderInfo+交易人資料","#TraderInfo],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
!#BUF1=X,42,S

#FdSearch=X,2,L
T(3,*)

#FdReconCode=X,2,L
T(3, )
#FdEntryDate=X,2,L
T(3, )
#FdCustNo=X,2,L
T(3, )
#FdTraderInfo=X,2,L
T(3, )
#FdRepayAmt=X,2,L
T(3, )
!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>
#ApHelp=x,1024,S
T(3,CdCode.BankReconCode)
S(XXR99,01,#ApHelp)
R(1,XXR99)
CALL(#HelpProc)
T(2,#HelpDesc1)

!DscptCodeHelp
#CDCODEDscptCode=x,20,S                  
T(3,CdCode.BankRmftCode)
#DscptCodeHelp=x,1024,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDCODEDscptCode)
R(1,XXR99)
CALL(#HelpProc)
T(2,@DscptCodeHelp,#HelpDesc1)



#CHAIN=A,1,S
T(4,CHAIN$)

!日曆日
#CDATE=D,8,S
T(4,CDATE$)

#CDATEX=D,7,S
T(1,#CDATE,2,7)

!本營業日
#DATE=D,8,S
T(4,DATE$)



!#BUF0=X,50,S
!T(2,1+0000000000000000000000000000000000000000000000000)

!#button=x,100,S
!T(2,{cmd=CHAIN; ntxcd=L8204; ntxbuf=#BUF0; ntxbuf5=type})ASGN(BTN_NEW1$)
!K(MODIFYBUTTON,1,新增記錄)
!K(MODIFYBUTTON,查詢)

!#button1=x,100,S
!T(2,{cmd=CHAIN; ntxcd=L8205; ntxbuf=1; ntxbuf5=type})ASGN(BTN_NEW2$)
!K(MODIFYBUTTON,2,產生疑似洗錢報表)


!對帳類別
#ReconCode=X,3,I
HELP(#ApHelp)
@IF(#ReconCode=="",s,$)
@T(3,@ReconCodeX,)
V(H,#ApHelp)

#ReconCodeX=X,20,L
IF(#ReconCode=="",S,$)
T(H,#ReconCode,#ApHelp)

!入帳日期
#EntryDate=D,7,I
T(1,#DATE,2,7)
D(8)
C(3,#EntryDate,0,S,$)
@A(DATE,0,#EntryDate)
!C(4,#EntryDate,S,V(5,0010101,#CDATEX))

!戶號
#CustNo=A,7,I
@
C(4,#ReconCode,$,s)
C(4,#EntryDate,$,s)
C(3,#CustNo,0,V(2,0),$)

!金額
#RepayAmt=m,14,I

!交易人資料
#TraderInfo=X,20,I
@
C(4,#ReconCode,C(3,#EntryDate,0,$,C(3,#CustNo,0,$,s)C(3,#RepayAmt,0,$,s)),C(3,#EntryDate,0,C(3,#CustNo,0,$,s)C(3,#RepayAmt,0,$,s),s))
V(7)



#RPTFG=A,1,S
E(0,0)

!<include src="COM.icf::RPTFG.dec"/>

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!---------- TXFORM = L4926 ----------
<sub type="FORM" name="L4926">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L4926,$,S)

#SCRTIL=_,_,S
T(3,[L4926] 匯款轉帳檔查詢)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

!對帳類別
#OOReconCode=X,3,S
#OOReconCodeX=X,20,S
IF(#OOReconCode=="",S,$)
T(H,#OOReconCode,#ApHelp)
#OOReconCodeX1=X,27,L
T(2,#OOReconCode+-+#OOReconCodeX)

#OOStatusX=X,6,S


!入帳日期  (TOTA)
#OOEntryDate=D,7,L

!戶號(TOTA)
#OOCustNo=A,7,L

!交易人資料(TOTA)
#OOTraderInfo=x,20,L


!還款金額(TOTA)
#OORepayAmt=m,14.2,S
#OORepayAmt1=m,14,S
E(0,#OORepayAmt)
#OORepayAmtX=X,17,L
IF(#OORepayAmt1==0,S,$)
X(#OORepayAmt1)

!會計日期
#OOAcDate=D,7,L
!批號
#OOBatchNo=X,6,L
!明細序號
#OODetailSeq=A,6,L
!還款類別
#OORepayType=X,2,S
!摘要代碼
#OODscptCode=X,4,S
#OODscptCodeX=X,20,O
T(H,#OODscptCode,#DscptCodeHelp)

#OODscptCodeX2=X,50,O
T(2,#OODscptCode+#OODscptCodeX)

!匯款銀行代碼
#OORemintBank=X,7,S

#OOTitaTlrNo=X,6,S
#OOTitaTxtNo=X,8,S


! 修改的按鈕
! 1+7+7+3+3+7+20+14.2
!#OOBUF2=X,70,S
!T(2,2+#OORecordDate+#OOCustNo+#OOFacmNo+#OOBormNo+#OORepayDate+#OOLogNo+#OORepayAmt1)
!#OOBTN2=X,4,S
!BIND(grid,{cmd=CHAIN;ntxcd=L8204;text=修改;ntxbuf=#OOBUF2})

! 刪除的按鈕
!#OOBUF4=X,70,S
!T(2,4+#OORecordDate+#OOCustNo+#OOFacmNo+#OOBormNo+#OORepayDate+#OOLogNo+#OORepayAmt1)
!#OOBTN4=X,4,S
!BIND(grid,{cmd=CHAIN;ntxcd=L8204;text=刪除;ntxbuf=#OOBUF4})

! 查詢的按鈕
#OOBUF5=X,70,S
T(2,5+#OOAcDate+#OOBatchNo+#OODetailSeq)
#OOBTN5=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L4927;text=查詢;ntxbuf=#OOBUF5})

! 複製的按鈕
!#OOBUF6=X,70,S
!T(2,3+#OORecordDate+#OOCustNo+#OOFacmNo+#OOBormNo+#OORepayDate+#OOLogNo+#OORepayAmt1)
!#OOBTN6=X,4,S
!BIND(grid,{cmd=CHAIN;ntxcd=L8204;text=複製;ntxbuf=#OOBUF6})

! 歷程的按鈕
!#OOBUF7=X,35,S
!T(2,2+#OOCustNo+#OOFacmNo+#OOBormNo+0010101+00+#OORecordDate)
!#OOBTN7=X,4,S
!C(3,#OOHaveLog,0,BIND(grid,cancel),BIND(grid,{cmd=CHAIN;ntxcd=L8924;text=歷程;ntxbuf=#OOBUF7}))

</sub>



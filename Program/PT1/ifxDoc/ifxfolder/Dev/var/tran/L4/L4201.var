!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L4201" layout="cols=3;screen.width=[130,370,130,370];printer.width=[20,80];order=1">
[
["[L4201]@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@匯款轉帳-明細維護"],
[#<->#,"@資料日期",#AcDate,"(會計日)"],
["@整批批號",#BatchNo],["@明細序號",#DetailSeq],
[#<->#,"@戶號",#CustNo,#CustName],
["@還款類別",#RepayTypeX],["@還款金額",#RepayAmt],
["@已作帳金額",#AcctAmt],["@入暫收金額",#DisacctAmt],
["@處理狀態",#ProcStsCode,#ProcStsCodeX],["@處理代碼",#ProcCode,#ProcCodeX],
["@處理說明",#ProcDscptCode],["@交易序號",#TxSn],
[#<->#,"@快捷按鈕",#BTN_L3921,#BTN_L3200,#BTN_L3210,#BTN_L3922,#BTN_L3420],
["@存摺帳號",#DepAcctNo],["@入帳日期",#EntryDate],
["@摘要",#DscptCode],["@虛擬帳號",#VirtualAcctNo],
["@提款",#WithdrawAmt],["@存款",#DepositAmt],
["@匯款銀行代碼",#RemintBankX],["@會計日期",#TitaEntdy],
["@交易人資料",#TraderInfo],["@經辦",#TitaTlrNo],
["@還款類別",#RepayTypeA,#RepayTypeAX],["@交易序號",#TitaTxtNo],
[#<->#,"@入帳戶號",#CustNoA,"-",#FacmNoA],
]
#BTN_L3921=X,94,I,ui:button;value:回收試算;enable:1;tabbable:0;
#BTN_L3200=X,94,I,ui:button;value:回收登錄;enable:1;tabbable:0;
#BTN_L3210=X,94,I,ui:button;value:暫收款登錄;enable:1;tabbable:0;
#BTN_L3922=X,94,I,ui:button;value:結案試算;enable:1;tabbable:0;
#BTN_L3420=X,94,I,ui:button;value:結案登錄;enable:1;tabbable:0;

<include src="HELPRIM.icf::HELPRIM.rtn"/>
! 還款類別
#CDDEF01=x,50,S
T(3,CdCode.RepayType)

#RepayTypeHelp=x,1024,S

! 處理狀態
#CDDEF02=x,20,S
T(3,CdCode.ProcStsCode)

#ProcStsCodeHelp=x,1024,S
#ProcStsCodeHelpB=x,1024,S
! 處理說明
#CDDEF03=x,20,S
T(3,CdCode.ProcCode)

#ProcCodeHelp=x,1024,S

! 銀行代號
#CDDEF04=x,20,S
T(3,CdCode.BankCd)

#BankCdHelp=x,1024,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF01,#CDDEF02,#CDDEF03,#CDDEF04)
R(1,XXR99)
CALL(#HelpProc)
T(2,@RepayTypeHelp,#HelpDesc1)
T(2,@ProcStsCodeHelp,#HelpDesc2)
T(HELP,@ProcStsCodeHelpB,#ProcStsCodeHelp,+,0;1;2)
T(2,@ProcCodeHelp,#HelpDesc3)
T(2,@BankCdHelp,#HelpDesc4)

#CHAIN=A,1,S
T(4,CHAIN$) 
IF(#CHAIN!=1,V(P,此為連動交易),$)

#NTXBUF=X,500,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@FuncCode,#NTXBUF,1,1)
T(1,@AcDate,#NTXBUF,2,7)
T(1,@BatchNo,#NTXBUF,9,6)
T(1,@DetailSeq,#NTXBUF,15,6)
T(1,@CustNo,#NTXBUF,21,7)
T(1,@RepayType,#NTXBUF,28,2)
T(1,@RepayAmtX,#NTXBUF,30,18)
T(1,@AcctAmtX,#NTXBUF,48,18)
T(1,@DisacctAmtX,#NTXBUF,66,18)
T(1,@ProcStsCode,#NTXBUF,84,1)
T(1,@ProcStsCodeTemp,#NTXBUF,84,1)
T(1,@ProcCode,#NTXBUF,85,5)
T(1,@ProcDscptCode,#NTXBUF,90,300)
T(1,@TxSn,#NTXBUF,394,14)
T(1,@FacmNoA,#NTXBUF,408,3)
INVOKEJS(SHOW,p1,02,02,1)
IF(#ProcStsCodeTemp == 4 ||
    #ProcStsCodeTemp == 3 || 
    #ProcStsCodeTemp == 2 || 
    #ProcStsCodeTemp == 1 || 
    #ProcStsCodeTemp == 0,T(F,@ProcStsCode,1),T(F,@ProcStsCode,0))

#RepayAmtX=X,18,S
#AcctAmtX=X,18,S
#DisacctAmtX=X,18,S
E(0,@RepayAmt,#RepayAmtX)
E(0,@AcctAmt,#AcctAmtX)
E(0,@DisacctAmt,#DisacctAmtX)

!功能 2修改5查詢
#FuncCode=A,1,S
C(3,#FuncCode,5,K(NOBUTTON,CLOSEY)T(F,@RepayTypeA,0)T(F,@CustNoA,0)T(F,@FacmNoA,0),$)

#UpdateCode=N,1,S
IF(#FuncCode==1 || #FuncCode==2 || #FuncCode==3,E(0,1),E(0,0))

! 會計日期
#AcDate=D,7,L
! 整批批號
#BatchNo=X,6,L
!明細序號
#DetailSeq=A,6,L

#RimFunCode=A,1,S

#RimL4r35A=X,1,S
C(3,#FuncCode,5,$,s)
E(0,@RimFunCode,#FuncCode)
E(0,@RimAcDate,#AcDate)
T(2,@RimBatchNo,#BatchNo)
E(0,@RimDetailSeq,#DetailSeq)
RESET_RIM(#RimL4r35A,L4R35)
S(L4R35,1,#RimFunCode,#RimAcDate,#RimBatchNo,#RimDetailSeq)
R(1,L4R35)
E(0,@CustNo,#L4r35CustNo)
E(0,@RepayType,#L4r35RepayType)
E(0,@RepayAmt,#L4r35RepayAmt)
E(0,@AcctAmt,#L4r35AcctAmt)
E(0,@DisacctAmt,#L4r35DisacctAmt)
T(2,@ProcStsCode,#L4r35ProcStsCode)
T(2,@ProcStsCodeTemp,#L4r35ProcStsCode)
T(2,@ProcCode,#L4r35ProcCode)
T(2,@ProcDscptCode,#L4r35ProcDscptCode)
T(2,@TxSn,#L4r35TxSn)
E(0,@FacmNoA,#L4r35FacmNoA)

!戶號
#CustNo=A,7,L
E(0,@RimEntryCode,2)

#RimCustId=X,10,S

#RimCustNo=A,7,S

#ExistFlag=X,1,S

#RimEntryCode=A,1,S

#RimL4r02_CustNo=X,1,S
C(3,#CustNo,0,s,$)
E(0,@RimCustNo,#CustNo)
RESET_RIM(#RimL4r02_CustNo,L4R02)
S(L4R02,1,#RimCustId,#RimCustNo,#RimEntryCode)
R(1,L4R02)
T(2,@CustName,#L4r02CustName)

!戶名
#CustName=X,60,L

! 會計日期
#RimAcDate=D,7,S
! 整批批號
#RimBatchNo=X,6,S
!明細序號
#RimDetailSeq=A,6,S

#RimL4r04=X,1,S
E(0,@RimAcDate,#AcDate)
T(2,@RimBatchNo,#BatchNo)
E(0,@RimDetailSeq,#DetailSeq)
RESET_RIM(#RimL4r04,L4R04)
S(L4R04,1,#RimAcDate,#RimBatchNo,#RimDetailSeq)
R(1,L4R04)
E(0,@DepAcctNo,#L4r04DepAcctNo)
E(0,@EntryDate,#L4r04EntryDate)
T(2,@DscptCode,#L4r04DscptCode)
T(2,@VirtualAcctNo,#L4r04VirtualAcctNo)
E(3,@WithdrawAmt,#L4r04WithdrawAmt)
E(3,@DepositAmt,#L4r04DepositAmt)
E(3,@Balance,#L4r04Balance)
T(1,@RemintBank,#L4r04RemintBank,1,3)
T(2,@TraderInfo,#L4r04TraderInfo)
E(0,@TitaEntdy,#L4r04TitaEntdy)
T(2,@TitaTlrNo,#L4r04TitaTlrNo)
T(2,@TitaTxtNo,#L4r04TitaTxtNo)

#RepayType=A,2,L

#RepayTypeX=X,10,L
T(3,)T(H,#RepayType,#RepayTypeHelp)

!還款金額
#RepayAmt=m,14,L
!已作帳金額
#AcctAmt=m,14,L
!入暫收金額
#DisacctAmt=m,14,L
!處理狀態
#ProcStsCodeTemp=X,1,L

#ProcStsCode=X,1,I
HELP(#ProcStsCodeHelpB)
T(F,@ProcStsCode,0)
C(3,#UpdateCode,1,$,s)
IF(#RepayAmt < 0,T(F,@ProcStsCode,0),T(F,@ProcStsCode,1))
IF(#RepayAmt >= 0,$,s)
IF(#ProcStsCode == 4 || #ProcStsCode == 3 || #ProcStsCode == 2 || #ProcStsCode == 1 || #ProcStsCode == 0,$,s)
@
C(5,#ProcStsCodeTemp,#ProcStsCode,s,C(5,#ProcStsCodeTemp,4,V(1,0,1),C(5,#ProcStsCodeTemp,3,V(1,1),C(5,#ProcStsCodeTemp,2,V(1,0,1),C(5,#ProcStsCodeTemp,0,V(1,1,2),V(1,0))))))
! IF(#ProcStsCodeTemp == #ProcStsCode,s,IF(#ProcStsCodeTemp == 4 ,V(1,0,1),IF(#ProcStsCodeTemp == 3,V(1,1),IF(#ProcStsCodeTemp == 2,V(1,0,1),IF(#ProcStsCodeTemp == 0,V(1,1,2),V(1,0))))))



#ProcStsCodeX=X,10,L
T(3,)T(H,#ProcStsCode,#ProcStsCodeHelp)
@
C(3,#ProcStsCode,1,T(F,@CustNoA,0)T(F,@FacmNoA,0)T(F,@RepayTypeA,0),T(F,@CustNoA,1)T(F,@FacmNoA,1)T(F,@RepayTypeA,1))
C(5,#ProcStsCodeTemp,4,C(5,#ProcStsCode,0,s,C(5,#ProcStsCode,1,s,V(P,只可選擇0或1))),$)
C(5,#ProcStsCodeTemp,3,C(5,#ProcStsCode,1,s,V(P,只可選擇1)),$)
C(5,#ProcStsCodeTemp,2,C(5,#ProcStsCode,0,s,C(5,#ProcStsCode,1,s,V(P,只可選擇0或1))),$)
C(5,#ProcStsCodeTemp,1,C(5,#ProcStsCode,0,s,V(P,只可選擇0)),$)
C(5,#ProcStsCodeTemp,0,C(5,#ProcStsCode,1,s,C(5,#ProcStsCode,2,s,V(P,只可選擇1或2))),$)

!處理說明
#ProcCode=X,5,L
#ProcCodeX=X,22,L
T(3,)T(H,#ProcCode,#ProcCodeHelp)
!T(A,#ProcCode,L4DEF,ProcCodeDef,ProcCode,ProcCodeX)

#ProcDscptCode=X,4.75,L

!交易序號
#TxSn=X,14,L
!<回收試算>
#BUF_L3921=X,74,S
T(2,L4201+#CustNo+000+000+TWD+0000000000000000+#EntryDate)
#BIND_L3921=X,1,S
BIND(#BTN_L3921,click, {cmd=CHAIN; ntxcd=L3921; ntxbuf=#BUF_L3921})

!<回收登錄>
#BUF_L3200=X,74,S
T(2,L4201+#CustNo+000+000+#EntryDate+00000000000000000000000000000000)
#BIND_L3200=X,1,S
BIND(#BTN_L3200,click, {cmd=CHAIN; ntxcd=L3200; ntxbuf=#BUF_L3200})

!<暫收款登錄>
#BUF_L3210=X,74,S
T(2,#CustNo+000)
#BIND_L3210=X,1,S
BIND(#BTN_L3210,click, {cmd=CHAIN; ntxcd=L3210; ntxbuf=#BUF_L3210})

!<結案試算>
#BUF_L3922=X,74,S
T(2,L4201+#CustNo+          +0000000+000+000+TWD+0+#EntryDate)
#BIND_L3922=X,1,S
BIND(#BTN_L3922,click, {cmd=CHAIN; ntxcd=L3922; ntxbuf=#BUF_L3922})

!<結案登錄>
#BUF_L3420=X,74,S
T(2,#CustNo)
#BIND_L3420=X,1,S
BIND(#BTN_L3420,click, {cmd=CHAIN; ntxcd=L3420; ntxbuf=#BUF_L3420})

! ------------------以下調RIM取值------------------
!存摺帳號
#DepAcctNo=A,14,L
!入帳日期
#EntryDate=D,7,L
!摘要
#DscptCode=X,50,L
!虛擬帳號
#VirtualAcctNo=x,14,L
!提款
#WithdrawAmt=+m,14,L
!存款
#DepositAmt=+m,14,L
!結餘
#Balance=m,14,S
!匯款銀行代碼
#RemintBank=A,3,L

#RemintBankX=X,16,L
T(3,)T(H,#RemintBank,#BankCdHelp)
!會計日
#TitaEntdy=D,7,L
!經辦
#TitaTlrNo=X,6,L
!交易序號
#TitaTxtNo=X,8,L

!交易人資料
#TraderInfo=X,40,L
! ------------------------------------------------

#RepayTypeA=A,2,I
E(0,#RepayType)
C(3,#UpdateCode,1,$,s)
C(3,#ProcStsCode,1,s,$)
HELP(#RepayTypeHelp)
@V(H,#RepayTypeHelp)
!HELP(01:期款;
!     02:部分償還;
!     03:結案;
!     04:帳管費;
!     05:火險費;
!     06:契變手續費;
!     07:法務費;
!     09:其他)
!@V(1,01,02,03,04,05,06,07,09)
#RepayTypeAX=X,10,L
T(3,)T(H,#RepayTypeA,#RepayTypeHelp)
!SWITCH(#RepayType,01,T(3,期款),
!                  02,T(3,部分償還),
!                  03,T(3,結案),
!                  04,T(3,帳管費),
!                  05,T(3,火險費),
!                  06,T(3,契變手續費),
!                  07,T(3,法務費),
!                  09,T(3,其他),T(3, ))
!戶號
#CustNoA=A,7,I
E(0,#CustNo)
C(3,#UpdateCode,1,$,s)
C(3,#ProcStsCode,1,s,$)

#FacmNoA=A,3,I
C(3,#UpdateCode,1,$,s)
C(3,#ProcStsCode,1,s,$)


#MRKEY=_,_,S
T(2,#CustNo)



<include src="ROM.icf::L4R02.dec"/>
<include src="ROM.icf::L4R35.dec"/>
<include src="ROM.icf::L4R04.dec"/>
</sub>

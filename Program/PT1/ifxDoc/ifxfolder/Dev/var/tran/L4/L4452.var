!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L4452" layout="cols=1;screen.width=[130,870];printer.width=[20,80];order=1">
[
["[L4452]@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@銀行扣款(媒體製作)"],
[#<->#,"#FdFunctionCode+功能",#FunctionCode,#FunctionCodeX],
[#<->#,"#FdEntryDate+入帳日期",#EntryDate],
[#<->#,"#FdOpItem+作業項目",#OpItem,#OpItemX],
]

#FdFunctionCode=X,2,L
T(3,*)

#FdEntryDate=X,2,L
T(3,*)

#FdOpItem=X,2,L
T(3,*)

#CHAIN=A,1,S
T(4,CHAIN$) 

#NextDay=D,7,S
! E(0,#SYSNDATE)

#OpItemHelp=x,40,S
T(3,1:ACH扣款;2:郵局扣款;9:全部)

#FunctionHelp=x,50,S
T(3,1:產出媒體檔;2:重置媒體碼)

! 調rim
<include src="ROM.icf::L4R11.dec"/>

#RimToday=D,7,S

! 功能
#FunctionCode=A,1,I
HELP(#FunctionHelp)
@V(H,#FunctionHelp)

! 功能中文
#FunctionCodeX=X,10,L
T(3,)T(H,#FunctionCode,#FunctionHelp)

#RimL4r11=X,1,S
C(2,#FKEY,$,s,s,s,$,$,s,s,$,$)
RESET_RIM(#RimL4r11,L4R11)
S(L4R11,1,#RimToday)
R(1,L4R11)
E(0,@EntryDate,#L4r11EntryDate)

! 入帳日期 = 下營業日
#EntryDate=D,7,I
! E(0,#NextDay)
@A(DATE,0,#EntryDate)
V(7)

C(3,#CHAIN,0,$,s)

#OpItem=A,1,I
HELP(#OpItemHelp)
@V(H,#OpItemHelp)

#OpItemX=X,10,L
T(3,)T(H,#OpItem,#OpItemHelp)

</sub>


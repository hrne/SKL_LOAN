!------------ TXFORM = L492A -----------
<sub type="FORM" name="L492A">
#SCRTIL=_,_,S
T(3,[L492A] 已入帳未還款By戶號)


!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
E(0,1)ASGN(LOOP$)

#OOCustNo=A,7,O

#OOFacmNo=A,3,O

#OOEntryDate=D,7,O

#OORepayCode=A,2,O
#OORepayCodeA=A,2,O
C(3,#OORepayCode,11,E(0,1),E(0,#OORepayCode))
! 01	匯款轉帳
! 02	銀行扣款
! 03	員工扣款
! 04	支票(客戶繳交)
! 05	支票(兌現支票)
! 06	理賠金
! 07	代收款-債權協商
! 09	其他
! 11	匯款轉帳預先作業
! 90	暫收抵繳
! 91	借新還舊
#OORepayCodeX=X,16,O
T(3,)T(H,#OORepayCode,#RepayCodeHelp)
!SWITCH(#OORepayCode,
!00,T(2,全部),
!01,T(2,匯款轉帳),
!02,T(2,銀行扣款),
!03,T(2,員工扣款),
!04,T(2,支票（客戶繳交）),
!05,T(2,法院扣薪),
!06,T(2,理賠金),
!07,T(2,代收款-債權協商),
!09,T(2,其他),
!11,T(2,匯款轉帳預先作業),
!90,T(2,暫收抵繳),
!91,T(2,借新還舊))


#OORepayCodeXX=X,19,O
T(2,#OORepayCode+-+#OORepayCodeX)

#OORepayType=A,2,O

#OORepayTypeX=X,10,O
T(3,)T(H,#OORepayType,#RepayTypeHelp)
!SWITCH(#OORepayType,01,T(3,期款),
!                    02,T(3,部分償還),
!                    03,T(3,結案),
!                    04,T(3,帳管費),
!                    05,T(3,火險費),
!                    06,T(3,契變手續費),
!                    07,T(3,法務費),
!                    09,T(3,其他),T(3, ))

#OORepayTypeXX=X,12,O
T(2,#OORepayType+-+#OORepayTypeX)

#OOAcDate=D,7,S
#OOBatchNo=X,6,S

#OODetailSeq=A,6,O

#OOAcctCode=X,3,O

! tota接值,by 幣別傳值
#OORepayAmt=m,14.2,O


! ADJUSTDOT 根據Precision 判斷外幣(2)/台幣(0)
! 將OORepayAmt調整為OORepayAmtX
#OORepayAmtX=X,22,O
CALL2(#ADJUSTDOT,#Precision,#OORepayAmt,#OORepayAmtX)
P(R,#OORepayAmtX)

#OOchequeTrans=A,1,S
C(3,#OORepayCode,4,
T(1,@OOChequeAcct,#OORvNo,1,9)
T(1,@OOChequeNo,#OORvNo,11,7)
E(0,@OOChequeAmt,#OORepayAmt),$)

#OOChequeAcctX=A,9,O
#OOChequeNoX=A,7,O
#OOChequeAmtX=+m,16,O

#OOChequeAcct=A,9,O
#OOChequeNo=A,7,O
#OOChequeAmt=+m,14.2,O

#OOAcNo=X,18,S

#OORvNo=X,30,S

#OODscpt=X,4,S
#OONote=x,80,S
</sub>


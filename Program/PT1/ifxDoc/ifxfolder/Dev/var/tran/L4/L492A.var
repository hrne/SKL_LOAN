!---------- AP TEXT AREA Variables ----------
! Help
<include src="HELPRIM.icf::HELPRIM.rtn"/>

![還款來源]
#CDDEF01=x,26,S
T(3,CdCode.BatchRepayCode)

#RepayCodeHelp=x,1024,S

![還款方式]
#CDDEF02=x,26,S
T(3,CdCode.RepayType)

#RepayTypeHelp=x,1024,S

![幣別小數位]
#CDDEF03=x,26,S
T(3,CdCode.CurrPrecision)

#CurrPrecisionHelp=x,1024,S 

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF01,#CDDEF02,#CDDEF03)
R(1,XXR99)
CALL(#HelpProc)
T(2,@RepayCodeHelp,#HelpDesc1) 
T(HELP,@RepayCodeHelp,#RepayCodeHelp,-,91;93)
T(2,@RepayCodeHelp,00:全部;+#RepayCodeHelp) 
T(2,@RepayTypeHelp,#HelpDesc2)
T(2,@CurrPrecisionHelp,#HelpDesc3)

#CHAIN=A,1,S
T(4,CHAIN$) 

#NTXBUF=X,100,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@CustNo,#NTXBUF,1,7)
T(1,@FacmNo,#NTXBUF,8,3)
T(1,@RepayCode,#NTXBUF,11,2)
T(1,@RepayType,#NTXBUF,13,2)

#HELP_CRDB=X,10,S
T(3,D:借;C:貸;)

#SECNO=_,_,S
!---------- HELP ----------------------------
![戶號]
#CustNo = A,7,I
E(0,@RimEntryCode,2)
C(3,#CHAIN,0,$,s)

![額度編號]
#FacmNo = A,3,I
C(3,#CHAIN,0,$,s)

<include src="ROM.icf::L4R02.dec"/>

#RimCustId=X,10,S

#RimCustNo=A,7,S

#ExistFlag=X,1,S

#RimEntryCode=A,1,S

#RimL4r02_CustNo=X,1,S
C(3,#CustNo,0,s,$)
E(0,@RimCustNo,#CustNo)
RESET_RIM(#RimL4r02_CustNo,L4R02)
S(L4R02,1,#RimCustId,#RimCustNo,#RimEntryCode)
R(1,L4R02)
T(2,@CustName,#L4r02CustName)

#CustName=X,100,L

! 還款來源
#RepayCode= X,2,I
C(3,#CHAIN,0,$,s)
T(2,00)
HELP(#RepayCodeHelp)
@V(H,#RepayCodeHelp)
!HELP(
!00:全部;
!01:匯款轉帳;
!02:銀行扣款;
!03:員工扣款;
!04:支票（客戶繳交）;
!05:支票（兌現支票）;
!06:理賠金;
!07:代收款-債權協商;
!09:其他;
!11:匯款轉帳預先作業;
!90:暫收抵繳;
!91:借新還舊)
!@V(3,00,01,02,03,04,05,06,07,09,11,90,91)

! 還款來源中文
#RepayCodeX= X,16,L 
T(3,)T(H,#RepayCode,#RepayCodeHelp)
!SWITCH(#RepayCode,
!00,T(2,全部),
!01,T(2,匯款轉帳),
!02,T(2,銀行扣款),
!03,T(2,員工扣款),
!04,T(2,支票（客戶繳交）),
!05,T(2,支票（兌現支票）),
!06,T(2,理賠金),
!07,T(2,代收款-債權協商),
!09,T(2,其他),
!11,T(2,匯款轉帳預先作業),
!90,T(2,暫收抵繳),
!91,T(2,借新還舊))

#RepayType=A,2,I
C(3,#CHAIN,0,$,s)
E(0,0)
HELP(#RepayTypeHelp)
@V(H,#RepayTypeHelp)
!HELP(
!00:全部;
!01:期款;
!02:部分償還;
!03:結案;
!04:帳管費;
!05:火險費;
!06:契變手續費;
!07:法務費;
!09:其他)
!@V(3,00,01,02,03,04,05,06,07,09)

#RepayTypeX=X,10,L
T(3,)T(H,#RepayType,#RepayTypeHelp)
!SWITCH(#RepayType,00,T(3,全部),
!                    01,T(3,期款),
!                    02,T(3,部分償還),
!                    03,T(3,結案),
!                    04,T(3,帳管費),
!                    05,T(3,火險費),
!                    06,T(3,契變手續費),
!                    07,T(3,法務費),
!                    09,T(3,其他),T(3, ))

#Precision=N,1,S
E(0,0)T(H,#CURNM,#CurrPrecisionHelp)
!T(A,#CURNM,CURR,CURDEF,ENAME,PRECISION)

#RPTFG=A,1,S

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)
</sub>



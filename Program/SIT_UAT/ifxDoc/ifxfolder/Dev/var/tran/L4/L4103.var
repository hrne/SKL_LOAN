!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L4103" layout="cols=1;screen.width=[130,870];printer.width=[20,80];order=1">
[
["[L4103]                                 抽退票傳票明細表"],
[#<->#,"#FdAcDate+會計日期",#AcDate],
]

#FdFunCode=X,2,L
#FdAcDate=X,2,L
#FdBatchNo=X,2,L

#CHAIN=A,1,S
T(4,CHAIN$) 

#NTXBUF=X,17,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@BatchNo,#NTXBUF,1,6)


! 會計日期
#AcDate=D,7,I
E(0,#SYSDATE)

![整批批號]
#BatchNo=X,6,S
T(3,BCK)


</sub>


!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L4607" layout="cols=1;screen.width=[200,700];printer.width=[20,20];">
[
["[L4607]                                 建築造價參考作業"],
["#FdCityCode+縣市代碼",#CityCode,#CityCodeX],
]

#FdCityCode=X,2,L
#FdFloorLowerLimit=X,2,L
!---------- AP Button AREA ----------
![Buttom資料查詢]
#BUF1=X,42,S
!---------- AP TEXT AREA Variables ----------

#CHAIN=A,1,S
T(4,CHAIN$)

#NTXBUF=X,13,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)


#CALLGRID=X,1,S
INVOKEJS(SHOW,p1,0)
##loop{times:50,n:1,k:2}
C(3,#Cost{n},0,INVOKEJS(SHOW,grd1_{k},0),INVOKEJS(SHOW,grd1_{k},1))
##end
INVOKEJS(SHOW,p1,1)INVOKEJS(SHOW,grd1_2,1)

!調RIM縣市 ST
<include src="ROM.icf::L6R03.dec"/>
!調RIM參數
#RimCityCode=X,2,S
#RimAreaCode=X,3,S
#RimFunCd=A,1,S

! 調RIM取縣市代碼
#RimL6r03City=X,1,S
E(0,@RimFunCd,1)
T(2,@RimCityCode,)
T(2,@RimAreaCode,)
S(L6R03,1,#RimFunCd,#RimCityCode,#RimAreaCode)
R(1,L6R03)

! 縣市代碼的下拉選單
#L6r03ResultCity=X,50000,S
T(2,#L6r03Result)

!縣市代碼
#CityCode=X,2,I
HELP(#L6r03ResultCity,cols:5)
@
V(H,#L6r03ResultCity)


! 縣市代碼對應名稱
#CityCodeX=X,10,L
T(H,#CityCode,#L6r03ResultCity)

<include src="ROM.icf::L4R28.dec"/>

! 調RIM取縣市代碼
#RimL4r28City=X,1,S
T(2,@RimCityCode,#CityCode)
S(L4R28,1,#RimCityCode)
R(1,L4R28)

#Final=A,3,S
E(0,0)

#recvL4r28=X,1,S
##loop{times:50,i:1,j:2}
E(0,@FloorLowerLimit{i},#L4r28FloorLowerLimit{i})
E(0,@Cost{i},#L4r28Cost{i})
IF(#Cost{i} >0,E(0,@Final,{i}),$)
##end

##loop{times:50,n:1,k:2}
IF(#Final >= {n},INVOKEJS(SHOW,grd1_{k},1),INVOKEJS(SHOW,grd1_{k},0))
##end


#FloorLowerLimit0=X,1,S
T(3,X)

#Cost0=X,1,S
T(3,X)

##loop{times:50,i:1}
#FloorLowerLimit{i}=A,3,L
E(0,{i})
##end


##loop{times:50,i:1,j:0,rowSeq:2}

#HIDE_ROW_{i}=@,1,S
C(3,#Cost{i},0,INVOKEJS(SHOW,grd1_{rowSeq},0),INVOKEJS(SHOW,grd1_{rowSeq},1))

#SHOW_ROW_{i}=@,1,S
INVOKEJS(SHOW,grd1_{rowSeq},1)

#Cost{i}=m,14.2,I
IF(#Final >= {j},CALL(#SHOW_ROW_{i}),CALL(#HIDE_ROW_{i}))
IF(#Final >= {j},$,S)


#Check{i}=X,1,S
IF(#Cost{i} != 0 && #Final == {j} ,E(0,@Final,{i}),$)


##end



#RPTFG=A,1,S

</sub>

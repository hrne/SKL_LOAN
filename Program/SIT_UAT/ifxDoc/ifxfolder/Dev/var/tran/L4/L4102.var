!---------- BASIC LABEL Variables ----------
<sub type="SYS">
</sub>

<sub type="SYS" name="label">
</sub>

<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)
</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L4102" layout="cols=1;screen.width=[130,870];printer.width=[20,80];order=1">
[
["[L4102]                                 匯款傳票明細表"],
[#<->#,"#FdAcDate+會計日期",#AcDate],
[#<->#,"#FdBatchNo+整批批號",#BatchNo],
]

#FdAcDate=X,2,L
#FdBatchNo=X,2,L

#CHAIN=A,1,S
T(4,CHAIN$) 

#NTXBUF=X,17,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@BatchNo,#NTXBUF,1,6)


! 會計日期
#AcDate=D,7,L
E(0,#SYSDATE)

![整批批號]
#BatchNo=X,6,I
C(4,#BatchNo,T(3,LN0100),$)
@V(7)


#FlagA=X,1,S
T(2,Y)
</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>


</sub>

!------ 上行電文 TEXT ------
<sub type="TIM">
#AcDate
#BatchNo
#END
</sub>
!------ 下行電文TOM ------
<sub type="TOM">
TXFORM=L4102^
<include src="COM.icf::TRCTL.tom"/>

</sub>


!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L4941" layout="cols=1;screen.width=[130,870];printer.width=[20,80];order=1">
[
["[L4941]                                 ACH授權資料歷史紀錄查詢"],
[#<->#,"#FdCustNo+戶號",#CustNo],
[#<->#,"#FdFacmNo+額度",#FacmNo],
]

#FdCustNo=X,2,L
T(3,*)
#FdFacmNo=X,2,L
T(3,*)


#CHAIN=A,1,S
T(4,CHAIN$) 

<include src="HELPRIM.icf::HELPRIM.rtn"/>
! 銀行下拉選單
#CDDEF01=x,50,S
T(3,CdCode.BankCd)

#BankCdHelp=x,1024,S

! 查詢方式下拉選單
#CDDEF02=x,20,S
T(3,CdCode.SrchWay)

#SearchFlagHelp=x,1024,S

! 授權狀態下拉選單
#CDDEF03=x,50,S
T(3,CdCode.AuthStatusCode)
#AuthStatusCodeHelp=x,1024,S

! 扣款別選單
#CDDEF04=x,20,S
T(3,CdCode.AuthCode)

! 授權狀態下拉選單
#AuthStatusRimName=x,50,S
T(3,CdCode.AuthStatus)
#AuthStatusHelp=x,1024,S

#AuthCodeHelp=x,1024,S

! 媒體碼下拉選單
#MediaCodeHelp2=x,50,S
T(3,Y:已產生媒體; :未產生媒體)


#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF01,#CDDEF02,#CDDEF03,#CDDEF04,#AuthStatusRimName)
R(1,XXR99)
CALL(#HelpProc)
T(2,@BankCdHelp,#HelpDesc1)
T(2,@SearchFlagHelp,#HelpDesc2)
T(2,@AuthStatusCodeHelp,#HelpDesc3)
T(2,@AuthCodeHelp,#HelpDesc4)
T(2,@AuthStatusHelp,#HelpDesc5)

#LABELDATEHelp=x,50,S
T(3,1:*    建檔日期;2:*    提出日期;3:*    提回日期)

#RepayBankHelp=x,50,S 
T(3,103:臺灣新光商銀;998:他行;999:全部)

#MediaCodeHelp=x,50,S 
T(3,N:N-新檔;O:O-舊檔;Y:Y)

#NTXBUF=X,29,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@CustNo,#NTXBUF,1,7)
T(1,@FacmNo,#NTXBUF,8,3)

#PREVALUE=A,1,S

! <交易明細>
#BufferCreate=X,1,S
T(2,1)


! 戶號
#CustNo=A,7,I
C(3,#CHAIN,0,$,s)
@V(2,0)

#MRKEY=_,_,S
C(3,#CustNo,0,S,T(2,#CustNo)) 

! 戶號
#FacmNo=A,3,I
C(3,#CHAIN,0,$,s)
@V(2,0)

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

#RPTFG=A,1,S

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!------------ TXFORM = L4941 -----------
<sub type="FORM" name="L4941">
#SCRTIL=_,_,S
T(3,[L4941] ACH授權資料歷史紀錄查詢)

#LOOP=X,1,S
E(0,1)ASGN(LOOP$)

#OOCustNo=A,7,O

#OOFacmNo=A,3,O

#OORepayBank=A,3,O
#OORepayBankX=x,16,O
T(H,@OORepayBankX,#OORepayBank,#BankCdHelp)

#OORepayAcct=A,14,O

#OORepayAcctX=X,14,O
T(2,@OORepayAcctX,#OORepayAcct)

#OOLimitAmt=m,14,O

#OOCreateFlag=x,1,O

#OOAuthCreateDate=D,7,O

#OOPropDate=D,7,O

#OORetrDate=D,7,O

#OOAuthStatus=X,1,O
#OOAuthStatusX=X,24,O
C(4,#OOAuthStatus,T(H,#OOAuthStatus,#AuthStatusHelp),$)
C(4,#OOAuthStatus,s,$)
C(3,#OOAuthStatus,0,
    C(5,#OOCreateFlag,D,T(2,取消授權扣款),
        C(5,#OOCreateFlag,A,T(2,成功新增),C(5,#OOCreateFlag,Z,T(2,暫停授權),T(2,)))),
T(3,)T(H,#OOAuthStatus,#AuthStatusHelp))

#OOMediaCode=X,1,O
#OOMediaCodeX=X,10,O
T(H,@OOMediaCodeX,#OOMediaCode,#MediaCodeHelp2)
#OOAmlRsp=X,1,O
#OOStampFinishDate=D,7,O
#OODeleteDate=D,7,O
#Deleted=x,1,O
IF(#OODeleteDate==0,T(2,),T(2,Y))

#OOUser=x,100,O
#OODate=D,7,O

! #OOBUF_CREAT=X,1,S
! T(2,1)
#OOBUF_EDIT=X,500,S
T(2,2
+#OOCustNo
+#OOFacmNo
+#OORepayBank
+#OORepayAcct
+#OOLimitAmt
+#OOCreateFlag
+#OOAuthStatus
+#OOMediaCode
+#OOAmlRsp
+#OOAuthCreateDate
+#OOStampFinishDate
)
#OOBUF_DELETE=X,500,S
T(2,4
+#OOCustNo
+#OOFacmNo
+#OORepayBank
+#OORepayAcct
+#OOLimitAmt
+#OOCreateFlag
+#OOAuthStatus
+#OOMediaCode
+#OOAmlRsp
+#OOAuthCreateDate
+#OOStampFinishDate
)
#OOBUF_SEARCH=X,500,S
T(2,5
+#OOCustNo
+#OOFacmNo
+#OORepayBank
+#OORepayAcct
+#OOLimitAmt
+#OOCreateFlag
+#OOAuthStatus
+#OOMediaCode
+#OOAmlRsp
+#OOAuthCreateDate
+#OOStampFinishDate
)

! #OOBTN_CREAT=A,2,O 改為頁面上方新增
! BIND(grid,{ cmd=CHAIN; ntxcd=L4410; ntxbuf=#OOBUF_CREAT})

#OOBTN_SEARCH=A,2,O
BIND(grid,{ cmd=CHAIN; ntxcd=L4410; ntxbuf=#OOBUF_SEARCH})


</sub>


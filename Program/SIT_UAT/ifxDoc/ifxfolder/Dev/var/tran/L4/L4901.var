!------------ TXFORM = L4901 -----------
<sub type="FORM" name="L4901">
#SCRTIL=_,_,S
T(3,[L4901] 撥款匯款記錄檔)

#LOOP=X,1,S
E(0,1)ASGN(LOOP$)
#OOAcDate=D,7,O

#OOBatchNo=X,6,O

#OODrawdownCode=A,2,O

#OODrawdownCodeX=X,22,O
T(H,#OODrawdownCode,#DrawdownCodeHelp)
!SWITCH(#OODrawdownCode,1,T(3,整批匯款),2,T(3,單筆匯款),3,T(3,退款-整批匯款),4,T(3,退款-存款憑條),9,T(3,全部))

#OOStatusCode=A,1,O

#OOStatusCodeX=X,10,O
T(3,)T(H,#OOStatusCode,#StatusCodeHelp)
!SWITCH(#OOStatusCode,0,T(3,正常),1,T(3,產檔後修正),2,T(3,產檔後訂正),9,T(3,全部))

#OORemitBank=A,3,O

#OORemitBankX=X,40,O

#OORemitBranch=A,4,O

#OORemitBranchX=X,40,O

#RemitAcctNo=A,14,O

#OOCustNo=A,7,O

#OOFacmNo=A,3,O

#OOBormNo=A,3,O

#OOCustNoX=X,15,O
T(2,#OOCustNo+-+#OOFacmNo+-+#OOBormNo)

#OOCustName=x,100,O

#OORemaker=X,60,O

#OOCurrencyCode=X,3,O

#OORemitAmt=m,14.2,O

#OOTellerNo=X,6,O

#OOTxtNo=X,8,O

#OOPayCode=X,1,O
#OOPayCodeX=x,30,O

#OOPayCodeXX=X,35,O
T(2,#OOPayCode+-+#OOPayCodeX)

#OOBUF_A=X,37,S
T(2,#OOAcDate+#OOTellerNo+#OOTxtNo+#OOCustNo+#OOFacmNo+#OOBormNo)

#OOBTN_A=A,2,O
BIND(grid,{ cmd=CHAIN; ntxcd=L3912; ntxbuf=#OOBUF_A})

</sub>


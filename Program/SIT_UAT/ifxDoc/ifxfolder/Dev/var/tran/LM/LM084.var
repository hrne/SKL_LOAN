!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[120,850];printer.width=[20,20];">
[
["[LM084]                               應收利息之帳齡分析表"],
["@年月份",#inputYear,"@/@",#inputMonth],
]
!---------- AP TEXT AREA Variables ----------

! 2021-07-28 智偉增加
#FUNCIND=_,_,S
E(0,5)
<include src="COM.icf::CheckAuth.rtn"/>

#LastMonthDate=D,8,S
D(7,3,#SYSDATE,1,0)

#inputYear=N,3,I
T(1,@inputYear,#LastMonthDate,2,3)
@V(5,0,999)

#inputMonth=A,2,I
T(1,@inputMonth,#LastMonthDate,5,2)
@V(5,1,12)

#tmp01=X,2,S
T(3,01)
#tmp0=X,1,S
T(3,0)

#inputEndOfMonthDate=D,8,S
@
T(2,#tmp0+#inputYear+#inputMonth+#tmp01)
D(7,2,#inputEndOfMonthDate,1,0)
D(6,3,#inputEndOfMonthDate,1)

</sub>


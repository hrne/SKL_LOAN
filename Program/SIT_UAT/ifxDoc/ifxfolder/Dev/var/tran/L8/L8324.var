#TranKey_Tmp=X,1,S
#TranKey=X,1,I
C(5,#TranKey_Tmp,1,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
HELP(#TranKeyHelp)
@V(H,#TranKeyHelp)
! C(5,#TranKey,C,K(MODIFYBUTTON,異動)T(F,@CustRegAddr,1)T(F,@CustComAddr,1)T(F,@CustRegTelNo,1)T(F,@CustComTelNo,1)T(F,@CustMobilNo,1)T(2,@FdCustRegAddr,*)T(2,@FdCustComAddr,*)T(2,@FdCustRegTelNo,*)T(2,@FdCustComTelNo,*)T(2,@FdCustMobilNo,*),C(5,#TranKey,D,K(MODIFYBUTTON,補件)T(F,@CustRegAddr,0)T(F,@CustComAddr,0)T(F,@CustRegTelNo,0)T(F,@CustComTelNo,0)T(F,@CustMobilNo,0)T(2,@FdCustRegAddr, )T(2,@FdCustComAddr, )T(2,@FdCustRegTelNo, )T(2,@FdCustComTelNo, )T(2,@FdCustMobilNo, ),$))
#TranKeyX=X,4,L
C(5,#TranKey_Tmp,1,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
T(H,#TranKey,#TranKeyHelp)

![債務人IDN]
#CustId=X,10,L

![債權金融機構代號]
#SubmitKey=X,3,L
#SubmitKeyX=X,80,L


!調解申請日
#ApplyDate=D,7,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
D(8)
@V(7)
A(DATE,0,#ApplyDate)

!受理調解機構代號-承審法院代碼OR郵遞區號
#CourtCode=X,3,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
@V(7)
T(2,@RimJcicBankCode,#CourtCode)
RESET_RIM(#RimL8R01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@CourtCodeX,#L8r01JcicBankName)

#CourtCodeX=X,80,L
T(2,@RimJcicBankCode,#CourtCode)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@CourtCodeX,#L8r01JcicBankName)

!最大債權金融機構代號
#MaxMainCode=X,3,I
C(5,#TranKey_Tmp,2,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
@V(7)
T(2,@RimJcicBankCode,#MaxMainCode)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@MaxMainCodeX,#L8r01JcicBankName)

#MaxMainCodeX=X,80,L
T(2,@RimJcicBankCode,#MaxMainCode)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@MaxMainCodeX,#L8r01JcicBankName)


!是否為最大債權金融機構報送
#IsMaxMain=X,1,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
HELP(#YesNoHelp)
@V(H,#YesNoHelp)

!帳號
#Account=X,50,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
@V(7)
V(8,0,50)


!擔保品類別
#GuarantyType=X,2,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
HELP(#TranKeyHelp,cols:6)
@V(H,#TranKeyHelp)
!看書
#GuarantyTypeX=X,50,L
T(H,#GuarantyType,#TranKeyHelp)

!原借款金額
#LoanAmt=m,12,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)

!本金
#Principal=m,10,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)

!利息
#Interest=m,10,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)

!違約金
#Penalty=m,10,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)

!其他費用
#Other=m,10,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)

!授信餘額
#CreditAmt=m,12,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
#Total=m,14,S
@E(0,#Principal+#Interest+#Penalty+#Other)
IF(#CreditAmt!=#Total,$,S)
V(P,授信餘額合計有誤，請重新輸入。〔本金＋利息＋違約金＋其他費用〕不等於〔授信餘額〕)

!每期應付金額
#TerminalPayAmt=m,10,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)

!最近一期繳款金額
#LatestPayAmt=m,10,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)


!最後繳息日
#FinalPayDay=D,7,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
D(8)
@A(DATE,0,#FinalPayDay)

!已到期尚未清償金額
#NotyetacQuit=m,10,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)

!每月應還款日
#MothPayDay=A,2,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)

#ChekcMothPayDay=X,1,S
IF(#MothPayDay>=1 && #MothPayDay<=31,S,$)
V(P,每月應還款日輸入有誤)

!契約起始年月
#BeginDate=X,5,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
@V(7)

#CheckBeginDate=D,7,S
T(2,#BeginDate+01)
@A(DATE,0,#CheckBeginDate)

!契約截止年月
#EndDate=X,5,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
@V(7)

#CheckEndDate=D,7,S
T(2,#EndDate+01)
@A(DATE,0,#CheckEndDate)
IF(#BeginDate>#EndDate,$,s)
V(P,契約起始年月不可大於契約截止年月)

!轉JCIC文字檔日期
#OutJcicTxtDate=D,7,L

! !<儲存>
! #BUF1=X,10,S
! T(2,#CUSTID)
! #BIND1=X,1,S
! BIND(#BTN1,click, {cmd=CHAIN; ntxcd=#CHAINCD; ntxbuf=#BUF1})

! 流水號
#Ukey=X,32,S

#RPTFG=X,1,S
E(0,0) 
</sub>


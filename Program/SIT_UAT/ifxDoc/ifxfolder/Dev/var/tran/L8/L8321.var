!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=3;screen.width=[200,300,200,300];printer.width=[20,20,20,20];order=1;">
[
["[L8321]                                 (63)變更還款方案結案通知資料"],
[#<->#,"#FdTranKey+交易代碼",#TranKey,#TranKeyX],
["#FdCustId+債務人IDN",#CustId],["#FdSubmitKey+報送單位代號",#SubmitKey,#SubmitKeyX],
["#FdRcDate+原前置協商申請日",#RcDate],["#FdChangePayDate+申請變更還款條件日",#ChangePayDate],
["#FdClosedDate+變更還款條件結案日期",#ClosedDate],["#FdClosedResult+結案原因",#ClosedResult,#ClosedResultX],
[#<->#,"#FdOutJcicTxtDate+轉JCIC文字檔日期",#OutJcicTxtDate],
]

#FdTranKey=X,2,L
#FdCustId=X,2,L
#FdSubmitKey=X,2,L
#FdRcDate=X,2,L
#FdChangePayDate=X,2,L
#FdClosedDate=X,2,L
#FdClosedResult=X,2,L
#FdOutJcicTxtDate=X,2,L

!---------- AP Button AREA ----------
![Buttom資料查詢]
!---------- 日期處理 ----------
#VIEW_SYSDATE=X,8,S
D(7,2,#SYSDATE,0,0)
T(1,#VIEW_SYSDATE,2,7)
E(0,1)ASGN(AGAIN$)
!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>
<include src="ROM.icf::L8R01.dec"/>
!TranKey
#CDDEF0001=x,20,S
T(3,CdCode.TranKey)
#TranKeyHelp=x,1024,S
! ClosedResult
#CDDEF0002=x,20,S
T(3,CdCode.ClosedResult)
#ClosedResultHelp=x,1024,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF0001,#CDDEF0002)
R(1,XXR99)
CALL(#HelpProc)
T(2,@TranKeyHelp,#HelpDesc1)
T(2,@ClosedResultHelp,#HelpDesc2)

#CHAIN=A,1,S
T(4,CHAIN$)
C(3,#CHAIN,0,V(P,本交易為連動交易，請由L8030-Jcic063連動進入),$)
T(F,@TranKey,0)T(F,@RcDate,0)T(F,@ChangePayDate,0)T(F,@ClosedDate,0)T(F,@ClosedResult,0)
#AutoReFresh=A,1,S
C(3,#CHAIN,1,$,S)
E(0,1)
ASGN(CAUTO$)

![連動交易]
#NTXBUF=X,138,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@TranKey_Tmp,#NTXBUF,1,1)
T(1,@TranKey,#NTXBUF,2,1)

! 新增進入
#Deal_New=X,1,S
C(5,#TranKey_Tmp,1,$,S)
T(1,@CustId,#NTXBUF,3,10)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(F,@RcDate,1)T(F,@ChangePayDate,1)T(F,@ClosedDate,1)T(F,@ClosedResult,1)
K(MODIFYBUTTON,新增)
T(2,@FdRcDate,*)
T(2,@FdChangePayDate,*)
T(2,@FdClosedDate,*)
T(2,@FdClosedResult,*)

! 異動進入
#Deal_Update=X,1,S
C(5,#TranKey_Tmp,2,$,S)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(F,@ClosedDate,1)T(F,@ClosedResult,1)
T(1,@Ukey,#NTXBUF,3,32)
T(2,@FdClosedResult,*)
T(2,@FdClosedDate,*)
! 查詢進入
#Deal_Search=X,1,S
C(5,#TranKey_Tmp,5,$,S)
T(1,@Ukey,#NTXBUF,3,32)
K(NOBUTTON,CLOSEY)

! 刪除進入
#Deal_Delete=X,1,S
C(5,#TranKey_Tmp,4,$,S)
T(1,@Ukey,#NTXBUF,3,32)
K(MODIFYBUTTON,刪除)


! 預設值
#Default=X,1,S
T(2,@SubmitKey,458)
#RimJcicBankCode=X,3,S
#RimL8r01=X,1,S
T(2,@RimJcicBankCode,#SubmitKey)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@SubmitKeyX,#L8r01JcicBankName)


! [調Rim相關]
! 定義Rom欄位名稱、型態、長度
<include src="ROM.icf::L8R32.dec"/>
#RimUkey=X,32,S
! Rim L8R32.java
#RimL8r32=X,1,S
C(5,#TranKey,A,S,$)
T(2,@RimUkey,#Ukey)
RESET_RIM(#RimL8r32,L8R32)
S(L8R32,1,#RimUkey)
R(1,L8R32)
T(2,@CustId,#L8r32CustId)
T(2,@SubmitKey,#L8r32SubmitKey)
E(0,@RcDate,#L8r32RcDate)
E(0,@ChangePayDate,#L8r32ChangePayDate)
E(0,@ClosedDate,#L8r32ClosedDate)
T(2,@ClosedResult,#L8r32ClosedResult)
E(0,@OutJcicTxtDate,#L8r32OutJcicTxtDate)
![下拉選單的值]
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(H,@ClosedResultX,#ClosedResult,#ClosedResultHelp)
#Space=X,1,S
IF(#TranKey_Tmp == "5" || #TranKey_Tmp=="4",$,S)
T(2,@TranKey,#L8r32TranKey)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
! 調Rim相關 結束

!---------- AP TEXT AREA Variables ----------
![交易代碼]
#TranKey_Tmp=X,1,S
#TranKey=X,1,L
C(5,#TranKey_Tmp,1,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
HELP(#TranKeyHelp)
@V(H,#TranKeyHelp)

#TranKeyX=X,4,L


![債務人IDN]
#CustId=X,10,L


![報送單位代號]
#SubmitKey=X,3,L

#SubmitKeyX=X,80,L


!原前置協商申請日
#RcDate=D,7,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
D(8)
@V(7)
@A(DATE,0,#RcDate)

!申請變更還款條件日
#ChangePayDate=D,7,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
D(8)
@V(7)
@A(DATE,0,#ChangePayDate)


!變更還款條件結案日期
#ClosedDate=D,7,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
D(8)
@V(7)
@A(DATE,0,#ClosedDate)


!結案原因
#ClosedResult=X,1,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
HELP(#ClosedResultHelp)
@V(H,#ClosedResultHelp)

#ClosedResultX=X,40,L
T(H,#ClosedResult,#ClosedResultHelp)

![轉JCIC文字檔日期]
#OutJcicTxtDate=D,7,L

! 流水號
#Ukey=X,32,S

! !<儲存>
! #BUF1=X,10,S
! T(2,#CUSTID)
! #BIND1=X,1,S
! BIND(#BTN1,click, {cmd=CHAIN; ntxcd=#CHAINCD; ntxbuf=#BUF1})
#RPTFG=X,1,S
E(0,0)

</sub>


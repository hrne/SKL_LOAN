#TranKeyHelp=x,1024,S

!ApplyType
#CODEF0003=x,20,S
T(3,CdCode.ApplyType)
#ApplyTypeHelp=x,1024,S

! CourtCode
#CODEF0004=x,20,S
T(3,CdCode.CourtCode)
#CourtCodeHelp=x,1024,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CODEF0001,#CODEF0003,#CODEF0004)
R(1,XXR99)
CALL(#HelpProc)
T(2,@TranKeyHelp,#HelpDesc1)
T(2,@ApplyTypeHelp,#HelpDesc2)
T(2,@CourtCodeHelp,#HelpDesc3)
! 預設值
#Default=X,1,S
T(2,@SubmitKey,458)

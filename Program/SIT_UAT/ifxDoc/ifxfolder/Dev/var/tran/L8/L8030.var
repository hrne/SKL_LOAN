!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>


!TranKey
#CDCODETranKey=x,20,S
T(3,CdCode.TranKey)
 
#TranKeyHelp=x,1024,S

!TranCode
#CDCODETranCode=x,20,S
T(3,CdCode.TranCode)

#TranCodeHelp=x,2024,S

!DateFieldName
#CDCODEDateFieldName=x,30,S
T(3,CdCode.DateFieldName)

#DateFieldNameHelp=x,1024,S

!CaseStatus56
#CDCODECaseStatus56=x,30,S
T(3,CdCode.CaseStatus56)

#CaseStatus56Help=x,1024,S

!CaseStatus55
#CDCODECaseStatus55=x,30,S
T(3,CdCode.CaseStatus55)

#CaseStatus55Help=x,1024,S


#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDCODETranKey,#CDCODETranCode,#CDCODEDateFieldName,#CDCODECaseStatus56,#CDCODECaseStatus55)
R(1,XXR99)
CALL(#HelpProc)
T(2,@TranKeyHelp,#HelpDesc1)
T(2,@TranCodeHelp,#HelpDesc2)
T(2,@DateFieldNameHelp,#HelpDesc3)
T(2,@CaseStatus56Help,#HelpDesc4)
T(2,@CaseStatus55Help,#HelpDesc5)


#CHAIN=A,1,S
T(4,CHAIN$)

!---------- 日期處理 ----------
#VeiwSysDate=X,8,S
D(7,2,#SYSDATE,0,0)
T(1,#VeiwSysDate,2,7)

!交易編號
#TranCode=A,3,I
HELP(#TranCodeHelp,cols:3)
@V(H,#TranCodeHelp)


!<交易名稱>
#TranCodeX=X,50,L
T(H,#TranCode,#TranCodeHelp)


![身分證字號]
#CustId=X,10,I
@V(7)A(ID_UNINO,0,#CustId)
<include src="ROM.icf::L2R07.dec"/>
#RimTxCode=X,1,S
T(2,0)
#RimCustId=X,10,S
T(2,@RimCustId,#CustId)
#RimCustNo=A,7,S
E(0,0)
! 檢核該統編是否存在於客戶檔
#RimL2R07=X,1,S
RESET_RIM(#RimL2R07,L2R07)
S(L2R07,1,#RimTxCode,#RimCustNo,#RimCustId)

! 0->L
! 2->2
! 3->P
! 4->Q
! 5->5
! 6->R
! 7->7
! 8->8
! C->M
! A->N
! B->O

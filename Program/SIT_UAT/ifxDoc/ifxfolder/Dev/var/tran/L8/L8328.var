!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=3;screen.width=[250,250,300,200];printer.width=[20,20,20,20];order=1;">
[
["[L8328]                                 (448)前置調解無擔保債務分配表資料"],
[#<->#,"#FdTranKey+交易代碼",#TranKey,#TranKeyX],
[#<->#,"#FdCustId+債務人IDN",#CustId,#CustIdX],
[#<->#,"#FdSubmitKey+報送單位代號",#SubmitKey,#SubmitKeyX],
["#FdApplyDate+調解申請日",#ApplyDate],["#FdCourtCode+受理調解機構代號",#CourtCode,#CourtCodeX],
[#<->#,"#FdMaxMainCode+債權金融機構代號",#MaxMainCode,#MaxMainCodeX],
["#FdSignPrin+簽約金額-本金",#SignPrin],["#FdSignOther+簽約金額-利息、違約金及其他費用",#SignOther],
["#FdOwnPercentage+債權比例",#OwnPercentage],["#FdAcQuitAmt+每月清償金額",#AcQuitAmt],
[#<->#,"#FdOutJcicTxtDate+轉JCIC文字檔日期",#OutJcicTxtDate],
]

#FdTranKey=X,2,L
#FdCustId=X,2,L
#FdSubmitKey=X,2,L
#FdApplyDate=X,2,L
#FdCourtCode=X,2,L
#FdMaxMainCode=X,2,L
#FdSignPrin=X,2,L
#FdSignOther=X,2,L
#FdOwnPercentage=X,2,L
#FdAcQuitAmt=X,2,L
#FdOutJcicTxtDate=X,2,L

!---------- AP Button AREA ----------
![Buttom資料查詢]
!---------- 日期處理 ----------
#VIEW_SYSDATE=X,8,S
D(7,2,#SYSDATE,0,0)
T(1,#VIEW_SYSDATE,2,7)
E(0,1)ASGN(AGAIN$)
!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>
<include src="ROM.icf::L8R01.dec"/>
![交易代碼]-(A:新增;C:異動;D:刪除;X:補件)
#CDDEF0001=x,20,S
T(3,CdCode.TranKey)
#TranKeyHelp=x,1024,S

! CourtCode
#CDDEF0002=x,20,S
T(3,CdCode.CourtCode)
#CourtCodeHelp=x,1024,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF0001,#CDDEF0002)
R(1,XXR99)
CALL(#HelpProc)
T(2,@TranKeyHelp,#HelpDesc1)
T(2,@CourtCodeHelp,#HelpDesc2)

#CHAIN=A,1,S
T(4,CHAIN$)
C(3,#CHAIN,0,V(P,本交易為連動交易,請由L8030-Jcic448連動進入),$)
T(F,@TranKey,0)T(F,@ApplyDate,0)T(F,@CourtCode,0)T(F,@MaxMainCode,0)T(F,@SignPrin,0)T(F,@SignOther,0)T(F,@OwnPercentage,0)T(F,@AcQuitAmt,0)
#AutoReFresh=A,1,S
C(3,#CHAIN,1,$,S)
E(0,1)
ASGN(CAUTO$)

![連動交易]
#NTXBUF=X,138,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@TranKey_Tmp,#NTXBUF,1,1)
T(1,@TranKey,#NTXBUF,2,1)

! 新增進入
#Deal_New=X,1,S
C(5,#TranKey_Tmp,1,$,S)
T(1,@CustId,#NTXBUF,3,10)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(F,@ApplyDate,1)T(F,@CourtCode,1)T(F,@MaxMainCode,1)T(F,@SignPrin,1)T(F,@SignOther,1)T(F,@OwnPercentage,1)T(F,@AcQuitAmt,1)
K(MODIFYBUTTON,新增)
T(2,@FdApplyDate,*)
T(2,@FdCourtCode,*)
T(2,@FdMaxMainCode,*)
T(2,@FdSignPrin,*)
T(2,@FdSignOther,*)
T(2,@FdOwnPercentage,*)
T(2,@FdAcQuitAmt,*)

! 異動進入
#Deal_Update=X,1,S
C(5,#TranKey_Tmp,2,$,S)
T(F,@TranKey,1)T(F,@SignPrin,1)T(F,@SignOther,1)T(F,@OwnPercentage,1)T(F,@AcQuitAmt,1)
T(1,@Ukey,#NTXBUF,3,32)
K(MODIFYBUTTON,異動)
T(2,@FdTranKey,*)
T(2,@FdSignPrin,*)
T(2,@FdSignOther,*)
T(2,@FdOwnPercentage,*)
T(2,@FdAcQuitAmt,*)
T(3,@TranKeyHelp,C:異動;D:刪除;X:補件))

! 查詢進入
#Deal_Search=X,1,S
C(5,#TranKey_Tmp,5,$,S)
T(1,@Ukey,#NTXBUF,3,32)
K(NOBUTTON,CLOSEY)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)

! 刪除進入
#Deal_Delete=X,1,S
C(5,#TranKey_Tmp,4,$,S)
T(1,@Ukey,#NTXBUF,3,32)
K(MODIFYBUTTON,刪除)


! 預設值
#Default=X,1,S
T(2,@SubmitKey,458)
#RimJcicBankCode=X,3,S
#RimL8r01=X,1,S
T(2,@RimJcicBankCode,#SubmitKey)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@SubmitKeyX,#L8r01JcicBankName)

! [調Rim相關]
! 定義Rom欄位名稱、型態、長度
<include src="ROM.icf::L8R45.dec"/>
! 宣告Rim欲傳送的欄位1
! 欄位名稱即為後端在titaVo get值時的KEY
! 要注意欄為型態跟長度
! 宣告Rim欲傳送的欄位
#RimUkey=X,32,S
! Rim L8R45.java
#RimL8r45=X,1,S
C(5,#TranKey,A,S,$)
T(2,@RimUkey,#Ukey)
RESET_RIM(#RimL8r45,L8R45)
S(L8R45,1,#RimUkey)
R(1,L8R45)
T(2,@TranKey,#L8r45TranKey)
T(2,@CustId,#L8r45CustId)
T(2,@SubmitKey,#L8r45SubmitKey)
E(0,@ApplyDate,#L8r45ApplyDate)
T(2,@CourtCode,#L8r45CourtCode)
T(2,@MaxMainCode,#L8r45MaxMainCode)
E(0,@SignPrin,#L8r45SignPrin)
E(0,@SignOther,#L8r45SignOther)
E(0,@OwnPercentage,#L8r45OwnPercentage)
E(0,@AcQuitAmt,#L8r45AcQuitAmt)
E(0,@OutJcicTxtDate,#L8r45OutJcicTxtDate)
![下拉選單的值]
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(H,@CourtCodeX,#CourtCode,#CourtCodeHelp)
#Space=X,1,S
IF(#TranKey_Tmp == "5" || #TranKey_Tmp=="4",$,S)
T(2,@TranKey,#L8r45TranKey)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@MaxMainCodeX,#L8r01JcicBankName)
! 調Rim相關 結束


![交易代碼]
#TranKey_Tmp=X,1,S
#TranKey=X,1,I
C(5,#TranKey_Tmp,1,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
HELP(#TranKeyHelp)
@V(H,#TranKeyHelp)
C(5,#TranKey,C,K(MODIFYBUTTON,異動)T(F,@SignPrin,1)T(F,@SignOther,1)T(F,@OwnPercentage,1)T(F,@AcQuitAmt,1)T(2,@FdSignPrin,*)T(2,@FdSignOther,*)T(2,@FdOwnPercentage,*)T(2,@FdAcQuitAmt,*),
C(5,#TranKey,D,K(MODIFYBUTTON,刪除)T(F,@SignPrin,0)T(F,@SignOther,0)T(F,@OwnPercentage,0)T(F,@AcQuitAmt,0)T(2,@FdSignPrin, )T(2,@FdSignOther, )T(2,@FdOwnPercentage, )T(2,@FdAcQuitAmt, ),$))
C(5,#TranKey,X,K(MODIFYBUTTON,補件)T(F,@SignPrin,0)T(F,@SignOther,0)T(F,@OwnPercentage,0)T(F,@AcQuitAmt,0)T(2,@FdSignPrin, )T(2,@FdSignOther, )T(2,@FdOwnPercentage, )T(2,@FdAcQuitAmt, ),$))

#TranKeyX=X,4,L
C(5,#TranKey_Tmp,1,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
T(H,#TranKey,#TranKeyHelp)

![債務人IDN]
#CustId=X,10,L

#RimCustId=X,10,S
<include src="ROM.icf::L8R57.dec"/>
T(2,@RimCustId,#CustId)
! 檢核該統編是否存在於客戶檔
#RimL8R57=X,1,S
RESET_RIM(#RimL8R57,L8R57)
S(L8R57,1,#RimCustId)
R(1,L8R57)
T(2,@CustIdX,#L8R57CustName)

#CustIdX=X,2.50,L

![債權金融機構代號]
#SubmitKey=X,3,L

#SubmitKeyX=X,80,L


!調解申請日
#ApplyDate=D,7,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
D(8)
@V(7)
@A(DATE,0,#ApplyDate)

!受理調解機構代號-承審法院代碼OR郵遞區號
!#RimJcicCourtZipCode=X,3,S
#CourtCode=X,3,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,D,T(2,@CourtCode,#L8r45CourtCode),$)
C(5,#TranKey,X,T(2,@CourtCode,#L8r45CourtCode),$)
C(5,#TranKey,D,s,$)
C(5,#TranKey,X,s,$)
HELP(#CourtCodeHelp)
@V(H,#CourtCodeHelp)

#CourtCodeX=X,80,L
T(H,#CourtCode,#CourtCodeHelp)


!最大債權金融機構代號
#MaxMainCode=X,3,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
C(5,#TranKey,D,T(2,@MaxMainCode,#L8r45MaxMainCode),$)
C(5,#TranKey,X,T(2,@MaxMainCode,#L8r45MaxMainCode),$)
C(5,#TranKey,D,s,$)
C(5,#TranKey,X,s,$)
@V(7)
T(2,@RimJcicBankCode,#MaxMainCode)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@MaxMainCodeX,#L8r01JcicBankName)

#MaxMainCodeX=X,80,L


!簽約金額-本金
#SignPrin=m,9,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,D,T(2,@SignPrin,#L8r45SignPrin),$)
C(5,#TranKey,X,T(2,@SignPrin,#L8r45SignPrin),$)
C(5,#TranKey,D,s,$)
C(5,#TranKey,X,s,$)

!簽約金額-利息、違約金及其他費用
#SignOther=m,9,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,D,T(2,@SignOther,#L8r45SignOther),$)
C(5,#TranKey,X,T(2,@SignOther,#L8r45SignOther),$)
C(5,#TranKey,D,s,$)
C(5,#TranKey,X,s,$)

!債權比例
#OwnPercentage=m,3.2,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,D,T(2,@OwnPercentage,#L8r45OwnPercentage),$)
C(5,#TranKey,X,T(2,@OwnPercentage,#L8r45OwnPercentage),$)
C(5,#TranKey,D,s,$)
C(5,#TranKey,X,s,$)

!每月清償金額
#AcQuitAmt=m,9,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,D,T(2,@AcQuitAmt,#L8r45OwnPercentage),$)
C(5,#TranKey,X,T(2,@AcQuitAmt,#L8r45OwnPercentage),$)
C(5,#TranKey,D,s,$)
C(5,#TranKey,X,s,$)

!轉JCIC文字檔日期
#OutJcicTxtDate=D,7,L

! 流水號
#Ukey=X,32,S

#RPTFG=X,1,S
E(0,0) 
</sub>


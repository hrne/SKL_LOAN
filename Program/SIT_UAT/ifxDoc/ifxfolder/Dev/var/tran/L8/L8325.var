!---------- AP Button AREA ----------
![Buttom資料查詢]
!---------- 日期處理 ----------
#VIEW_SYSDATE=X,8,S
D(7,2,#SYSDATE,0,0)
T(1,#VIEW_SYSDATE,2,7)
E(0,1)ASGN(AGAIN$)
!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>
<include src="ROM.icf::L8R01.dec"/>

#CDDEF0001=x,20,S
T(3,CdCode.TranKey)
#TranKeyHelp=x,1024,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF0001)
R(1,XXR99)
CALL(#HelpProc)
T(2,@TranKeyHelp,#HelpDesc1)

#CHAIN=A,1,S
T(4,CHAIN$)
C(3,#CHAIN,0,V(P,本交易為連動交易，請由L8030-Jcic444連動進入),$)
T(F,@TranKey,0)T(F,@ApplyDate,0)T(F,@CourtCode,0)T(F,@CustRegAddr,0)T(F,@CustComAddr,0)T(F,@CustRegTelNo,0)T(F,@CustComTelNo,0)T(F,@CustMobilNo,0) 
#AutoReFresh=A,1,S
C(3,#CHAIN,1,$,S)
E(0,1)
ASGN(CAUTO$)

![連動交易]
#NTXBUF=X,138,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@TranKey_Tmp,#NTXBUF,1,1)
T(1,@TranKey,#NTXBUF,2,1)

! 新增進入
#Deal_New=X,1,S
C(5,#TranKey_Tmp,1,$,S)
T(1,@CustId,#NTXBUF,3,10)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(F,@ApplyDate,1)T(F,@CourtCode,1)T(F,@CustRegAddr,1)T(F,@CustComAddr,1)T(F,@CustRegTelNo,1)T(F,@CustComTelNo,1)T(F,@CustMobilNo,1) 
K(MODIFYBUTTON,新增)
T(2,@FdApplyDate,*)
T(2,@FdCourtCode,*)
T(2,@FdCustRegAddr,*)
T(2,@FdCustComAddr,*)
T(2,@FdCustRegTelNo,*)
T(2,@FdCustComTelNo,*)
T(2,@FdCustMobilNo,*)


! 異動進入
#Deal_Update=X,1,S
C(5,#TranKey_Tmp,2,$,S)
T(F,@TranKey,1)T(F,@CustRegAddr,1)T(F,@CustComAddr,1)T(F,@CustRegTelNo,1)T(F,@CustComTelNo,1)T(F,@CustMobilNo,1) 
T(1,@Ukey,#NTXBUF,3,32)
T(2,@FdApplyDate,*)
T(2,@FdCustRegAddr,*)
T(2,@FdCustComAddr,*)
T(2,@FdCustRegTelNo,*)
T(2,@FdCustComTelNo,*)
T(2,@FdCustMobilNo,*)
T(3,@TranKeyHelp,C:異動;D:刪除;X:補件))

! 查詢進入
#Deal_Search=X,1,S
C(5,#TranKey_Tmp,5,$,S)
T(1,@Ukey,#NTXBUF,3,32)
K(NOBUTTON,CLOSEY)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
! 刪除進入
#Deal_Delete=X,1,S
C(5,#TranKey_Tmp,4,$,S)
T(1,@Ukey,#NTXBUF,3,32)
K(MODIFYBUTTON,刪除)

! 預設值
#Default=X,1,S
T(2,@SubmitKey,458)
#RimJcicBankCode=X,3,S
#RimL8r01=X,1,S
T(2,@RimJcicBankCode,#SubmitKey)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@SubmitKeyX,#L8r01JcicBankName)

! [調Rim相關]
! 定義Rom欄位名稱、型態、長度
! Rim L8R42.java
<include src="ROM.icf::L8R42.dec"/>
#RimUkey=X,32,S
#RimL8r42=X,1,S
C(5,#TranKey,A,S,$)
T(2,@RimUkey,#Ukey)
RESET_RIM(#RimL8r42,L8R42)
S(L8R42,1,#RimUkey)
R(1,L8R42)
T(2,@TranKey,#L8r42TranKey)
T(2,@CustId,#L8r42CustId)
T(2,@SubmitKey,#L8r42SubmitKey)
E(0,@ApplyDate,#L8r42ApplyDate)
T(2,@CourtCode,#L8r42CourtCode)
T(2,@CustRegAddr,#L8r42CustRegAddr)
T(2,@CustComAddr,#L8r42CustComAddr)
T(2,@CustRegTelNo,#L8r42CustRegTelNo)
T(2,@CustComTelNo,#L8r42CustComTelNo)
T(2,@CustMobilNo,#L8r42CustMobilNo)
E(0,@OutJcicTxtDate,#L8r42OutJcicTxtDate)
![下拉選單的值]
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
#Space=X,1,S
IF(#TranKey_Tmp == "5" || #TranKey_Tmp=="4",$,S)
T(2,@TranKey,#L8r42TranKey)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(2,@RimJcicBankCode,#CourtCode)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@CourtCodeX,#L8r01JcicBankName)
! 調Rim相關 結束

![交易代碼]
#TranKey_Tmp=X,1,S
#TranKey=X,1,I
C(5,#TranKey_Tmp,1,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
HELP(#TranKeyHelp)
@V(H,#TranKeyHelp)
C(5,#TranKey,C,K(MODIFYBUTTON,異動)T(F,@CustRegAddr,1)T(F,@CustComAddr,1)T(F,@CustRegTelNo,1)T(F,@CustComTelNo,1)T(F,@CustMobilNo,1)T(2,@FdCustRegAddr,*)T(2,@FdCustComAddr,*)T(2,@FdCustRegTelNo,*)T(2,@FdCustComTelNo,*)T(2,@FdCustMobilNo,*),
C(5,#TranKey,D,K(MODIFYBUTTON,刪除)T(F,@CustRegAddr,0)T(F,@CustComAddr,0)T(F,@CustRegTelNo,0)T(F,@CustComTelNo,0)T(F,@CustMobilNo,0)T(2,@FdCustRegAddr, )T(2,@FdCustComAddr, )T(2,@FdCustRegTelNo, )T(2,@FdCustComTelNo, )T(2,@FdCustMobilNo, ),$))
C(5,#TranKey,X,K(MODIFYBUTTON,補件)T(F,@CustRegAddr,0)T(F,@CustComAddr,0)T(F,@CustRegTelNo,0)T(F,@CustComTelNo,0)T(F,@CustMobilNo,0)T(2,@FdCustRegAddr, )T(2,@FdCustComAddr, )T(2,@FdCustRegTelNo, )T(2,@FdCustComTelNo, )T(2,@FdCustMobilNo, ),$))
#TranKeyX=X,4,L
C(5,#TranKey_Tmp,1,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
T(H,#TranKey,#TranKeyHelp)

![債務人IDN]
#CustId=X,10,L

![債權金融機構代號]
#SubmitKey=X,3,L
#SubmitKeyX=X,80,L


!款項統一收付申請日
#ApplyDate=D,7,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
D(8)
@V(7)
A(DATE,0,#ApplyDate)

!受理調解機構代號-承審法院代碼OR郵遞區號
#CourtCode=X,3,I
C(5,#TranKey_Tmp,2,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
@V(7)
T(2,@RimJcicBankCode,#CourtCode)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@CourtCodeX,#L8r01JcicBankName)

#CourtCodeX=X,80,L
T(2,@RimJcicBankCode,#CourtCode)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@CourtCodeX,#L8r01JcicBankName)

!債務人戶籍之郵遞區號及地址
#CustRegAddr=C,76,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,D,T(2,@CustRegAddr,#L8r42CustRegAddr),$)
C(5,#TranKey,X,T(2,@CustRegAddr,#L8r42CustRegAddr),$)
C(5,#TranKey,D,s,$)
C(5,#TranKey,X,s,$)
@V(8,0,38)

!債務人通訊地之郵遞區號及地址
#CustComAddr=C,76,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,D,T(2,@CustComAddr,#L8r42CustComAddr),$)
C(5,#TranKey,X,T(2,@CustComAddr,#L8r42CustComAddr),$)
C(5,#TranKey,D,s,$)
C(5,#TranKey,X,s,$)
@V(8,0,38)

!債務人戶籍電話
#CustRegTelNo=X,16,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,D,T(2,@CustRegTelNo,#L8r42CustRegTelNo),$)
C(5,#TranKey,X,T(2,@CustRegTelNo,#L8r42CustRegTelNo),$)
C(5,#TranKey,D,s,$)
C(5,#TranKey,X,s,$)
@V(8,0,16)

!債務人通訊電話
#CustComTelNo=X,16,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,D,T(2,@CustComTelNo,#L8r42CustComTelNo),$)
C(5,#TranKey,X,T(2,@CustComTelNo,#L8r42CustComTelNo),$)
C(5,#TranKey,D,s,$)
C(5,#TranKey,X,s,$)
@V(8,0,16)

!債務人行動電話
#CustMobilNo=X,16,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,D,T(2,@CustMobilNo,#L8r42CustMobilNo),$)
C(5,#TranKey,X,T(2,@CustMobilNo,#L8r42CustMobilNo),$)
C(5,#TranKey,D,s,$)
C(5,#TranKey,X,s,$)
@V(8,0,16)

#CheckConnect=X,1,S
C(4,#CustRegTelNo,$,s)
C(4,#CustComTelNo,$,s)
C(4,#CustMobilNo,$,s)
V(P,債務人戶籍電話、通訊電話及行動電話至少需擇一報送。)

!轉JCIC文字檔日期
#OutJcicTxtDate=D,7,L

! 流水號
#Ukey=X,32,S

#RPTFG=X,1,S
E(0,0) 

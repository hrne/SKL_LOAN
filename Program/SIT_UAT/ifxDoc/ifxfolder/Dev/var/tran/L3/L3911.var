#OStoreRate1X=X,9,L
C(3,#OStoreRate1,0,S,$)
T(2,#WkORate1+#WkPercent)
P(L,#OStoreRate1X)

#OStoreRate2=n,2.4,S

#WkORate2=X,7,S
X(#OStoreRate2)
P(R,#WkORate2)

#OStoreRate2X=X,9,L
C(3,#OStoreRate2,0,S,$)
T(2,#WkORate2+#WkPercent)
P(L,#OStoreRate2X)

#OStoreRate3=n,2.4,S

#WkORate3=X,7,S
X(#OStoreRate3)
P(R,#WkORate3)

#OStoreRate3X=X,9,L
C(3,#OStoreRate3,0,S,$)
T(2,#WkORate3+#WkPercent)
P(L,#OStoreRate3X)

#OEffectDate1=D,7,S
#OEffectDate1X=X,9,L
C(3,#OEffectDate1,0,S,$)
CALL2(#AdjustDate,#OEffectDate1,#OEffectDate1X)

#OEffectDate2=D,7,S
#OEffectDate2X=X,9,L
C(3,#OEffectDate2,0,S,$)
CALL2(#AdjustDate,#OEffectDate2,#OEffectDate2X)

#OEffectDate3=D,7,S
#OEffectDate3X=X,9,L
C(3,#OEffectDate3,0,S,$)
CALL2(#AdjustDate,#OEffectDate3,#OEffectDate3X)

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)


#OOFacmNo=A,3,O


#OOEntryDate=D,7,O

#OOIntStartDate=D,7,O

#OOIntEndDate=D,7,O

#OOCurrencyCode=X,3,O

#OOPrecision=N,1,S
E(0,0)T(H,#OOCurrencyCode,#CurrPrecisionHelp)


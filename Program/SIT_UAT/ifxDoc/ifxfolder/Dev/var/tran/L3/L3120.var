C(3,#HIDE5FG,1,CALL(#HIDEP5),$)
C(3,#HIDE6FG,1,CALL(#HIDEP6),$)
C(3,#HIDE7FG,1,CALL(#HIDEP7),$)
CALL(#KLIGHT)

!--- P5 扣款帳號
#HIDEP5=@,1,S

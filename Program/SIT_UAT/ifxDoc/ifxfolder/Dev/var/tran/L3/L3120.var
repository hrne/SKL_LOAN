C(3,#HIDE4FG,1,K(LIGHT,#BTNHIDEP4,TXTRED,0),K(LIGHT,#BTNHIDEP4,TXTRED,-1))
C(3,#HIDE4FG,1,$,s)
C(3,#HIDE2FG,1,CALL(#HIDEP2),$)

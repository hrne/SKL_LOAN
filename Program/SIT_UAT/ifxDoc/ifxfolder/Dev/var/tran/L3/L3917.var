T(1,@EntryDate,#NTXBUF,16,7)
T(1,@IntStartDate,#NTXBUF,23,7)
T(1,@IntEndDate,#NTXBUF,30,7)
T(1,@AcDate,#NTXBUF,37,7)
T(1,@TellerNo,#NTXBUF,44,6)
T(1,@TxtNo,#NTXBUF,50,8)




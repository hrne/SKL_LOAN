! Help
<include src="HELPRIM.icf::HELPRIM.rtn"/>

![幣別小數位]
#CDDEF01=x,26,S
T(3,CdCode.CurrPrecision)

#CurrPrecisionHelp=x,1024,S 

![幣別名稱]
#CDDEF02=x,30,S
T(3,CdCode.Currency)

#CurrencyCodeHelp=x,1024,S

![減免違約金]
#CDDEF03=x,30,S
T(3,CdCode.BreachFlag)

#BreachFlagHelp=x,1024,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF01,#CDDEF02,#CDDEF03)
R(1,XXR99)
CALL(#HelpProc) 
T(2,@CurrPrecisionHelp,#HelpDesc1)
T(2,@CurrencyCodeHelp,#HelpDesc2)
T(2,@BreachFlagHelp,#HelpDesc3)

#FdMain=X,2,L
T(3,*)

#CHAIN=A,1,S
T(4,CHAIN$)
T(2,@RimTxCode,#TXCD) 
E(0,@RimFKey,#FKEY)

#BUF1A=X,18,S
T(3,00000009999999000)

#BIND1A=X,1,S
BIND(#BTN1,click, {cmd=CHAIN; ntxcd=L2015; ntxbuf=#BUF1A;ntxbuf5=type<-1:FacmNo<-OOFacmNo:CustNo<-OOCustNo:CustNoX<-OOCustName})

![借戶戶號]
!借戶戶號, 統一編號或核准編號擇一輸入
#FdCustNo=X,2,L

#CustNo=A,7,I
C(3,#CHAIN,1,T(1,#NTXBUF,6,7),$)
C(3,#CHAIN,1,s,$)
! INVOKEJS(SHOW,p1,1,3,1)
SHOW(#BTN1)HIDE(#BTN2,#BTN3)

#WKBTN1=X,1,S
! C(3,#CustNo,0,INVOKEJS(SHOW,p1,1,1,0),INVOKEJS(SHOW,p1,2,3,0))

#BUF1B=X,18,S
T(2,#CustNo+9999999000)

#BIND1B=X,1,S
BIND(#BTN1,click, {cmd=CHAIN; ntxcd=L2015; ntxbuf=#BUF1B;ntxbuf5=type<-1:FacmNo<-OOFacmNo:CustNo<-OOCustNo:CustNoX<-OOCustName})

#SendL2r07A=X,1,S
C(3,#CustNo,0,S,$)
T(3,@RimCustId, )
E(0,@RimCustNo,#CustNo)
RESET_RIM(#SendL2r07A,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo)
R(1,L2R07)

#CustNoX=X,100,L
C(3,#CustNo,0,S,T(2,#L2r07CustName))

#BUF2A=X,100,S

#BIND2A=X,1,S
BIND(#BTN2,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUF2A;ntxbuf5=type<-1:CustId<-OOCustId:CustIdX<-OOCustName})

![統一編號]
#FdCustId=X,2,L

#CustId=X,10,I
T(3,@FdApplNo, )
C(3,#CustNo,0,$,s)
! INVOKEJS(SHOW,p1,2,3,1)
SHOW(#BTN2)HIDE(#BTN3)
@C(4,#CustId,S,$)
V(7)

#BUF2B=X,100,S
T(2,0000000+#CustId)

#BIND2B=X,1,S
BIND(#BTN2,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUF2B;ntxbuf5=type<-1:CustId<-OOCustId:CustIdX<-OOCustName})

#CheckCustId=X,1,S
C(4,#CustId,S,$)
A(ID_UNINO,0,#CustId)

#WKBTN2=X,1,S
C(4,#CustId,HIDE(#BTN2),SHOW(#BTN2))
! C(4,#CustId,INVOKEJS(SHOW,p1,2,2,0),INVOKEJS(SHOW,p1,3,3,0))

#SendL2r07B=X,1,S
C(4,#CustId,S,$)
T(2,@RimCustId,#CustId)
E(0,@RimCustNo,0)
RESET_RIM(#SendL2r07B,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo,#END)
R(1,L2R07)

#CustIdX=X,100,L
C(4,#CustId,S,T(2,#L2r07CustName))

#BUF3A=X,18,S
T(3,000000099999990)

#BIND3A=X,1,S
BIND(#BTN3,click, {cmd=CHAIN; ntxcd=L2016; ntxbuf=#BUF3A;ntxbuf5=type<-1:ApplNo<-OOApplNo:FacmNo<-OOFacmNo:ApplNoX<-OOCustName})


![核准編號]
#FdApplNo=X,2,L
C(3,#CustNo,0,$,S)
C(4,#CustId,$,S)
T(3,*)

#ApplNo=A,7,I
C(3,#CustNo,0,$,S)
C(4,#CustId,$,S)
! INVOKEJS(SHOW,p1,3,3,1)
SHOW(#BTN3)
@C(3,#ApplNo,0,V(P,需擇一輸入),$)

#WKBTN3=X,1,S
C(3,#ApplNo,0,HIDE(#BTN3),SHOW(#BTN3))

#BUF3B=X,18,S
T(2,#ApplNo+99999990)

#BIND3B=X,1,S
BIND(#BTN3,click, {cmd=CHAIN; ntxcd=L2016; ntxbuf=#BUF3B;ntxbuf5=type<-1:ApplNo<-OOApplNo:FacmNo<-OOFacmNo:ApplNoX<-OOCustName})

#HIDEALLBTN=X,1,S
C(3,#CHAIN,1,$,S)
HIDE(#BTN1,#BTN2,#BTN3)

!用核准號碼(ApplNo)調L2R05
#SendL2r05A=X,1,S
C(3,#ApplNo,0,S,$)
E(0,@RimFuncCode,5)
E(0,@RimCustNo,#CustNo)
E(0,@RimFacmNo,0)
E(0,@RimApplNo,#ApplNo)
E(0,@RimCaseNo,0)
RESET_RIM(#SendL2r05A,L2R05)
S(L2R05,1,#RimFuncCode,#RimTxCode,#RimFKey,#RimCustNo,#RimFacmNo,#RimApplNo,#RimCaseNo)
R(1,L2R05)
E(0,@FacmNo,#L2r05FacmNo)

#SendL2r07C=X,1,S
C(3,#ApplNo,0,S,$)
T(2,@RimCustId,#L2r05CustId)
E(0,@RimCustNo,#CustNo)
RESET_RIM(#SendL2r07C,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo,#END)
R(1,L2R07)

#ApplNoX=X,100,L
C(3,#ApplNo,0,S,T(2,#L2r07CustName))

#TimCustNo=A,7,S
E(0,0)
C(3,#CustNo,0,$,E(0,#CustNo))
C(4,#CustId,$,E(0,#L2r07CustNo))
C(3,#ApplNo,0,$,E(0,#L2r05CustNo))

#BUF4A=X,50,S
T(2,0000000+#CustNo+#CustId+#ApplNo+000)

#BIND4A=X,1,S
BIND(#BTN4,click, {cmd=CHAIN; ntxcd=L3002; ntxbuf=#BUF4A;ntxbuf5=type<-1:FacmNo<-OOFacmNo:BormNo<-OOBormNo})

![額度編號]
!可不輸入
#FdFacmNo=X,2,L
C(3,#CHAIN,1,s,$)
C(3,#ApplNo,0,T(F,@FacmNo,1),T(F,@FacmNo,0))

#FacmNo=A,3,I
C(3,#CHAIN,1,T(1,#NTXBUF,13,3),$)
C(3,#CHAIN,1,s,$)
C(3,#ApplNo,0,$,s)

!用CustNo,FacmNo調L2R05
#SendL2r05B=X,1,S
C(3,#FacmNo,0,S,$)
E(0,@RimFuncCode,5)
E(0,@RimCustNo,#TimCustNo)
E(0,@RimFacmNo,#FacmNo)
E(0,@RimApplNo,0)
E(0,@RimCaseNo,0)
RESET_RIM(#SendL2r05B,L2R05)
S(L2R05,1,#RimFuncCode,#RimTxCode,#RimFKey,#RimCustNo,#RimFacmNo,#RimApplNo,#RimCaseNo)
R(1,L2R05)

![撥款序號]
!可不輸入
#FdBormNo=X,2,L
C(3,#CHAIN,1,s,$)
C(3,#FacmNo,0,T(F,@BormNo,0),T(F,@BormNo,1))

#BormNo=A,3,I
C(3,#CHAIN,1,T(1,#NTXBUF,16,3),$)
C(3,#CHAIN,1,s,$)
C(3,#FacmNo,0,S,$)


#SendL3r02=X,1,S
C(3,#BormNo,0,S,$)
E(0,@RimFuncCode,5)
E(0,#RimCustNo,#TimCustNo)
E(0,@RimFacmNo,#FacmNo)
E(0,@RimBormNo,#BormNo)
RESET_RIM(#SendL3r02,L3R02)
S(L3R02,1,#RimTxCode,#RimFKey,#RimFuncCode,#RimCustNo,#RimFacmNo,#RimBormNo)
R(1,L3R02)

!放款餘額
#LoanBal=m,14.2,S
E(0,#L2r07LoanBal)
C(3,#FacmNo,0,$,E(0,#L2r05LoanBal))
C(3,#BormNo,0,$,E(0,#L3r02LoanBal))

#WkLoanBal=m,14.2,S
E(0,#LoanBal-1)

![幣別]
#FdCurrencyCode=X,2,L

#CurrencyCode=X,3,L
T(3,TWD)
C(3,#FacmNo,0,s,T(2,#L2r05CurrencyCode))
C(3,#BormNo,0,s,T(2,#L3r02CurrencyCode))

#CurrencyCodeX=X,10,S
T(3,)T(H,#CurrencyCode,#CurrencyCodeHelp)

#Precision=N,1,S
E(0,0)T(H,#CurrencyCode,#CurrPrecisionHelp)
C(3,#Precision,0,SHOW(#TwRepayAmt)HIDE(#UsRepayAmt)
                ,SHOW(#UsRepayAmt)HIDE(#TwRepayAmt))


#MRKEY=_,_,S
C(3,#FacmNo,0,T(2,#TimCustNo),T(2,#TimCustNo+-+#FacmNo))
C(3,#BormNo,0,$,T(2,#TimCustNo+-+#FacmNo+-+#BormNo))


![金額]
!必須輸入
!放款餘額>金額> 0
#FdRepayAmt=X,2,L
T(3,*)

#TwRepayAmt=m,14,I
C(3,#Precision,0,$,S)
@V(2,0)
V(5,1,#WkLoanBal)

#UsRepayAmt=m,14.2,I
C(3,#Precision,0,S,$)
@V(2,0)
V(5,1,#WkLoanBal)

#TimRepayAmt=m,14.2,S
C(3,#Precision,0,E(0,#TwRepayAmt),E(0,#UsRepayAmt))

![減免違約金]
!i.必須輸入 0.不減免 1.減免
#FdBreachFlag=X,2,L
T(3,*)

#BreachFlag=A,1,I
HELP(#BreachFlagHelp)
E(0,0)
@V(H,#BreachFlagHelp)

#BreachFlagX=X,6,L
T(3,)T(H,#BreachFlag,#BreachFlagHelp)

![入帳日期]
!必須輸入, >=系統帳務日期
#FdEntryDate=X,2,L
T(3,*)

#EntryDate=D,7,I
C(3,#EntryDate,0,E(0,#SYSDATE),$)
@V(7)V(5,#SYSDATE,9991231)
A(DATE,0,#EntryDate)

#FdRptFg=X,2,L
T(3,*)



![額度編號]
!可不輸入
#FdFacmNo=X,2,L
T(3, )
T(F,@FacmNo,0)
IF(#L2r07FacmCount==1 && #L2r07BormCount==1,$,T(F,@BormNo,1))
! IF(#L2r07FacmCount==1 && #L2r07BormCount==1,$,T(3,@FdBormNo,*)T(F,@BormNo,1))
C(3,#ApplNo,0,$,S)
IF(#L2r07FacmCount==1 && #L2r07BormCount==1,S,$)
T(3,*)
T(F,@FacmNo,1)

#FacmNo=A,3,I
C(3,#ApplNo,0,$,s)
IF(#L2r07FacmCount==1 && #L2r07BormCount==1,E(0,#L2r07FacmNo),$)

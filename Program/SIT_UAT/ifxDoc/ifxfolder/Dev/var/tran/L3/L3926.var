                ,SHOW(#UsNewDueAmt,#UsExtraRepay)HIDE(#TwNewDueAmt,#TwExtraRepay))

![原攤還金額]
#FdOldDueAmt=X,2,L

#OldDueAmt=m,14.2,S
C(3,#BormNo,0,S,$)
E(0,#L3r02DueAmt)

#OldDueAmtX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#OldDueAmt,#OldDueAmtX)

#WkOldDueAmt=m,14.2,S

!必須輸入
!原攤還金額 > 新攤還金額 > 0
#FdNewDueAmt=X,2,L
T(3,)

#TwNewDueAmt=m,14,I
C(3,#Precision,0,$,S)
C(3,#BormNo,0,S,$)
@
C(3,#TwNewDueAmt,0,$,V(5,1,#WkOldDueAmt))

#UsNewDueAmt=m,14.2,I
C(3,#Precision,0,S,$)
C(3,#BormNo,0,S,$)
@
C(3,#UsNewDueAmt,0,$,V(5,1,#WkOldDueAmt))

#NewDueAmt=m,14.2,S
C(3,#Precision,0,E(0,#TwNewDueAmt),E(0,#UsNewDueAmt))
!部分償還金額
#FdExtraRepay=X,2,L
C(3,#NewDueAmt,0,T(3,*),T(3,))


#TwExtraRepay=m,14,I
C(3,#Precision,0,$,S)
C(3,#NewDueAmt,0,$,S)
@
C(3,#TwExtraRepay,0,V(P,新攤還金額、部分償還金額需擇一輸入),$)

#UsExtraRepay=m,14.2,I
C(3,#Precision,0,S,$)
C(3,#NewDueAmt,0,$,S)
@
C(3,#TwExtraRepay,0,V(P,新攤還金額、部分償還金額需擇一輸入),$)

#TimExtraRepay=m,14.2,S
C(3,#Precision,0,E(0,#TwExtraRepay),E(0,#UsExtraRepay))

![入帳日期]
!必須輸入, >=系統帳務日期

SHOW(#BTN1)HIDE(#BTN2,#BTN3,#BTN4,#BTN5)
@V(2,0)


#BUF1B=X,100,S
T(2,#TimCustNo+#TimCustNo+#FacmNo)


#BIND1B=X,1,S

RESET_RIM(#SendL2r07C,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo,#END)
R(1,L2R07)

#ApplNoX=X,100,L
C(3,#ApplNo,0,S,T(2,#L2r07CustName))

#TimCustNo=A,7,S
E(0,0)
C(3,#CustNo,0,$,E(0,#CustNo))
C(4,#CustId,$,E(0,#L2r07CustNo))
C(3,#ApplNo,0,$,E(0,#L2r05CustNo))

#WkA=X,1,S
IF(#L2r07FacmCount==1 && #L2r07BormCount==1,
   T(F,@FacmNo,0)T(F,@BormNo,0)
  ,T(F,@FacmNo,1)T(F,@BormNo,1))

#BUF1A=X,100,S
T(2,#TimCustNo+#TimCustNo+#FacmNo)


#BIND1A=X,1,S

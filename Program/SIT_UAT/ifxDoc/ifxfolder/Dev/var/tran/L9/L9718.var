!---------- AP TEXT AREA Variables ----------

! 2021-07-28 智偉增加
#FUNCIND=_,_,S
E(0,5)
<include src="COM.icf::CheckAuth.rtn"/>

#tmpYear=N,3,S
T(1,#SYSDATE,2,3)
#tmpMonth=N,2,S
T(1,#SYSDATE,5,2)
#tmp01=N,2,S
T(3,01)
#tmp0=N,1,S
T(3,0)

#defaultEntryDateMin=D,7,S
T(2,#tmpYear+#tmp01+#tmp01)

#tmpEntryDateMax=D,8,S
T(2,#tmp0+#tmpYear+#tmpMonth+#tmp01)
D(7,2,#tmpEntryDateMax,1,0)
D(6,3,#tmpEntryDateMax,1)

#defaultEntryDateMax=D,7,S
T(1,#tmpEntryDateMax,1,7)
! i don't know either but it works this way instead of ,2,7

#inputYearMonth=N,5,I
T(1,#SYSDATE,2,5)
@V(7)

#inputEntryDateMin=D,7,I
E(0,#defaultEntryDateMin)
@V(7)

#inputEntryDateMax=D,7,I
E(0,#defaultEntryDateMax)
@V(7)

#inputCollectorShow=X,6,I

#inputCollector=X,6,S
@C(4,#inputCollectorShow,T(3,999999),T(2,#inputCollector))

#inputDrawdownDate=D,7,I
T(3,0700101)
@V(7)


</sub>


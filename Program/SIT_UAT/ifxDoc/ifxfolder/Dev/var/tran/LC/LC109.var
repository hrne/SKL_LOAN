!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[LC109]                                 報表簽核"],
["報表序號",#FileNo],
["報表代號",#FileCode],
["報表名稱",#FileItem],
["製表日期",#FileDate],
]
!---------- AP Button AREA ----------

! 必輸欄位


!---------- AP TEXT AREA Variables ----------
#CHAIN=A,1,S
T(4,CHAIN$)
K(NOBUTTON,SAVE)

#NTXBUF=X,255,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@FileNo,#NTXBUF,1,8)
T(1,@FileCode,#NTXBUF,9,80)
T(1,@FileItem,#NTXBUF,89,160)
T(1,@FileDate,#NTXBUF,249,7)

![PDF區域]
#SHOWPDF=X,1,O,ui:pdf

! 報表序號 / PDF 序號
#FileNo=X,8,L
![加載PDF]
PDF(#FileNo)

! 報表代號
#FileCode=x,80,L

! 報表名稱
#FileItem=x,160,L

! 報表日期
#FileDate=D,7,L

</sub>

<sub type="DC" name="p2" layout="cols=1;screen.width=[110,0,0,0];printer.width=[20,20,20,20];order=1;">
[
[""],
[#SHOWPDF],
]
</sub>


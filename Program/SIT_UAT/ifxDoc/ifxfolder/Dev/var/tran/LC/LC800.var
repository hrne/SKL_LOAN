["[LC800] 系統換日"],
[#<->#,"日期類別",#iType,#iTypeX],
[#<->#,"系統日期",#iEntday],
]

!---------- AP Button AREA ----------
#CHAIN=A,1,S
T(4,CHAIN$)
C(2,#CHAIN,$,T(F,@FunCode,0))

#NTXBUF=X,6,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@FunCode,#NTXBUF,1,1)
T(1,@TranNo,#NTXBUF,2,5)
T(2,@FKEY,7)


!---------- AP TEXT AREA Variables ----------

#TypeH=X,100,S
T(3,1:連線日期檔;2:批次日期檔)

#iType=A,1,I
HELP(#TypeH)
@T(3,@iTypeX,)
V(H,#TypeH)

#iTypeX=X,20,L
T(H,#iType,#TypeH)

![功能選項]
#iEntday=D,7,I
E(0,#SYSNDATE)
D(8)
@V(7)A(DATE,0,#iEntday)

</sub>



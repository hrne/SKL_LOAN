!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L5905]                                 覆審案件明細檔查詢"],
["#FdInqFg+查詢條件",#InqFg,#InqFgX],
["#FdYear+資料年月",#Year,"/",#Month],
["#FdConditionCode+條件代碼",#ConditionCode,#ConditionCodeX],
["#FdYearS+資料年份",#YearS,"~",#YearE],
["#FdReChkMonth+覆審月份",#ReChkMonth],
["#FdCustNo+借款人戶號",#CustNo],
["#FdRPTFG+顯示方式",
<include src="COM.icf::RPTFG.scr"/>
],
]

#FdInqFg=X,2,L
T(3,*)
#FdYear=X,2,L
T(3,*)
#FdConditionCode=X,2,L
T(3,*)
#FdYearS=X,2,L
T(3,*)
#FdReChkMonth=X,2,L
#FdCustNo=X,2,L
#FdRPTFG=X,2,L
T(3,*)

!---------- AP Button AREA ----------
![Buttom資料查詢]
!#BUF1=X,42,S

#Submit=X,4,S
K(MODIFYBUTTON,查詢)

!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>

! 條件代碼
#CDCODECondition=x,30,S
T(3,CdCode.ConditionCode)
#ConditionHelp=x,500,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDCODECondition)
R(1,XXR99)
CALL(#HelpProc)
T(2,@ConditionHelp,#HelpDesc1)

!查詢條件
#InqFgHelp=X,100,S
T(3,1:依年月份;2:依年份區間)

!覆審記號(TOTA)
#CheckHelp=X,100,S
T(3,1:免覆審;2:要覆審;3:不覆審)

!追蹤記號(TOTA)
#FollowHelp=X,100,S
T(3,1:免追蹤;2:要追蹤;3:不追蹤)


#CHAIN=A,1,S
T(4,CHAIN$)

#NTXBUF=X,4,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)

#WK=X,1,S
INVOKEJS(SHOW,p1,2,2,0)
INVOKEJS(SHOW,p1,3,3,0)
INVOKEJS(SHOW,p1,4,4,0)
INVOKEJS(SHOW,p1,5,5,0)

#DATE=D,8,S
T(4,DATE$)

#YYY=A,3,S
T(1,#DATE,2,3)
E(0,@Year,#YYY)
E(0,@YearS,#YYY)
E(0,@YearE,#YYY)

#MM=A,2,S
T(1,#DATE,5,2)
E(0,@Month,#MM)
E(0,@ReChkMonth,#MM)

!查詢條件
#InqFg=A,1,I
HELP(#InqFgHelp)
@V(H,#InqFgHelp)

#InqFgX=X,20,L
T(H,#InqFg,#InqFgHelp)

#WKBTN=X,1,S
C(3,#InqFg,1,INVOKEJS(SHOW,p1,4,4,0),INVOKEJS(SHOW,p1,4,4,1))
C(3,#InqFg,1,INVOKEJS(SHOW,p1,5,5,0),INVOKEJS(SHOW,p1,5,5,1))
C(3,#InqFg,2,INVOKEJS(SHOW,p1,2,2,0),INVOKEJS(SHOW,p1,2,2,1))
C(3,#InqFg,2,INVOKEJS(SHOW,p1,3,3,0),INVOKEJS(SHOW,p1,3,3,1))

!資料年月
#Year=A,3,I
C(3,#InqFg,1,$,s)
@V(2,0)

#Month=A,2,I
C(3,#InqFg,1,$,s)
@V(5,01,12)

#YearMonth=A,5,S
T(2,#Year+#Month)

!條件代碼
#ConditionCode=A,2,I
C(3,#InqFg,1,$,s)
HELP(#ConditionHelp)
@V(H,#ConditionHelp)

#ConditionCodeX=X,40,L
C(3,#InqFg,1,$,S)
T(H,#ConditionCode,#ConditionHelp)

!資料年份-S
#YearS=A,3,I
C(3,#InqFg,2,$,s)
@V(2,0)

#YearMonthS=A,5,S
T(2,#YearS+01)

!資料年份-E
#YearE=A,3,I
C(3,#InqFg,2,$,s)
@V(2,0)V(5,#YearS,999)

#YearMonthE=A,5,S
T(2,#YearE+12)

!覆審月份
#ReChkMonth=A,2,I
C(3,#InqFg,2,$,s)
@V(5,00,12)

!借款人戶號
#CustNo=A,7,I

#MRKEY=_,_,S
C(3,#CustNo,0,S,T(2,#CustNo))

!輸出格式     
#RPTFG1=A,1,S
C(3,#InqFg,1,$,S)
C(3,#ConditionCode,06,E(0,1),E(0,0)) 


<include src="COM.icf::RPTFG.dec"/>

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!---------- TXFORM = L5905 ----------
<sub type="FORM" name="L5905">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L5905,$,S)

#SCRTIL=_,_,S
T(2,[L5905] 覆審案件明細檔查詢+_+#ConditionCodeX)


!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

! 資料年月(TOTA)
#OOYearMonth=A,5,S

#OOYearX=A,3,S
T(1,#OOYearMonth,1,3)

#OOMonthX=A,2,S
T(1,#OOYearMonth,4,2)

#OOYearMonthX=X,6,L
T(2,#OOYearX+/+#OOMonthX)

!條件代碼(TOTA)
#OOConditionCode=A,2,S
#OOConditionCodeX=X,40,L
T(H,#OOConditionCode,#ConditionHelp)

!借款人戶號(TOTA)
#OOCustNo=A,7,S

!額度號碼(TOTA)
#OOFacmNo=A,3,S

#OOCustNoX=X,11,L
T(2,#OOCustNo+-+#OOFacmNo)

!戶名(TOTA)
#OOCustName=X,100,L

!覆審記號(TOTA)  
#OOReCheckCode=X,1,S
#OOReCheckCodeX=X,6,L
T(3, )
T(H,#OOReCheckCode,#CheckHelp)

!追蹤記號(TOTA) 
#OOFollowMark=X,1,S
#OOFollowMarkX=X,6,L
T(3, )
T(H,#OOFollowMark,#FollowHelp)

! 覆審年月(TOTA)
#OOReChkYearMonth=A,5,S

#OOReChkYearX=A,3,S
T(1,#OOReChkYearMonth,1,3)

#OOReChkMonthX=A,2,S
T(1,#OOReChkYearMonth,4,2)

#OOReChkYearMonthX=X,6,L
T(2,#OOReChkYearX+/+#OOReChkMonthX)

!撥款日期(TOTA)
#OODrawdownDate=D,7,L

!貸放餘額(TOTA)
#OOLoanBal=m,14.2,S
#OOLoanBal1=m,14,S
E(0,#OOLoanBal)
#OOLoanBalX=X,17,L
IF(#OOLoanBal1==0,S,$)
X(#OOLoanBal1)

!評等(TOTA)
#OOEvaluation=N,2,L

!客戶別(TOTA)
#OOCustTypeItem=X,20,S

!用途別(TOTA)
#OOUsageItem=X,20,S

!地區別(TOTA)
#OOCityItem=X,20,S

!應覆審單位(TOTA)
#OOReChkUnit=X,20,S

!備註(TOTA)
#OORemark=X,120,S

! 登錄的按鈕
#OOBUF2=X,17,S
T(2,#OOYearMonth+#OOConditionCode+#OOCustNo+#OOFacmNo)

#OOBTN2=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L5105;text=維護;ntxbuf=#OOBUF2})

</sub>



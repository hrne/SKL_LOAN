!------------ TXFORM = L5060 -----------
<sub type="FORM" name="L5060">
#SCRTIL=_,_,S
T(3,[L5060] 案件處理清單)

#LOOP=X,1,S
E(0,1)ASGN(LOOP$)
! 上次作業日期
#OOTxDate=D,7,O
! 上次作業項目
#OOTxCode=X,1,O
#OOTxCodeX=X,12,O
C(1,#OOTxCode,S,S,$)
T(H,#OOTxCode,#TxCodeHelp)

! 戶號
#OOCustNo=A,7,O
! 額度
#OOFacmNo=A,3,O
! 姓名
#OOCUSTNM=X,20,O
! 繳息迄日
#OOPrevIntDate=D,7,O
! 逾期數
#OOOverDueterm=m,3,O
! 逾期天數
#OOOvduDays=m,4,O
! 幣別
#OOCurrencyCode=X,3,O
! 本金餘額
#OOPrinBalance=M,14,O
! 催收員
#OOAccCollPsn=X,8,O
! 法務人員
#OOLegalPsn=X,8,O
! 同擔保品額度
#OOClFacmNo=A,10,O
! 催收人員姓名
#OOAccCollPsnX=X,49,O
! 法務人員姓名
#OOLegalPsnX=X,49,O
! 提醒日------用戶號額度抓取
#OOAlertDate=D,7,O
! 是否為指定
#OOIsSpecify=X,1,O
! 戶況
#OOStatus=A,2,O
#OOStatusX=X,22,O
T(H,#OOStatus,#StatusHelp)
! 登錄人員編號+姓名
#EmpNoSpace=X,1,S
T(2, )

#EditAccCollPsn=X,48,S
T(2,#OOAccCollPsn+#EmpNoSpace+#OOAccCollPsnX)
#EditLegalPsn=X,48,S
T(2,#OOLegalPsn+#EmpNoSpace+#OOLegalPsnX)

#AlertDateYYY=X,3,O
T(1,#OOAlertDate,1,3)
#AlertDateMM=X,2,O
T(1,#OOAlertDate,4,2)
#AlertDateDD=X,2,O
T(1,#OOAlertDate,6,2)

#XXAlertDate=X,9,O
T(2,#AlertDateYYY+/+#AlertDateMM+/+#AlertDateDD)

! 1+7+3+1 +1+7+7+5+2+160+6+20=220
#OOBufEntry=X,12,S
T(2,#OOCustNo+#OOFacmNo+#CaseCode)

#OOBufAlert=X,13,S
T(2,#CaseCode+#OOCustNo+#OOFacmNo+1)


#XXCustNo=A,10,S
T(2,#OOCustNo+#OOFacmNo)

#OOClFlag=A,1,S
#OOClFlagA=A,1,S
#OOClFlagB=A,1,S
! [案件] ---同擔保品的主額度(逾期天數最大)
#OOBtnEntry=A,2,O
C(3,#OOClFlag,1,BIND(grid,{ cmd=CHAIN; ntxcd=L5960; ntxbuf=#OOBufEntry}),
                        BIND(grid,cancel))

#OOBtnEntryB=A,2,O
C(3,#OOClFlagA,0,BIND(grid,cancel),
                        BIND(grid,{ cmd=CHAIN; ntxcd=L5960; ntxbuf=#OOBufEntry}))
                        

! [提醒]---若超過提醒日才顯示，否則不顯示按鈕
#OOBtnAlert=A,2,O
! C(3,#OOClFlag,#OOClFacmNo,
!     IF(#TODAY>=#OOAlertDate,
!         BIND(grid,{ cmd=CHAIN; ntxcd=L5965; text=#XXAlertDate; ntxbuf=#OOBufAlert}),
!         BIND(grid,cancel)),
!         BIND(grid,cancel))
C(3,#OOClFlagB,0,BIND(grid,cancel),
                        BIND(grid,{ cmd=CHAIN; ntxcd=L5965; text=#XXAlertDate; ntxbuf=#OOBufAlert}))

#secret-no=X,99,S
T(6A,#OOLegalPsn)

#batch-detail=x,100000,S

#batch-print=A,1,S

!#batch-rows 總筆數
#batch-rows=X,2,S

!#batch-rows-selected 勾選之筆數
#batch-rows-selected=X,2,S

!#batch-rows-ok  傳送成功筆數
#batch-rows-ok=X,2,S

!#batch-rows-error 傳送失敗筆數
#batch-rows-error=X,2,S

!#batch-row-status 該列傳送狀態
#batch-row-status=X,50,S

!#batch-row-txtno 該列傳送序號
#batch-row-txtno=X,14,O

</sub>


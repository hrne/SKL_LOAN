!---------- BASIC LABEL Variables ----------
<sub type="SYS">
</sub>

<sub type="SYS" name="label">
!#TXCD
!T(3,L5712)
</sub>

<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)
</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L5712" layout="cols=3;screen.width=[150,250,150,250];printer.width=[20,80];order=1">
[
["[L5712]                   債務協商作業-暫收解入"],
[#<->#,"戶號",#CustNo],
[#<->#,"身分證字號",#CustId],["戶名",#OOCustName],
[#<->#,"入帳日期",#OOEntryDate],
[#<->#,"交易金額",#OOPayAmt],
[#<->#,"債權機構代號",#FinCode],
[#<->#,"資料檔交易序號",#TxSeq],
]
#SCR=X,100,O

!---------- AP TEXT AREA Variables ----------

#CHAIN=A,1,S
T(4,CHAIN$)
IF(#CHAIN!=1,V(P,本交易為連動交易，不可直接進入),$)

!做完交易自動更新原畫面
#AutoReFresh=A,1,S
C(3,#CHAIN,1,$,S)
E(0,1)
ASGN(CAUTO$)

![業務別] 01:撥款匯款(含暫收退還且非退票) 02:支票繳款 03:債協 09:放款
#SECNO=_,_,S
T(3,03)

#NTXBUF=X,200,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@CustId,#NTXBUF,1,10)
T(1,@CustNo,#NTXBUF,11,7)
T(1,@OOEntryDate,#NTXBUF,18,7)
T(1,@FinCode,#NTXBUF,25,8)
T(1,@TxSeq,#NTXBUF,33,10)
T(1,@OOPayAmt,#NTXBUF,43,14)
T(1,@OOCustName,#NTXBUF,57,100)


#CustId=X,10,L
#CustNo=X,7,L
#OOEntryDate=D,7,L
#FinCode=X,8,L
#TxSeq=X,10,L
#OOPayAmt=m,14,L
#OOCustName=X,100,L


#AGAIN=X,1,S
E(0,1)ASGN(AGAIN$)

!訂正時顯示之欄位
!參考編號
#MRKEY=_,_,S
T(2,#CustId)
![交易金額]
#TXAMT=_,_,S
E(0,#OOPayAmt)

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>
!------------ TXFORM = L5712 -----------
<sub type="FORM" name="L5712">
</sub>




!------ 上行電文 TEXT ------
<sub type="TIM">
#OOEntryDate
#FinCode
#TxSeq
#CustId
#CustNo
#OOPayAmt
#OOCustName

</sub>

!------ 下行電文TOM ------
<sub type="TOM">
<include src="COM.icf::TRCTL.tom"/>
TXFORM=L5712

^
</sub>

!---------- 單據輸出組合 ----------
<sub type="SELECT">
<include src="PRT.icf::UPD01.sel"/>
</sub>

!---------- TXFORM = L5073 ----------
<sub type="FORM" name="L5073">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L5073,$,S)

#SCRTIL=_,_,S
T(3,[L5073] 撥付日期查詢)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------


!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)



#OOYyyyMm=A,5,S

#OOKindCode=A,1,S

#OOKindCodeItem=X,20,S
C(2,#OOKindCode,s,T(2,一分),T(2,二分、調解),T(2,更生),T(2,清算))

#OOExportDate=D,7,S

#OOApprAcDate=D,7,S

#OOBringUpDate=D,7,S

#OOExportMark=A,1,S
#OOApprAcMark=A,1,S
#OOBringUpMark=A,1,S


! 修改的按鈕
#OOFunc2=A,1,S
E(0,2)

#OOBUF2=X,106,S
T(2,#OOFunc2+#OOYyyyMm+#OOKindCode)

#OOBTN2=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L5704;text=修改;ntxbuf=#OOBUF2})
IF (#OOExportMark==1,$,S)
BIND(grid,cancel)


! 刪除的按鈕
#OOFunc4=A,1,S
E(0,4)

#OOBUF4=X,106,S
T(2,#OOFunc4+#OOYyyyMm+#OOKindCode)

#OOBTN4=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L5704;text=刪除;ntxbuf=#OOBUF4})
IF (#OOExportMark==1 && #OOApprAcMark==1 && #OOBringUpMark==1,S,IF (#OOExportMark==0 && #OOApprAcMark==0 && #OOBringUpMark==0,S,$))
BIND(grid,cancel)

</sub>



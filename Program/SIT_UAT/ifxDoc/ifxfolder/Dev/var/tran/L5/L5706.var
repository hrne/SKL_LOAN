!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L5706" layout="cols=1;screen.width=[110,0,0,0];printer.width=[20,20,20,20];order=1">
[
["[L5706]                                 債權比例分攤資料維護(匯入)"],
["#FDBUTTON_FLD+選擇檔案","#BUTTON_FLD",""],
]

#FDBUTTON_FLD=X,2,L

#CHAIN=A,1,S
T(4,CHAIN$)

#RPTFG=A,1,S
E(0,0)

<include src="HELPRIM.icf::HELPRIM.rtn"/>

!---------- AP TEXT AREA Variables ----------
!建議寫在第一個輸入欄位前

! 宣告按鈕物件
! multiple:true : 可上傳多個檔案 (綠底)
! fieldName : 設定上傳欄名返回傳接收變數 (黃底)
! width : 上傳檔名顯示框的寛度 (藍底)
#BUTTON_FLD=X,1,O,ui:button;value:瀏覽;enable:1;tabbable:1;file:true;multiple:true;fieldName:#FILENA;width:600

! 宣告按鈕事件
#BIND_ABA=X,1,S
BIND(#BUTTON_FLD,click)

! 上傳欄名返回傳接收變數 (請定為小x)
#FILENA=x,1000,S

! CustLoanKind
#CustLoanKindCode=x,20,S
T(3,CdCode.CustLoanKind)
#CustLoanKindHelpRim=X,1,S
RESET_RIM(#CustLoanKindHelpRim,XXR99)
S(XXR99,01,#CustLoanKindCode)
R(1,XXR99)
CALL(#HelpProc)
T(2,@CustLoanKindHelp,#HelpDesc1)
#CustLoanKindHelp=x,1024,S

</sub>


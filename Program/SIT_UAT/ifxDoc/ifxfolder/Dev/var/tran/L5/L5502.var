#ORGFG=_,_,S
E(0,0)
</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L5502" layout="cols=3;screen.width=[180,350,180,350];printer.width=[20,80];order=1">
[
["[L5502]                                 房貸專員件數金額資料維護"],
["#<->#,"#FdFunCode+功能選項",#FunCodeX],
["#<->#,"#FdCustNo+借款人戶號",#CustNo,"-",#FacmNo],
["#<->#,"#FdCustNm+戶名",#CustNm],
["#<->#,"#FdIntroducer+介紹人",#Introducer,#IntroducerName],
["#<->#,"#FdBsOfficer+房貸專員",#BsOfficer,#BsOfficerName],
["#<->#,"#FdPerfAmt+業績金額",#PerfAmt],
["#<->#,"#FdAdjPerfAmt+額度業績金額調整",#AdjPerfAmtSign,#AdjPerfAmt],
["#<->#,"#FdAdjPerfCnt+額度業績件數調整",#AdjPerfCntSign,#AdjPerfCnt],
["@","※僅可異動本月資料！"],["工作年月",#WorkMonth],
]
!["件數",#PerfCnt],

#FdFunCode=X,2,L
#FdCustNo=X,2,L
#FdCustNm=X,2,L
#FdIntroducer=X,2,L
#FdBsOfficer=X,2,L
#FdPerfAmt=X,2,L
#FdAdjPerfAmt=X,2,L
C(3,#FunCode,2,T(3,*),T(3, ))
#FdAdjPerfCnt=X,2,L
C(3,#FunCode,2,T(3,*),T(3, ))

<include src="HELPRIM.icf::HELPRIM.rtn"/>

!不可重新交易
#AGAIN=X,1,S
E(0,1)ASGN(AGAIN$)

!FuncCode1(1:新增;2:修改;4:刪除;5:查詢)
#CODEFuncCode1=x,20,S                  
T(3,CdCode.FuncCode1)                  
#FuncCode1Help=x,50,S
!
#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CODEFuncCode1)
R(1,XXR99)
CALL(#HelpProc)
T(2,@FuncCode1Help,#HelpDesc1)

![Buttom資料查詢]
!#BtnEmpno=X,15,I,ui:button;value:員工資料查詢;enable:1;tabbable:0;title:L190A 員工資料檔查詢;

#CHAIN=A,1,S
T(4,CHAIN$) 
IF(#CHAIN==0,V(P,本交易為連動交易，請由L5052連動進入),$)

!#BufEmpNo=X,7,S
!T(3,      Y)
!#BindEmpNo=X,1,S
!BIND(#BtnEmpno,click, {cmd=CHAIN; ntxcd=L190A; ntxbuf=#BufEmpNo;ntxbuf5=type<-1:TempAdjBsOfficer<-OOEmployeeNo:AdjBsOfficer<-OOEmployeeNo:AdjBsOfficerName<-OOFullname:L5r28FullName<-OOFullname})

! 連動L5052
#NTXBUF=X,21,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@FunCode,#NTXBUF,1,1)
T(1,@CustNo,#NTXBUF,2,7)
T(1,@FacmNo,#NTXBUF,9,3)
T(1,@WorkMonth,#NTXBUF,12,5)

#AdjLogNo=A,8,S

#Tf=x,1,S
C(3,#FunCode,4,$,S)
T(F,@AdjPerfAmtSign,0)
T(F,@AdjPerfAmt,0)
T(F,@AdjPerfCntSign,0)
T(F,@AdjPerfCnt,0)

! FuncCode1(1:新增;2:修改;4:刪除;5:查詢)
#FunCode=A,1,S
!C(2,#CHAIN,$,s)
!HELP(#FuncCode1Help)
!@V(H,#FuncCode1Help)

![維護記號名稱]
#FunCodeX=X,4,L
!C(3,#FunCode,2,T(3,調整),T(H,#FunCode,#FuncCode1Help))
T(H,#FunCode,#FuncCode1Help)
K(MODIFYBUTTON,#FunCodeX)
C(3,#FunCode,5,K(NOBUTTON,CLOSEY),$)

<include src="ROM.icf::L5R29.dec"/>
#RimCustNo=A,7,S
#RimFacmNo=A,3,S
#RimBormNo=A,3,S
#RimPerfDate=D,7,S

#RimL5r29=X,1,S
IF(#CHAIN==1,$,S)
RESET_RIM(#RimL5r29,L5R29)
S(L5R29,1,#CustNo,#FacmNo,#WorkMonth)
R(1,L5R29)
E(0,@CustNo,#L5r29CustNo)
E(0,@FacmNo,#L5r29FacmNo)
T(2,@CustNm,#L5r29CustNm)
E(0,@WorkMonth,#L5r29WorkMonth)
T(2,@Introducer,#L5r29Introducer)
T(2,@IntroducerName,#L5r29IntroducerName)
T(2,@BsOfficer,#L5r29BsOfficer)
T(2,@BsOfficerName,#L5r29BsOfficerName)
E(0,@PerfAmt,#L5r29PerfAmt)
T(2,@AdjPerfCntSign,#L5r29AdjPerfCntSign)
E(0,@AdjPerfCnt,#L5r29AdjPerfCnt)
T(2,@AdjPerfAmtSign,#L5r29AdjPerfAmtSign)
E(0,@AdjPerfAmt,#L5r29AdjPerfAmt)
E(0,@AdjLogNo,#L5r29AdjLogNo)


! 借款人戶號
#CustNo=A,7,L
! C(2,#FUNCTIONCD,S,$,s,s,s,s)

! 額度編號
#FacmNo=A,3,L
! C(2,#FUNCTIONCD,S,$,s,s,s,s)

! 撥款序號
#BormNo=A,3,L
! C(2,#FUNCTIONCD,S,$,s,s,s,s)

#MRKEY=_,_,S
T(2,#CustNo+-+#FacmNo)

!工作年月
#WorkMonth=X,5,L

! 戶名
#CustNm=X,2.100,L
! C(2,#FUNCTIONCD,S,$,s,s,s,s)

!<瀏覽>---戶號
! #BUFA=X,6,S
! T(2,#CustNo)
! #BINDA=X,1,S
! BIND(#BTNA,click, {cmd=CHAIN; ntxcd=L1XXX; ntxbuf=#BUFA})

! 介紹人NM
#Introducer=X,6,L
#IntroducerName=X,50,L
! C(2,#FUNCTIONCD,S,$,s,s,s,s)

! 房貸專員NM
#BsOfficer=X,6,L
#BsOfficerName=X,50,L
! C(2,#FUNCTIONCD,S,$,s,s,s,s)

#AdjBsOfficer=X,6,S
@IF(#AdjBsOfficer=="",T(2,@AdjBsOfficerName,) T(2,@L5r28FullName,),$)
#TempAdjBsOfficer=X,6,S

<include src="ROM.icf::L5R28.dec"/>
#RimEmployeeNo=X,6,S
#RimErrorSkip=X,1,S

!#SendL5r28Introducer=X,1,S
!IF(#TempAdjBsOfficer!="" && #TempAdjBsOfficer==#AdjBsOfficer,S,$)
!C(4,#AdjBsOfficer,S,$)
!T(2,@RimEmployeeNo,#AdjBsOfficer)
!T(2,@RimErrorSkip,Y)
!RESET_RIM(#SendL5r28Introducer,L5R28)
!S(L5R28,1,#RimErrorSkip,#RimEmployeeNo)
!R(1,L5R28)

#AdjBsOfficerName=X,50,L
T(2,@AdjBsOfficerName,#L5r28FullName)

!#CheckIsEmp=X,1,S
!IF(#AdjBsOfficer!="" ,$,S)
!IF(#AdjBsOfficerName=="" ,$,S)
!V(P,此員工編號不存在)

#AdjHelp=x,1024,S
T(3, :空白不調整;+:增加;-:增少)

! 業績金額
#PerfAmt=+m,15,L

#AdjPerfAmtSign=X,1,I
C(3,#FunCode,2,$,s)
HELP(#AdjHelp)
@
V(H,#AdjHelp)

! 調整後業績金額
#AdjPerfAmt=m,15,I
C(3,#FunCode,2,$,s)
C(4,#AdjPerfAmtSign,S,$)
@
V(2,0)

! 件數
#PerfCnt=+m,2.1,L

#AdjPerfCntSign=X,1,I
C(3,#FunCode,2,$,s)
HELP(#AdjHelp)
@
V(H,#AdjHelp)

! 調整後件數
#AdjPerfCnt=m,2.1,I
C(3,#FunCode,2,$,s)
C(4,#AdjPerfCntSign,S,$)
@
V(2,0)

</sub>


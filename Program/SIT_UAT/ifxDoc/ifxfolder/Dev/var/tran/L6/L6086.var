<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!---------- TXFORM = L6086 ----------
<sub type="FORM" name="L6086">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6086,$,S)

#SCRTIL=_,_,S
T(3,[L6086] 單位代號)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

! 單位代號(TOTA)

<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD2.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)
!HIDE(#BTN1)

!#button=x,100,S
!T(2,{cmd=CHAIN; ntxcd=L6756; ntxbuf=1; ntxbuf5=type})ASGN(BTN_NEW1$)
!K(MODIFYBUTTON,1,新增)

!#FirstIn=A,1,S
!E(0,1)

</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L6086]                                 單位代號查詢"],
["#FdUnitCode+單位代號",#UnitCode],
["#FDDistCode+區部代號",#DistCode],
["#FdDeptCode+部室代號",#DeptCode],
!["#FdRPTFG+顯示方式",
!<include src="COM.icf::RPTFG.scr"/>
!],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
!#BTN1=X,15,I,ui:button;value:新增單位代號;enable:1;tabbable:0;title:L6756 分公司資料維護
!#BUF1=X,42,S

#Submit=X,4,S
K(MODIFYBUTTON,查詢)
!---------- AP TEXT AREA Variables ----------

#FdUnitCode=X,2,L
#FdDeptCode=X,2,L
#FDDistCode=X,2,L
#FdRPTFG=X,2,L
T(3,*)

#RPTFG=A,1,S
E(0,0)

#CHAIN=A,1,S
T(4,CHAIN$) 

#NTXBUF=X,18,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)

#NTXBUF5=X,10,S
C(3,#CHAIN,1,T(4,NTXBUF5$),S)

#RPTFG2=A,1,S
C(4,#NTXBUF5,T(3,2),T(3,0))

!單位代號
#UnitCode=X,6,I
C(3,#CHAIN,1,T(1,#NTXBUF,1,6),$)
!C(3,#CHAIN,1,s,$)

!區部代號
#DistCode=X,6,I
C(3,#CHAIN,1,T(1,#NTXBUF,7,6),$)
!C(3,#CHAIN,1,s,$)
C(4,#UnitCode,C(4,#DeptCode,$,S),S)

!部室代號
#DeptCode=X,6,I
C(3,#CHAIN,1,T(1,#NTXBUF,13,6),$)
!C(3,#CHAIN,1,s,$)
C(4,#UnitCode,$,S)




!#BUF1=X,7,S
!E(0,@FirstIn,0)

!#CHAINXMT=X,1,S
!C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

!<include src="COM.icf::RPTFG.dec"/>

</sub>


!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L6043]                                 權限群組查詢"],
["#Fd+使用單位",#BranchNo],
["#Fd+權限群組",#AuthNo],
]
!---------- AP Button AREA ----------

<include src="HELPRIM.icf::HELPRIM.rtn"/>

!---------- AP TEXT AREA Variables ----------
#CHAIN=A,1,S
!T(4,CHAIN$) 
E(0,1)

#NTXBUF=X,4,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)

#Fd=X,2,L
T(3, )

!單位別
#Help1=X,10,S
T(3,CDBR)

#BrHelp=X,1024,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#Help1)
R(1,XXR99)
CALL(#HelpProc)
T(2,@BrHelp,#HelpDesc1)

!YesNoHelp
#StatusHelp=X,50,S

!#RPTFG=A,1,S
!E(0,0)

#BranchNo=X,4,I
HELP(#BrHelp)
@C(4,#BranchNo,S,$)V(H,#BrHelp)

!權限群組
#AuthNo=X,6,I

#RPTFG=A,1,S
E(0,0)

!<include src="COM.icf::RPTFG.dec"/>

#RPTFG=_,_,S

#TXMT=A,1,S
C(3,#CHAIN,0,$,E(0,@CHAIN,0)XMT())

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!---------- TXFORM = L6043 ----------
<sub type="FORM" name="L6043">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6043,$,S)

#SCRTIL=_,_,S
T(3,[L6043] 權限群組)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

! 交易代碼
#OAuthNo=X,6,L

! 交易名稱
#OAuthItem=X,20,L


! 交易說明
#ODesc=X,80,L

! 使用單位
#OBranchNo=X,4,L
#OBranchItem=x,40,L

#OBranchItemX=x,45,L
T(2,#OBranchNo+ +#OBranchItem)
!
#OLevelFg=X,1,S

#OLevelFgX=X,4,S
T(H,#OLevelFg,#LevelHelp)

! 修改的按鈕
#OFunc2=A,1,S
E(0,2)

#OBUF2=X,65,S
T(2,#OFunc2+#OAuthNo)

#OBTN2=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6403;text=修改;ntxbuf=#OBUF2})

! 刪除的按鈕
#OFunc4=A,1,S
E(0,4)

#OBUF4=X,65,S
T(2,#OFunc4+#OAuthNo)

#OBTN4=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6403;text=刪除;ntxbuf=#OBUF4})

</sub>



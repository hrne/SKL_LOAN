<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD2.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)
!HIDE(#BTN1)

!#FirstIn=A,1,S
!E(0,1)

#Tmp1=X,40,S
#TmpRate=m,2.4,S
#TmpDate=D,7,S
#BUF=X,136,S
T(2,100+#Tmp1+#TmpRate+#TmpDate)

!#button=x,100,S
!T(2,{cmd=CHAIN; ntxcd=L6032; ntxbuf=0; ntxbuf5=type})ASGN(BTN_NEW1$)
!K(MODIFYBUTTON,1,指標利率查詢)
!K(MODIFYBUTTON,查詢)

#button1=x,100,S
T(2,{cmd=CHAIN; ntxcd=L6301; ntxbuf=#BUF; ntxbuf5=type})ASGN(BTN_NEW2$)
K(MODIFYBUTTON,2,新增指標利率種類)


</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L6031]                                 指標利率種類查詢"],
!["#FdCurrencyCode+幣別",#CurrencyCode],
["#FdBaseRateCode+指標利率種類",#BaseRateCode,#BaseRateItem],
["#FdRPTFG+顯示方式",
<include src="COM.icf::RPTFG.scr"/>
],
]
!---------- AP Button AREA ----------

!---------- AP TEXT AREA Variables ----------

#FdCurrencyCode=X,2,L
#FdBaseRateCode=X,2,L
T(3,*)
#FdRPTFG=X,2,L
T(3,*)

#CHAIN=A,1,S
T(4,CHAIN$)

#NTXBUF=X,27,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)


<include src="HELPRIM.icf::HELPRIM.rtn"/>
#ApHelp=x,1024,S
RESET_RIM(#ApHelp,XXR99)
T(3,CdCode.BaseRate)
S(XXR99,01,#ApHelp)
R(1,XXR99)
CALL(#HelpProc)
T(2,#HelpDesc1)
T(2,@ApHelp,00:全部;+#HelpDesc1)

#FuncCode=A,1,S
E(0,1)
IF(#CHAIN==1,T(1,#NTXBUF,1,1),s)
!@V(1,1,2,3)

![幣別]
#CurrencyCode=X,3,L
T(3,TWD)

![指標利率種類]
#BaseRateCode=A,2,I
C(3,#CHAIN,1,T(1,#NTXBUF,1,4),$)
C(3,#CHAIN,1,s,$)
T(2,@BaseRateItem, )
HELP(#ApHelp)

#BaseRateItem=X,40,L
T(H,#BaseRateCode,#ApHelp)

#BUF1=X,3,S
!C(4,#BaseRateCode,HIDE(#BTN1),SHOW(#BTN1))
!T(2,1+#BaseRateCode)
!E(0,@FirstIn,0)


#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

<include src="COM.icf::RPTFG.dec"/>

</sub>
!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!---------- TXFORM = L6031 ----------
<sub type="FORM" name="L6031">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6031,$,S)

#SCRTIL=_,_,S
T(3,[L6031] 指標利率種類)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------

! 幣別
#OCurrencyCode=X,3,L
T(2,#CurrencyCode)

! 指標利率種類
#OBaseRateCode=A,2,L
T(2,#BaseRateCode)

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)
         
 
       

! 指標利率種類
#OOBaseRateCode=X,2,L

! 指標利率種類名稱
#OOBaseRateItem=X,40,L
!T(H,#OOBaseRateCode,#ApHelp) 

!生效記號
#OOEffectFlag=N,1,S
#OOEffectFlagX=X,6,L
C(2,#OOEffectFlag,T(3,),
                  T(3,),
                  T(3,未放行))


! 修改的按鈕
#OOBUF_2=X,136,S
T(2,2+#OOBaseRateCode+#OOBaseRateItem)

#OOBTN_2=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6301;text=修改;ntxbuf=#OOBUF_2})

! 刪除的按鈕
#OOBUF_4=X,136,S
T(2,4+#OOBaseRateCode+#OOBaseRateItem)

#OOBTN_4=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6301;text=刪除;ntxbuf=#OOBUF_4})

! 查詢的按鈕
#OOBUF_5=X,10,S
T(2,#OOBaseRateCode)

#OOBTN_5=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6032;text=查詢指標利率;ntxbuf=#OOBUF_5})


</sub>



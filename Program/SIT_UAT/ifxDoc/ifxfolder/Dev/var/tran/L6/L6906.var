</sub>

<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD2.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)

</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L6906]                                 會計分錄查詢"],
["#FdSearch+查詢條件","(戶號、經辦、整批批號、交易代號，需擇一輸入"],
["#FdAcDate+會計日期",#AcDate],
["#FdBranchNo+單位別",#BranchNo],
["#FdCustNo+．戶號",#CustNo],
["#FdTitaTlrNo+．經辦",#TitaTlrNo],
["#FdTitaBatchNo+．整批批號",#TitaBatchNo],
["#FdTitaTxCd+．交易代號",#TitaTxCd,],
["#FdRelSeqNo+登放序號",#RelTlrNo,"-",#RelSeqNo]
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
!#BTN1=X,15,I,ui:button;value:瀏覽;enable:0;tabbable:0
!#BTN2=X,15,I,ui:button;value:瀏覽;enable:0;tabbable:0
!#BTN3=X,15,I,ui:button;value:瀏覽;enable:0;tabbable:0
#BUF1=X,42,S
!ENABLE(#BTN1)ENABLE(#BTN2)ENABLE(#BTN3)

#Submit=X,4,S
K(MODIFYBUTTON,查詢)

!---------- AP TEXT AREA Variables ----------
#CHAIN=A,1,S
T(4,CHAIN$)

#NTXBUF=X,25,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)

#CHAIN_RelDy=X,7,S
T(1,#NTXBUF,1,7)

#CHAIN_RelBranchNo=X,4,S
T(1,#NTXBUF,8,4)

#CHAIN_RelTlrNo=X,6,S
T(1,#NTXBUF,12,6)

#CHAIN_RelSeqNo=X,8,S
T(1,#NTXBUF,18,8)

C(3,#CHAIN,1,T(2,@AcDate,#CHAIN_RelDy),)
C(3,#CHAIN,1,T(2,@TitaTlrNo,#CHAIN_RelTlrNo),)
C(3,#CHAIN,1,T(2,@RelTlrNo,#CHAIN_RelTlrNo),)
C(3,#CHAIN,1,T(2,@RelSeqNo,#CHAIN_RelSeqNo),)

! 必輸欄位
#FdBranchNo=X,2,L
#FdCustNo=X,2,L
#FdTitaTlrNo=X,2,L
#FdTitaBatchNo=X,2,L
#FdRelSeqNo=X,2,L
#FdTitaTxCd=X,2,L


! 永遠為必輸欄位
#FdAcDate=X,2,L
T(3,*)

#FdSearch=X,2,L
T(3,*)

#WKHIDE=X,1,S
INVOKEJS(SHOW,p1,1)


#RPTFG=A,1,S
E(0,0)

!會計日期 必須輸入
#DATE=D,8,S
T(4,DATE$)

#DT=D,7,S
T(1,#DATE,2,7)

#AcDate=D,7,I
C(3,#CHAIN,1,s,$)
T(2,#DT)
@V(7)A(4,0,#AcDate)

!單位別
#BranchNo=A,4,L
T(2,#ACBRNO)

![幣別]
#CurrencyCode=X,3,L
T(3,TWD)

!戶號
#CustNo=A,7,I
!INVOKEJS(SHOW,p1,3,5,1)
C(3,#CHAIN,1,s,$)

#WKBTN1=X,1,S
!C(3,#CustNo,0,INVOKEJS(SHOW,p1,3,3,0),INVOKEJS(SHOW,p1,4,5,0))

#MRKEY=_,_,S
C(3,#CustNo,0,S,T(2,#CustNo))

!經辦
#TitaTlrNo=X,6,I
C(3,#CHAIN,1,s,$)
!INVOKEJS(SHOW,p1,4,5,1)
C(3,#CustNo,0,$,S)

#WKBTN2=X,1,S
!C(3,#TitaTlrNo,0,INVOKEJS(SHOW,p1,4,4,0),INVOKEJS(SHOW,p1,5,5,0))

!整批批號
#TitaBatchNo=X,6,I
!INVOKEJS(SHOW,p1,5,5,1)
C(3,#CustNo,0,$,S)
C(3,#TitaTlrNo,0,$,S)

#WKBTN3=X,1,S
!C(3,#TitaBatchNo,0,INVOKEJS(SHOW,p1,5,5,0),INVOKEJS(SHOW,p1,6,6,0))

!交易代號
#TitaTxCd=X,5,I
!INVOKEJS(SHOW,p1,6,6,1)
C(3,#CustNo,0,$,S)
C(3,#TitaTlrNo,0,$,S)
C(3,#TitaBatchNo,0,$,S)
@C(3,#CustNo,0,$,s)
C(4,#TitaTlrNo,$,s)
C(4,#TitaBatchNo,$,s)
C(4,#TitaTxCd,V(P,需擇一輸入查詢條件),$)


#WKBTN4=X,1,S
!C(3,#TitaTxCd,0,INVOKEJS(SHOW,p1,6,6,0),)


!登放序號
#RelTlrNo=X,6,I
C(3,#CHAIN,1,s,$)

#RelSeqNo=A,8,I
C(3,#CHAIN,1,s,$)

#RelTxseq=X,18,S
C(1,#RelSeqNo,S,S,$)
T(2,#BranchNo+#RelTlrNo+#RelSeqNo)  

#SendXmt=X,1,S
C(3,#CHAIN,1,XMT(),$)

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!---------- TXFORM = L6906 ----------
<sub type="FORM" name="L6906">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6906,$,S)

#SCRTIL=_,_,S
T(3,[L6906] 會計分錄)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

!交易序號
#OORelTxseq=X,18,S


!戶號
#OOCustNo=A,7,S
!額度編號
#OOFacmNo=A,3,S
!撥款序號
#OOBormNo=A,3,S
#OOCustNoX=X,15,S
T(2,#OOCustNo+-+#OOFacmNo+-+#OOBormNo)

!科子細目
#OOAcNoCode=X,11,S
#OOAcSubCode=X,5,S
#OOAcDtlCode=X,2,S
#OOAcNoCodeX=X,20,S
T(2,#OOAcNoCode+-+#OOAcSubCode+-+#OOAcDtlCode)

!科子細目名稱
#OOAcNoItem=X,80,S

!借方金額
#OODbAmt=m,14.2,S
#OODbAmt1=m,14,S
E(0,#OODbAmt)
#OODbAmtX=X,17,S
IF(#OODbAmt1==0,S,$)
X(#OODbAmt1)

!貸方金額
#OOCrAmt=m,14.2,S
#OOCrAmt1=m,14,S
E(0,#OOCrAmt)
#OOCrAmtX=X,17,S
IF(#OOCrAmt1==0,S,$)
X(#OOCrAmt1)

!整批批號
#OOTitaBatchNo=X,6,S

!摘要
#OOSlipNote=X,160,S

!交易代號
#OOTitaTxCd=X,5,S
#OOTranItem=X,80,S
#OOTitaTxCdX=X,86,S
T(2,#OOTitaTxCd+ +#OOTranItem)

!經辦
#OOTitaTlrNo=X,6,S
#OOTlrItem=X,20,S
#OOTitaTlrNoX=X,27,S
T(2,#OOTitaTlrNo+ +#OOTlrItem)

!主管
#OOTitaSupNo=X,6,S
#OOSupItem=X,20,S
#OOTitaSupNoX=X,27,S
T(2,#OOTitaSupNo+ +#OOSupItem)

!分錄清單
#OOBUF5=X,32,S
T(2,#AcDate+0000000+#OORelTxseq)

#OOBTN5=X,4,S
BIND(grid,{ cmd=CHAIN; ntxcd=L6901;text=分錄清單;ntxbuf=#OOBUF5})

</sub>



----- BASIC LABEL Variables ----------
<sub type="SYS">
</sub>

<sub type="SYS" name="label">

!#TXCD
!T(3,L6933)

![資料表頭(HEADER)長度]
#INQHD=n,3,S
E(0,73)ASGN(INQHD$)

![每筆資料明細(OCCURS)長度]
#INQLEN=n,3,S
E(0,217)ASGN(INQLEN$)

![多筆查詢之一個畫面有N筆資料]
#INQREC=n,2,S
E(0,20)ASGN(INQREC$)

![畫面顯示的明細間的高度]
#LOOPH=n,3,S
E(0,1)ASGN(LOOPHEIGHT$)

![每張印錄單列印的筆數]
#INQPRT=n,2,S
E(0,40)ASGN(INQPRT$)

</sub>

<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)

</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L6933]                                 資料變更明細查詢"],
["#FdTranNo+交易代號",#TranNo,#TranNo2],
["#FDMrKey+參考編號",#MrKey],

]
!---------- AP Button AREA ----------

! 必輸欄位
#FdTranNo=X,2,L
T(3, )
#FDMrKey=X,2,L
T(3, )

#Submit=A,1,S
K(MODIFYBUTTON,查詢)

!---------- AP TEXT AREA Variables ----------
#CHAIN=A,1,S
T(4,CHAIN$)
IF(#CHAIN==0,V(P,本交易為連動交易，請由入口交易連動進入),$)

#NTXBUF=x,40,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@TranNo,#NTXBUF,1,5)
T(1,@MrKey,#NTXBUF,6,20)
T(1,@TranNo2,#NTXBUF,26,5)

#TranNo=x,5,L

#TranNo2=x,5,L

#MrKey=x,20,L

#RPTFG=A,1,S
E(0,0)


#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!---------- TXFORM = L6933 ----------
<sub type="FORM" name="L6933">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6933,$,S)

#SCRTIL=_,_,S
T(3,[L6933] 資料變更交易)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------

#OTranName=X,40,L
#OCustNo=A,7,L
#OFacmNo=A,3,L
#OBormNo=A,3,L
#OMrKey=x,20,L

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

#OReason=x,120,S
#OLastUpdate=x,40,S
#OLastEmp=x,30,S
!會計日期
#OTxDate=A,7,S
!交易序號
#OTxSeq=X,18,S
!明細序號
#OTxSno=A,2,S

#BufA=X,50,S
T(2,#OTxDate+#OTxSeq+#OTxSno)

#OOBtnA=A,1,O
BIND(grid,{ cmd=CHAIN; ntxcd=L6934; ntxbuf=#BufA})

</sub>


!------ 上行電文 ------
<sub type="TIM">
#TranNo
#TranNo2
#MrKey
#END
</sub>


!------ 下行電文TOM ------
<sub type="TOM">
<include src="COM.icf::TRCTL.tom"/>
TXFORM=L6933
#OTranName
#OCustNo
#OFacmNo
#OBormNo
#OMrKey
#OReason
#OLastUpdate
#OLastEmp
#OTxDate
#OTxSeq
#OTxSno
^
</sub>

!--------- OUTPUT畫面 --------
!---------- 單據輸出組合 ----------
<sub type="PART" name="L6933" layout="cols=1;screen.width=[100,1000];printer.width=[20,20];">
[
["[L6933]                                 資料變更明細查詢"],
["交易名稱",#OTranName],
["戶@@@@號",#OCustNo,"-",#OFacmNo,"-",#OBormNo],
["參考編號",#OMrKey],
[""],
]
</sub>

<sub type="SELECT">

#RPTFG=0,QUERY.GRID,L6933
#any={detail:true, header:'L6933.part', caption:'', width:1000, rowNum:20, rowList:[20,40,60]}

#OOBtnA=修改內容
{search:false,formatter:'cust:bind-button:查詢',align:'center',canExport:false,width:70}

#OReason=修改原因
{width:300,align:'left'}
#OLastUpdate=修改日期
{width:140,align:'left'}
#OLastEmp=修改人員
{width:140,align:'left'}
^
</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L6024]                                 錯誤代碼查詢"],
["#FdErrCode+錯誤代碼",#ErrCode],
]
!---------- AP Button AREA ----------



!---------- AP TEXT AREA Variables ----------
#FdErrCode=X,2,L


#CHAIN=A,1,S
T(4,CHAIN$) 

#NTXBUF=X,4,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)

#NTXBUF5=X,10,S



!錯誤代碼
#ErrCode=X,8,I
@V(NL)

#RPTFG=A,1,S
E(0,0)

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

!<include src="COM.icf::RPTFG.dec"/>

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!---------- TXFORM = L6024 ----------
<sub type="FORM" name="L6024">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6024,$,S)

#SCRTIL=_,_,S
T(3,[L6024] 錯誤代碼查詢)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

! 錯誤代碼
#OOErrCode=X,8,L

! 錯誤訊息
#OOErrContent=X,125,L


! 修改的按鈕
#OOFunc2=A,1,S
E(0,2)

#OOBUF2=X,10,S
T(2,#OOFunc2+#OOErrCode)

#OOBTN2=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6204;text=修改;ntxbuf=#OOBUF2})

! 刪除的按鈕
#OOFunc4=A,1,S
E(0,4)

#OOBUF4=X,10,S
T(2,#OOFunc4+#OOErrCode)

#OOBTN4=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6204;text=刪除;ntxbuf=#OOBUF4})

</sub>


!------ 上行電文 ------
<sub type="TIM">
#ErrCode
#END
</sub>


!------ 下行電文TOM ------
<sub type="TOM">
TXFORM=L6024
#OOErrCode
#OOErrContent
^
<include src="COM.icf::TRCTL.tom"/>
</sub>


<sub type="FORM" name="L6908">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6908,$,S)

#SCRTIL=_,_,S
T(3,[L6908] 銷帳歷史明細)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

! 起/銷帳
#OOClsFlag=A,1,S
#OOClsFlagX=X,4,L
SWITCH(#OOClsFlag,0,T(3,起帳),
                  1,T(3,銷帳))

! 銷帳編號
#OORvNo=x,30,L

! 交易日期
#OOAcDate=D,7,L

! 交易金額
#OORvAmt=+m,14.2,S
#OORvAmt1=+m,14,S
E(0,#OORvAmt)
#OORvAmtX=X,17,L
IF(#OORvAmt1==0,S,$)
X(#OORvAmt1)

!起帳金額
#OORvAmt00=X,18,L
IF(#OORvAmt1==0,S,$)
C(3,#OOClsFlag,0,X(#OORvAmt1),E(0,0))

!銷帳金額
#OORvAmt11=X,18,L
IF(#OORvAmt1==0,S,$)
C(3,#OOClsFlag,1,X(#OORvAmt1),E(0,0))

!登放序號
#OOTitaTlrNo=X,6,S
#OOTitaTxtNo=A,8,S
#OORelNo=X,15,L
T(2,#OOTitaTlrNo+-+#OOTitaTxtNo)

!交易代號
#OOTitaTxCd=X,5,S
#OOTranItem=X,80,S
#OOTitaTxCdX=X,86,S
T(2,#OOTitaTxCd+ +#OOTranItem)

!摘要
#OOSlipNote=X,160,L

</sub>



!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L6083]                                 房貸專員所屬業務部室查詢"],
["@員工編號",#EmployeeNo],
!["@顯示方式",
!<include src="COM.icf::RPTFG.scr"/>
!],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
!#BTN1=X,15,I,ui:button;value:新增員工編號;enable:1;tabbable:0;title:L6753 房貸專員所屬業務部室維護
!#BUF1=X,42,S

!---------- AP TEXT AREA Variables ----------
#CHAIN=A,1,S
T(4,CHAIN$) 

#NTXBUF=X,4,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)

#RPTFG=A,1,S
E(0,0)

!員工編號
#EmployeeNo=X,6,I
C(3,#CHAIN,1,T(1,#NTXBUF,1,4),$)
C(3,#CHAIN,1,s,$)
@C(4,#EmployeeNo,T(2,000000),$)
V(7)



#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

#BUF1=X,7,S
!C(4,#EmployeeNo,HIDE(#BTN1),SHOW(#BTN1))
!T(2,1+#EmployeeNo)


!#BIND1=X,1,S
!BIND(#BTN1,click, {cmd=CHAIN; ntxcd=L6753; ntxbuf=#BUF1})

!<include src="COM.icf::RPTFG.dec"/>

</sub>


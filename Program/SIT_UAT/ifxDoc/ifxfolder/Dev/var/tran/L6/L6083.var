!---------- TXFORM = L6083 ----------
<sub type="FORM" name="L6083">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6083,$,S)

#SCRTIL=_,_,S
T(3,[L6083] 房貸專員所屬業務部室)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

! 員工編號(TOTA)
#OOEmployeeNo=X,6,L

! 員工姓名(TOTA)
#OOFullname=X,40,L

! 部室代號(TOTA)
#OODeptCode=X,6,L

! 部室名稱(TOTA)
#OODeptItem=X,40,L

! 修改的按鈕
#OOFunc2=A,1,S
E(0,2)

#OOBUF2=X,93,S
T(2,#OOFunc2+#OOEmployeeNo+#OOFullname+#OODeptCode+#OODeptItem)

#OOBTN2=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6753;text=修改;ntxbuf=#OOBUF2})

! 刪除的按鈕
#OOFunc4=A,1,S
E(0,4)

#OOBUF4=X,93,S
T(2,#OOFunc4+#OOEmployeeNo+#OOFullname+#OODeptCode+#OODeptItem)

#OOBTN4=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6753;text=刪除;ntxbuf=#OOBUF4})

</sub>


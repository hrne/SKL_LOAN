! 轉催收日
#TransCollDate=D,7,S
T(2,#DT)

! 查詢範圍 功能: 9.查詢時須輸入，其它隱藏
#SelectCode=A,1,S
C(3,#CHAIN,1,s,$)
! HELP(1: 昨日留存;2: 本日新增;3: 全部;4: 本日處理;5: 本日刪除;6:保留;7: 未處理;9: 未處理-撥款)
! @V(1,1,2,3,4,5,6,7,9)

! #SelectCodeX=X,16,L
! C(2,#SelectCode,T(3,),
!                T(3,昨日留存),
!                T(3,本日新增),
!                T(3,全部),
!                T(3,本日處理),
!                T(3,本日刪除),
!                T(3,保留),
!                T(3,未處理),
!                T(3,),
!                T(3,未處理-撥款),
!  )


#RPTFG=A,1,S
C(2,#SelectCode,
S,
E(0,4),
E(0,4),
E(0,4),
E(0,5),
E(0,6),
E(0,2),
E(0,3),
S,
E(0,7),
)


#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

</sub>


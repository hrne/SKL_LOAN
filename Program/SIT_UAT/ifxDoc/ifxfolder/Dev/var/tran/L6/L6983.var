!---------- TXFORM = L6983 ----------
<sub type="FORM" name="L6983">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6983,$,S)

#SCRTIL=_,_,S
T(3,[L6983] 法務費轉列催收作業)

!HEAD戶號
#OCUSTNO=A,7,L
T(2,#CustNo)

!HEAD轉催收日
#OTR_OV_DT=D,7,L
T(2,#TransCollDate)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------

#O_FUNCD=X,1,L

#O_SELECT_CD=X,1,L
! IF(#FUNCD==9,T(2,#SelectCode),T(3,))


!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

! 狀態
#OOProcStatus=A,1,L

#OOProcStatusX=X,16,L
SWITCH(#OOProcStatus,0,T(3,0.未處理),
                          1,T(3,1.已保留),
                          2,T(3,2.已處理),
                          3,T(3,3.已刪除),
)

! 戶號
#OOCustNo=A,7,S
#OOFacmNo=A,3,S
#OOCustNoX=X,12,L
T(2,#OOCustNo+－+#OOFacmNo)

! 戶名
#OOCustName=X,60,L

! 單據日期
#OOEntryDate=D,7,L

! 法拍費用
#OOFee=m,12.2,L


! 科目名稱
#OOFeeCode=X,2,S

#OOFeeCodeX=X,20,L
T(H,#OOFeeCode,#FeeCodeHelp)


! 借貸(1:借 2:貸)]
#OOTitaCrDb=A,1,S

#OOTitaCrDbX=X,10,L
SWITCH(#OOTitaCrDb,1,T(3,借),
				   2,T(3,貸),
				   T(3,))

! 收件日
#OOAcDate=D,7,L

! 登放序號
#OORelNo=X,25,L

! 項目
#OOItemCode=X,6,S

! 撥款
#OOBormNo=A,3,S

! 細項鍵值
#OODtlValue=X,7,S


!交易序號
#TxtNo=A,8,L
T(1,@TxtNo,#OORelNo,18,8)

#OOKINBR=X,4,S
T(1,@OOKINBR,#OORelNo,8,4)
#OOTlrNo=X,6,S
T(1,@OOTlrNo,#OORelNo,12,6)
#OOTxtNo=A,8,L
T(1,@OOTxtNo,#OORelNo,18,8)

#ColA=A,1,S
T(2,@TXAMT,#OOFee)
T(2,@MRKEY,#OOCustNo+-+#OOFacmNo)
T(2,@BATCHNO,#OOItemCode)

T(2,@TxItemCode,#OOItemCode)
T(2,@TxCustNo,#OOCustNo)
T(2,@TxFacmNo,#OOFacmNo)
T(2,@TxBormNo,#OOBormNo)
T(2,@TxDtlValue,#OODtlValue)
T(2,@TxSelectCode,#SelectCode)

#TxItemCode=X,6,S
#TxCustNo=A,7,S
#TxFacmNo=A,3,S
#TxBormNo=A,3,S
#TxDtlValue=X,7,S
#TxSelectCode=X,1,S

#TITFCD=_,_,S
E(0,0)


! ifxBatch相關參數
#secret-no=x,1000,S
IF(#SelectCode==4,T(2,@ORGKIN,#OOKINBR)T(2,@ORGTLR,#OOTlrNo)T(2,@ORGTNO,#OOTxtNo)E(0,@OrgEntdy,#SYSDATE)E(0,@HCODE,1),$)
T(6A,#TxItemCode+#TxCustNo+#TxFacmNo+#TxBormNo+#TxDtlValue+#TxSelectCode+#TXAMT+#MRKEY+#BATCHNO+#TITFCD+#ORGKIN+#ORGTLR+#ORGTNO+#OrgEntdy+#HCODE)




#batch-detail=x,100000,S

#batch-print=A,1,S

!#batch-rows 總筆數
#batch-rows=X,2,S

!#batch-rows-selected 勾選之筆數
#batch-rows-selected=X,2,S

!#batch-rows-ok  傳送成功筆數
#batch-rows-ok=X,2,S

!#batch-rows-error 傳送失敗筆數
#batch-rows-error=X,2,S

!#batch-row-status 該列傳送狀態
#batch-row-status=X,50,S

!#batch-row-txtno 該列傳送序號
#batch-row-txtno=X,14,O
</sub>



!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>
#SelectCodeHelp=x,100,S
T(3,1:昨日留存;2:本日新增;3:全部;4:本日處理;5:本日刪除;6:保留;7:未處理;9:未處理-轉催)

#FdCustNo=X,2,L

!FeeCode
#CDDEF0001=x,20,S
T(3,CdCode.FeeCode)
#FeeCodeHelp=x,1024,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF0001)
R(1,XXR99)
CALL(#HelpProc)
T(2,@FeeCodeHelp,#HelpDesc1)


#CHAIN=A,1,S
T(4,CHAIN$)
INVOKEJS(SHOW,p1,4,5,0)


#NTXBUF=X,15,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,#SelectCode,@NTXBUF,1,1)

#SHOW_ME_SELECT=X,1,S
T(1,#NTXBUF,1,1)
IF(#SHOW_ME_SELECT==9,INVOKEJS(SHOW,p1,4,5,1),INVOKEJS(SHOW,p1,4,5,0))

#DATE=D,8,S
T(4,DATE$)

#DT=D,7,S
T(1,#DATE,2,7)

!戶號
#CustNo=A,7,I
C(3,#CHAIN,1,s,$)
INVOKEJS(SHOW,p1,1,2,1)
! SHOW(#BTN1)

#MRKEY=_,_,S
C(3,#CustNo,0,S,$)
T(2,#CustNo)

#WKBTN1=X,1,S
! C(3,#CustNo,0,HIDE(#BTN1),SHOW(#BTN1))
C(3,#CustNo,0,INVOKEJS(SHOW,p1,1,1,0),INVOKEJS(SHOW,p1,2,2,0))

! 轉催收日
#TransCollDate=D,7,S
C(3,#CHAIN,1,s,$)
C(3,#CustNo,0,$,S)
T(2,#DT)
@V(7)A(DATE,0,#TransCollDate)

! 查詢範圍 功能: 9.查詢時須輸入，其它隱藏
#SelectCode=A,1,S
C(3,#CHAIN,1,s,$)
E(0,9)
!HELP(1: 昨日留存;2: 本日新增;3: 全部;4: 本日處理;5: 本日刪除;6:保留;7: 未處理;9: 未處理-轉催)
!@V(1,1,2,3,4,5,6,7,9)

#SelectCodeX=X,16,L 
T(3,)T(H,#SelectCode,#SelectCodeHelp)      
!C(2,#SelectCode,T(3,),
!               T(3,昨日留存),
!               T(3,本日新增),
!               T(3,全部),
!               T(3,本日處理),
!               T(3,本日刪除),
!               T(3,保留),
!               T(3,未處理),
!               T(3,),
!               T(3,未處理-轉催),
! )


#RPTFG=A,1,S
C(2,#SelectCode,
S,
E(0,4),
E(0,4),
E(0,4),
E(0,5),
E(0,6),
E(0,2),
E(0,3),
S,
E(0,7),
)


#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

</sub>


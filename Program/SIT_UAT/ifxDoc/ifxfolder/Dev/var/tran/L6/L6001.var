----- BASIC LABEL Variables ----------
<sub type="SYS">
</sub>

<sub type="SYS" name="label">

!#TXCD
!T(3,L6001)

![資料表頭(HEADER)長度]
#INQHD=n,3,S
E(0,0)ASGN(INQHD$)

![每筆資料明細(OCCURS)長度]
#INQLEN=n,3,S
E(0,86)ASGN(INQLEN$)

![多筆查詢之一個畫面有N筆資料]
#INQREC=n,2,S
E(0,20)ASGN(INQREC$)

![畫面顯示的明細間的高度]
#LOOPH=n,3,S
E(0,1)ASGN(LOOPHEIGHT$)

![每張印錄單列印的筆數]
#INQPRT=n,2,S
E(0,40)ASGN(INQPRT$)

</sub>

<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)

#AmlH0=X,3,S
T(3,H90)
#AmlM0=X,3,S
T(3,M90)
#AmlL0=X,3,S
T(3,L90)
!
#AmlH1=X,3,S
T(3,H91)
#AmlM1=X,3,S
T(3,M91)
#AmlL1=X,3,S
T(3,L91)
!
#AmlH9=X,3,S
T(3,H99)
#AmlM9=X,3,S
T(3,M99)
#AmlL9=X,3,S
T(3,L99)

#AutoYes=X,1,S
T(3,1)
#AutoNo=X,1,S
T(3,0)
!
#PSYSDATE=A,8,S
E(0,01000101)

</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L6001]                                 應處理清單                                                            "],
]
!---------- AP Button AREA ----------

#button=x,1,S
K(MODIFYBUTTON,查詢)

![Buttom資料查詢]
!#BTN1=X,15,I,ui:button;value:瀏覽;enable:0;tabbable:0
!#BTN2=X,15,I,ui:button;value:瀏覽;enable:0;tabbable:0
!#BTN3=X,15,I,ui:button;value:瀏覽;enable:0;tabbable:0
#BUF1=X,42,S
!ENABLE(#BTN1)ENABLE(#BTN2)ENABLE(#BTN3)
!---------- AP TEXT AREA Variables ----------


#CHAIN=A,1,S
T(4,CHAIN$)

#RPTFG=A,1,S
E(0,0)

!功能 必須輸入
#FunCd=A,1,S
E(0,8)XMT()

#RETURN=A,1,S
E(0,0)

![折返KEY]
#RTNTXT=X,9,S

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!---------- TXFORM = L6001 ----------
<sub type="FORM" name="L6001">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6001,$,S)

#SCRTIL=_,_,S
T(3,[L6001] 應處理清單)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------

#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

! 項目代號(TOTA-OCCURS)
#ItemCode=X,6,S

! 項目中文名稱(TOTA-OCCURS)
#ItemDesc=X,30,S

#ITEM_LB=X,36,L
T(2,#ItemCode+#ItemDesc)

! 項目對應的交易代號(TOTA-OCCURS)
! 查詢交易
#ChainInqTxcd=X,5,S
! 處理交易
#ChainUpdTxcd=X,5,S
! 本日處理按鈕

! 顯示記號-昨日留存(TOTA-OCCURS)
#YdReserveFg=X,1,S

! 昨日留存筆數(TOTA-OCCURS)
#YdReserve=m,5,S
E(0,#YdReserve)

! *-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*
! 判斷Flag == Y && Cnt >= 1, 筆數數值轉文字, or 空白
! 空白者不Show出按鈕
! *-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*

! 若LD_FG為Y,才顯示,否則顯示空白
#LDCNT_LB=X,5,S
IF(#YdReserveFg=="Y" && #YdReserve >= 1,X(#YdReserve),T(3,))

! 本日新增筆數(TOTA-OCCURS)
#TdNew=m,5,S
E(0,#TdNew)

#TDNEW_LB=X,5,S
IF(#TdNew >= 1,X(#TdNew),T(3,))

! 總筆數(TOTA-OCCURS)
#TotalCnt=m,5,S
E(0,#TotalCnt)

#TOTALCNT_LB=X,5,L
IF(#TotalCnt >= 1,X(#TotalCnt),T(3,))


! 本日處理筆數(TOTA-OCCURS)
#TdProcess=m,5,S
E(0,#TdProcess)

#TDDONE_LB=X,5,L
IF(#TdProcess >= 1,X(#TdProcess),T(3,))

! 本日刪除筆數(TOTA-OCCURS)
#TdDelete=m,5,S
E(0,#TdDelete)

! 顯示記號-本日刪除(TOTA-OCCURS)
#DeleteFg=X,1,S

! 若TDDELETE_FG為Y,才顯示,否則顯示空白
#TD_DELETE_LB=X,5,S
IF(#DeleteFg=="Y" && #TdDelete >= 1,X(#TdDelete),T(3,))

! 保留筆數(TOTA-OCCURS)
#ReserveCnt=m,5,L
E(0,#ReserveCnt)

! 顯示記號-保留(TOTA-OCCURS)
#ReserveFg=X,1,S

! 若RESERVE_FG為Y,才顯示,否則顯示空白
#RESERVE_CNT_LB=X,5,L
IF(#ReserveFg=="Y" && #ReserveCnt >= 1,X(#ReserveCnt),T(3,))

! 未處理筆數(TOTA-OCCURS)
#UnProcessCnt=m,5,L
E(0,#UnProcessCnt)

#UNDONE_CNT_LB=X,5,L
IF(#UnProcessCnt >= 1,X(#UnProcessCnt),T(3,))

! 關帳未處理＝0檢核(TOTA-OCCURS)
#AcClsCheck=X,1,L
IF(#AcClsCheck=="Y",$,T(3,))

!人工處理記號(TOTA-OCCURS)
#AutoFg=X,1,S

! 昨日留存筆數按鈕
#LDCNT_CD=A,1,S
E(0,1)
! T(2,#LDCNT_CD+#AutoFg+#ReserveFg+#DeleteFg+#ITEM_LB)

#SYSCDATE3=D,8,S
E(0,#SYSCDATE+3)

#L2921Buf=X,60,S
T(2,+0000000+0000000+          +0000000+      +000+2+00000000+#SYSCDATE3)

#LDCNT_BUF=X,60,S
T(2,#LDCNT_CD+#ITEM_LB)
C(5,#ItemCode,AMLH,T(2,#AmlH0+#PSYSDATE+#SYSLDATE+#AutoYes),$)
C(5,#ItemCode,AMLM,T(2,#AmlM0+#PSYSDATE+#SYSLDATE+#AutoYes),$)
C(5,#ItemCode,AMLL,T(2,#AmlL0+#PSYSDATE+#SYSLDATE+#AutoYes),$)
C(5,#ItemCode,L2921,T(2,#L2921Buf),$)

#LDCNT_BTN=X,5,S
C(4,#LDCNT_LB,BIND(grid,cancel),BIND(grid,{ cmd=CHAIN; ntxcd=#ChainInqTxcd;text=#LDCNT_LB;ntxbuf=#LDCNT_BUF}))

! 本日新增筆數按鈕
#TDNEW_CD=A,1,S
E(0,2)

#TDNEW_BUF=X,60,S
T(2,#TDNEW_CD+#ITEM_LB)
C(5,#ItemCode,AMLH,T(2,#AmlH9+#SYSDATE+#SYSDATE+#AutoYes),$)
C(5,#ItemCode,AMLM,T(2,#AmlM9+#SYSDATE+#SYSDATE+#AutoYes),$)
C(5,#ItemCode,AMLL,T(2,#AmlL9+#SYSDATE+#SYSDATE+#AutoYes),$)
C(5,#ItemCode,L2921,T(2,#L2921Buf),$)

#TDNEW_BTN=X,5,S
C(4,#TDNEW_LB,BIND(grid,cancel),BIND(grid,{ cmd=CHAIN; ntxcd=#ChainInqTxcd;text=#TDNEW_LB;ntxbuf=#TDNEW_BUF}))

! 總筆數按鈕
#TOTALCNT_CD=A,1,S
E(0,3)
#TOTALCNT_BUF=X,60,S
T(2,#TOTALCNT_CD+#ITEM_LB)
C(5,#ItemCode,AMLH,T(2,#AmlH0+#PSYSDATE+#SYSDATE+#AutoYes),$)
C(5,#ItemCode,AMLM,T(2,#AmlM0+#PSYSDATE+#SYSDATE+#AutoYes),$)
C(5,#ItemCode,AMLL,T(2,#AmlL0+#PSYSDATE+#SYSDATE+#AutoYes),$)
C(5,#ItemCode,L2921,T(2,#L2921Buf),$)

#TOTALCNT_BTN=X,5,S
C(4,#TOTALCNT_LB,BIND(grid,cancel),BIND(grid,{ cmd=CHAIN; ntxcd=#ChainInqTxcd;text=#TOTALCNT_LB;ntxbuf=#TOTALCNT_BUF}))

! 本日處理筆數按鈕
#TDDONE_CD=A,1,S
E(0,4)

#TDDONE_BUF=X,60,S
T(2,#TDDONE_CD+#ITEM_LB)
C(5,#ItemCode,AMLH,T(2,#AmlH1+#SYSDATE+#SYSDATE+#AutoYes),$)
C(5,#ItemCode,AMLM,T(2,#AmlM1+#SYSDATE+#SYSDATE+#AutoYes),$)
C(5,#ItemCode,AMLL,T(2,#AmlL1+#SYSDATE+#SYSDATE+#AutoYes),$)
C(5,#ItemCode,L2921,T(2,#L2921Buf),$)

#TDDONE_BTN=X,5,S
C(4,#TDDONE_LB,BIND(grid,cancel),BIND(grid,{ cmd=CHAIN; ntxcd=#ChainInqTxcd;text=#TDDONE_LB;ntxbuf=#TDDONE_BUF}))

! 本日刪除筆數按鈕
#TD_DELETE_CD=A,1,S
E(0,5)
#TD_DELETE_BUF=X,60,S
T(2,#TD_DELETE_CD+#ITEM_LB)
#TD_DELETE_BTN=X,5,S
C(4,#TD_DELETE_LB,BIND(grid,cancel),BIND(grid,{ cmd=CHAIN; ntxcd=#ChainInqTxcd;text=#TD_DELETE_LB;ntxbuf=#TD_DELETE_BUF}))

! 保留筆數按鈕
#RESERVE_CD=A,1,S
E(0,6)
#RESERVE_CNT_BUF=X,60,S
T(2,#RESERVE_CD+#ITEM_LB)
#RESERVE_CNT_BTN=X,5,S
C(4,#RESERVE_CNT_LB,BIND(grid,cancel),BIND(grid,{ cmd=CHAIN; ntxcd=#ChainInqTxcd;text=#RESERVE_CNT_LB;ntxbuf=#RESERVE_CNT_BUF}))

! 未處理筆數按鈕
#UNDONE_CD=A,1,S
E(0,7)

#UNDONE_CNT_BUF=X,60,S
T(2,#UNDONE_CD+#ITEM_LB)
C(5,#ItemCode,AMLH,T(2,#AmlH0+#SYSDATE+#SYSDATE+#AutoYes),$)
C(5,#ItemCode,AMLM,T(2,#AmlM0+#SYSDATE+#SYSDATE+#AutoYes),$)
C(5,#ItemCode,AMLL,T(2,#AmlL0+#SYSDATE+#SYSDATE+#AutoYes),$)
C(5,#ItemCode,L2921,T(2,#L2921Buf),$)

#UNDONE_CNT_BTN=X,5,S
C(4,#UNDONE_CNT_LB,BIND(grid,cancel),BIND(grid,{ cmd=CHAIN; ntxcd=#ChainInqTxcd;text=#UNDONE_CNT_LB;ntxbuf=#UNDONE_CNT_BUF}))

! 執行交易時固定傳 9:未處理-執行
#EXEC_FUNCD=X,1,S
T(3,9)
#OOBUF01=X,60,S
T(2,#EXEC_FUNCD+#ITEM_LB+#AutoFg+#TITFCD)
C(5,#ItemCode,AMLH,T(2,#AmlH0+#PSYSDATE+#SYSDATE+#AutoNo),$)
C(5,#ItemCode,AMLM,T(2,#AmlM0+#PSYSDATE+#SYSDATE+#AutoNo),$)
C(5,#ItemCode,AMLL,T(2,#AmlL0+#PSYSDATE+#SYSDATE+#AutoNo),$)
C(5,#ItemCode,L2921,T(2,#L2921Buf),$)

! 執行交易
#EXECUTE=X,1,S
C(4,#ItemDesc,BIND(grid,cancel),
C(4,#ChainUpdTxcd,BIND(grid,cancel),BIND(grid,{ cmd=CHAIN; ntxcd=#ChainUpdTxcd;text=#ItemDesc;ntxbuf=#OOBUF01})))



</sub>



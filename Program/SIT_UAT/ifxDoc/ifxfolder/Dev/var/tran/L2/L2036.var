!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[200,1000];printer.width=[20,20];">
[
["[L2036]                                 借款戶關係人/關係企業明細查詢"],
["#Fdspace+借戶戶號",#CustNo],
["#Fdcase+ELoan案件編號",#CaseNo],
]

#Fdspace=X,2,L
#Fdcase=X,2,L
!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>

#CDDEF01=x,20,S
T(3,CdCode.RelsCode)

#CDDEFRelationCode2=x,30,S
T(3,CdCode.RelationCode2)

#CDDEFRelationCode3=x,30,S
T(3,CdCode.RelationCode3)

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEFRelationCode2,#CDDEFRelationCode3)
R(1,XXR99)
CALL(#HelpProc)
T(2,@RelationCode2Help,#HelpDesc1)
T(2,@RelationCode3Help,#HelpDesc2)

#RelationCode2Help=x,200,S

#RelationCode3Help=x,200,S

#CHAIN=A,1,S
T(4,CHAIN$)
IF(#CHAIN!=1,V(P,此為連動交易，請從交易:[L2035借款戶關係人/關係企業查詢]進入),$)

#NTXBUF=X,14,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@CustNo,#NTXBUF,1,7)
T(1,@CaseNo,#NTXBUF,8,7)

#NTXBUF5=X,14,S
C(3,#CHAIN,1,T(4,NTXBUF5$),S)
C(4,#NTXBUF5,E(0,@RPTFG2,1),E(0,@RPTFG2,2))

!戶號
#CustNo=A,7,L

#CaseNo=A,7,L

#RPTFG=A,1,S


#RPTFG2=A,1,S
C(3,#RPTFG,0,$,S)
C(4,#NTXBUF5,T(3,1),T(3,2))

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

</sub>


#CHAIN=A,1,S
T(4,CHAIN$) 
T(2,@RimTxCode,#TXCD)
E(0,@RimFKey,#FKEY)

#NTXBUF=X,100,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)

#NTXBUF5=X,100,S
C(3,#CHAIN,1,T(4,NTXBUF5$),S)
C(4,#NTXBUF5,E(0,@RPTFG2,1),E(0,@RPTFG2,2))

#CHAINTXCD=_,_,S
C(3,#CHAIN,1,T(1,#NTXBUF,1,5),S)

#CustNo=A,7,I
C(3,#CHAIN,1,T(1,#NTXBUF,6,7),$)
C(3,#CHAIN,1,s,$)
@V(2,0)

#RimL1r09=X,1,S
E(0,@RimFunCd,1)
T(2,@RimCustId,)
E(0,@RimCustNo,#CustNo)
RESET_RIM(#RimL1r09,L1R09)
S(L1R09,1,#RimFunCd,#RimCustId,#RimCustNo)
R(1,L1R09)
T(2,@CustName,#L1r09CustName)

#CustName=x,100,L

#RPTFG=A,0,S

#RPTFG2=A,1,S
C(4,#NTXBUF5,E(0,0),E(0,2))

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)


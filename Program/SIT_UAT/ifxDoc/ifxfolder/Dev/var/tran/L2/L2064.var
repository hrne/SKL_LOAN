!---------- TXFORM = L2064 ----------
<sub type="FORM" name="L2064">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L2064,$,S)

#SCRTIL=_,_,S
T(3,[L2064] 聯貸案訂約明細資料)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------


!借戶戶號
#OCustNo=A,7,L
E(0,#CustNo)
#OCustNoX=x,100,L
T(2,#CustName)

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

! 戶號
#OOCustNo=A,7,O
! 額度
#OOFacmNo=A,3,O
! 聯貸費用代碼+說明
#OOSyndFeeItem=x,50,O
! 費用金額
#OOSyndFee=m,14.2,O

#OOSyndFeeX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#OOSyndFee,#OOSyndFeeX)
P(R,#OOSyndFeeX)

!已銷金額
#OOClsAmt=m,14.2,O

#OOClsAmtX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#OOClsAmt,#OOClsAmtX)
P(R,#OOClsAmtX)

#OOEntryAmt=m,14.2,O

#OOEntryAmtX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#OOEntryAmt,#OOEntryAmtX)
P(R,#OOEntryAmtX)

!備註
#OORmk=x,40,O

#OOEntryFG=X,1,O

! rvno
#OORvNo=X,15,O
! 業務科目
#OOAcctCode=X,3,O

! Y為已銷
#OOcheckamt=m,14.2,S
E(0,#OOSyndFee-#OOClsAmt)
#OOCheck=X,1,S
IF(#OOcheckamt==0,T(3,Y),T(3,N))
!修改
#OOBUF1=X,100,S
T(2,2+#OOCustNo+#OOFacmNo+#OORvNo+#OOAcctCode+#OOCheck)

#OOBTN1=X,1,S
C(5,#OOCheck,Y,BIND(grid,cancel),BIND(grid,{ cmd=CHAIN; ntxcd=L2604; ntxbuf=#OOBUF1}))
! BIND(grid,{ cmd=CHAIN; ntxcd=L2604; ntxbuf=#OOBUF1})

!刪除
#OOBUF3=X,100,S
T(2,4+#OOCustNo+#OOFacmNo+#OORvNo+#OOAcctCode)

#OOBTN3=X,1,S
C(5,#OOCheck,Y,BIND(grid,cancel),BIND(grid,{ cmd=CHAIN; ntxcd=L2604; ntxbuf=#OOBUF3}))


!查詢
#OOBUF4=X,100,S
T(2,5+#OOCustNo+#OOFacmNo+#OORvNo+#OOAcctCode)

#OOBTN4=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L2604; ntxbuf=#OOBUF4})


</sub>



----- BASIC LABEL Variables ----------
<sub type="SYS">
#KeyInq=X,1,S
K(MODIFYBUTTON,查詢)
</sub>

<sub type="SYS" name="label">

!#TXCD
!T(3,L2021)

![資料表頭(HEADER)長度]
#INQHD=n,3,S
E(0,0)ASGN(INQHD$)

![每筆資料明細(OCCURS)長度]
#INQLEN=n,3,S
E(0,221)ASGN(INQLEN$)

![多筆查詢之一個畫面有N筆資料]
#INQREC=n,2,S
E(0,20)ASGN(INQREC$)

![畫面顯示的明細間的高度]
#LOOPH=n,3,S
E(0,1)ASGN(LOOPHEIGHT$)

![每張印錄單列印的筆數]
#INQPRT=n,2,S
E(0,40)ASGN(INQPRT$)

</sub>

<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)

#BTNADDBUF=X,18,S
T(2,1+0000000+          )


#BtnAddA=x,100,S
T(2,{cmd=CHAIN; ntxcd=L2221; ntxbuf=#BTNADDBUF; ntxbuf5=type})
ASGN(BTN_NEW1$)
K(MODIFYBUTTON,1,新增)

!#FKEY

</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,1000];printer.width=[20,20];">
[
["[L2021]                                 交易關係人查詢"],
["#FdMain+查詢條件","@(案件編號、統一編號需擇一輸入)"],
["#FdCaseNo+案件編號",#CaseNo],
["#FdCustId+身分證字號",#CustId],
]

#FdMain=X,2,L
#FdCaseNo=X,2,L

!---------- AP Button AREA ----------
![Buttom資料查詢]

!---------- AP TEXT AREA Variables ----------
#CHAIN=A,1,S
T(4,CHAIN$)

<include src="HELPRIM.icf::HELPRIM.rtn"/>
#CDFacRelationCode=x,40,S
T(3,CdCode.FacRelationCode)

#FacRelationCodeHelp=x,3000,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDFacRelationCode)
R(1,XXR99)
CALL(#HelpProc)
T(2,@FacRelationCodeHelp,#HelpDesc1)


!案件編號, 統一編號或核准編號擇一輸入
!案件編號
#CaseNo=A,7,I
C(3,#CHAIN,1,s,$)

#FdCustId=X,2,L
C(3,#CaseNo,0,T(3,*),S)

!身分證號/統號
#CustId=X,10,I
C(3,#CHAIN,1,s,$)
C(3,#CaseNo,0,$,S)
@V(7)A(ID_UNINO,0,#CustId)

#RPTFG=A,1,S
E(0,0)
</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!---------- TXFORM = L2021 ----------
<sub type="FORM" name="L2021">

#SCRTIL=_,_,S
T(3,[L2021] 保證人明細資料)

!修改
#FUNC2=A,1,S
E(0,2)

!刪除
#FUNC4=A,1,S
E(0,4)

!查詢
#FUNC5=A,1,S
E(0,5)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

#OOCreditSysNo=A,7,S
#OOCustId=X,10,S
#OOCustName=X,100,S
#OOFacRelationCode=X,2,S

#OOFacRelationCodeX=X,30,L
T(H,#OOFacRelationCode,#FacRelationCodeHelp)

#OOCreateDate=D,7,L
#OOCreateEmpNo=X,6,S
#OOCreateEmpName=X,10,L


#OOLastUpdate=D,7,L
#OOLastUpdateEmpNo=X,6,S
#OOLastUpdateEmpName=X,10,L

#OOName=X,18,L
T(2,#OOCreateEmpNo+  +#OOCreateEmpName)

#OOName1=X,18,L
T(2,#OOLastUpdateEmpNo+  +#OOLastUpdateEmpName)

!修改
#OOBUF2=X,18,S
T(2,#FUNC2+#OOCreditSysNo+#OOCustId)

#OOBTN2=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L2221; ntxbuf=#OOBUF2})

!刪除
#OOBUF4=X,18,S
T(2,#FUNC4+#OOCreditSysNo+#OOCustId)

#OOBTN4=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L2221; ntxbuf=#OOBUF4})

!查詢
#OOBUF5=X,18,S
T(2,#FUNC5+#OOCreditSysNo+#OOCustId)

#OOBTN5=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L2221; ntxbuf=#OOBUF5})

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------

</sub>



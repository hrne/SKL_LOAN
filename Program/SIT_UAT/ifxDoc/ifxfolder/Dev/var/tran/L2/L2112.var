#WKINIT=X,1,S
T(3,@ProdName, )
T(3,@AcctCodeX, )
!C(3,#FuncCode,1,SHOW(#ProdNo,#BTN2,#BTN3,#BTN4,#BTN5,#BTN6,#AcctCode)
!                T(3,@FdProdNo,*)
!                T(3,@FdAcctCode,*)
!                T(F,#ApplDate,1)T(3,@FdApplDate,*)

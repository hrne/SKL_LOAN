C(3,#Precision,0,$,s)
@V(2,0)

#usApplAmt{i}=m,14.2,I
C(4,#CustId{i},S,$)
C(3,#Precision,0,s,$)
@V(2,0)

#timApplAmt{i}=m,14.2,S
C(3,#Precision,0,E(0,#twApplAmt{i}),E(0,#usApplAmt{i}))


#ApplNo{i}=X,7,L

!戶名
#CustIdX{i}=X,100,L

#WKI{i}=X,1,S
T(2,@CheckCustId{i},#CustId{i})

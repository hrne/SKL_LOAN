["#FdLeadingBank+主辦行",#LeadingBank],
["#FdSigningDateStart+簽約日",#SigningDateStart,"@至",#SigningDateEnd],
["#FdDrawdownStartDateStart+動撥起日",#DrawdownStartDateStart,"@至",#DrawdownStartDateEnd],

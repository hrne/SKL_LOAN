T(3,*)
#FdCustNo=X,2,L
#FdCustId=X,2,L
#FdApplNo=X,2,L
#FdLeadingBank=X,2,L
#FdSigningDateStart=X,2,L
#FdDrawdownStartDateStart=X,2,L
#FdDrawdownEndDateStart=X,2,L
! #FdCommitFeeFlag=X,2,L
! T(3,*)
#FdRPTFG=X,2,L
T(3,*)

!---------- AP Button AREA ----------
![Buttom資料查詢]
#BTN1=X,15,I,ui:button;value:額度資料查詢;enable:1;tabbable:0;title: L2015 額度明細資料查詢;
#BTN2=X,15,I,ui:button;value:顧客資料查詢;enable:1;tabbable:0;title: L1001 顧客明細資料查詢;
#BTN3=X,15,I,ui:button;value:核准號碼查詢;enable:1;tabbable:0;title: L2016 核准號碼明細資料查詢;
#BUF1=X,42,S
ENABLE(#BTN1)ENABLE(#BTN2)ENABLE(#BTN3)
!---------- AP TEXT AREA Variables ----------
! Help
<include src="HELPRIM.icf::HELPRIM.rtn"/>
#CDDEF0001=x,26,S
T(3,CdCode.CurrPrecision)

#StatusHelpRim=X,1,S
RESET_RIM(#StatusHelpRim,XXR99)
S(XXR99,01,#CDDEF0001)
R(1,XXR99)
CALL(#HelpProc) 
T(2,@CurrPrecisionHelp,#HelpDesc1) 

#CurrPrecisionHelp=x,1024,S 

#CommitFeeFlagHelp=x,50,S
T(3, :Space 全部;Y:有收承諾費;N:沒收承諾費)

#CommitFeeFlagHelp1=x,50,S
T(3, :全部;Y:有收承諾費;N:沒收承諾費)

#CHAIN=A,1,S
T(4,CHAIN$) 
T(2,@RimTxCode,#TXCD)
E(0,@RimFKey,#FKEY)

#NTXBUF=X,100,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)

#NTXBUF5=X,10,S
C(3,#CHAIN,1,T(4,NTXBUF5$),S)
C(4,#NTXBUF5,E(0,@RPTFG2,1),E(0,@RPTFG2,2))

#CHAINTXCD=_,_,S
C(3,#CHAIN,1,T(1,#NTXBUF,1,5),S)

#BUF1A=X,18,S
T(3,00000019999999000)

#BIND1A=X,1,S
BIND(#BTN1,click, {cmd=CHAIN; ntxcd=L2015; ntxbuf=#BUF1A;ntxbuf5=type<-1:CustNo<-OOCustNo})

![借款人戶號]
!借款人戶號, 統一編號或核准編號擇一輸入
#CustNo=A,7,I
C(3,#CHAIN,1,T(1,#NTXBUF,6,7),$)
C(3,#CHAIN,1,s,$)
! INVOKEJS(SHOW,p1,1,3,1)
SHOW(#BTN1)HIDE(#BTN2,#BTN3)
C(3,#FirstIn,1,S,$)

#WKBTN1=X,1,S
C(3,#FirstIn,1,S,$)
! C(3,#CustNo,0,INVOKEJS(SHOW,p1,1,1,0),INVOKEJS(SHOW,p1,2,3,0))

#BUF1B=X,18,S
C(3,#CustNo,0,T(3,00000019999999000),T(2,#CustNo+#CustNo+000))
! T(2,#CustNo+9999999000)

#BIND1B=X,1,S
BIND(#BTN1,click, {cmd=CHAIN; ntxcd=L2015; ntxbuf=#BUF1B;ntxbuf5=type<-1:CustNo<-OOCustNo})

#SendL2r07A=X,1,S
C(3,#CustNo,0,S,$)
T(3,@RimCustId, )
E(0,@RimCustNo,#CustNo)
RESET_RIM(#SendL2r07A,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo)
R(1,L2R07)


#BUF2A=X,100,S

#BIND2A=X,1,S
BIND(#BTN2,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUF2A;ntxbuf5=type<-1:CustId<-OOCustId})


#CustNoX=X,100,L
C(3,#CustNo,0,S,T(2,#L2r07CustName))

![統一編號]
#CustId=X,10,I
C(3,#FirstIn,1,S,$)
C(3,#CustNo,0,$,s)
! INVOKEJS(SHOW,p1,2,3,1)
SHOW(#BTN2)HIDE(#BTN1,#BTN3)
@C(4,#CustId,S,$)V(7)A(ID_UNINO,0,#CustId)


#BUF2B=X,100,S
T(2,0000000+#CustId)

#BIND2B=X,1,S
BIND(#BTN2,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUF2B;ntxbuf5=type<-1:CustId<-OOCustId})


#WKBTN2=X,1,S
C(3,#FirstIn,1,S,$)
C(4,#CustId,HIDE(#BTN2),SHOW(#BTN2))
! C(4,#CustId,INVOKEJS(SHOW,p1,2,2,0),INVOKEJS(SHOW,p1,3,3,0))

#SendL2r07B=X,1,S
C(4,#CustId,S,$)
T(2,@RimCustId,#CustId)
E(0,@RimCustNo,0)
RESET_RIM(#SendL2r07B,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo,#END)
R(1,L2R07)

#CustIdX=X,100,L
C(4,#CustId,S,T(2,#L2r07CustName))

#BUF3A=X,18,S
T(3,000000199999990)

#BIND3A=X,1,S
BIND(#BTN3,click, {cmd=CHAIN; ntxcd=L2016; ntxbuf=#BUF3A;ntxbuf5=type<-1:ApplNo<-OOApplNo})


![核准編號]
#ApplNo=A,7,I
C(3,#FirstIn,1,S,$)
C(3,#CustNo,0,$,S)
C(4,#CustId,$,S)
! INVOKEJS(SHOW,p1,3,3,1)
SHOW(#BTN3)
@C(3,#ApplNo,0,V(P,需擇一輸入),$)


#WKBTN3=X,1,S
C(3,#FirstIn,1,S,$)
C(3,#ApplNo,0,HIDE(#BTN3),SHOW(#BTN3))

#BUF3B=X,18,S
C(3,#ApplNo,0,T(3,000000199999990),T(2,#ApplNo+99999990))
! T(2,#ApplNo+99999990)

#BIND3B=X,1,S
BIND(#BTN3,click, {cmd=CHAIN; ntxcd=L2016; ntxbuf=#BUF3B;ntxbuf5=type<-1:ApplNo<-OOApplNo})

#HIDEALLBTN=X,1,S
C(3,#CHAIN,1,$,S)
HIDE(#BTN1,#BTN2,#BTN3)

!用核准號碼(ApplNo)調L2R05
#SendL2r05A=X,1,S
C(3,#ApplNo,0,S,$)
E(0,@RimFuncCode,5)
E(0,@RimCustNo,#CustNo)
E(0,@RimFacmNo,0)
E(0,@RimApplNo,#ApplNo)
E(0,@RimCaseNo,0)
RESET_RIM(#SendL2r05A,L2R05)
S(L2R05,1,#RimFuncCode,#RimTxCode,#RimFKey,#RimCustNo,#RimFacmNo,#RimApplNo,#RimCaseNo)
R(1,L2R05)
! E(0,@FacmNo,#L2r05FacmNo)

#SendL2r07C=X,1,S
C(3,#ApplNo,0,S,$)
T(2,@RimCustId,#L2r05CustId)
E(0,@RimCustNo,#CustNo)
RESET_RIM(#SendL2r07C,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo,#END)
R(1,L2R07)

#ApplNoX=X,100,L
C(3,#ApplNo,0,S,T(2,#L2r07CustName))

#TimCustNo=A,7,S
E(0,0)
C(3,#CustNo,0,$,E(0,#CustNo))
C(4,#CustId,$,E(0,#L2r07CustNo))
C(3,#ApplNo,0,$,E(0,#L2r05CustNo))


![主辦行]
!可不輸入
#LeadingBank=X,7,I
C(3,#FirstIn,1,S,$)

![簽約日]
!可不輸入
#SigningDateStart=D,7,I
C(3,#FirstIn,1,S,$)
E(0,0)
@
A(DATE,0,#SigningDateStart)

#SigningDateEnd=D,7,I
E(0,#SYSDATE)
C(3,#FirstIn,1,s,$)
@A(DATE,0,#SigningDateEnd)
V(5,#SigningDateStart,9991231)

![動撥起日]
!可不輸入
#DrawdownStartDateStart=D,7,I
C(3,#FirstIn,1,S,$)
E(0,0)
@A(DATE,0,#DrawdownStartDateStart)

#DrawdownStartDateEnd=D,7,I
C(3,#FirstIn,1,S,$)
@A(DATE,0,#DrawdownStartDateEnd)
V(5,#DrawdownStartDateStart,9991231)

![動撥迄日]
!可不輸入
#DrawdownEndDateStart=D,7,I
C(3,#FirstIn,1,S,$)
E(0,#DrawdownStartDateStart)
@A(DATE,0,#DrawdownEndDateStart)

#DrawdownEndDateEnd=D,7,I
C(3,#FirstIn,1,S,$)
@A(DATE,0,#DrawdownEndDateEnd)
V(5,#DrawdownEndDateStart,9991231)

![有收承諾費]

! !可不輸入
! #CommitFeeFlag=X,1,I
! C(3,#FirstIn,1,S,$)
! HELP(#CommitFeeFlagHelp) 
! @V(H,#CommitFeeFlagHelp1)

! #CommitFeeFlagX=X,10,L
! T(3,)T(H,#CommitFeeFlag,#CommitFeeFlagHelp1)
! C(3,#FirstIn,1,E(0,@CHAIN,1),$)
! E(0,@FirstIn,0)

<include src="COM.icf::RPTFG.dec"/>

#RPTFG2=A,1,S
C(3,#RPTFG,0,$,S)
C(4,#NTXBUF5,T(3,1),T(3,2))

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

<include src="ROM.icf::L0Rim.dec"/>
<include src="ROM.icf::L2Rim.dec"/>
<include src="ROM.icf::L2R05.dec"/>
<include src="ROM.icf::L2R07.dec"/>

</sub>


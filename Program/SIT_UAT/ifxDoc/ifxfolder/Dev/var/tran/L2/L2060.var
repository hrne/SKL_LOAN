!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#SPC10=X,10,S

#ZRO07=A,7,S

#DASH=X,1,S
T(3,-)

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

#OOCustNo=A,7,S

#OOSyndNo=A,3,S

#OOSyndNoX=X,11,O
T(2,#OOCustNo+#DASH+#OOSyndNo)

#OOCustId=X,10,O

#OOCustIdX=X,100,O

#OOCustIdXX=X,40,S
T(1,#OOCustIdX,1,40)

#OOLeadingBank=A,7,O

#OOSigningDate=D,7,O

#OODrawdownStartDate=D,7,O

#OODrawdownEndDate=D,7,O

#OOCurrencyCode=X,3,O

#Precision=N,1,S
E(0,0)T(H,#OOCurrencyCode,#CurrPrecisionHelp)
 
#OOSyndAmt=m,14.2,S

#OOSyndAmtX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#OOSyndAmt,#OOSyndAmtX)
P(R,#OOSyndAmtX)

#OOPartAmt=m,14.2,S

#OOPartAmtX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#OOPartAmt,#OOPartAmtX)
P(R,#OOPartAmtX)

!修改
#OOBUF1=X,100,S
T(2,2+#OOCustId+#OOCustNo+#OOSyndNo)

#OOBTN1=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L3600; ntxbuf=#OOBUF1})

!複製
#OOBUF2=X,100,S
T(2,3+#OOCustId+#OOCustNo+#OOSyndNo)

#OOBTN2=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L3600; ntxbuf=#OOBUF2})

!刪除
#OOBUF3=X,100,S
T(2,4+#OOCustId+#OOCustNo+#OOSyndNo)

#OOBTN3=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L3600; ntxbuf=#OOBUF3})

!查詢
#OOBUF4=X,100,S
T(2,5+#OOCustId+#OOCustNo+#OOSyndNo)

#OOBTN4=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L3600; ntxbuf=#OOBUF4})

!補收費用
#OOBUF5=X,100,S
! T(2,#OOCustNo+#OOSyndNo)

#OOBTN5=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L3230; ntxbuf=#OOBUF5})
</sub>



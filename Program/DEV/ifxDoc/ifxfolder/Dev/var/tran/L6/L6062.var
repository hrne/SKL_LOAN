!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L6062]                                 行業別代號資料查詢"],
["#FdIndustryCode+行業代號",#IndustryCode],

]
!---------- AP Button AREA ----------
![Buttom資料查詢]
!#BTN1=X,15,I,ui:button;value:新增行業別代號;enable:1;tabbable:0;title:L6602 行業別代號資料維護
!#BUF1=X,42,S

!---------- AP TEXT AREA Variables ----------

#FdIndustryCode=X,2,L
#FdRPTFG=X,2,L
T(3,*)


#CHAIN=A,1,S
T(4,CHAIN$) 

#NTXBUF=X,4,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)

#NTXBUF5=X,10,S
C(3,#CHAIN,1,T(4,NTXBUF5$),S)
C(4,#NTXBUF5,E(0,@RPTFG2,1),E(0,@RPTFG2,2))

#button=x,100,S
T(2,{cmd=CHAIN; ntxcd=L6602; ntxbuf=1; ntxbuf5=type})ASGN(BTN_NEW1$)
K(MODIFYBUTTON,查詢)
C(3,#CHAIN,1,S,K(MODIFYBUTTON,1,新增行業別代號))


#RPTFG=A,1,S
E(0,0)


!行業代號
#IndustryCode=A,6,I
C(3,#CHAIN,1,T(1,#NTXBUF,1,4),$)
C(3,#CHAIN,1,s,$)
!C(3,#FirstIn,1,E(0,@CHAIN,1),$)
!(3,#FirstIn,1,S,$)

#BUF1=X,7,S
!C(4,#IndustryCode,HIDE(#BTN1),SHOW(#BTN1))
!T(2,1+#IndustryCode)
!!(0,@FirstIn,0)

!#BIND1=X,1,S
!BIND(#BTN1,click, {cmd=CHAIN; ntxcd=L6602; ntxbuf=#BUF1})

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

!<include src="COM.icf::RPTFG.dec"/>

#RPTFG2=A,1,S
C(3,#RPTFG,0,$,S)
C(4,#NTXBUF5,T(3,1),T(3,2))

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!---------- TXFORM = L6062 ----------
<sub type="FORM" name="L6062">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6062,$,S)

#SCRTIL=_,_,S
T(3,[L6062] 行業別代號資料)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------

#OIndustryCode=X,6,L
T(2,#IndustryCode)

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

! 行業代號(TOTA)
#OOIndustryCode=X,6,L

! 行業說明(TOTA)
#OOIndustryItem=X,100,L

! 主計處大類(TOTA)
#OOMainType=X,1,L

#OOCHAIN=A,1,S
T(4,CHAIN$)

! 修改的按鈕
#OOFunc2=A,1,S
E(0,2)

#OOBUF2=X,108,S
T(2,#OOFunc2+#OOIndustryCode+#OOIndustryItem+#OOMainType)

! 連結時隱藏按鈕
#OOBTN2=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6602;text=修改;ntxbuf=#OOBUF2})
IF(#OOCHAIN==1,BIND(grid,cancel),$)

! 刪除的按鈕
#OOFunc4=A,1,S
E(0,4)

#OOBUF4=X,108,S
T(2,#OOFunc4+#OOIndustryCode+#OOIndustryItem+#OOMainType)

! 連結時隱藏按鈕
#OOBTN4=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6602;text=刪除;ntxbuf=#OOBUF4})
IF(#OOCHAIN==1,BIND(grid,cancel),$)

</sub>



!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L6932]                                 資料變更交易查詢"],
["#FdDate+查詢條件","會計日期、交易日期，需擇一輸入"],
["#FD_ST_DT+．會計日期",#ST_DT,"~",#ED_DT],
["#FD_TX_DT+．交易日期",#SX_DT,"~",#EX_DT],
["#FdSearch+查詢條件","交易序號、交易代號、戶號，需擇一輸入"],
["#FdTxtNo+．交易序號",#TxtNo],
["#FD_TRN_CODE+．交易代號",#TRN_CODE],
["#FD_CUST_NO+．戶@@@@號",#CUST_NO,"-",#FACM_NO,"-",#BORM_SEQ],
["#FD_MrKey+．參考編號",#MrKey],

]
!---------- AP Button AREA ----------
![Buttom資料查詢]
!#BTN1=X,15,I,ui:button;value:瀏覽;enable:0;tabbable:0
!#BTN2=X,15,I,ui:button;value:瀏覽;enable:0;tabbable:0
!#BTN3=X,15,I,ui:button;value:瀏覽;enable:0;tabbable:0
#BUF1=X,42,S
!ENABLE(#BTN1)ENABLE(#BTN2)ENABLE(#BTN3)

! 必輸欄位
#FdDate=X,2,L
T(3,*)

#FdSearch=X,2,L
T(3,*)
#FD_TRN_CODE=X,2,L
#FD_CUST_NO=X,2,L
#FdTxtNo=X,2,L

! 永遠為必輸欄位
#FD_ST_DT=X,2,L
T(3, )

#FD_TX_DT=X,2,L
T(3, )

#FD_MrKey=X,2,L

#WKHIDE=X,1,S
INVOKEJS(SHOW,p1,1)

#Submit=A,1,S
K(MODIFYBUTTON,查詢)
!---------- AP TEXT AREA Variables ----------
#CHAIN=A,1,S
T(4,CHAIN$)

#CHAINFlag=A,1,S
C(3,#CHAIN,1,E(0,1),E(0,0))

#NTXBUF=x,150,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)

#CHAIN_ST_DT=X,7,S
T(1,#NTXBUF,1,7)

#CHAIN_ED_DT=X,7,S
T(1,#NTXBUF,8,7)

#CHAIN_TRN_CODE=X,5,S
T(1,#NTXBUF,15,5)

#CHAIN_TXSEQ=X,18,S
T(1,#NTXBUF,20,18)

#CHAIN_CUST_NO=X,7,S
T(1,#NTXBUF,38,7)

#CHAIN_FACM_NO=X,3,S
T(1,#NTXBUF,45,3)

#CHAIN_BORM_SEQ=X,3,S
T(1,#NTXBUF,48,3)

#CHAIN_MrKey=x,20,S
T(1,#NTXBUF,51,20)

#CHAIN_TRN_CODE2=X,5,S
T(1,#NTXBUF,71,5)

#CHAIN_SX_DT=X,7,S
T(1,#NTXBUF,76,7)

#CHAIN_EX_DT=X,7,S
T(1,#NTXBUF,83,7)

#CHAIN_MrKey2=x,50,S
T(1,#NTXBUF,90,50)

#CHAINSET=X,1,S
E(0,@ST_DT,#CHAIN_ST_DT)
E(0,@ED_DT,#CHAIN_ED_DT)
T(2,@TxtNo,#CHAIN_TXSEQ)
T(2,@TRN_CODE,#CHAIN_TRN_CODE)
E(0,@CUST_NO,#CHAIN_CUST_NO)
E(0,@FACM_NO,#CHAIN_FACM_NO)
E(0,@BORM_SEQ,#CHAIN_BORM_SEQ)
C(4,#CHAIN_MrKey2,T(2,@MrKey,#CHAIN_MrKey),T(2,@MrKey,#CHAIN_MrKey2))
T(2,@TRN_CODE2,#CHAIN_TRN_CODE2)
E(0,@SX_DT,#CHAIN_SX_DT)
E(0,@EX_DT,#CHAIN_EX_DT)

#RPTFG=A,1,S
E(0,0)

! 作業日期
#DATE=D,8,S
T(4,DATE$)

#DT=D,7,S
T(1,#DATE,2,7)

#DT_MONTH=A,5,S
T(1,#DATE,2,5)

#DT_01=A,2,S
E(0,1)

#ST_DTS=D,7,S
T(2,#DT_MONTH+#DT_01)
#ED_DTS=D,7,S
E(0,#DT)

#ST_DT=D,7,I
C(3,#CHAIN,1,s,$)
T(2,#ST_DTS)
@
C(3,#ST_DT,0,S,A(4,0,#ST_DT))

#ED_DT=D,7,I
C(3,#CHAIN,1,s,$)
C(3,#ST_DT,0,S,$)
T(2,#ED_DTS)
D(8)
@
V(7)
A(4,0,#ED_DT)
V(5,#ST_DT,#ED_DTS)

#SX_DT=D,7,I
C(3,#CHAIN,1,s,$)
C(3,#ST_DT,0,$,S)
C(3,#SX_DT,0,E(0,#SYSCDATE),$)
@
V(7)
A(4,0,#SX_DT)

#EX_DT=D,7,I
C(3,#CHAIN,1,s,$)
C(3,#SX_DT,0,S,$)
C(3,#EX_DT,0,E(0,#SX_DT),$)
@
V(7)
A(4,0,#EX_DT)
V(5,#SX_DT,#SYSCDATE)

!交易序號
#TxtNo=X,18,I
C(3,#CHAIN,1,s,$)
!INVOKEJS(SHOW,p1,2,4,1)

#WKBTN1=X,1,S
!C(4,#TxtNo,INVOKEJS(SHOW,p1,2,2,0),INVOKEJS(SHOW,p1,3,4,0))

! 交易代號
#TRN_CODE=X,5,I
C(3,#CHAIN,1,s,$)
!C(4,#TxtNo,$,S)
!INVOKEJS(SHOW,p1,3,4,1)

#TRN_CODE2=X,5,S

#WKBTN=X,1,S
!C(3,#TRN_CODE,0,INVOKEJS(SHOW,p1,3,3,0),INVOKEJS(SHOW,p1,4,4,0))

! 戶號
#CUST_NO=A,7,I
C(3,#CHAIN,1,s,$)
!C(4,#TxtNo,$,S)
!C(3,#TRN_CODE,0,$,S)

#FACM_NO=A,3,I
C(3,#CHAIN,1,s,$)
!C(4,#TxtNo,$,S)
!C(3,#TRN_CODE,0,$,S)
C(3,#CUST_NO,0,S,$)

#BORM_SEQ=A,3,I
C(3,#CHAIN,1,s,$)
!C(4,#TxtNo,$,S)
!C(3,#TRN_CODE,0,$,S)
C(3,#FACM_NO,0,S,$)

#MrKey=x,50,I
C(3,#CHAIN,1,s,$)
!C(4,#TxtNo,$,S)
!C(3,#CUST_NO,0,$,S)

#MRKEY=_,_,S
C(3,#CUST_NO,0,S,$)
C(3,#FACM_NO,0,T(2,#CUST_NO),T(2,#CUST_NO+-+#FACM_NO))
C(3,#BORM_SEQ,0,$,T(2,#CUST_NO+-+#FACM_NO+-+#BORM_SEQ))

!檢核戶號是否為0
#VCUSTNO=X,13,S
C(3,#CHAIN,1,s,$)
T(2,#CUST_NO+#FACM_NO+#BORM_SEQ)
C(4,#TxtNo,$,S)
C(3,#TRN_CODE,0,$,S)
C(4,#MrKey,$,S)
@C(3,#VCUSTNO,0,V(P,交易序號、交易代號、戶號、交易編號，需擇一輸入),$)



#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

</sub>


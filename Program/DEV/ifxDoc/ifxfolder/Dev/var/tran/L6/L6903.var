<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!---------- TXFORM = L6903 ----------
<sub type="FORM" name="L6903">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6903,$,S)

#SCRTIL=_,_,S
T(3,[L6903] 會計帳務明細)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------

! 借方合計
#OODbAmt=m,14.2,S
#OODbAmtX=m,14,L
E(0,#OODbAmt)
!#OODbAmtX=X,17,S
!IF(#OODbAmt1==0,S,$)
!X(#OODbAmt1)P(R,#OODbAmtX, )

! 貸方合計
#OOCrAmt=m,14.2,S
#OOCrAmtX=m,14,L
E(0,#OOCrAmt)
!#OOCrAmtX=X,17,S
!IF(#OOCrAmt1==0,S,$)
!X(#OOCrAmt1)P(R,#OOCrAmtX, )

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)
! 會計日期
#OOAcDate=D,7,S

!登放日期
#OORelDy=D,7,S

! 登放序號
#OORelTxseq=X,18,S

#OORelTxseqX=X,26,S
T(2,#OORelDy+-+#OORelTxseq)
! 交易代號
#OOTitaTxCd=X,5,S
#OOTranItem=X,80,S
#OOTitaTxCdX=X,86,S
T(2,#OOTitaTxCd+ +#OOTranItem)

! 借貸
#OODbCr=X,1,S

#OODbCrX=X,2,S
IF(#OODbCr=="D",T(3,借),T(3,貸))

! 交易金額
#OOTxAmt=+m,14.2,S
#OOTxAmt1=+m,14,S
E(0,#OOTxAmt)
#OOTxAmtX=X,17,S
IF(#OOTxAmt1==0,S,$)
X(#OOTxAmt1)

! 餘額
#OOTdBal=+m,14.2,S
#OOTdBal1=+m,14,S
E(0,#OOTdBal)
#OOTdBalX=X,17,S
IF(#OOTdBal1==0,S,$)
X(#OOTdBal1)

! 摘要
#OOSlipNote=X,160,S

</sub>



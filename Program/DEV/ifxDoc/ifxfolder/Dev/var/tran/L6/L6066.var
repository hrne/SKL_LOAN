!---------- TXFORM = L6066 ----------
<sub type="FORM" name="L6066">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6066,$,S)

#SCRTIL=_,_,S
T(3,[L6066] 主管理由)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------

!#OSupvReasonCode=X,4,L
!T(2,#SupvReasonCode)

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------

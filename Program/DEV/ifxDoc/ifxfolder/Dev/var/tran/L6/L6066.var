E(0,2)

#OOBUF2=X,86,S
T(2,#OOFunc2+#OOSupvReasonCode+#OOSupvReasonItem+#OOSupvReasonLevel)

#OOBTN2=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6606;text=修改;ntxbuf=#OOBUF2})

! 刪除的按鈕

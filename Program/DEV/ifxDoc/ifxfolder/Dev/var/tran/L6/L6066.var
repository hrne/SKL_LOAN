E(0,4)

#OOBUF4=X,86,S
T(2,#OOFunc4+#OOSupvReasonCode+#OOSupvReasonItem+#OOSupvReasonLevel)

#OOBTN4=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6606;text=刪除;ntxbuf=#OOBUF4})

</sub>



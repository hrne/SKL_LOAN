----- BASIC LABEL Variables ----------
<sub type="SYS">
</sub>

<sub type="SYS" name="label">

!#TXCD
!T(3,L6084)

![資料表頭(HEADER)長度]
#INQHD=n,3,S
E(0,0)ASGN(INQHD$)

![每筆資料明細(OCCURS)長度]
#INQLEN=n,3,S
E(0,168)ASGN(INQLEN$)

![多筆查詢之一個畫面有N筆資料]
#INQREC=n,2,S
E(0,20)ASGN(INQREC$)

![畫面顯示的明細間的高度]
#LOOPH=n,3,S
E(0,1)ASGN(LOOPHEIGHT$)

![每張印錄單列印的筆數]
#INQPRT=n,2,S
E(0,40)ASGN(INQPRT$)

</sub>

<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD2.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)
!HIDE(#BTN1)

#FirstIn=A,1,S
E(0,1)

#button=x,100,S
T(2,{cmd=CHAIN; ntxcd=L6754; ntxbuf=1; ntxbuf5=type})ASGN(BTN_NEW1$)
K(MODIFYBUTTON,1,新增標準設定)
K(MODIFYBUTTON,查詢)

</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L6084]                                 業績件數及金額核算標準設定查詢"],
["#FdWorkMonth+工作月",#WorkMonth,"@(輸入0查全部)"],
]

#FdWorkMonth=X,2,L

#WorkMonth=A,5,I
@
C(3,#WorkMonth,0,$,A(YM13,1,#WorkMonth))

#RPTFG=A,1,S
E(0,0)
</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!---------- TXFORM = L6084 ----------
<sub type="FORM" name="L6084">

#SCRTIL=_,_,S
T(3,[L6084] 業績件數及金額核算標準設定)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

! 計件代碼(TOTA)
#OOWorkMonth=A,5,S

! 複製
#OOBUF3=X,6,S
T(2,3+#OOWorkMonth)

#OOBTN3=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6757;text=複製;ntxbuf=#OOBUF3})

! 刪除
#OOBUF4=X,6,S
T(2,4+#OOWorkMonth)

#OOBTN4=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6757;text=刪除;ntxbuf=#OOBUF4})

! 查詢的按鈕
#OOFunc5=A,1,S
E(0,5)

#OOBUF5=X,5,S
T(2,#OOWorkMonth)

#OOBTN5=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6994;text=查詢;ntxbuf=#OOBUF5})
</sub>


!------ 上行電文 ------
<sub type="TIM">
#WorkMonth
#END
</sub>


!------ 下行電文TOM ------
<sub type="TOM">
<include src="COM.icf::TRCTL.tom"/>
<include src="HELPRIM.icf::HELPRIM.tom"/>

TXFORM=L6084
#OOWorkMonth
^
</sub>

!--------- OUTPUT畫面 --------
<sub type="PART" name="L6084" layout="cols=1;screen.width=[100,900];printer.width=[20,20];">
[
["[L6084]                                 業績件數及金額核算標準設定查詢                                             "],
]
</sub>


!---------- 單據輸出組合 ----------
<sub type="SELECT">

#RPTFG=0,QUERY.GRID,L6084
#any={detail:true, header:'L6084.part', caption:'', width:1000, rowNum:20, rowList:[20,40,60]}

#OOBTN3=複製
{width:50,search:false,formatter:'cust:bind-button:#OOBTN3',align:'center',canExport:false}

#OOBTN4=刪除
{width:50,search:false,formatter:'cust:bind-button:#OOBTN4',align:'center',canExport:false}

#OOBTN5=查詢
{width:50,search:false,formatter:'cust:bind-button:#OOBTN5',align:'center',canExport:false}

#OOWorkMonth=工作月
{width:60,align:'center'}

^

</sub>

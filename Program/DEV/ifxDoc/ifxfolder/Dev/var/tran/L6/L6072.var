!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L6072]                                 營業單位對照檔查詢"],
["@單位別",#BranchNo],
!["@顯示方式",
!<include src="COM.icf::RPTFG.scr"/>
!],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
!#BTN1=X,15,I,ui:button;value:新增單位別;enable:1;tabbable:0;title:L6702 營業單位對照檔維護
!#BUF1=X,42,S

!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>

#H1=X,10,S
T(3,CDBR)

#BrHelp=X,1024,S
#AuthHelp=X,1024,S

#HR=X,1,S
S(XXR99,01,#H1)
R(1,XXR99)
CALL(#HelpProc)
T(2,@BrHelp,#HelpDesc1)

#CHAIN=A,1,S
T(4,CHAIN$) 

#NTXBUF=X,4,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)

#RPTFG=A,1,S
E(0,0)

!單位別
#BranchNo=X,4,I
C(3,#CHAIN,1,T(1,#NTXBUF,1,4),$)
C(3,#CHAIN,1,s,$)
!C(3,#FirstIn,1,E(0,@CHAIN,1),$)
!C(3,#FirstIn,1,S,$)
HELP(#BrHelp)
@C(4,#BranchNo,S,$)V(H,#BrHelp)

#BUF1=X,5,S
!C(4,#BranchNo,HIDE(#BTN1),SHOW(#BTN1))
!T(2,1+#BranchNo)
!E(0,@FirstIn,0)

!#BIND1=X,1,S
!BIND(#BTN1,click, {cmd=CHAIN; ntxcd=L6702; ntxbuf=#BUF1})

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

!<include src="COM.icf::RPTFG.dec"/>

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!---------- TXFORM = L6072 ----------
<sub type="FORM" name="L6072">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6072,$,S)

#SCRTIL=_,_,S
T(3,[L6072] 營業單位對照檔)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------

#OBranchNo=X,4,L
T(2,#BranchNo)

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

! 單位別(TOTA)
#OOBranchNo=X,4,L

! 單位簡稱(TOTA)
#OOBranchShort=x,28,L

! 單位名稱(TOTA)
#OOBranchItem=x,80,L


! 修改的按鈕
#OOFunc2=A,1,S
E(0,2)

#OOBUF2=X,113,S
T(2,#OOFunc2+#OOBranchNo+#OOBranchShort+#OOBranchItem)

#OOBTN2=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6702;text=修改;ntxbuf=#OOBUF2})

! 刪除的按鈕
#OOFunc4=A,1,S
E(0,4)

#OOBUF4=X,113,S
T(2,#OOFunc4+#OOBranchNo+#OOBranchShort+#OOBranchItem)

#OOBTN4=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6702;text=刪除;ntxbuf=#OOBUF4})

! 查詢的按鈕
#OOFunc5=A,1,S
E(0,5)

#OOBUF5=X,113,S
T(2,#OOFunc5+#OOBranchNo+#OOBranchShort+#OOBranchItem)

#OOBTN5=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L6702;text=查詢;ntxbuf=#OOBUF5})
</sub>



<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD2.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)
!HIDE(#BTN1)

!#FirstIn=A,1,S
!E(0,1)

#button=x,100,S
T(2,{cmd=CHAIN; ntxcd=L6702; ntxbuf=1; ntxbuf5=type})ASGN(BTN_NEW1$)

----- BASIC LABEL Variables ----------
<sub type="SYS">
</sub>

<sub type="SYS" name="label">

!#TXCD
!T(3,L6934)

![資料表頭(HEADER)長度]
#INQHD=n,3,S
E(0,233)ASGN(INQHD$)

![每筆資料明細(OCCURS)長度]
#INQLEN=n,3,S
E(0,450)ASGN(INQLEN$)

![多筆查詢之一個畫面有N筆資料]
#INQREC=n,2,S
E(0,20)ASGN(INQREC$)

![畫面顯示的明細間的高度]
#LOOPH=n,3,S
E(0,1)ASGN(LOOPHEIGHT$)

![每張印錄單列印的筆數]
#INQPRT=n,2,S
E(0,40)ASGN(INQPRT$)

</sub>

<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)

</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L6934]                                 資料變更內容查詢"],
["#FdMain+查詢序號",#TxDate,"-",#TxSeq,"-",#TxSno],
]
!---------- AP Button AREA ----------

! 必輸欄位
#FdMain=X,2,L
T(3, )

#Submit=A,1,S
K(MODIFYBUTTON,查詢)

!---------- AP TEXT AREA Variables ----------
#CHAIN=A,1,S
T(4,CHAIN$)
IF(#CHAIN==0,V(P,本交易為連動交易，請由L6933連動進入),$)

#NTXBUF=x,30,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@TxDate,#NTXBUF,1,7)
T(1,@TxSeq,#NTXBUF,8,18)
T(1,@TxSno,#NTXBUF,26,2)

#TxDate=A,7,L
#TxSeq=X,18,L
#TxSno=A,2,L

#RPTFG=A,1,S
E(0,0)


#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!---------- TXFORM = L6934 ----------
<sub type="FORM" name="L6934">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L6934,$,S)

#SCRTIL=_,_,S
T(3,[L6934] 資料變更交易)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
#OTranName=X,80,L

#OCustNo=A,7,L
#OFacmNo=A,3,L
#OBormNo=A,3,L
#OMrKey=x,50,L
		
#OReason=X,100,L

#OLastUpdate=X,20,L

#OLastEmp=X,40,L



!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

#OColumnName=x,50,S
#ODataBefore=x,200,S
#ODataAfter=x,200,S

</sub>


!------ 上行電文 ------
<sub type="TIM">
#TxDate
#TxSeq
#TxSno
</sub>


!------ 下行電文TOM ------
<sub type="TOM">
<include src="COM.icf::TRCTL.tom"/>
TXFORM=L6934
#OTranName
#OCustNo
#OFacmNo
#OBormNo
#OMrKey
#OReason
#OLastUpdate
#OLastEmp
#OColumnName

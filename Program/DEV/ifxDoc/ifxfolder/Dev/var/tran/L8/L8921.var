!---------- AP Button AREA ----------
![Buttom資料查詢]
!#BUF1=X,42,S

#FdFactor1TotLimit=X,2,L
T(3, )
#FdFactor2Count=X,2,L
T(3, )
#FdFactor2AmtStart=X,2,L
T(3, )
#FdFactor3TotLimit=X,2,L
T(3, )
#FdAcDate=X,2,L
T(3,*)
#FdRPTFG=X,2,L

!---------- AP TEXT AREA Variables ----------
#CHAIN=A,1,S
T(4,CHAIN$)

#NTXBUF=X,32,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)

! 功能 (5: 查詢)
#FuncCode=A,1,S
E(0,5)
K(MODIFYBUTTON,查詢)

! 業務類型
#BusinessType=X,2,S
T(3,LN)

!#SendL8R49=X,1,S
!E(0,@RimFuncCode,#FuncCode)
!T(2,@RimTxCode,#TXCD)
!T(2,@RimBusinessType,#BusinessType)
!RESET_RIM(#SendL8R49,L8R49)
!S(L8R49,1,#RimFuncCode,#RimTxCode,#RimBusinessType)
!R(1,L8R49)

!E(0,@Factor1TotLimit,#L8R49Factor1TotLimit)
!E(0,@Factor2Count,#L8R49Factor2Count)
!E(0,@Factor2AmtStart,#L8R49Factor2AmtStart)
!E(0,@Factor2AmtEnd,#L8R49Factor2AmtEnd)
!E(0,@Factor3TotLimit,#L8R49Factor3TotLimit)

!T(F,@Factor1TotLimit,0)
!T(F,@Factor2Count,0)
!T(F,@Factor2AmtStart,0)
!T(F,@Factor2AmtEnd,0)
!T(F,@Factor3TotLimit,0)

!本營業日
#DATE=D,8,S
T(4,DATE$)

#LDATE1=D,8,S
C(3,#CHAIN,1,$,s)
T(1,@LDATE1,#NTXBUF,1,7)
E(0,@AcDateStart,#LDATE1)

#LDATE=D,8,S
T(4,LBSDY$)
C(3,#CHAIN,1,$,s)
T(1,@LDATE,#NTXBUF,8,7)
E(0,@AcDateEnd,#LDATE)

!入帳日期起止
#AcDateStart=D,7,I
C(3,#CHAIN,0,E(0,#DATE),s)
D(8)	
@V(7)A(4,0,#AcDateStart)V(5,0010101,#DATE)

#AcDateEnd=D,7,I
C(3,#CHAIN,0,E(0,#DATE),s)
D(8)
@V(7)A(4,0,#AcDateEnd)V(5,#AcDateStart,#DATE)

! 樣態一-金額合計超過
!#Factor1TotLimit=m,14,S

! 樣態二-次數
!#Factor2Count=N,4,S

! 樣態二-單筆金額起迄
!#Factor2AmtStart=m,14,S

!#Factor2AmtEnd=m,14,S

! 樣態三-金額合計超過
!#Factor3TotLimit=m,14,S

!#CHAINXMT=X,1,S
!C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

#RPTFG=A,1,S
E(0,0)


<include src="ROM.icf::L0Rim.dec"/>
<include src="ROM.icf::L8R49.dec"/>
!<include src="COM.icf::RPTFG.dec"/>

</sub>


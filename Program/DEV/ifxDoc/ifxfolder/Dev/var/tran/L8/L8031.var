#OONotBankId2=X,3,S
#OONotBankId2X=X,50,S
#OOBankId_Combine2=X,60,S
T(2,#OONotBankId2+ +#OONotBankId2X)
#OONotBankId3=X,3,S
#OONotBankId3X=X,50,S
#OOBankId_Combine3=X,60,S
T(2,#OONotBankId3+ +#OONotBankId3X)
#OONotBankId4=X,3,S
#OONotBankId4X=X,50,S
#OOBankId_Combine4=X,60,S
T(2,#OONotBankId4+ +#OONotBankId4X)
#OONotBankId5=X,3,S
#OONotBankId5X=X,50,S
#OOBankId_Combine5=X,60,S
T(2,#OONotBankId5+ +#OONotBankId5X)
#OONotBankId6=X,3,S
#OONotBankId6X=X,50,S
#OOBankId_Combine6=X,60,S
T(2,#OONotBankId6+ +#OONotBankId6X)
! 以下固定
#OOLastUpdate=X,30,S
#OOLastUpdateEmpNo=X,6,S
#OOLastUpdateEmpNoName=X,50,S
#OOLastUpdate_Combine=X,57,L
T(2,#OOLastUpdateEmpNo+ +#OOLastUpdateEmpNoName)

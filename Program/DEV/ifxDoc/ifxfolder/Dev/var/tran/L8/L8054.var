]

#FdCustId=X,2,L
#FdRcDate=X,2,L
#FdSubmitKey=X,2,L
#FdCourtCode=X,2,L
#FdMaxMainCode=X,2,L
#FdAccount=X,2,L

!---------- AP TEXT AREA Variables ----------
#CHAIN=A,1,S
T(4,CHAIN$)
C(3,#CHAIN,0,V(P,本交易為連動交易，請由L8030連動進入),$)

<include src="HELPRIM.icf::HELPRIM.rtn"/>
<include src="ROM.icf::L8R01.dec"/>
#CODEF0001=x,20,S
T(3,CdCode.TranKey)
#TranKeyHelp=x,1024,S

#CODEF0002=x,25,S
T(3,CdCode.CollateralType)
#CollateralTypeHelp=x,10000,S

! CourtCode
#CODEF0003=x,20,S
T(3,CdCode.CourtCode)
#CourtCodeHelp=x,1024,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CODEF0001,#CODEF0002,#CODEF0003)
R(1,XXR99)
CALL(#HelpProc)
T(2,@TranKeyHelp,#HelpDesc1)
T(2,@CollateralTypeHelp,#HelpDesc2)
T(2,@CourtCodeHelp,#HelpDesc3)

! 預設值
#Default=X,1,S
T(2,@MaxMainCode,458)
#RimJcicBankCode=X,3,S
#RimL8r01=X,1,S
T(2,@RimJcicBankCode,#MaxMainCode)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@MaxMainCodeX,#L8r01JcicBankName)

#NTXBUF=X,138,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@ChainCd,#NTXBUF,1,5)
T(1,@Ukey,#NTXBUF,6,32)

#CustId=X,10,L
#CustIdX=X,2.50,L

#RcDate=D,7,L
#SubmitKey=X,3,L
#CourtCode=X,3,L
#MaxMainCode=X,3,L
#Account=x,50,L
#MaxMainCodeX=X,80,L
#CourtCodeX=X,80,L
<include src="ROM.icf::L8R55.dec"/>
<include src="ROM.icf::L8R59.dec"/>
#RimUkey=X,32,S
#RimChainCd=X,5,S
#RimCourtCode=X,3,S
#RimL8r55=X,1,S
T(2,@RimUkey,#Ukey)
T(2,@RimChainCd,#ChainCd)
RESET_RIM(#RimL8r55,L8R55)
S(L8R55,1,#RimChainCd,#RimUkey)
R(1,L8R55)
T(2,@CustId,#L8r55CustId)
T(2,@SubmitKey,#L8r55SubmitKey)
E(0,@RcDate,#L8r55ApplyDate)
T(2,@CourtCode,#L8r55CourtCode)
T(2,@MaxMainCode,#L8r55MaxMainCode)
T(2,@Account,#L8r55Account)
#Lr859=X,1,S
T(2,@RimCourtCode,#CourtCode)
S(L8R59,1,#RimCourtCode)
R(1,L8R59)
T(2,@CourtCodeX,#L8R59CourtCodeX)
#ChainCd=X,5,S
#Ukey=X,32,S
#RPTFG=A,1,S

#RimCustId=X,10,S
<include src="ROM.icf::L8R57.dec"/>
T(2,@RimCustId,#CustId)
! 檢核該統編是否存在於客戶檔
#RimL8R57=X,1,S
RESET_RIM(#RimL8R57,L8R57)
S(L8R57,1,#RimCustId)
R(1,L8R57)
T(2,@CustIdX,#L8R57CustName)

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)
</sub>


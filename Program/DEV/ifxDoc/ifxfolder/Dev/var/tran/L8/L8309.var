!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=3;screen.width=[300,150,250,150];printer.width=[20,20,20,20];order=1;">
[
["[L8309]                                 (48)債務人基本資料"],
[#<->#,"#FdTranKey+交易代碼",#TranKey,#TranKeyX],
[#<->#,"#FdCustId+債務人IDN",#CustId,#CustIdX],
[#<->#,"#FdSubmitKey+報送單位代號",#SubmitKey,#SubmitKeyX],
[#<->#,"#FdRcDate+協商申請日",#RcDate],
[#<->#,"#FdCustRegAddr+債務人戶籍之郵遞區號及地址",#CustRegAddr],
[#<->#,"#FdCustComAddr+債務人通訊地之郵遞區號及地址",#CustComAddr],
[#<->#,"#FdCustRegTelNo+債務人戶籍電話<font color=red>例:02-28225252",#CustRegTelNo],
[#<->#,"#FdCustComTelNo+債務人通訊電話",#CustComTelNo],
[#<->#,"#FdCustMobilNo+債務人行動電話",#CustMobilNo],
[#<->#,"#FdOutJcicTxtDate+轉JCIC文字檔日期",#OutJcicTxtDate],
]

#FdTranKey=X,2,L
#FdCustId=X,2,L
#FdSubmitKey=X,2,L
#FdRcDate=X,2,L
#FdCustRegAddr=X,2,L
#FdCustComAddr=X,2,L
#FdCustRegTelNo=X,2,L
#FdCustComTelNo=X,2,L
#FdCustMobilNo=X,2,L
#FdOutJcicTxtDate=X,2,L

!---------- AP Button AREA ----------
![Buttom資料查詢]
!---------- 日期處理 ----------
#VIEW_SYSDATE=X,8,S
D(7,2,#SYSDATE,0,0)
T(1,#VIEW_SYSDATE,2,7)
E(0,1)ASGN(AGAIN$)
!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>
<include src="ROM.icf::L8R01.dec"/>

#CDDEF0001=x,20,S
T(3,CdCode.TranKey)
#TranKeyHelp=x,1024,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF0001)
R(1,XXR99)
CALL(#HelpProc)
T(2,@TranKeyHelp,#HelpDesc1)

#CHAIN=A,1,S
T(4,CHAIN$)
C(3,#CHAIN,0,V(P,本交易為連動交易，請由L8030-Jcic048連動進入),$)
T(F,@TranKey,0)T(F,@RcDate,0)T(F,@CustRegAddr,0)T(F,@CustComAddr,0)
T(F,@CustRegTelNo,0)T(F,@CustComTelNo,0)T(F,@CustMobilNo,0)
#AutoReFresh=A,1,S
C(3,#CHAIN,1,$,S)
E(0,1)
ASGN(CAUTO$)

![連動交易]
#NTXBUF=X,138,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@TranKey_Tmp,#NTXBUF,1,1)
T(1,@TranKey,#NTXBUF,2,1)
! 連動傳值處理

! 新增進入
#Deal_New=X,1,S
C(5,#TranKey_Tmp,1,$,S)
! T(2,@TranKey,A)
T(1,@CustId,#NTXBUF,3,10)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(F,@RcDate,1)T(F,@CustRegAddr,1)T(F,@CustComAddr,1)
T(F,@CustRegTelNo,1)T(F,@CustComTelNo,1)T(F,@CustMobilNo,1)
K(MODIFYBUTTON,新增)
T(2,@FdRcDate,*)
T(2,@FdCustRegAddr,*)
T(2,@FdCustComAddr,*)
T(2,@FdCustRegTelNo,*)
T(2,@FdCustComTelNo,*)
T(2,@FdCustMobilNo,*)

! 異動進入
#Deal_Update=X,1,S
C(5,#TranKey_Tmp,2,$,S)
! T(2,@TranKey,C)
T(F,@TranKey,1)T(F,@CustRegAddr,1)T(F,@CustComAddr,1)
T(F,@CustRegTelNo,1)T(F,@CustComTelNo,1)T(F,@CustMobilNo,1)
T(1,@Ukey,#NTXBUF,3,32)
K(MODIFYBUTTON,異動)
T(2,@FdTranKey,*)
T(2,@FdCustRegAddr,*)
T(2,@FdCustComAddr,*)
T(2,@FdCustRegTelNo,*)
T(2,@FdCustComTelNo,*)
T(2,@FdCustMobilNo,*)
T(3,@TranKeyHelp,C:異動;X:補件))

! 查詢進入
#Deal_Search=X,1,S
C(5,#TranKey_Tmp,5,$,S)
T(1,@Ukey,#NTXBUF,3,32)
K(NOBUTTON,CLOSEY)

! 刪除進入
#Deal_Delete=X,1,S
C(5,#TranKey_Tmp,4,$,S)
T(1,@Ukey,#NTXBUF,3,32)
K(MODIFYBUTTON,刪除)

! 預設值
#Default=X,1,S
T(2,@SubmitKey,458)
! Rim L8R21.java
#RimJcicBankCode=X,3,S
#RimL8r01=X,1,S
T(2,@RimJcicBankCode,#SubmitKey)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@SubmitKeyX,#L8r01JcicBankName)

! [調Rim相關]
! 定義Rom欄位名稱、型態、長度
<include src="ROM.icf::L8R20.dec"/>
#RimUkey=X,32,S
#RimL8r20=X,1,S
C(5,#TranKey,A,S,$)
T(2,@RimUkey,#Ukey)
RESET_RIM(#RimL8r20,L8R20)
S(L8R20,1,#RimUkey)
R(1,L8R20)
T(2,@TranKey,#L8r20TranKey)
T(2,@CustId,#L8r20CustId)
T(2,@SubmitKey,#L8r20SubmitKey)
E(0,@RcDate,#L8r20RcDate)
T(2,@CustRegAddr,#L8r20CustRegAddr)
T(2,@CustComAddr,#L8r20CustComAddr)
T(2,@CustRegTelNo,#L8r20CustRegTelNo)
T(2,@CustComTelNo,#L8r20CustComTelNo)
T(2,@CustMobilNo,#L8r20CustMobilNo)
E(0,@OutJcicTxtDate,#L8r20OutJcicTxtDate)
![下拉選單的值]
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
#Space=X,1,S
IF(#TranKey_Tmp == "5" || #TranKey_Tmp=="4",$,S)
T(2,@TranKey,#L8r20TranKey)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
! 調Rim相關 結束

![交易代碼]
#TranKey_Tmp=X,1,S
#TranKey=X,1,I
C(5,#TranKey_Tmp,1,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
HELP(#TranKeyHelp)
@V(H,#TranKeyHelp)
C(5,#TranKey,C,K(MODIFYBUTTON,異動)T(F,@TranKey,1)T(F,@CustRegAddr,1)T(F,@CustComAddr,1)
T(F,@CustRegTelNo,1)T(F,@CustComTelNo,1)T(F,@CustMobilNo,1)
T(2,@FdCustRegAddr,*)T(2,@FdCustComAddr,*)T(2,@FdCustRegTelNo,*)
T(2,@FdCustComTelNo,*)T(2,@FdCustMobilNo,*),
C(5,#TranKey,X,K(MODIFYBUTTON,補件)T(F,@CustRegAddr,0)T(F,@CustComAddr,0)
T(F,@CustRegTelNo,0)T(F,@CustComTelNo,0)T(F,@CustMobilNo,0)
T(2,@FdCustRegAddr, )T(2,@FdCustComAddr, )T(2,@FdCustRegTelNo, )
T(2,@FdCustComTelNo, )T(2,@FdCustMobilNo, ),$))

#TranKeyX=X,4,L
C(5,#TranKey_Tmp,1,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
T(H,#TranKey,#TranKeyHelp)
![債務人IDN]
#CustId=X,10,L
#RimCustId=X,10,S
<include src="ROM.icf::L8R57.dec"/>
T(2,@RimCustId,#CustId)
! 檢核該統編是否存在於客戶檔
#RimL8R57=X,1,S
RESET_RIM(#RimL8R57,L8R57)
S(L8R57,1,#RimCustId)
R(1,L8R57)
T(2,@CustIdX,#L8R57CustName)

#CustIdX=X,2.50,L

![報送單位代號]
#SubmitKey=X,3,L

#SubmitKeyX=X,80,L


![協商申請日]
#RcDate=D,7,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
D(8)
@V(7)
@A(DATE,0,#RcDate)

!債務人戶籍之郵遞區號及地址 只能存38個全形中文字
#CustRegAddr=X,114,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,X,T(2,@CustRegAddr,#L8r20CustRegAddr),$)
C(5,#TranKey,X,s,$)
@V(8,0,38)

!債務人通訊地之郵遞區號及地址 只能存38個全形中文字
#CustComAddr=X,114,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,X,T(2,@CustComAddr,#L8r20CustComAddr),$)
C(5,#TranKey,X,s,$)
@V(8,0,38)

!債務人戶籍電話
#CustRegTelNo=X,16,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,X,T(2,@CustRegTelNo,#L8r20CustRegTelNo),$)
C(5,#TranKey,X,s,$)
@V(8,0,16)

!債務人通訊電話
#CustComTelNo=X,16,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,X,T(2,@CustComTelNo,#L8r20CustComTelNo),$)
C(5,#TranKey,X,s,$)
@V(8,0,16)

!債務人行動電話
#CustMobilNo=X,16,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,X,T(2,@CustMobilNo,#L8r20CustMobilNo),$)
C(5,#TranKey,X,s,$)

#CheckConnectTel=X,1,S
IF(#CustRegTelNo=="" && #CustComTelNo=="" && #CustMobilNo=="",$,s)
V(P,債務人戶籍電話，通訊電話，行動電話必須至少填寫一項)

![轉JCIC文字檔日期]
#OutJcicTxtDate=D,7,L

! 流水號
#Ukey=X,32,S

! !<儲存>
! #BUF1=X,10,S
! T(2,#CUSTID)
! #BIND1=X,1,S
! BIND(#BTN1,click, {cmd=CHAIN; ntxcd=#CHAINCD; ntxbuf=#BUF1})

#RPTFG=X,1,S
E(0,0)

</sub>


!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L8924]                                 疑似洗錢資料變更查詢"],
["#FdTRN_CODE+交易代號",#TRN_CODE,#TRN_CODEX],
["#FdCDATESTART+交易日期",#CDATESTART,"~",#CDATEEND],
["#FdCUST_NO+戶號",#CUST_NO],
]
!---------- AP Button AREA ----------


#FdTRN_CODE=X,2,L
T(3,*)
#FdCDATESTART=X,2,L
T(3,*)
#FdCUST_NO=X,2,L
T(3,*)

#Submit=A,1,S
K(MODIFYBUTTON,查詢)
!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>

#TypeHelp=x,100,S
T(3,1:L8203疑似洗錢交易合理性維護;2:L8204疑似洗錢交易訪談記錄維護)

#RimFunCd=A,1,S
#RimCustId=X,10,S
#RimCustNo=A,7,S

#CHAIN=A,1,S
T(4,CHAIN$)

#NTXBUF=X,32,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)

#CHAIN_Code=X,1,S
T(1,#NTXBUF,1,1)

#CHAIN_CutNo=X,7,S
T(1,#NTXBUF,2,7)

#CHAINSET=X,1,S
C(3,#CHAIN,1,$,S)
E(0,@TRN_CODE,#CHAIN_Code)
E(0,@CUST_NO,#CHAIN_CutNo)



#RPTFG=A,1,S
E(0,0)

! 作業日期
#DATE=D,8,S
T(4,DATE$)



! 交易代號
#TRN_CODE=A,1,I
C(3,#CHAIN,0,$,s)
HELP(#TypeHelp)
@V(H,#TypeHelp)

#TRN_CODEX=X,30,L
T(H,#TRN_CODE,#TypeHelp)

#SUMDATE1=N,7,S
T(1,#DATE,1,4)
E(0,@SUMDATE1,#SUMDATE1-1)

#SUMDATE2=N,3,S
E(0,@SUMDATE2,#SUMDATE1)


!去年交易日期
#SUMDATE3=X,7,S
T(1,#DATE,5,8)
T(2,#SUMDATE2+#SUMDATE3)


!交易日期起日
#CDATESTART=D,7,I
E(0,#SUMDATE3)
C(3,#CHAIN,0,$,s)
D(8)
@V(7)A(DATE,0,#CDATESTART)
V(5,0010101,#DATE)

!交易日期止日
#CDATEEND=D,7,I
E(0,#DATE)
C(3,#CHAIN,0,$,s)
D(8)
@V(7)A(DATE,0,#CDATEEND)
V(5,#CDATESTART,#DATE)

! 戶號
#CUST_NO=A,7,I
C(3,#CHAIN,0,$,s)
@V(2,0)
#CustNo=A,7,L
#RimL1r09=X,1,S
C(3,#CUST_NO,0,s,$)
E(0,@RimFunCd,1)
T(2,@RimCustId,)
E(0,@RimCustNo,#CUST_NO)
RESET_RIM(#RimL1r09,L1R09)
S(L1R09,1,#RimFunCd,#RimCustId,#RimCustNo)
R(1,L1R09)
T(2,@CustName,#L1r09CustName)

#CustName=x,2.50,L


#MRKEY=_,_,S
C(3,#CUST_NO,0,S,T(2,#CUST_NO)) 

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

<include src="ROM.icf::L1R09.dec"/>

</sub>


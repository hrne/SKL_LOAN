!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=3;screen.width=[250,250,300,200];printer.width=[20,20,20,20];order=1;">
[
["[L8334]                                 (572)前置調解無擔保債務分配表資料"],
[#<->#,"#FdTranKey+交易代碼",#TranKey,#TranKeyX],
[#<->#,"#FdCustId+債務人IDN",#CustId],
[#<->#,"#FdSubmitKey+報送單位代號",#SubmitKey,#SubmitKeyX],
["#FdApplyDate+調解申請日",#ApplyDate],["#FdStartDate+生效日期",#StartDate],
["#FdPayDate+本分配表首繳日",#PayDate],["#FdBankId+債權金融機構代號",#BankId,#BankIdX],
["#FdAllotAmt+參與分配債權金額",#AllotAmt],["#FdOwnPercentage+債權比例",#OwnPercentage],
["#FdOutJcicTxtDate+轉JCIC文字檔日期",#OutJcicTxtDate],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
#FdTranKey=X,2,L
#FdCustId=X,2,L
#FdSubmitKey=X,2,L
#FdApplyDate=X,2,L
#FdStartDate=X,2,L
#FdPayDate=X,2,L
#FdBankId=X,2,L
#FdAllotAmt=X,2,L
#FdOwnPercentage=X,2,L
#FdOutJcicTxtDate=X,2,L
!---------- 日期處理 ----------
#VIEW_SYSDATE=X,8,S
D(7,2,#SYSDATE,0,0)
T(1,#VIEW_SYSDATE,2,7)
E(0,1)ASGN(AGAIN$)
!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>
#CDDEF0001=x,20,S
T(3,CdCode.TranKey)
#TranKeyHelp=x,1024,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF0001)
R(1,XXR99)
CALL(#HelpProc)
T(2,@TranKeyHelp,#HelpDesc1)

#CHAIN=A,1,S
T(4,CHAIN$)
C(3,#CHAIN,0,V(P,本交易為連動交易，請由L8030-Jcic572連動進入),$)
T(F,@TranKey,0)T(F,@ApplyDate,0)T(F,@StartDate,0)T(F,@PayDate,0)T(F,@BankId,0)T(F,@AllotAmt,0)T(F,@OwnPercentage,0)
#AutoReFresh=A,1,S
C(3,#CHAIN,1,$,S)
E(0,1)
ASGN(CAUTO$)


![連動交易]
#NTXBUF=X,138,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@TranKey_Tmp,#NTXBUF,1,1)
T(1,@TranKey,#NTXBUF,2,1)



! 新增進入
#Deal_New=X,1,S
C(5,#TranKey_Tmp,1,$,S)
T(1,@CustId,#NTXBUF,3,10)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(F,@ApplyDate,1)T(F,@StartDate,1)T(F,@PayDate,1)T(F,@BankId,1)T(F,@AllotAmt,1)T(F,@OwnPercentage,1)
K(MODIFYBUTTON,新增)
T(2,@FdApplyDate,*)
T(2,@FdStartDate,*)
T(2,@FdPayDate,*)
T(2,@FdBankId,*)
T(2,@FdAllotAmt,*)
T(2,@FdOwnPercentage,*)

! 異動進入
#Deal_Update=X,1,S
C(5,#TranKey_Tmp,2,$,S)
T(1,@Ukey,#NTXBUF,3,32)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(F,@StartDate,1)T(F,@AllotAmt,1)T(F,@OwnPercentage,1)
K(MODIFYBUTTON,異動)
T(2,@FdTranKey,*)
T(2,@FdStartDate,*)
T(2,@FdAllotAmt,*)
T(2,@FdOwnPercentage,*)

! 查詢進入
#Deal_Search=X,1,S
C(5,#TranKey_Tmp,5,$,S)
T(1,@Ukey,#NTXBUF,3,32)
K(NOBUTTON,CLOSEY)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)

! 刪除進入
#Deal_Delete=X,1,S
C(5,#TranKey_Tmp,4,$,S)
T(1,@Ukey,#NTXBUF,3,32)
K(MODIFYBUTTON,刪除)


! 預設值
#Default=X,1,S
T(2,@SubmitKey,458)
<include src="ROM.icf::L8R01.dec"/>
#RimJcicBankCode=X,3,S
#RimL8r01=X,1,S
T(2,@RimJcicBankCode,#SubmitKey)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@SubmitKeyX,#L8r01JcicBankName)


<include src="ROM.icf::L8R35.dec"/>
! Rim L8R35.java
#RimUkey=X,32,S
#RimL8r35=X,1,S
C(5,#TranKey,A,S,$)
T(2,@RimUkey,#Ukey)
RESET_RIM(#RimL8r35,L8R35)
S(L8R35,1,#RimUkey)
R(1,L8R35)
T(2,@CustId,#L8r35CustId)
T(2,@SubmitKey,#L8r35SubmitKey)
E(0,@ApplyDate,#L8r35ApplyDate)
E(0,@StartDate,#L8r35StartDate)
E(0,@PayDate,#L8r35PayDate)
T(2,@BankId,#L8r35BankId)
E(0,@AllotAmt,#L8r35AllotAmt)
E(0,@OwnPercentage,#L8r35OwnPercentage)
E(0,@OutJcicTxtDate,#L8r35OutJcicTxtDate)
![下拉選單的值]
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
#Space=X,1,S
IF(#TranKey_Tmp == "5" || #TranKey_Tmp=="4",$,S)
T(2,@TranKey,#L8r35TranKey)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(2,@RimJcicBankCode,#BankId)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@BankIdX,#L8r01JcicBankName)
! 調Rim相關 結束



![交易代碼]
#TranKey_Tmp=X,1,S
#TranKey=X,1,L
C(5,#TranKey_Tmp,1,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
HELP(#TranKeyHelp)
@V(H,#TranKeyHelp)

#TranKeyX=X,4,L


![債務人IDN]
#CustId=X,10,L


![債權金融機構代號]
#SubmitKey=X,3,L
#SubmitKeyX=X,80,L


!調解申請日
#ApplyDate=D,7,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
D(8)
@V(7)
@A(DATE,0,#ApplyDate)

!生效日期
#StartDate=D,7,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
D(8)
@V(7)A(DATE,0,#StartDate)

!本分配表首繳日
#PayDate=D,7,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
D(8)
@V(7)A(DATE,0,#PayDate)

!債權金融機構代號
#BankId=X,3,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
@V(7)
T(2,@RimJcicBankCode,#BankId)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@BankIdX,#L8r01JcicBankName)

#BankIdX=X,80,L
T(2,@RimJcicBankCode,#BankId)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@BankIdX,#L8r01JcicBankName)

!參與分配債權金額
#AllotAmt=M,9,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)

@V(2,0)

!債權比例
#OwnPercentage=m,3.2,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
@V(2,0)

!轉JCIC文字檔日期
#OutJcicTxtDate=D,7,L


! 流水號
#Ukey=X,32,S

#RPTFG=X,1,S
E(0,0) 
</sub>


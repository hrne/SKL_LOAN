E(0,@Factor2AmtEnd,#L8R49Factor2AmtEnd)
E(0,@FactorDays3,#L8R49FactorDays3)
E(0,@Factor3TotLimit,#L8R49Factor3TotLimit)
T(F,@FactorDays,0)
T(F,@Factor1TotLimit,0)
T(F,@Factor2Count,0)
T(F,@Factor2AmtStart,0)
T(F,@Factor2AmtEnd,0)
T(F,@FactorDays3,0)
T(F,@Factor3TotLimit,0)

!本營業日
#DATE=D,8,S
T(4,DATE$)

!上營業日
#LDATE=D,8,S
T(4,LBSDY$)

!入帳日期起止
#AcDateStart=D,7,I
E(0,#LDATE)

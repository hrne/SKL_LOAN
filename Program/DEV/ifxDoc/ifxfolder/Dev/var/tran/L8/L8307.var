!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=3;screen.width=[350,150,350,150];printer.width=[20,20,20,20];order=1;">
[
["[L8307]                                 (46)結案通知資料檔案格式"],
[#<->#,"#FdTranKey+交易代碼",#TranKey,#TranKeyX],
[#<->#,"#FdCustId+債務人IDN",#CustId,#CustIdX],
[#<->#,"#FdSubmitKey+報送單位代號",#SubmitKey,#SubmitKeyX],
["#FdRcDate+協商申請日",#RcDate],
[#<->#,"#FdCloseCode+結案原因代號",#CloseCode,#CloseCodeX],
[#<->#,"#FdCloseDate+結案日期",#CloseDate],
[#<->#,"#FdBreakCode+毀諾原因代號",#BreakCode,#BreakCodeX],
[#<->#,"#FdOutJcicTxtDate+轉JCIC文字檔日期",#OutJcicTxtDate],
]

#FdTranKey=X,2,L
#FdCustId=X,2,L
#FdSubmitKey=X,2,L
#FdRcDate=X,2,L
#FdCloseCode=X,2,L
#FdBreakCode=X,2,L
#FdCloseDate=X,2,L
#FdOutJcicTxtDate=X,2,L

!---------- AP Button AREA ----------
![Buttom資料查詢]
!---------- 日期處理 ----------
#VIEW_SYSDATE=X,8,S
D(7,2,#SYSDATE,0,0)
T(1,#VIEW_SYSDATE,2,7)
E(0,1)ASGN(AGAIN$)
!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>
<include src="ROM.icf::L8R01.dec"/>

#CDDEF0001=x,20,S
T(3,CdCode.TranKey)
#TranKeyHelp=x,1024,S

! CloseCode
#CDDEF0002=x,20,S                  
T(3,CdCode.CloseCode) 
#CloseCodeHelp=x,1024,S

! BreakCode
#CDDEF0003=x,20,S                  
T(3,CdCode.BreakCode) 
#BreakCodeHelp=x,1024,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF0001,#CDDEF0002,#CDDEF0003)
R(1,XXR99)
CALL(#HelpProc)
T(2,@TranKeyHelp,#HelpDesc1)
T(2,@CloseCodeHelp,#HelpDesc2)
T(2,@BreakCodeHelp,#HelpDesc3)

#CHAIN=A,1,S
T(4,CHAIN$)
C(3,#CHAIN,0,V(P,本交易為連動交易，請由L8030-Jcic046連動進入),$)
T(F,@TranKey,0)T(F,@SubmitKey,0)T(F,@RcDate,0)T(F,@CloseCode,0)T(F,@BreakCode,0)T(F,@CloseDate,0)
#AutoReFresh=A,1,S
C(3,#CHAIN,1,$,S)
E(0,1)
ASGN(CAUTO$)

![連動交易]
#NTXBUF=X,138,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@TranKey_Tmp,#NTXBUF,1,1)
T(1,@TranKey,#NTXBUF,2,1)
! 連動傳值處理

! 新增進入
#Deal_New=X,1,S
C(5,#TranKey_Tmp,1,$,S)
! T(2,@TranKey,A)
T(1,@CustId,#NTXBUF,3,10)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(F,@RcDate,1)T(F,@SubmitKey,1)T(F,@CloseCode,1)T(F,@BreakCode,1)T(F,@CloseDate,1)
K(MODIFYBUTTON,新增)
T(2,@FdRcDate,*)
T(2,@FdCloseCode,*)
T(2,@FdBreakCode,*)
T(2,@FdCloseDate,*)
T(2,@FdSubmitKey,*)
! 異動進入
#Deal_Update=X,1,S
C(5,#TranKey_Tmp,2,$,S)
! T(2,@TranKey,C)
T(F,@TranKey,1)T(F,@CloseCode,1)T(F,@BreakCode,1)
K(MODIFYBUTTON,異動)
T(1,@Ukey,#NTXBUF,3,32)
T(2,@FdCloseCode,*)
T(2,@FdBreakCode,*)
T(2,@FdTranKey,*)
T(3,@TranKeyHelp,C:異動;D:刪除))

! 查詢進入
#Deal_Search=X,1,S
C(5,#TranKey_Tmp,5,$,S)
T(1,@Ukey,#NTXBUF,3,32)
K(NOBUTTON,CLOSEY)

! 刪除進入
#Deal_Delete=X,1,S
C(5,#TranKey_Tmp,4,$,S)
T(1,@Ukey,#NTXBUF,3,32)
K(MODIFYBUTTON,刪除)


! 預設值
#Default=X,1,S
T(2,@SubmitKey,458)
#RimJcicBankCode=X,3,S
#RimL8r01=X,1,S
T(2,@RimJcicBankCode,#SubmitKey)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@SubmitKeyX,#L8r01JcicBankName)

! [調Rim相關]
! 定義Rom欄位名稱、型態、長度
<include src="ROM.icf::L8R17.dec"/>
#RimUkey=X,32,S
#RimL8r17=X,1,S
C(5,#TranKey,A,S,$)
T(2,@RimUkey,#Ukey)
RESET_RIM(#RimL8r17,L8R17)
S(L8R17,1,#RimUkey)
R(1,L8R17)
T(2,@TranKey,#L8r17TranKey)
T(2,@CustId,#L8r17CustId)
T(2,@SubmitKey,#L8r17SubmitKey)
E(0,@RcDate,#L8r17RcDate)
E(0,@CloseDate,#L8r17CloseDate)
T(2,@CloseCode,#L8r17CloseCode)
T(2,@BreakCode,#L8r17BreakCode)
E(0,@OutJcicTxtDate,#L8r17OutJcicTxtDate)
![下拉選單的值]
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(H,@CloseCodeX,#CloseCode,#CloseCodeHelp)
T(H,@BreakCodeX,#BreakCode,#BreakCodeHelp)
#Space=X,1,S
IF(#TranKey_Tmp == "5" || #TranKey_Tmp=="4",$,S)
T(2,@TranKey,#L8r17TranKey)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
! 調Rim相關 結束

![交易代碼]
#TranKey_Tmp=X,1,S
#TranKey=X,1,I
C(5,#TranKey_Tmp,1,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
HELP(#TranKeyHelp)
@V(H,#TranKeyHelp)
C(5,#TranKey,C,K(MODIFYBUTTON,異動)T(F,@CloseCode,1)T(F,@BreakCode,1)T(2,@FdCloseCode,*)T(2,@FdBreakCode,*),
C(5,#TranKey,D,K(MODIFYBUTTON,刪除)T(F,@CloseCode,0)T(F,@BreakCode,0)T(2,@FdCloseCode, )T(2,@FdBreakCode, ),$))


#TranKeyX=X,4,L
C(5,#TranKey_Tmp,1,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
T(H,#TranKey,#TranKeyHelp)

![債務人IDN]
#CustId=X,10,L
#RimCustId=X,10,S
<include src="ROM.icf::L8R57.dec"/>
T(2,@RimCustId,#CustId)
! 檢核該統編是否存在於客戶檔
#RimL8R57=X,1,S
RESET_RIM(#RimL8R57,L8R57)
S(L8R57,1,#RimCustId)
R(1,L8R57)
T(2,@CustIdX,#L8R57CustName)

#CustIdX=X,2.50,L

![報送單位代號]
#SubmitKey=X,3,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
@V(7)
#SubmitKeyX=X,80,L
T(2,@RimJcicBankCode,#SubmitKey)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@SubmitKeyX,#L8r01JcicBankName)


![協商申請日]
#RcDate=D,7,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
D(8)
@V(7)A(DATE,0,#RcDate)


!結案原因代號
#CloseCode=X,2,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,D,T(2,@CloseCode,#L8r17CloseCode),$)
C(5,#TranKey,D,s,$)
HELP(#CloseCodeHelp,cols:3)
@V(H,#CloseCodeHelp)

#ReBreakCode=X,1,S
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,D,s,$)
C(5,#CloseCode,00,T(2,@FdBreakCode,*)T(F,@BreakCode,1),T(2,@FdBreakCode, )T(F,@BreakCode,0)T(2,@BreakCode, )T(2,@BreakCodeX, ))

#CloseCodeX=X,50,L
T(H,#CloseCode,#CloseCodeHelp)

! 會計日(民國年)
#SDate=D,7,S
T(1,#SYSDATE,2,8)

!結案日期
#CloseDate=D,7,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
D(8)
@V(7)A(DATE,0,#CloseDate)
IF(#CloseDate<#RcDate,V(P,結案日不可早於協商申請日),$)
IF(#CloseDate>#SDate,V(P,結案日不可晚於報送日),$)

!毀諾原因代號
#BreakCode=X,2,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey,D,T(2,@BreakCode,#L8r17BreakCode),$)
C(5,#TranKey,D,s,$)
C(5,#CloseCode,00,$,S)
HELP(#BreakCodeHelp)
@V(H,#BreakCodeHelp)
#BreakCodeX=X,50,L
C(5,#CloseCode,00,$,S)
T(H,#BreakCode,#BreakCodeHelp)


![轉JCIC文字檔日期]
#OutJcicTxtDate=D,7,L

! 流水號
#Ukey=X,32,S

! !<儲存>
! #BUF1=X,10,S
! T(2,#CUSTID)
! #BIND1=X,1,S
! BIND(#BTN1,click, {cmd=CHAIN; ntxcd=#CHAINCD; ntxbuf=#BUF1})

![列印用系統變數]
#RPTFG=X,1,S
E(0,0)
</sub>


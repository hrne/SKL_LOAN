!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L8923]                                 疑似洗錢交易訪談記錄查詢"],
["#FdSearch+查詢條件","(訪談日期、還款日期，擇一輸入)"],
["#FdRecordDate+訪談日期  ",#RecordDateStart,"~",#RecordDateEnd],
["#FdActualRepayDate+還款日期",#ActualRepayDateStart,"~",#ActualRepayDateEnd],
["#FdCustNo+戶號","#CustNo],
["#FdCustName+戶名","#CustName],
!["#FdRPTFG+顯示方式",
!<include src="COM.icf::RPTFG.scr"/>
!],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
!#BUF1=X,42,S

#FdSearch=X,2,L
T(3,*)

#FdRecordDate=X,2,L
T(3, )
#FdActualRepayDate=X,2,L
T(3, )
#FdCustNo=X,2,L
T(3, )
#FdCustName=X,2,L
T(3, )
#FdRPTFG=X,2,L
T(3,*)
!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>
#ApHelp=x,1024,S
T(3,CdCode.RepaySource)
S(XXR99,01,#ApHelp)
R(1,XXR99)
CALL(#HelpProc)
T(2,#HelpDesc1)


#CHAIN=A,1,S
T(4,CHAIN$)

#NTXBUF=X,4,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)

#button=x,100,S
T(2,{cmd=CHAIN; ntxcd=L8204; ntxbuf=1; ntxbuf5=type})ASGN(BTN_NEW1$)
K(MODIFYBUTTON,1,新增記錄)
K(MODIFYBUTTON,查詢)



#button1=x,100,S
T(2,{cmd=CHAIN; ntxcd=L8205; ntxbuf=1; ntxbuf5=type})ASGN(BTN_NEW2$)
K(MODIFYBUTTON,2,產生疑似洗錢報表)

!日曆日
#CDATE=D,8,S
T(4,CDATE$)

#CDATEX=D,7,S
T(1,#CDATE,2,7)

!本營業日
#DATE=D,8,S
T(4,DATE$)

!訪談日期  起止
#RecordDateStart=D,7,I
T(1,#DATE,2,7)
D(8)
@A(DATE,0,#RecordDateStart)
C(4,#RecordDateStart,S,V(5,0010101,#CDATEX))

#RecordDateEnd=D,7,I
C(4,#RecordDateStart,S,$)
E(0,#RecordDateStart)
D(8)
@A(DATE,0,#RecordDateEnd)
V(5,#RecordDateStart,#CDATEX)

!還款日期  起止
#ActualRepayDateStart=D,7,I
T(1,#DATE,2,7)
D(8)
C(3,#RecordDateStart,0,$,S)
C(3,#RecordDateEnd,0,$,S)
@V(7)A(DATE,0,#ActualRepayDateStart)
V(5,0010101,#CDATEX)

#ActualRepayDateEnd=D,7,I
E(0,#ActualRepayDateStart)
D(8)
C(3,#RecordDateStart,0,$,S)
C(3,#RecordDateEnd,0,$,S)
@V(7)A(DATE,0,#ActualRepayDateEnd)
V(5,#ActualRepayDateStart,#CDATEX)


#CustNo=A,7,I
#CustName=X,50,I

#RPTFG=A,1,S
E(0,0)

!<include src="COM.icf::RPTFG.dec"/>

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!---------- TXFORM = L8923 ----------
<sub type="FORM" name="L8923">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L8923,$,S)

#SCRTIL=_,_,S
T(3,[L8923] 疑似洗錢交易訪談記錄查詢)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

!訪談日期  (TOTA)
#OORecordDate=D,7,L

!戶號(TOTA)
#OOCustNo=A,7,S

!額度號碼(TOTA)
#OOFacmNo=A,3,S

!撥款序號(TOTA)
#OOBormNo=A,3,S

#OOCustNoX=X,15,L
T(2,#OOCustNo+-+#OOFacmNo+-+#OOBormNo)

!戶名(TOTA)
#OOCustName=x,200,L

!預定還款日期(TOTA)
#OORepayDate=D,7,L

!實際還款日期(TOTA)
#OOActualRepayDate=D,7,L

!還款金額(TOTA)
#OORepayAmt=m,14.2,S
#OORepayAmt1=m,14,S
E(0,#OORepayAmt)
#OORepayAmtX=X,17,L
IF(#OORepayAmt1==0,S,$)
X(#OORepayAmt1)

!職業別(TOTA)
#OOCareer=x,40,L

!年收入(萬)(TOTA)
#OOIncome=x,60,L

!還款來源(TOTA)
#OORepaySource=A,2,S
#OORepaySourceX=X,20,L
T(H,#OORepaySource,#ApHelp)

!代償銀行(TOTA)
#OORepayBank=x,20,L

!其他說明(TOTA)
#OODescription=x,120,L

!經辦(TOTA)
#OOEmpNo=X,6,L

!異動日期(TOTA)
#OOUpdate=X,9,L

! 修改的按鈕
#OOBUF2=X,21,S
T(2,2+#OORecordDate+#OOCustNo+#OOFacmNo+#OOBormNo)

#OOBTN2=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L8204;text=修改;ntxbuf=#OOBUF2})

! 明細的按鈕
#OOBUF3=X,20,S
T(2,#OOCustNo+#OOFacmNo+#OOBormNo+#OOActualRepayDate)

#OOBTN3=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L3005;text=明細;ntxbuf=#OOBUF3})

! 刪除的按鈕
#OOBUF4=X,21,S
T(2,4+#OORecordDate+#OOCustNo+#OOFacmNo+#OOBormNo)

#OOBTN4=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L8204;text=刪除;ntxbuf=#OOBUF4})

! 歷程的按鈕
#OOBUF5=X,32,S
T(2,2+#OOCustNo+#OOFacmNo+#OOBormNo+000000000)

#OOBTN5=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L8924;text=歷程;ntxbuf=#OOBUF5})

</sub>



!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=3;screen.width=[50,50,600,300];printer.width=[20,20,20,20];order=1;">
[
["[L8333]                                 (571)更生款項統一收付回報債權資料"],
[#<->#,"#FdTranKey+交易代碼",#TranKey,#TranKeyX],
[#<->#,"#FdCustId+債務人IDN",#CustId,#CustIdX],
[#<->#,"#FdSubmitKey+報送單位代號",#SubmitKey,#SubmitKeyX],
["#FdApplyDate+申請日期",#ApplyDate],["#FdBankId+受理款項統一收付之債權金融機構代號",#BankId,#BankIdX],
["#FdOwnerYn+是否為更生債權人",#OwnerYn],["#FdPayYn+債務人是否仍依更生方案正常還款予本金融機構",#PayYn],
["#FdOwnerAmt+本金融機構更生債權總金額",#OwnerAmt],["#FdAllotAmt+參與分配債權金額",#AllotAmt],
["#FdUnallotAmt+未參與分配債權金額",#UnallotAmt],
[#<->#,"#FdOutJcicTxtDate+轉JCIC文字檔日期",#OutJcicTxtDate],
]

#FdTranKey=X,2,L
#FdCustId=X,2,L
#FdSubmitKey=X,2,L
#FdApplyDate=X,2,L
#FdBankId=X,2,L
#FdOwnerYn=X,2,L
#FdPayYn=X,2,L
#FdOwnerAmt=X,2,L
#FdAllotAmt=X,2,L
#FdUnallotAmt=X,2,L
#FdOutJcicTxtDate=X,2,L

!---------- AP Button AREA ----------
![Buttom資料查詢]
!---------- 日期處理 ----------
#VIEW_SYSDATE=X,8,S
D(7,2,#SYSDATE,0,0)
T(1,#VIEW_SYSDATE,2,7)
E(0,1)ASGN(AGAIN$)
!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>
<include src="ROM.icf::L8R01.dec"/>
#CDDEF0001=x,20,S
T(3,CdCode.TranKey)
#TranKeyHelp=x,1024,S

#CDDEF0002=x,20,S
T(3,CdCode.YesNo)
#YesNoHelp=x,1024,S

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF0001,#CDDEF0002)
R(1,XXR99)
CALL(#HelpProc)
T(2,@TranKeyHelp,#HelpDesc1)
T(2,@YesNoHelp,#HelpDesc2)

#CHAIN=A,1,S
T(4,CHAIN$)
C(3,#CHAIN,0,V(P,本交易為連動交易,請由L8030-Jcic571連動進入),$)
T(F,@TranKey,0)T(F,@ApplyDate,0)T(F,@BankId,0)T(F,@OwnerYn,0)T(F,@PayYn,0)T(F,@OwnerAmt,0)T(F,@AllotAmt,0)T(F,@UnallotAmt,0)
#AutoReFresh=A,1,S
C(3,#CHAIN,1,$,S)
E(0,1)
ASGN(CAUTO$)
![連動交易]
#NTXBUF=X,138,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@TranKey_Tmp,#NTXBUF,1,1)
T(1,@TranKey,#NTXBUF,2,1)

! 新增進入
#Deal_New=X,1,S
C(5,#TranKey_Tmp,1,$,S)
T(1,@CustId,#NTXBUF,3,10)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(F,@ApplyDate,1)T(F,@BankId,1)T(F,@OwnerYn,1)T(F,@PayYn,1)T(F,@OwnerAmt,1)T(F,@AllotAmt,1)T(F,@UnallotAmt,1)
K(MODIFYBUTTON,新增)
T(2,@FdApplyDate,*)
T(2,@FdBankId,*)
T(2,@FdOwnerYn,*)
T(2,@FdPayYn,*)
T(2,@FdOwnerAmt,*)
T(2,@FdAllotAmt,*)
T(2,@FdUnallotAmt,*)

! 異動進入
#Deal_Update=X,1,S
C(5,#TranKey_Tmp,2,$,S)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(F,@OwnerYn,1)T(F,@PayYn,1)T(F,@OwnerAmt,1)T(F,@AllotAmt,1)T(F,@UnallotAmt,1)
T(1,@Ukey,#NTXBUF,3,32)
K(MODIFYBUTTON,異動)
T(2,@FdTranKey,*)
T(2,@FdOwnerYn,*)
T(2,@FdPayYn,*)
T(2,@FdOwnerAmt,*)
T(2,@FdAllotAmt,*)
T(2,@FdUnallotAmt,*)


! 查詢進入
#Deal_Search=X,1,S
C(5,#TranKey_Tmp,5,$,S)
T(1,@Ukey,#NTXBUF,3,32)
K(NOBUTTON,CLOSEY)
! 刪除進入
#Deal_Delete=X,1,S
C(5,#TranKey_Tmp,4,$,S)
T(1,@Ukey,#NTXBUF,3,32)
K(MODIFYBUTTON,刪除)
! 預設值
#Default=X,1,S
T(2,@SubmitKey,458)

#RimJcicBankCode=X,3,S
#RimL8r01=X,1,S
T(2,@RimJcicBankCode,#SubmitKey)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@SubmitKeyX,#L8r01JcicBankName)

! [調Rim相關]
! 定義Rom欄位名稱、型態、長度
! Rim L8R34.java
<include src="ROM.icf::L8R34.dec"/>
#RimUkey=X,32,S
#RimL8r34=X,1,S
C(5,#TranKey,A,S,$)
T(2,@RimUkey,#Ukey)
RESET_RIM(#RimL8r34,L8R34)
S(L8R34,1,#RimUkey)
R(1,L8R34)
T(2,@CustId,#L8r34CustId)
T(2,@SubmitKey,#L8r34SubmitKey)
E(0,@ApplyDate,#L8r34ApplyDate)
T(2,@BankId,#L8r34BankId)
T(2,@OwnerYn,#L8r34OwnerYn)
T(2,@PayYn,#L8r34PayYn)
E(0,@OwnerAmt,#L8r34OwnerAmt)
E(0,@AllotAmt,#L8r34AllotAmt)
E(0,@UnallotAmt,#L8r34UnallotAmt)
E(0,@OutJcicTxtDate,#L8r34OutJcicTxtDate)
![下拉選單的值]
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
#Space=X,1,S
IF(#TranKey_Tmp == "5" || #TranKey_Tmp=="4",$,S)
T(2,@TranKey,#L8r34TranKey)
T(H,@TranKeyX,#TranKey,#TranKeyHelp)
T(2,@RimJcicBankCode,#BankId)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@BankIdX,#L8r01JcicBankName)
! 調Rim相關 結束

![交易代碼]
#TranKey_Tmp=X,1,S
#TranKey=X,1,L
C(5,#TranKey_Tmp,1,s,$)
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
HELP(#TranKeyHelp)
@V(H,#TranKeyHelp)
#TranKeyX=X,4,L

![債務人IDN]
#CustId=X,10,L

#RimCustId=X,10,S
<include src="ROM.icf::L8R57.dec"/>
T(2,@RimCustId,#CustId)
! 檢核該統編是否存在於客戶檔
#RimL8R57=X,1,S
RESET_RIM(#RimL8R57,L8R57)
S(L8R57,1,#RimCustId)
R(1,L8R57)
T(2,@CustIdX,#L8R57CustName)

#CustIdX=X,2.50,L

![債權金融機構代號]
#SubmitKey=X,3,L
#SubmitKeyX=X,80,L

!申請日期
#ApplyDate=D,7,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
D(8)
@V(7)
A(DATE,0,#ApplyDate)

!受理調解機構代號-承審法院代碼OR郵遞區號
#BankId=X,3,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#TranKey_Tmp,2,s,$)
@V(7)

#RimL8r01A=X,1,S
T(2,@RimJcicBankCode,#BankId)
RESET_RIM(#RimL8r01,L8R01)
S(L8R01,1,#RimJcicBankCode)
R(1,L8R01)
T(2,@BankIdX,#L8r01JcicBankName)

#BankIdX=X,80,L

!是否為更生債權人
#OwnerYn=X,1,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
HELP(#YesNoHelp)
@V(H,#YesNoHelp)
C(5,#OwnerYn,N,T(3,@PayYn,Y)E(0,@OwnerAmt,0)E(0,@AllotAmt,0)E(0,@UnallotAmt,0)
T(F,@PayYn,0)T(F,@OwnerAmt,0)T(F,@AllotAmt,0)T(F,@UnallotAmt,0)
T(2,@FdPayYn, )T(2,@FdOwnerAmt, )T(2,@FdAllotAmt, )T(2,@FdUnallotAmt, )
,$)
C(5,#OwnerYn,Y,T(3,@PayYn,Y)E(0,@OwnerAmt,0)E(0,@AllotAmt,0)E(0,@UnallotAmt,0)
T(F,@PayYn,1)T(F,@OwnerAmt,1)T(F,@AllotAmt,1)T(F,@UnallotAmt,1)
T(2,@FdPayYn,*)T(2,@FdOwnerAmt,*)T(2,@FdAllotAmt,*)T(2,@FdUnallotAmt,*)
,$)
!債務人是否仍依更生方案正常還款予本金融機構
#PayYn=X,1,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#OwnerYn,N,s,$)
HELP(#YesNoHelp)
@V(H,#YesNoHelp)
C(5,#PayYn,Y,E(0,@AllotAmt,0)T(F,@AllotAmt,0)T(2,@FdAllotAmt, ),$)
C(5,#PayYn,N,E(0,@AllotAmt,0)T(F,@AllotAmt,1)T(2,@FdAllotAmt,*),$)
!本金融機構更生債權總金額
#OwnerAmt=M,9,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#OwnerYn,N,S,$)

!參與分配債權金額
#AllotAmt=M,9,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#OwnerYn,N,S,$)
C(5,#PayYn,Y,s,$)

!未參與分配債權金額
#UnallotAmt=M,9,I
C(5,#TranKey_Tmp,5,s,$)
C(5,#TranKey_Tmp,4,s,$)
C(5,#OwnerYn,N,S,$)


!轉JCIC文字檔日期
#OutJcicTxtDate=D,7,L

! 流水號
#Ukey=X,32,S

#RPTFG=X,1,S
E(0,0) 
</sub>


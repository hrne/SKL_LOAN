!----- BASIC LABEL Variables ----------
<sub type="SYS">
</sub>

<sub type="SYS" name="label">

!#TXCD
!T(3,L8950) 

![資料表頭(HEADER)長度]
#INQHD=n,3,S
E(0,0)ASGN(INQHD$)

![每筆資料明細(OCCURS)長度-Occ回來後去切的長度]
#INQLEN=n,3,S
E(0,62)ASGN(INQLEN$)

![多筆查詢之一個畫面有N筆資料]
#INQREC=n,2,S
E(0,40)ASGN(INQREC$)
    
![畫面顯示的明細間的高度]
#LOOPH=n,3,S
E(0,1)ASGN(LOOPHEIGHT$)

![每張印錄單列印的筆數]
#INQPRT=n,2,S
E(0,40)ASGN(INQPRT$)

#button=x,100,S
T(2,{cmd=CHAIN; ntxcd=L8350; ntxbuf=1; ntxbuf5=type})ASGN(BTN_NEW1$)
K(MODIFYBUTTON,1,新增)
K(MODIFYBUTTON,查詢)

</sub>

<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)

</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L8950" layout="cols=1;screen.width=[130,870];printer.width=[20,20];order=1">
[
["[L8950]                                 MU1人員名冊查詢"],
[#<->#,"#FdEmpId+員工編號",#EmpId],
[#<->#,"#FdDataDate+資料日期",#DataDate],
]
!---------- AP Button AREA ----------

!---------- AP TEXT AREA Variables ----------

#FdEmpId=X,2,L
#FdDataDate=X,2,L

#CHAIN=A,1,S
T(4,CHAIN$)

!員工代號
#EmpId=X,6,I

!資料日期
#DataDate=D,7,I
@C(4,#DataDate,$,A(DATE,0,#DataDate))

#RPTFG=A,1,S

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!------------ TXFORM = L8950 -----------
<sub type="FORM" name="L8950">
#LOOP=X,1,S
E(0,1)ASGN(LOOP$)
! 總行代號
#OOHeadOfficeCode=X,3,S
! 分行代號
#OOBranchCode=X,4,S
! 資料日期
#OODataDate=D,7,S
! 員工代號
#OOEmpId=X,6,S
! 姓名
#OOEmpName=X,42,S

#OOBUF2=X,21,S
T(2,2+#OOHeadOfficeCode+#OOBranchCode+#OODataDate+#OOEmpId)
#OOBTN2=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L8350; ntxbuf=#OOBUF2})
! #OOBTN2=X,1,S
! BIND(grid,{ cmd=CHAIN; ntxcd=L8350; ntxbuf=#OOBUF1})
! #OOBTN3=X,1,S
! BIND(grid,{ cmd=CHAIN; ntxcd=L8350; ntxbuf=#OOBUF1})
</sub>

!------ 上行電文 TEXT ------
<sub type="TIM">
#EmpId
#DataDate
</sub>


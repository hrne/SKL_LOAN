----- BASIC LABEL Variables ----------
<sub type="SYS">
#KeyInq=X,1,S
K(MODIFYBUTTON,查詢)
</sub>

<sub type="SYS" name="label">

!#TXCD
!T(3,L2062)

![資料表頭(HEADER)長度]
#INQHD=n,3,S
E(0,0)ASGN(INQHD$)

![每筆資料明細(OCCURS)長度]
#INQLEN=n,3,S
E(0,41)ASGN(INQLEN$)

![多筆查詢之一個畫面有N筆資料]
#INQREC=n,2,S
E(0,20)ASGN(INQREC$)

![畫面顯示的明細間的高度]
#LOOPH=n,3,S
E(0,1)ASGN(LOOPHEIGHT$)

![每張印錄單列印的筆數]
#INQPRT=n,2,S
E(0,40)ASGN(INQPRT$)

</sub>

<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)



#BTNADDBUF=X,18,S
T(2,1+0000000+000)

#BtnAddA=x,100,S
T(2,{cmd=CHAIN; ntxcd=L2670; ntxbuf=#BTNADDBUF; ntxbuf5=type})
ASGN(BTN_NEW1$)
K(MODIFYBUTTON,1,新增)

</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,1050];printer.width=[20,20];">
[
["[L2062]                                 貸後契變手續費明細資料查詢"],
["#FdCUSTNO+借款人戶號",#CustNo,#BTN1,#CustName],
["#FdFACMNO+額度編號",#FacmNo],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
#BTN1=X,15,I,ui:button;value:顧客資料查詢;enable:1;tabbable:0

! 永遠為必輸欄位
#FdCUSTNO=X,2,L
T(3,*)
#FdFACMNO=X,2,L
T(3,*)

!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>
#CDDEF01=x,30,S
T(3,CdCode.ChangeItemCode)
#ChangeItemHelp=x,1024,S

#CDDEF02=x,26,S
T(3,CdCode.CurrPrecision)
#CurrPrecisionHelp=x,1024,S

#HelpRim=X,1,S 
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF01,#CDDEF02)
R(1,XXR99)
CALL(#HelpProc)
T(2,@ChangeItemHelp,#HelpDesc1)
T(2,@CurrPrecisionHelp,#HelpDesc2)


#CHAIN=A,1,S
T(4,CHAIN$)

#BUF1=X,42,S

#BIND1=X,1,S
BIND(#BTN1,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUF1;ntxbuf5=type<-1:CustNo<-OOCustNo})

!借款人戶號
#CustNo=A,7,I
@V(2,0)


<include src="ROM.icf::L2Rim.dec"/>
<include src="ROM.icf::L1R01.dec"/>
!調RIML1R01取CustName
#RimL1r01=X,1,S
E(0,@RimFunCd,5)
E(0,@RimCustNo,#CustNo)
RESET_RIM(#RimL1r01,L1R01)
S(L1R01,1,#RimCustId,#RimFunCd,#RimCustNo)
R(1,L1R01)


#checkL1R01=X,1,S
T(2,@CustName,#L1r01CustName)



#CustName=X,100,L



!額度編號
#FacmNo=A,3,I
@V(2,0)


#RPTFG=A,1,S


#MRKEY=_,_,S
C(3,#FacmNo,0,T(2,#CustNo),T(2,#CustNo+-+#FacmNo))

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>


!---------- TXFORM = L2062 ----------
<sub type="FORM" name="L2062">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L2062,$,S)

#SCRTIL=_,_,S
T(3,[L2062] 貸後契變手續費明細資料)

#OCUSTNO=A,7,L
T(2,#CustNo)


!修改
#FUNC2=A,1,S
E(0,2)
!查詢
#FUNC5=A,1,S
E(0,5)
!列印
#FUNC7=A,1,S
E(0,7)
!補列印
#FUNC8=A,1,S
E(0,8)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)
!戶號
#OOCustNo=A,7,S
!額度
#OOFacmNo=A,3,S

!貸後契變日期
#OOContractChgDate=D,7,L
!貸後契變序號
#OOContractChgNo=A,2,L

![貸後契變項目] 
#OOContractChgCode=A,2,L

#OOContractChgCodeX=X,10,L
C(3,#OOContractChgCode,0,S,$)
T(H,#OOContractChgCode,#ChangeItemHelp)
                    
#OOCCIND=X,13,L
T(2,#OOContractChgCode+-+#OOContractChgCodeX)

#OOCurrencyCode=X,3,O
#Precision=N,1,S
T(H,#OOCurrencyCode,#CurrPrecisionHelp)

#OOFeeAmt=m,14.2,L

#OOFeeAmtX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#OOFeeAmt,#OOFeeAmtX)
P(R,#OOFeeAmtX)

#OOEntryFG=X,1,L

#OOPrintCode=X,1,L

!修改
#OOBUF2=X,30,S
T(2,#FUNC2+#OOCustNo+#OOFacmNo+#OOContractChgDate+#OOContractChgNo+#OOContractChgCode)

#OOBTN2=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L2670; ntxbuf=#OOBUF2})
IF(#OOEntryFG=="Y",BIND(grid,cancel),$)

!查詢
#OOBUF5=X,30,S
T(2,#FUNC5+#OOCustNo+#OOFacmNo+#OOContractChgDate+#OOContractChgNo+#OOContractChgCode)

#OOBTN5=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L2670; ntxbuf=#OOBUF5})
!列印

#BTNNAME=X,1,S
T(3,@OOBTN7,列印)
C(3,#OOPrintCode,1,T(3,@OOBTN7,補列印),$)

#OOBUF7=X,30,S
C(3,#OOPrintCode,1,T(2,#FUNC8+#OOCustNo+#OOFacmNo+#OOContractChgDate+#OOContractChgNo+#OOContractChgCode),
                   T(2,#FUNC7+#OOCustNo+#OOFacmNo+#OOContractChgDate+#OOContractChgNo+#OOContractChgCode))


#OOBTN7=X,10,S
BIND(grid,{ cmd=CHAIN; ntxcd=L2670;text=#OOBTN7; ntxbuf=#OOBUF7})
IF(#OOEntryFG==0,BIND(grid,cancel),$)


!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------

</sub>



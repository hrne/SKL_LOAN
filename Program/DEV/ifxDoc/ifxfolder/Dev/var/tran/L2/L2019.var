!---------- TXFORM = L2019 ----------
<sub type="FORM" name="L2019">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L2019,$,S)

#SCRTIL=_,_,S
T(3,[L2019] 合併額度控管資料查詢)

!修改
#Func2=A,1,S
E(0,2)

!刪除
#Func4=A,1,S
E(0,4)

!查1
#Func5=A,1,S
E(0,5)

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

!核准號碼
#OOApplNo=A,7,O
!戶號
#OOCustNo=A,7,O
!戶名
#OOCustName=X,100,O
!幣別
#OOCurrencyCode=X,3,O
!幣別小數點
#OOPrecision=N,1,S
E(0,0)T(H,#OOCurrencyCode,#CurrPrecisionHelp)
!總額度
#OOLineAmt=m,14.2,O
#OOLineAmtX=X,22,L
CALL2(#ADJUSTDOT,#OOPrecision,#OOLineAmt,#OOLineAmtX)
P(R,#OOLineAmtX)

#OOCreateDate=D,7,O

#OOCreateEmpNo=X,6,O

#OOCreateEmpName=X,10,O

#OOCreateName=X,18,O
T(2,#OOCreateEmpNo+  +#OOCreateEmpName)

#OOLastUpdate=D,7,O

#OOLastUpdateEmpNo=X,6,O

#OOLastUpdateEmpName=X,10,O

#OOLastUpdateName=X,18,O
T(2,#OOLastUpdate+  +#OOLastUpdateEmpName)

!主要核准號碼
#OOMainApplNo=A,7,O

!刪除
#OOBUF4=X,18,S
T(2,#Func4+#OOMainApplNo)

#OOBTN4=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L2119; ntxbuf=#OOBUF4})

!查詢
#OOBUF5=X,18,S
T(2,#OOMainApplNo)

#OOBTN5=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L291B; ntxbuf=#OOBUF5})

</sub>



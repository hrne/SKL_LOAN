#COL_TXCD=X,5,S
SWITCH(#OOClCode1,1,T(3,L2911),
                  2,T(3,L2911),
                  3,T(3,L2913),
                  4,T(3,L2913),
                  5,T(3,L2914),
                  9,T(3,L2912),
                  )

#CHANGE_TXCD=X,5,S
SWITCH(#OOClCode1,1,T(3,L2411),
                  2,T(3,L2411),
                  3,T(3,L2413),
                  4,T(3,L2413),
                  5,T(3,L2414),
                  9,T(3,L2412),
                  )

#DEL_TXCD=X,5,S

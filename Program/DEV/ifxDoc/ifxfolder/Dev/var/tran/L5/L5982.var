<sub type="FORM" name="L5982">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L5982,$,S)

#SCRTIL=_,_,S
T(3,[L5982] 國率局申報檔)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------


!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

!資料年月
#OOYearMonth=A,5,S

!戶號
#OOCustNo=A,7,S
#OOFacmNo=A,3,S

!資金用途別
#OOUsageCode=X,2,S
#OOUsageCode1=A,2,S
E(0,#OOUsageCode)
#OOUsageCodeX=X,40,S
!C(3,#OOUsageCode,0,S,$)
T(H,#OOUsageCode1,#UsageCodeHelp)

!業務科目
#OOAcctCode=A,3,S
#OOAcctCodeX=X,40,S
T(H,#OOAcctCode,#AcctCodeHelp)

!繳款方式
#OORepayCode=A,2,S
#OORepayCodeX=X,40,S
C(3,#OORepayCode,0,S,$)
T(H,#OORepayCode,#RepayCodeHelp)

!撥款金額
#OOLoanAmt=m,14,S

!放款餘額
#OOLoanBal=m,14,S

!初貸日
#OOFirstDrawdownDate=D,7,S

!到期日
#OOMaturityDate=D,7,S

!年度繳息金額
#OOYearlyInt=m,14,S

!房屋取得日期
#OOHouseBuyDate=D,7,S



! 修改的按鈕
#OOFunc2=A,1,S
E(0,2)

#OOBUF2=X,100,S
T(2,2+#OOYearMonth+#OOCustNo+#OOFacmNo+#OOUsageCode)

#OOBTN2=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L5812;text=修改;ntxbuf=#OOBUF2})

! 刪除的按鈕
#OOFunc4=A,1,S
E(0,4)

#OOBUF4=X,100,S
T(2,4+#OOYearMonth+#OOCustNo+#OOFacmNo+#OOUsageCode)

#OOBTN4=X,4,S
BIND(grid,{cmd=CHAIN;ntxcd=L5812;text=刪除;ntxbuf=#OOBUF4})

</sub>



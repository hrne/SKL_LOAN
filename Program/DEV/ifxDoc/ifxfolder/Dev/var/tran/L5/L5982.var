!---------- AP Button AREA ----------


!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>

#FdYearMonth=X,2,L
#FdCustNo=X,2,L
#FdCondition=X,2,L



#CODEAcctCod=x,20,S                  
T(3,CdCode.AcctCode)  
                
#AcctCodeHelp=x,1024,S   

#CODERepayCode=x,20,S                  
T(3,CdCode.RepayCode)  
                
#RepayCodeHelp=x,1024,S   

#CODEUsageCode=x,20,S
T(3,CdCode.UsageCode)

#UsageCodeHelp=x,1024,S  

#CODEConditionsCode=x,30,S                  
T(3,CdCode.ConditionsCode)  

#ConditionsCodeHelp=x,1024,S 

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CODEAcctCod,#CODERepayCode,#CODEUsageCode,#CODEConditionsCode)
R(1,XXR99)
CALL(#HelpProc)
T(2,@AcctCodeHelp,#HelpDesc1)
T(2,@RepayCodeHelp,#HelpDesc2)
T(2,@UsageCodeHelp,00:全部;+#HelpDesc3)
T(2,@ConditionsCodeHelp,#HelpDesc4)

#CHAIN=A,1,S
T(4,CHAIN$) 

#NTXBUF=X,4,S
!C(3,#CHAIN,1,T(4,NTXBUF$),S)


#RPTFG=A,1,S
E(0,0)

#RPTFG2=A,1,S
C(3,#RPTFG,0,$,S)

!資料年月
#YearMonth=A,5,I
@A(YM,0,#YearMonth)


!戶號
#CustNo=A,7,I

!檢核條件
#Condition=A,2,I
HELP(#ConditionsCodeHelp)
@
C(3,#Condition,0,s,V(H,#ConditionsCodeHelp))

#ConditionX=X,30,L
C(3,#Condition,0,s,$)
T(H,#Condition,#ConditionsCodeHelp)

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)


!<include src="COM.icf::RPTFG.dec"/>

<include src="ROM.icf::L0Rim.dec"/>
</sub>


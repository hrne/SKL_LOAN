<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)

#BufLD010=X,3,S
T(3,111)

#Button=x,100,S
T(2,{cmd=CHAIN; ntxcd=LD010; ntxbuf=#BufLD010})ASGN(BTN_NEW1$)
K(MODIFYBUTTON,1,LD010 介紹人換算業績報酬檢核表)
</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L5510" layout="cols=1;screen.width=[130,870];printer.width=[20,80];order=1">
[
["[L5510]                           產生換算業績、業務報酬發放媒體"],
[#<->#,"#FdFunCode+功能",#FunCode,#FunCodeX],
[#<->#,"#FdWorkMonth+工作年月",#WorkMonth],
[#<->#,"#FdDateFm+撥款起訖日",#DateFm,"~",#DateTo],
[#<->#,"#FdMonths+檢核月數",#Months],
]

!不可重新交易
#AGAIN=X,1,S
E(0,1)ASGN(AGAIN$)
INVOKEJS(SHOW,L5510_4,0)

#FdFunCode=X,2,L
T(3,*)
#FdWorkMonth=X,2,L
T(3,*)
#FdDateFm=X,2,L
#FdMonths=X,2,L
T(3,*)

#CHAIN=A,1,S
T(4,CHAIN$)

! 37*2+3*2+2=74+6
#FunCodeHelp=X,200,S
T(3,1:保費檢核;2:產生換算業績、業務報酬發放媒體;3:取消換算業績、業務報酬發放媒體)


<include src="ROM.icf::L5R38.rtn"/>

#RimL5R38=X,1,S
RESET_RIM(#RimL5R38,L5R38)
E(0,@L5R38iCode,1)
E(0,@L5R38iDate,#SYSDATE)
S(L5R38,01,#L5R38iCode,#L5R38iDate)
R(1,L5R38)
E(0,@WorkMonth,#L5R38oWorkYM)
E(0,@WorkMonth2,#L5R38oWorkYM)
E(0,@DateFm,#L5R38oStartDate)
E(0,@DateTo,#L5R38oEndDate)

#FunCode=X,1,I
HELP(#FunCodeHelp)
@V(H,#FunCodeHelp)

#FunCodeX=X,40,L
T(H,#FunCode,#FunCodeHelp)
C(2,#FunCode,$,K(MODIFYBUTTON,保費檢核),K(MODIFYBUTTON,產生媒體),K(MODIFYBUTTON,取消媒體))
C(3,#FunCode,1,INVOKEJS(SHOW,L5510_4,1),INVOKEJS(SHOW,L5510_4,0))

#WorkMonth2=A,5,S

#WorkMonth=A,5,I
@
A(YM13,1,#WorkMonth)

#RimL5R38B=X,1,S
C(5,#WorkMonth,#WorkMonth2,S,$)
RESET_RIM(#RimL5R38B,L5R38)
E(0,@L5R38iCode,2)
E(0,@L5R38iWorkYM,#WorkMonth)
S(L5R38,01,#L5R38iCode,#L5R38iWorkYM)
R(1,L5R38)
E(0,@DateFm,#L5R38oStartDate)
E(0,@DateTo,#L5R38oEndDate)

! 撥款起始日
#DateFm=D,7,L

! 撥款結束日
#DateTo=D,7,L

#Months=m,2,I
C(3,#FunCode,1,$,S)
C(3,#Months,0,E(0,4),$)
@
V(2,0)

</sub>


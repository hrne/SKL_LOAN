!---------- BASIC LABEL Variables ----------
<sub type="SYS">
</sub>

<sub type="SYS" name="label">
!#TXCD
!T(3,L5906)

![資料表頭(HEADER)長度]
#INQHD=n,3,S
E(0,0)ASGN(INQHD$)

![每筆資料明細(OCCURS)長度]
#INQLEN=n,3,S
E(0,111)ASGN(INQLEN$)

![多筆查詢之一個畫面有N筆資料]
#INQREC=n,2,S
E(0,40)ASGN(INQREC$)

![畫面顯示的明細間的高度]
#LOOPH=n,3,S
E(0,1)ASGN(LOOPHEIGHT$)

![每張印錄單列印的筆數]
#INQPRT=n,2,S
E(0,40)ASGN(INQPRT$)

</sub>

<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)

#Chain_L5116=X,12,S
T(2,100000000000)
#button=x,100,S
T(2,{cmd=CHAIN; ntxcd=L5116; ntxbuf=#Chain_L5116; ntxbuf5=type})ASGN(BTN_NEW1$)
K(MODIFYBUTTON,1,新增)
#button1=x,100,S
T(2,{cmd=CHAIN; ntxcd=LM016; ntxbuf=1; ntxbuf5=type})ASGN(BTN_NEW2$)
K(MODIFYBUTTON,2,LM016-寬限條件控管查詢)
K(MODIFYBUTTON,查詢)
</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L5906" layout="cols=1;screen.width=[130,870];printer.width=[20,80];order=1">
[
["[L5906]                                 寬限條件控管繳息查詢"],
[#<->#,"#FdCustNo+戶號",#CustNoS,"-",#CustNoE,"@(輸入0,表示查詢全部戶號)"],
]
#FdCustNo=X,2,L
T(3, )
#CHAIN=A,1,S
T(4,CHAIN$) 

#NTXBUF=x,7,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@CustNoS,#NTXBUF,1,7)
T(1,@CustNoE,#NTXBUF,1,7)

#Sapce=X,1,S
C(3,#CustNoS,0,$,T(2,@Chain_L5116,1+#CustNoS+000))

#CustNoS=A,7,I
C(3,#CHAIN,1,s,$)
@
T(2,@Chain_L5116,1+#CustNoS+000)

#CustNoE=A,7,I
C(3,#CHAIN,1,s,$)
@
V(5,#CustNoS,9999999)
#RPTFG=A,1,S

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,1)XMT(),$)

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!------------ TXFORM = L5906 -----------
<sub type="FORM" name="L5906">
#SCRTIL=_,_,S
T(3,[L5906] 寬限條件控管繳息查詢)

#LOOP=X,1,S
E(0,1)ASGN(LOOP$)

! 戶號
#OOCustNo=A,7,S
! 戶名
#OOCustName=X,100,O
! 額度編號
#OOFacmNo=A,3,O
! 使用號碼
#OOActUse=X,1,O

#OOBUF_EDIT=X,107,S
T(2,2+#OOCustNo+#OOFacmNo)
#OOBUF_DELETE=X,107,S
T(2,4+#OOCustNo+#OOFacmNo)

#OOBTN_EDIT=A,2,O
BIND(grid,{ cmd=CHAIN; ntxcd=L5116; ntxbuf=#OOBUF_EDIT})
#OOBTN_DELETE=A,2,O
BIND(grid,{ cmd=CHAIN; ntxcd=L5116; ntxbuf=#OOBUF_DELETE})


</sub>

!------ 上行電文 TEXT ------
<sub type="TIM">
#CustNoS
#CustNoE
#END
</sub>

!------ 下行電文TOM ------
<sub type="TOM">
<include src="COM.icf::TRCTL.tom"/>
TXFORM=L5906
#OOCustNo#OOCustName#OOFacmNo#OOActUse
^
</sub>



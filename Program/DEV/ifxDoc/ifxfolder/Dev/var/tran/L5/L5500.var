!---------- BASIC LABEL Variables ----------
<sub type="SYS">
</sub>

<sub type="SYS" name="label">
!#TXCD
!T(3,L5500)
</sub>

<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)
</sub>


!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L5500" layout="cols=1;screen.width=[150,850];printer.width=[20,80];order=1">
[
["[L5500]                                 工作日業績結算"],
["@工作日",#WorkDate],
[" ","@說明：1.工作日業績結算作業，系統自動排程於每工作日15:30作業"],
[" ","@@@@@@@2.本交易為(1).系統自動啟動失敗或(2).需再重新結算業績時，人工啟動使用"],
]

#button=x,100,S
K(MODIFYBUTTON,結算)

!不可重新交易
#AGAIN=X,1,S
E(0,1)ASGN(AGAIN$)

#PreValue=A,1,S
T(1,@WorkDate,#SYSDATE,2,7)

#CHAIN=A,1,S
T(4,CHAIN$)


#NTXBUF=X,1,S

! 業績日期
#WorkDate=D,7,L

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!------------ TXFORM = L5500 -----------
<sub type="FORM" name="L5500">
</sub>

!------ 上行電文 TEXT ------
<sub type="TIM">
#WorkDate
</sub>

!------ 下行電文TOM ------
<sub type="TOM">
<include src="COM.icf::TRCTL.tom"/>
TXFORM=L5500

^
</sub>

!---------- 單據輸出組合 ----------
<sub type="SELECT">
<include src="PRT.icf::UPD01.sel" map="printNo=PRINTRIM"/>
</sub>

!---------- BASIC LABEL Variables ----------
<sub type="SYS">
</sub>

<sub type="SYS" name="label">
!#TXCD
!T(3,L5024)

![資料表頭(HEADER)長度]
#INQHD=n,3,S
E(0,0)ASGN(INQHD$)

![每筆資料明細(OCCURS)長度]
#INQLEN=n,3,S
E(0,40)ASGN(INQLEN$)

![多筆查詢之一個畫面有N筆資料]
#INQREC=n,2,S
E(0,40)ASGN(INQREC$)

![畫面顯示的明細間的高度]
#LOOPH=n,3,S
E(0,1)ASGN(LOOPHEIGHT$)

![每張印錄單列印的筆數]
#INQPRT=n,2,S
E(0,40)ASGN(INQPRT$)

</sub>

<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)

#button=x,100,S
T(2,{cmd=CHAIN; ntxcd=L5405; ntxbuf=1; ntxbuf5=type})ASGN(BTN_NEW1$)
K(MODIFYBUTTON,1,新增)
K(MODIFYBUTTON,查詢)
</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L5024" layout="cols=1;screen.width=[130,870];printer.width=[20,80];order=1">
[
["[L5024]                                 目標金額、累計目標金額查詢"],
[#<->#,"#FdDeptCode+部室代號",#DeptCode,"@空白查全部"],
[#<->#,"#FdDistCode+區部代號",#DistCode,"@空白查全部"],
[#<->#,"#FdUnitCode+單位代號",#UnitCode,"@空白查全部"],
]

#FdDeptCode=X,2,L
#FdDistCode=X,2,L
#FdUnitCode=X,2,L

#CHAIN=A,1,S
T(4,CHAIN$) 

#NTXBUF=X,7,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
! T(1,@YYY,#NTXBUF,1,3)
! INVOKEJS(SHOW,p1,02,02,1)
! 
#PREVALUE=A,1,S
! T(1,@YYY,#SYSDATE,2,3)
! T(1,@MM,#SYSDATE,5,2)

#DeptCode=X,6,I
#DistCode=X,6,I
#UnitCode=X,6,I

#RPTFG=A,1,S

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!------------ TXFORM = L5024 -----------
<sub type="FORM" name="L5024">

#SCRTIL=_,_,S
T(3,[L5024] 目標金額累計目標金額)

#LOOP=X,1,S
E(0,1)ASGN(LOOP$)

! 單位代號
#OOUnitCode=X,6,O
! 單位中文
#OOUnitItem=x,40,O

! 區部代號
#OODistCode=X,6,O
! 區部中文
#OODistItem=x,40,O

! 部室代號
#OODeptCode=X,6,O
! 部室中文
#OODeptItem=x,40,O

! 處長主任別
#OODirectorCode=X,1,O

! 員工代號
#OOEmpNo=X,6,O
! 員工姓名
#OOEmpName=X,8,O

! 專員姓名
#OODepartOfficer=X,8,O

! 目標件數
#OOGoalCnt=m,5,O

! 累計目標件數
#OOSumGoalCnt=m,5,O

! 目標金額
#OOGoalAmt=M,14,O

! 累計目標金額
#OOSumGoalAmt=M,14,O


#OOBUF_EDIT=X,210,S
T(2,2+#OOUnitCode+#OOUnitItem+#OODistCode+#OODistItem+#OODeptCode+#OODeptItem+#OODirectorCode+#OOEmpNo+#OOEmpName+#OODepartOfficer+#OOGoalCnt+#OOSumGoalCnt+#OOGoalAmt+#OOSumGoalAmt)

#OOBUF_DELETE=X,210,S
T(2,4+#OOUnitCode+#OOUnitItem+#OODistCode+#OODistItem+#OODeptCode+#OODeptItem+#OODirectorCode+#OOEmpNo+#OOEmpName+#OODepartOfficer+#OOGoalCnt+#OOSumGoalCnt+#OOGoalAmt+#OOSumGoalAmt)

#OOBTN_EDIT=A,2,O
BIND(grid,{ cmd=CHAIN; ntxcd=L5405; ntxbuf=#OOBUF_EDIT})

#OOBTN_DELETE=A,2,O
BIND(grid,{ cmd=CHAIN; ntxcd=L5405; ntxbuf=#OOBUF_DELETE})


</sub>


!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L5914" layout="cols=1;screen.width=[130,870];printer.width=[20,80];order=1">
[
["[L5914]                                 協辦人員等級歷程查詢"],
[#<->#,"#FdEmpNo+員工代號",#EmpNo,#EmpNoX],
[#<->#,"#FdEffectiveDate+生效日期",#EffectiveDate],
]
<include src="HELPRIM.icf::HELPRIM.rtn"/>
<include src="ROM.icf::L5R28.dec"/>
#CDCODEClassType=x,20,S
T(3,CdCode.ClassType)
#ClassTypeHelp=x,100,S
#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDCODEClassType)
R(1,XXR99)
CALL(#HelpProc)
T(2,@ClassTypeHelp,#HelpDesc1)

#FdEmpNo=X,2,L
#FdEffectiveDate=X,2,L

#CHAIN=A,1,S
T(4,CHAIN$) 
IF(#CHAIN!=1,V(P,此為連動交易，請從交易:L5022進入),$)

#NTXBUF=X,65,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@EmpNo,#NTXBUF,1,6)
T(1,@EffectiveDate,#NTXBUF,7,7)

#RimEmployeeNo=X,6,S
#RimL5R28=X,1,S
T(2,@RimEmployeeNo,#EmpNo)
RESET_RIM(#RimL5R28,L5R28)
S(L5R28,1,#RimEmployeeNo)
R(1,L5R28)
T(2,@EmpNoX,#L5r28FullName)

! 員工代號
#EmpNo=X,6,L
#EmpNoX=X,50,L
! 生效日期
#EffectiveDate=D,7,L

#RPTFG=A,1,S

C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!------------ TXFORM = L5914 -----------
<sub type="FORM" name="L5914">

#SCRTIL=_,_,S
T(3,[L5914] 協辦人員等級歷程查詢)

#LOOP=X,1,S
E(0,1)ASGN(LOOP$)
!停效日期
#OOIneffectiveDate=D,7,L
C(3,#OOIneffectiveDate,0,S,$)
!協辦等級
#OOEmpClass=X,1,L
#OOEmpClassX=X,10,L
T(H,#OOEmpClass,#ClassTypeHelp)
!初接授信通過
#OOClassPass=X,1,L
!上次更新日期
#OOLastUpdate=X,10,L
!上次更新人員
#OOLastUpdateEmpNo=X,20,L

</sub>

!------ 上行電文 TEXT ------
<sub type="TIM">
#EmpNo
#EffectiveDate
#END
</sub>

!------ 下行電文TOM ------
<sub type="TOM">
<include src="COM.icf::TRCTL.tom"/>
<include src="ROM.icf::L5R28.tom"/>
<include src="HELPRIM.icf::HELPRIM.tom"/>
TXFORM=L5914
#OOIneffectiveDate#OOEmpClass#OOClassPass#OOLastUpdate#OOLastUpdateEmpNo
^
</sub>


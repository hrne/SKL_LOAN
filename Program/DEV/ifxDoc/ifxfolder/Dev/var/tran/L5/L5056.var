!--使用資料庫 PfItDetail
!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L5056" layout="cols=1;screen.width=[130,870];printer.width=[20,80];order=1">
[
["[L5056]                                 內網報表業績維護查詢"],
["#FdCustNo+戶號",#CustNo,"-",#FacmNo,"-",#BormNo],
["#FdWorkYM+工作年月",#WorkYM],
]

#FdCustNo=X,2,L
T(3, )
#FdWorkYM=X,2,L
T(3, )

!---------- AP TEXT AREA Variables ----------
! ["查詢方式",#FunctionCd,#FunctionCdX],

#CHAIN=A,1,S
T(4,CHAIN$)

#NTXBUF=X,24,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)


#CustNo=A,7,I

#FacmNo=A,3,I
C(3,#CustNo,0,S,$)

#BormNo=A,3,I
C(3,#FacmNo,0,S,$)

#WorkYM=A,5,I
@
C(3,#CustNo,0,A(YM,1,#WorkYM),A(YM,0,#WorkYM))


#MRKEY=_,_,S
C(3,#CustNo,0,S,$)
C(3,#FacmNo,0,T(2,#CustNo),T(2,#CustNo+-+#FacmNo)) 

![顯示方式]
#RPTFG=A,1,S
E(0,1)

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!------------ TXFORM = L5056 -----------
<sub type="FORM" name="L5056">

#SCRTIL=_,_,S
T(3,[L5056] 內網報表業績維護清單)

#LOOP=X,1,S
E(0,1)ASGN(LOOP$)

!序號 
#OOLogNo=A,8,S

! 戶號
#OOCustNo=A,7,S
! 額度編號
#OOFacmNo=A,3,S
! 撥款序號
#OOBormNo=A,3,S

#OOCustNoX=X,15,S
T(2,#OOCustNo+-+#OOFacmNo+-+#OOBormNo)

! 戶名
#OOCustName=X,100,S

#OOWorkMonth=A,5,S

#OOCreateEmp=X,20,S

#OOCreateDate=X,50,S

#OOLastUpdateEmp=X,20,S

#OOLastUpdate=X,50,S

#OOBuf2=X,9,S
T(2,2+#OOLogNo)

#OOBtn2=A,2,O
BIND(grid,{ cmd=CHAIN; ntxcd=L5506; ntxbuf=#OOBuf2})
!C(3,#OOCanModify,1,$,BIND(grid,cancel))

#OOBuf4=X,9,S
T(2,4+#OOLogNo)

#OOBtn4=A,2,O
BIND(grid,{ cmd=CHAIN; ntxcd=L5506; ntxbuf=#OOBuf4})
!C(3,#OOCanModify,1,$,BIND(grid,cancel))

#OOBuf5=X,9,S
T(2,5+#OOLogNo)

#OOBtn5=A,2,O
BIND(grid,{ cmd=CHAIN; ntxcd=L5506; ntxbuf=#OOBuf5})
!C(3,#OOCanModify,1,$,BIND(grid,cancel))

</sub>


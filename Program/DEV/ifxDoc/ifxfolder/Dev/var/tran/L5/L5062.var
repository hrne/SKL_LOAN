C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

<include src="ROM.icf::L0Rim.dec"/>
<include src="ROM.icf::L6R12Rim.dec"/>
<include src="ROM.icf::L6R12.dec"/>

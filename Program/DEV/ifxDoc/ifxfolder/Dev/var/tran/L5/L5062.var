T(2,#OOCityCode)

#OOBTN2=X,4,S
C(3,#CHAIN,1,HIDE(#OOBTN2),SHOW(#OOBTN2))
BIND(grid,{cmd=CHAIN;ntxcd=L5606;text=修改;ntxbuf=#OOBUF2})


</sub>



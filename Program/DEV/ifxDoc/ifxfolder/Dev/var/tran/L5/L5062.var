#OOAccCollPsnX=X,100,L
#OOLegalPsn=X,6,L
#OOLegalArea=X,5,L
#OOLegalNo=X,10,L
#OOLegalExt=X,5,L
#OOLegalPsnX=X,100,L

#OOAccCollPsnCombine=X,150,L
T(2,#OOAccCollPsn+ +#OOAccCollPsnX)

#DASH1=X,1,L
C(4,#OOAccTelArea,T(3,),T(3,-))
#DASH2=X,1,L
C(4,#OOAccTelExt,T(3,),T(3,-))

#OOAccTelCombine=X,24,L
IF(#OOAccTelNo=="",S,$)
T(B,#OOAccTelArea+#DASH1+#OOAccTelNo+#DASH2+#OOAccTelExt)


#OOLegalPsnCombine=X,150,L
T(2,#OOLegalPsn+ +#OOLegalPsnX)

#DASH3=X,1,L
C(4,#OOLegalArea,T(3,),T(3,-))
#DASH4=X,1,L
C(4,#OOLegalExt,T(3,),T(3,-))

#OOLegalCombine=X,24,L
IF(#OOLegalNo=="",S,$)
T(B,#OOLegalArea+#DASH3+#OOLegalNo+#DASH4+#OOLegalExt)

! 修改的按鈕
#OOFunc2=A,1,S
E(0,2)

! 1+2+20+6+6+6
! T(2,#OOFunc2+#OOCityCode+#OOCityItem+#OOUnitCode+#OOIntRateCeiling+#OOIntRateFloor)

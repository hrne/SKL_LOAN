C(3,#ChequeFlag,1,T(3,*),T(3, ))

#TwTempAmt=m,14,I
C(3,#ChequeFlag,1,$,s)
C(3,#Precision,0,$,s)
@V(2,0)

#UsTempAmt=m,14.2,I
C(3,#ChequeFlag,1,$,s)
C(3,#Precision,0,s,$)
@V(2,0)

#TimTempAmt=m,14.2,S
C(3,#Precision,0,E(0,#TwTempAmt),E(0,#UsTempAmt))

#TempAmtX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#TimTempAmt,#TempAmtX)

![交易金額]
#TXAMT=_,_,S
E(0,#TimTempAmt)

![暫收來源]
!暫收原因為期票者，暫收來源需輸入 4 （支票)  
!兌現票據入帳,暫收源須為支票
!暫收原因0:債協暫收款,則暫收來源須為9
#FdTempSourceCode=X,2,L
!IF(#TempReasonCode==00 || #TempReasonCode==03 ||
!   #TempReasonCode==06 || #TempReasonCode==08,S,T(3,*))

#TempSourceCode=A,2,S
HIDE(#TempSourceCode)
! IF(#TempReasonCode==00,E(0,9),$)
IF(#TempReasonCode==00,s,$)
C(3,#ChequeFlag,1,E(0,4),$)
C(3,#ChequeFlag,1,s,$)
C(3,#TempReasonCode,8,E(0,4),$)
C(3,#TempReasonCode,8,s,$)
SHOW(#TempSourceCode)
!HELP(L3DEF,TempSourceCodeDef,TempSourceCode,TempSourceCodeX)
!@V(E,0,L3DEF,TempSourceCodeDef)
!V(2,4)

#TempSourceCodeX=X,16,L
T(3,)T(H,#TempSourceCode,#TempSourceCodeHelp)  

![發票人姓名]
!暫收原因為期票或即期票現金者必須輸入,其他不必輸入
#FdChequeName=X,2,L
C(3,#ChequeFlag,1,T(3,*),S)

#ChequeName=X,60,I
C(3,#ChequeFlag,1,$,s)
T(2,@ChequeName,#L2r07CustName)
@V(7)

![支票帳號]
#FdChequeAcct=X,2,L
C(3,#ChequeFlag,1,T(3,*),S)

#ChequeAcct=A,9,I
C(3,#ChequeFlag,1,$,s)
@V(2,0)

![支票號碼]
#FdChequeNo=X,2,L
C(3,#ChequeFlag,1,T(3,*),S)

#ChequeNo=A,7,I
C(3,#ChequeFlag,1,$,s)
@V(2,0)

#SendL3r04A=X,1,S
C(3,#ChequeFlag,1,$,S)
E(0,@RimFuncCode,1)
E(0,@RimChequeAcct,#ChequeAcct)
E(0,@RimChequeNo,#ChequeNo)
RESET_RIM(#SendL3r04A,L3R04)
S(L3R04,1,#RimFuncCode,#RimTxCode,#RimChequeAcct,#RimChequeNo)
R(1,L3R04)

#SendL3r04B=X,1,S
C(3,#TempReasonCode,8,$,S)
E(0,@RimFuncCode,5)
E(0,@RimChequeAcct,#ChequeAcct)
E(0,@RimChequeNo,#ChequeNo)
RESET_RIM(#SendL3r04B,L3R04)
S(L3R04,1,#RimFuncCode,#RimTxCode,#RimChequeAcct,#RimChequeNo)
R(1,L3R04)

#RcvL3r04B=X,1,S
C(3,#TempReasonCode,8,$,S)
T(2,@ChequeName,#L3r04ChequeName)
E(0,@ChequeDate,#L3r04ChequeDate)
E(0,@AreaCode,#L3r04AreaCode)
T(2,@BankCode,#L3r04BankCode)
T(2,@BankItem,#L3r04BankItem)
T(2,@BktwFlag,#L3r04BktwFlag)
T(2,@TsibFlag,#L3r04TsibFlag)
T(2,@MediaFlag,#L3r04MediaFlag)
E(0,@OutsideCode,#L3r04OutsideCode)
E(0,@UsageCode,#L3r04UsageCode)
T(2,@ServiceCenter,#L3r04ServiceCenter)

#WKNDY=D,8,S
E(0,#SYSNDATE)

#WKBDY=D,8,S
D(7,3,#SYSDATE,12,0)

#WKEDY=D,8,S
D(7,2,#SYSDATE,60,0)

![支票到期日]
!即期票入暫收,開票日須<=次一會計日
!支票到期日未到期不可超過五年(不可收五年後才到期的支票)
!支票到期日已到期不可超過一年(不可收一年前的支票)
#FdChequeDate=X,2,L
C(3,#ChequeFlag,1,T(3,*),S)

#ChequeDate=D,7,I
C(3,#ChequeFlag,1,$,s)
D(8)
@V(2,0)
A(DATE,0,#ChequeDate)
C(3,#TempReasonCode,6,V(5,#WKBDY,#WKNDY),V(5,#WKBDY,#WKEDY))

![交換區號]
#FdAreaCode=X,2,L
C(3,#ChequeFlag,1,T(3,*),S)

#AreaCode=A,2,I
C(3,#ChequeFlag,1,$,s)
HELP(#AreaCodeHelp,cols:2)
@V(H,#AreaCodeHelp)

#AreaCodeX=X,10,L
T(3,)T(H,#AreaCode,#AreaCodeHelp)

!交換區域為01時,本埠外埠為本埠,交換區域為01外的,本埠外埠為外埠
#checkareacode=X,1,S
C(3,#ChequeFlag,1,$,s)
C(3,#AreaCode,01,E(0,@OutsideCode,1),E(0,@OutsideCode,2))

![支票金額]
#FdChequeAmt=X,2,L

#ChequeAmt=m,14.2,S

#ChequeAmtX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#ChequeAmt,#ChequeAmtX)


#BUF5A=X,7,S

#BIND5A=X,1,S
BIND(#BTN5,click, {cmd=CHAIN; ntxcd=L6071; ntxbuf=#BUF5A;ntxbuf5=type<-1:BankCode<-OOTBankCode})

![行庫代號]
#FdBankCode=X,2,L
C(3,#ChequeFlag,1,T(3,*)SHOW(#BTN5),HIDE(#BTN5))


#BankCode=X,7,I
C(3,#ChequeFlag,1,$,s)
@V(7)
V(2,0)

#SendL2r09A=X,1,S
C(4,#BankCode,S,$)
T(2,@RimBankCode,#BankCode)
RESET_RIM(#SendL2r09A,L2R09)
S(L2R09,1,#RimTxCode,#RimBankCode)
R(1,L2R09)

#BankItem=X,40,L
C(4,#BankCode,S,$)
T(B,#L2r09BankItem+#L2r09BranchItem)

![是否為台支]
#FdBktwFlag=X,2,L
C(3,#ChequeFlag,1,T(3,*),S)

#BktwFlag=X,1,I
C(3,#ChequeFlag,1,$,s)
HELP(#YNHelp)
T(3,N)
@V(H,#YNHelp)

#BktwFlagX=X,2,L
C(4,#BktwFlag,S,$)T(H,#BktwFlag,#YNHelp)

![是否為台新]
#FdTsibFlag=X,2,L
C(3,#ChequeFlag,1,T(3,*),S)

#TsibFlag=X,1,S
C(3,#ChequeFlag,1,$,s)
HELP(#YNHelp)
T(3,N)
@V(H,#YNHelp)

#TsibFlagX=X,2,L
C(4,#TsibFlag,S,$)T(H,#TsibFlag,#YNHelp)

![入媒體檔]
#FdMediaFlag=X,2,L
C(3,#ChequeFlag,1,T(3,*),S)

#MediaFlag=X,1,S
C(3,#ChequeFlag,1,$,s)
HELP(#YNHelp)
T(3,Y)
@V(H,#YNHelp)

#MediaFlagX=X,2,L
C(4,#MediaFlag,S,$)
T(H,#TsibFlag,#YNHelp)

![本埠外埠]
#FdOutsideCode=X,2,L
C(3,#ChequeFlag,1,T(3,*),S)

#OutsideCode=A,1,I
C(3,#ChequeFlag,1,$,s)
HELP(#OutsideCodeHelp)
@V(H,#OutsideCodeHelp)

#OutsideCodeX=X,4,L
C(3,#OutsideCode,0,S,$)
T(H,#OutsideCode,#OutsideCodeHelp)

![支票用途]
!暫收來源=4(支票)時必須輸入,其他不可輸入
#FdUsageCode=X,2,L
C(3,#ChequeFlag,1,T(3,*),S)

#UsageCode=A,2,I
C(3,#ChequeFlag,1,$,s)
HELP(#UsageCodeHelp,cols:2)
@V(H,#UsageCodeHelp)

#UsageCodeX=X,16,L
T(3,)T(H,#UsageCode,#UsageCodeHelp)

![服務中心別]
!可不輸入
#FdServiceCenter=X,2,L
C(3,#ChequeFlag,1,T(3,*),S)
T(2,@ServiceCenter,Y)

#ServiceCenter=X,1,I
C(3,#ChequeFlag,1,$,s)
HELP(#ServiceCenterHelp,cols:2)
@V(H,#ServiceCenterHelp)

#ServiceCenterX=X,10,L
T(3,)T(H,#ServiceCenter,#ServiceCenterHelp)

![統一編號]
!暫收來源為代收款-債權協商,統一編號及債權機構須輸入,其他不必輸入
#FdCreditorId=X,2,L
T(F,@CreditorId,0)
C(3,#TempSourceCode,7,$,S)
T(3,*)
T(F,@CreditorId,1)

#CreditorId=X,10,I
C(3,#TempSourceCode,7,$,S)
@V(7)A(ID_UNINO,0,#CreditorId)

![債權機構]
!暫收來源為代收款-債權協商,統一編號及債權機構須輸入,其他不必輸入
#FdCreditorBankCode=X,2,L
T(F,@CreditorBankCode,0)
C(3,#TempSourceCode,7,$,S)
T(3,*)
T(F,@CreditorBankCode,1)

#CreditorBankCode=X,7,I
C(3,#TempSourceCode,7,$,S)
@V(2,0)

#SendL2r09B=X,1,S
C(4,#CreditorBankCode,S,$)
T(2,@RimBankCode,CreditorBankCode)
RESET_RIM(#SendL2r09B,L2R09)
S(L2R09,1,#RimTxCode,#RimBankCode)
R(1,L2R09)

#CreditorBankItem=X,40,L
C(4,#CreditorBankCode,S,$)
T(2,#L2r09BranchItem)

! ![對方業務科目]
! #FdOtherAcctCode=X,2,L

! #OtherAcctCode=X,3,I
! HELP(L2DEF,FacmAcctCodeDef,FacmAcctCode,FacmAcctCodeX)
! @C(3,#OtherAcctCode,0,S,$)V(E,0,L2DEF,FacmAcctCodeDef)

! #OtherAcctCodeX=X,20,L
! T(A,#OtherAcctCode,L2DEF,FacmAcctCodeDef,FacmAcctCode,FacmAcctCodeX)

![繳息迄日]
!自動顯示不須輸入
#FdPrevIntDate=X,2,L

#PrevIntDate=D,7,L
E(0,#L2r05PrevPayIntDate)

#PrevIntDateX=X,9,L
CALL2(#AdjustDate,#PrevIntDate,#PrevIntDateX)

! ![收據號碼]
! !可不輸入
! #FdReceiptNo=X,2,L

! #ReceiptNo=X,5,I

!應收付類別,1:回收,3:期票
#TMPRPFG=X,1,S
C(3,#TempReasonCode,3,E(0,@RPFG,3),E(0,@RPFG,1))

!還款來源,1:匯款轉帳 93:暫收支票
#TMPRPTYP=X,1,S
C(3,#TempReasonCode,3,E(0,@RPTYPZ,93),E(0,@RPTYPZ,#TempSourceCode))

!應收付總金額,轉暫收金額
#TMPRPTOT=X,1,S
E(3,@RPTOT,#TimTempAmt)

!實際收付金額,轉暫收金額
#TMPRPAMT=X,1,S
E(3,@RPAMTZ,#TimTempAmt)

#MRKEY=_,_,S
C(3,#FacmNo,0,T(2,#TimCustNo),T(2,#TimCustNo+-+#FacmNo))
! ----------1:還款、2:結案，需輸入----------
! 明細序號
#TempSEQ=X,1,S
E(0,@RPSEQ,#DetailSeq)

! 入帳日期
#TempEntryDate=X,1,S
E(0,@RP_ENTRYDATE,#EntryDate)

! 還款類別  1:期款 2:部分償還
#TempREPCAT=X,1,S
C(3,#RP_REPCAT,0,$,s)
E(0,@RP_REPCAT,1)

! 額度
#TempFacmNo1=X,1,S
E(0,@RP_FACMNO,#FacmNo)

!類別   摘要
#TempNOTE=X,1,S
T(2,@RP_NOTE,#DetailNote)

! ----------3:期票、8:抽退票，需輸入----------
! 戶號
#TempRpCustNo=X,1,S
E(0,@RP_CUSTNO,#TimCustNo)

! 額度
#TempFacmNo2=X,1,S
E(0,@RP_FACMNO,#FacmNo)

! 支票帳號
#TempChequeAcct=X,1,S
E(0,@RP_CHQACT,#ChequeAcct)

! 支票號碼 
#TempChequeNo=X,1,S
E(0,@RP_CHQUENO,#ChequeNo)

! 支票金額   
#TempChequeAmt=X,1,S
E(0,@RP_CHQUEAMT,#TimTempAmt)


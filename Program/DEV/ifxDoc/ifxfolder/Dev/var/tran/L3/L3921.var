#CHAIN=A,1,S
T(4,CHAIN$) 
T(2,@RimTxCode,#TXCD)
E(0,@RimFKey,#FKEY)

#NTXBUF=X,100,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)


#CHAINTXCD=_,_,S
C(3,#CHAIN,1,T(1,#NTXBUF,1,5),S)

#txcd=X,2,S
C(3,#CHAIN,1,T(1,#NTXBUF,1,2),S)


#BUF1A=X,18,S
T(3,00000009999999000)

#BIND1A=X,1,S
BIND(#BTN1,click, {cmd=CHAIN; ntxcd=L2015; ntxbuf=#BUF1A;ntxbuf5=type<-1:FacmNo<-OOFacmNo:CustNo<-OOCustNo})



![借戶戶號]
!借戶戶號, 統一編號或核准編號擇一輸入
#FdCustNo=X,2,L

#CustNo=A,7,I
CALL(#InitValue)
C(3,#CHAIN,1,T(1,#NTXBUF,6,7),$)
C(3,#CHAIN,1,s,$)
INVOKEJS(SHOW,p1,1,3,1)
SHOW(#BTN1)HIDE(#BTN2,#BTN3)

#WKBTN1=X,1,S
C(3,#CustNo,0,INVOKEJS(SHOW,p1,1,1,0),INVOKEJS(SHOW,p1,2,3,0))

#BUF1B=X,18,S
T(2,#CustNo+9999999000)

#BIND1B=X,1,S
BIND(#BTN1,click, {cmd=CHAIN; ntxcd=L2015; ntxbuf=#BUF1B;ntxbuf5=type<-1:FacmNo<-OOFacmNo:CustNo<-OOCustNo})

#SendL2r07A=X,1,S
C(3,#CustNo,0,S,$)
T(3,@RimCustId, )
E(0,@RimCustNo,#CustNo)
RESET_RIM(#SendL2r07A,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo)
R(1,L2R07)

#CustNoX=X,100,L
C(3,#CustNo,0,S,T(2,#L2r07CustName))

#BUF2A=X,100,S

#BIND2A=X,1,S
BIND(#BTN2,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUF2A;ntxbuf5=type<-1:CustId<-OOCustId})

#BUF6A=X,15,S
T(2,10000000+#CustNo)

#BIND6A=X,1,S
BIND(#BTN6,click, {cmd=CHAIN; ntxcd=L8204; ntxbuf=#BUF6A})

![統一編號]
#FdCustId=X,2,L

#CustId=X,10,I
CALL(#InitValue)
C(3,#CustNo,0,$,S)
INVOKEJS(SHOW,p1,2,3,1)
SHOW(#BTN2)HIDE(#BTN3)
@C(4,#CustId,S,$)
V(7)

#BUF2B=X,100,S
T(2,0000000+#CustId)

#BIND2B=X,1,S
BIND(#BTN2,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUF2B;ntxbuf5=type<-1:CustId<-OOCustId})

#CheckCustId=X,1,S
C(4,#CustId,S,$)
A(ID_UNINO,0,#CustId)

#WKBTN2=X,1,S
C(4,#CustId,HIDE(#BTN2),SHOW(#BTN2))
C(4,#CustId,INVOKEJS(SHOW,p1,2,2,0),INVOKEJS(SHOW,p1,3,3,0))

#SendL2r07B=X,1,S
C(4,#CustId,S,$)
T(2,@RimCustId,#CustId)
E(0,@RimCustNo,0)
RESET_RIM(#SendL2r07B,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo,#END)
R(1,L2R07)

#CustIdX=X,100,L
C(4,#CustId,S,T(2,#L2r07CustName))

#BUF3A=X,18,S
T(3,000000099999990)

#BIND3A=X,1,S
BIND(#BTN3,click, {cmd=CHAIN; ntxcd=L2016; ntxbuf=#BUF3A;ntxbuf5=type<-1:ApplNo<-OOApplNo:FacmNo<-OOFacmNo})


![核准編號]
#FdApplNo=X,2,L
C(3,#CustNo,0,$,S)
C(4,#CustId,$,S)
T(3,*)

#ApplNo=A,7,I
C(3,#CustNo,0,$,S)
C(4,#CustId,$,S)
INVOKEJS(SHOW,p1,3,3,1)
SHOW(#BTN3)
@C(3,#ApplNo,0,V(P,需擇一輸入查詢條件),$)

#WKBTN3=X,1,S
C(3,#ApplNo,0,HIDE(#BTN3),SHOW(#BTN3))

#BUF3B=X,18,S
T(2,#ApplNo+99999990)

#BIND3B=X,1,S
BIND(#BTN3,click, {cmd=CHAIN; ntxcd=L2016; ntxbuf=#BUF3B;ntxbuf5=type<-1:ApplNo<-OOApplNo:FacmNo<-OOFacmNo})

#HIDEALLBTN=X,1,S
C(3,#CHAIN,1,$,S)
HIDE(#BTN1,#BTN2,#BTN3,#BTN4)

!用核准號碼(ApplNo)調L2R05
#SendL2r05A=X,1,S
C(3,#ApplNo,0,S,$)
E(0,@RimFuncCode,5)
E(0,@RimCustNo,#CustNo)
E(0,@RimFacmNo,0)
E(0,@RimApplNo,#ApplNo)
E(0,@RimCaseNo,0)
RESET_RIM(#SendL2r05A,L2R05)
S(L2R05,1,#RimFuncCode,#RimTxCode,#RimFKey,#RimCustNo,#RimFacmNo,#RimApplNo,#RimCaseNo)
R(1,L2R05)
E(0,@FacmNo,#L2r05FacmNo)

#SendL2r07C=X,1,S
C(3,#ApplNo,0,S,$)
T(2,@RimCustId,#L2r05CustId)
E(0,@RimCustNo,#CustNo)
RESET_RIM(#SendL2r07C,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo,#END)
R(1,L2R07)

#ApplNoX=X,100,L
C(3,#ApplNo,0,S,T(2,#L2r07CustName))

#TimCustNo=A,7,S
E(0,0)
C(3,#CustNo,0,$,E(0,#CustNo))
C(4,#CustId,$,E(0,#L2r07CustNo))
C(3,#ApplNo,0,$,E(0,#L2r05CustNo))

#BUF6B=X,15,S
T(2,10000000+#TimCustNo)

#BIND6B=X,1,S
BIND(#BTN6,click, {cmd=CHAIN; ntxcd=L8204; ntxbuf=#BUF6B})

#WkCaseNo=A,7,S
#WkCustId=X,10,S
#WkApplNo=A,7,S
#WkFacmNo=A,3,S

#BUF4A=X,50,S
T(2,#WkCaseNo+#CustNo+#WkCustId+#WkApplNo+#WkFacmNo)

#BIND4A=X,1,S
BIND(#BTN4,click, {cmd=CHAIN; ntxcd=L3002; ntxbuf=#BUF4A;ntxbuf5=type<-1:FacmNo<-OOFacmNo:BormNo<-OOBormNo})

![額度編號]
!可不輸入
#FdFacmNo=X,2,L
C(3,#ApplNo,0,T(F,@FacmNo,1),T(F,@FacmNo,0))

#FacmNo=A,3,I
SHOW(#BTN4)
C(3,#CHAIN,1,T(1,#NTXBUF,13,3),$)
C(3,#CHAIN,1,s,$)
C(3,#ApplNo,0,$,s)

!用CustNo,FacmNo調L2R05
#SendL2r05B=X,1,S
C(3,#FacmNo,0,S,$)
E(0,@RimFuncCode,5)
E(0,@RimCustNo,#TimCustNo)
E(0,@RimFacmNo,#FacmNo)
E(0,@RimApplNo,0)
E(0,@RimCaseNo,0)
RESET_RIM(#SendL2r05B,L2R05)
S(L2R05,1,#RimFuncCode,#RimTxCode,#RimFKey,#RimCustNo,#RimFacmNo,#RimApplNo,#RimCaseNo)
R(1,L2R05)

![撥款序號]
!可不輸入
#FdBormNo=X,2,L
HIDE(#BTN4)

#BormNo=A,3,I
C(3,#CHAIN,1,C(5,#txcd,L4,$,T(1,#NTXBUF,16,3)),$)
C(3,#CHAIN,1,s,$)
C(3,#FacmNo,0,S,$)

#SendL3r02=X,1,S
C(3,#BormNo,0,S,$)
E(0,@RimFuncCode,5)
E(0,#RimCustNo,#TimCustNo)
E(0,@RimFacmNo,#FacmNo)
E(0,@RimBormNo,#BormNo)
RESET_RIM(#SendL3r02,L3R02)
S(L3R02,1,#RimTxCode,#RimFKey,#RimFuncCode,#RimCustNo,#RimFacmNo,#RimBormNo)
R(1,L3R02)

#MRKEY=_,_,S
C(3,#FacmNo,0,T(2,#TimCustNo),T(2,#TimCustNo+-+#FacmNo))
C(3,#BormNo,0,$,T(2,#TimCustNo+-+#FacmNo+-+#BormNo))

!放款餘額
#LoanBal=m,14.2,S
E(0,#L2r07LoanBal)
C(3,#FacmNo,0,$,E(0,#L2r05LoanBal))
C(3,#BormNo,0,$,E(0,#L3r02LoanBal))

#MRKEY=_,_,S
C(3,#FacmNo,0,T(2,#TimCustNo),T(2,#TimCustNo+-+#FacmNo))
C(3,#BormNo,0,$,T(2,#TimCustNo+-+#FacmNo+-+#BormNo))

!撥款日期
#DrawdownDate=D,7,S
C(3,#BormNo,0,S,E(0,#L3r02DrawdownDate))

!到日期
#MaturityDate=D,7,S
C(3,#BormNo,0,E(0,9991231),E(0,#L3r02MaturityDate))

![繳息迄日]
!自動顯示不必輸入
#FdPrevPayIntDate=X,2,L

#PrevPayIntDate=D,7,L
E(0,#L2r07PrevPayIntDate)
C(3,#FacmNo,0,$,E(0,#L2r05PrevPayIntDate))
C(3,#BormNo,0,$,E(0,#L3r02PrevPayIntDate))

#PrevPayIntDateX=X,9,L
CALL2(#AdjustDate,#PrevPayIntDate,#PrevPayIntDateX)

![幣別]
#FdCurrencyCode=X,2,L

#CurrencyCode=X,3,L
C(3,#CHAIN,1,T(1,#NTXBUF,19,3),$)
C(3,#CHAIN,1,s,$)
T(3,TWD)
C(3,#FacmNo,0,s,T(2,#L2r05CurrencyCode))
C(3,#BormNo,0,s,T(2,#L3r02CurrencyCode))

#CurrencyCodeX=X,10,S
T(3,)T(H,#CurrencyCode,#CurrencyCodeHelp)

#Precision=N,1,S
E(0,0)T(H,#CurrencyCode,#CurrPrecisionHelp)
C(3,#Precision,0,SHOW(#TwExtraRepay)HIDE(#UsExtraRepay)
                ,SHOW(#UsExtraRepay)HIDE(#TwExtraRepay))

#WKREPAY=m,14.2,S
E(0,#LoanBal-1) 

![部分償還本金]
!可不輸入,部份償還本金需＜放款餘額
#FdExtraRepay=X,2,L

#TwExtraRepay=m,14,I
C(3,#Precision,0,$,S)
IF(#CHAIN==1 && #CHAINTXCD=="L3130",T(1,#NTXBUF,22,14),$)
IF(#CHAIN==1 && #CHAINTXCD=="L3200",T(1,#NTXBUF,22,14),$)
C(3,#CHAIN,1,s,$)
! C(3,#FacmNo,0,S,$) 改為不檢核
! C(3,#BormNo,0,S,$) 改為不檢核
@
IF(#TwExtraRepay==#LoanBal,V(P,"部分償還金額=放款餘額，請改由結案試算"),$)
V(5,0,#WKREPAY)

#UsExtraRepay=m,14.2,I
C(3,#Precision,0,S,$)
IF(#CHAIN==1 && #CHAINTXCD=="L3130",T(1,#NTXBUF,22,16),$)
IF(#CHAIN==1 && #CHAINTXCD=="L3200",T(1,#NTXBUF,22,16),$)
C(3,#CHAIN,1,s,$)
C(3,#FacmNo,0,S,$)
C(3,#BormNo,0,S,$)
@
IF(#UsExtraRepay==#LoanBal,V(P,"部分償還金額=放款餘額，請改由結案試算"),$)
V(5,0,#WKREPAY)

#TimExtraRepay=m,14.2,S
C(3,#Precision,0,E(0,#TwExtraRepay),E(0,#UsExtraRepay))

![是否內含利息]
!當部分償還本金>0時,必須輸入 Y: 部分償還本金內含利息 N: 部分償還本金不含利息
#IncludeIntFlag=X,1,I
IF(#CHAIN==1 && #CHAINTXCD=="L3130",T(3,N),$)
IF(#CHAIN==1 && #CHAINTXCD=="L3200",T(1,#NTXBUF,45,1),$)
C(3,#CHAIN,1,s,$)
C(3,#TimExtraRepay,0,S,$)
T(3,N)
HELP(#YNHelp)
@V(H,#YNHelp)

#IncludeIntFlagX=X,2,L
T(3,)T(H,#IncludeIntFlag,#YNHelp)

![回收期數]
!若此欄未輸入時,則計算至入帳日當日該繳之期數
!回收期數不可大於預收期數(預收期數須設為系統參數，目前預收期數=0)
!部份償還本金時，回收期數不可輸入
#FdRepayTerms=X,2,L

#RepayTerms=A,3,I
IF(#CHAIN==1 && #CHAINTXCD=="L3130",S,$)
IF(#CHAIN==1 && #CHAINTXCD=="L3200",T(1,#NTXBUF,46,3),$)
C(3,#CHAIN,1,s,$)
C(3,#TimExtraRepay,0,$,S)

![入帳日期]
#FdEntryDate=X,2,L
T(3,*)

#EntryDate=D,7,I
C(3,#CHAIN,1,T(1,#NTXBUF,38,7),$)
C(3,#CHAIN,1,s,$)
C(3,#EntryDate,0,E(0,#SYSDATE),$)
@V(7)
! V(5,#DrawdownDate,#MaturityDate)
A(DATE,0,#EntryDate)

#BUF5=X,50,S
T(2,#TXCD+#CustNo+#FacmNo+#BormNo+#EntryDate+#TimExtraRepay+#IncludeIntFlag+#RepayTerms)

#BIND5=X,1,S
BIND(#BTN5,click, {cmd=CHAIN; ntxcd=L3200; ntxbuf=#BUF5})

#FdRptFg=X,2,L
<include src="COM.icf::RPTFG.dec"/>

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

<include src="ROM.icf::L0Rim.dec"/>
<include src="ROM.icf::L2Rim.dec"/>
<include src="ROM.icf::L2R05.dec"/>

#OORateX=X,9,L
X(#OORate)
T(B,#OORateX+%)

#OOPrincipal=m,14.2,O

#OOPrincipalX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#OOPrincipal,#OOPrincipalX)
P(R,#OOPrincipalX)

#OOInterest=m,14.2,O

#OOInterestX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#OOInterest,#OOInterestX)
P(R,#OOInterestX)


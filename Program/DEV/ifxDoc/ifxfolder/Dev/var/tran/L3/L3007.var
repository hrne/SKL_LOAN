#OOChequeAmt=m,14.2,S

#OOChequeAmtX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#OOChequeAmt,#OOChequeAmtX)
P(R,#OOChequeAmtX)

#OOChequeDate=D,7,O

#OOStatusCode=A,1,S

#OOStatusCodeX=X,10,O
T(3,)T(H,#OOStatusCode,#ChequeStatusCodeHelp)

#OOChequeBank=X,20,O

#OOChequeBranch=X,20,O

#OOEntryDate=D,7,O
C(3,#OOEntryDate,0,S,$)

!查詢 連結[支票內容查詢]
#OOBUF1=X,37,S
T(2,#OOChequeAcct+#OOChequeNo)

#OOBTN1=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L3943; ntxbuf=#OOBUF1})

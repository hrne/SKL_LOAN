![郵局存款別]
#PostCode=X,1,S

#PostCodeX=X,4,L
C(3,#RepayBank,700,$,S)
T(3,)T(H,#PostCode,#PostCodeHelp)  

![與借款人關係]
#RelationCode=X,2,S

#RelationCodeX=X,4,L
T(3,)T(H,#RelationCode,#RelationCodeHelp)  

![帳戶戶名]
#RelationName=X,100,L
C(4,#RelationCode,S,$)

![身份證字號]
#RelationId=X,10,L
C(4,#RelationCode,S,$)

![出生日期]
#RelationBirthday=D,7,S

#RelationBirthdayX=X,9,L
CALL2(#AdjustDate,#RelationBirthday,#RelationBirthdayX)

![性別]
#RelationGender=X,1,S

#RelationGenderX=X,2,L
C(4,#RelationCode,S,$)
T(H,#RelationGender,#SexHelp)

![擔保品]
#Collateral=X,14,S

![帳管費]
#AcctFee=m,14.2,S

#AcctFeeX=X,22,L
CALL2(#ADJUSTDOT,#Precision,#AcctFee,#AcctFeeX)

!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#LOOP=X,1,S
T(3,1)ASGN(LOOP$)

#OOClKey1=A,1,S

#OOClKey2=A,2,S

#OOClKey3=A,7,S

#OOClKey=X,12,O
C(3,#OOClKey1,0,S,T(2,#OOClKey1+-+#OOClKey2+-+#OOClKey3))

#OOApplNo=A,7,S

#OOApplNoX=X,7,O
C(3,#OOClKey1,0,S,T(2,#OOApplNo))

#OOCustNo=A,7,S

#OOCustNoX=X,7,O
C(3,#OOClKey1,0,S,T(2,#OOCustNo))

#OOFacmNo=A,3,S

#OOFacmNoX=X,3,O
C(3,#OOClKey1,0,S,T(2,#OOFacmNo))

#OOMainFlag=X,1,O

! 1: 房地         L2042 不動產建物明細資料查詢
! 2: 土地         L2041 不動產土地明細資料查詢
! 3: 股票         L2913 股票擔保品資料查詢
! 4: 其他有價證券 L2914 其他擔保品資料查詢
! 5: 銀行保證     L2914 其他擔保品資料查詢
! 9: 動產         L2047 動產擔保品明細資料查詢

#COLTXCD=X,5,S
SWITCH(#OOClKey1,1,T(3,L2911),
                 2,T(3,L2911),
                 3,T(3,L2913),
                 4,T(3,L2913),
                 5,T(3,L2914),
                 9,T(3,L2912),
                 )
                  
#COLTXNM=X,50,S
SWITCH(#OOClKey1,1,T(3,查詢;L2911 不動產擔保品資料查詢),
                 2,T(3,查詢;L2911 不動產擔保品資料查詢),
                 3,T(3,查詢;L2913 股票擔保品資料查詢),
                 4,T(3,查詢;L2913 股票擔保品資料查詢),
                 5,T(3,查詢;L2914 其他擔保品資料查詢),
                 9,T(3,查詢;L2912 動產擔保品資料查詢),
                 )                    

#COLBUF=X,17,S
T(2,#OOClKey1+#OOClKey2+#OOClKey3+#OOApplNo)

#OOBTNCOL=X,1,S
C(3,#OOClKey1,0,BIND(grid,cancel),BIND(grid,{ cmd=CHAIN; ntxcd=#COLTXCD;text=#COLTXNM;ntxbuf=#COLBUF}))

</sub>



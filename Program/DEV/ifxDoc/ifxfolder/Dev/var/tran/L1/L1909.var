!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L1909]                                 疑似準利害關係人明細查詢"],
["#FdMain+自然人姓名",#repCustName],
]

#FdMain=X,2,L
T(3,*)

!---------- AP Button AREA ----------
![Buttom資料查詢]


#Submit=A,1,S
K(MODIFYBUTTON,查詢)
!---------- AP TEXT AREA Variables ----------
#CHAIN=A,1,S
T(4,CHAIN$)

#NTXBUF=X,100,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@repCustName,#NTXBUF,1,100)

!統一編號
#repCustName=x,50,I
C(3,#CHAIN,1,s,$)
@
V(7)

#repCustName2=x,50,L
T(2,#repCustName)

#RPTFG=A,1,S
E(0,0)

#TXMT=A,1,S
C(3,#CHAIN,0,$,E(0,@CHAIN,0)XMT())

</sub>


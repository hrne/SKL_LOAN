E(0,@ReviewLongFitRatio,#C1)

!純益率(稅後)=本期淨利(稅後)/營業收入
#ReviewNetProfitRatioProc=@,1,S
IF(#ReviewOperatingRevenue==0,E(0,@C1,0),E(3,@C1,#ReviewProfitAfterTax/#ReviewOperatingRevenue*100))
E(0,@ReviewNetProfitRatio,#C1)


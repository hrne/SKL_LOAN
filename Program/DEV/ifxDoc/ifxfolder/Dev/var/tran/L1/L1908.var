!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[120,880];printer.width=[20,20];">
[
["[L1908]                                 申請不列印書面通知書查詢"],
["#FdMain+查詢條件","@(戶號、統一編號，需擇一輸入)"],
["#FdCustNo+．戶號",#CustNo,"-",#FacmNo,"（000=全部額度）",#BTN2,#CustName],
["#FdCustId+．統一編號",#CustId,#BTN3,#CustName],
["#FdRPTFG+顯示方式",
<include src="COM.icf::RPTFG.scr"/>
],
]
#FdMain=X,2,L
T(3,*)
#FdCustNo=X,2,L
#FdCustId=X,2,L
#FdRPTFG=X,2,L
T(3,*)

!---------- AP Button AREA ----------
![Buttom資料查詢]
! #BTN1=X,15,I,ui:button;value:新增;enable:0;tabbable:0;title:L1108 申請不列印書面通知書維護
#BTN2=X,15,I,ui:button;value:顧客資料查詢;enable:1;tabbable:0
#BTN3=X,15,I,ui:button;value:顧客資料查詢;enable:1;tabbable:0
!#BTN3=X,15,I,ui:button;value:顧客資料查詢;enable:0;tabbable:0
! #BUF1=X,42,S
! ENABLE(#BTN1)
!ENABLE(#BTN2)ENABLE(#BTN3)
#Submit=A,1,S
K(MODIFYBUTTON,查詢)
!---------- AP TEXT AREA Variables ----------
!調RIM參數
#RimFunCd=A,1,S

#CHAIN=A,1,S
T(4,CHAIN$)

#BUFA=X,1,S

#BINDA=X,1,S
BIND(#BTN2,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUFA;ntxbuf5=type<-1:CustNo<-OOCustNo})

#BUFB=X,1,S

#BINDB=X,1,S
BIND(#BTN3,click, {cmd=CHAIN; ntxcd=L1001; ntxbuf=#BUFA;ntxbuf5=type<-1:CustId<-OOCustId})

!戶號
#CustNo=A,7,I
! INVOKEJS(SHOW,p1,1,2,1)
C(3,#FirstIn,1,E(0,@CHAIN,1),$)
C(3,#FirstIn,1,S,$)
@C(3,#CustNo,0,T(2,@BTNADDBUF,1+0000000+000+          ),T(2,@BTNADDBUF,1+#CustNo+000+          ))
!戶名
#CustName=X,100,L

!額度編號
#FacmNo=A,3,I
C(3,#FirstIn,1,E(0,@CHAIN,1),$)
C(3,#FirstIn,1,S,$)
C(3,#CustNo,0,S,$)
@C(3,#FacmNo,0,T(2,@BTNADDBUF,1+#CustNo+000+          ),T(2,@BTNADDBUF,1+#CustNo+#FacmNo+          ))
! 統編
#CustId=X,10,I
! C(3,#CustNo,0,INVOKEJS(SHOW,p1,1,1,0),INVOKEJS(SHOW,p1,2,2,0))
C(3,#CustNo,0,$,S)
@
C(3,#CustNo,0,C(4,#CustId,V(P,需擇一輸入查詢條件),$),S)
C(3,#CustNo,0,V(7)A(ID_UNINO,0,#CustId),$)

#MRKEY=_,_,S
C(3,#FacmNo,0,T(2,#CustNo),T(2,#CustNo+-+#FacmNo))

#RETURN=A,1,S
E(0,0)

![折返KEY]
#RTNTXT=X,9,S

#FunCd1=A,1,S
T(3,1)
#FunCd2=A,1,S
T(3,2)
#FunCd3=A,1,S
T(3,3)
#FunCd4=A,1,S
T(3,4)
#FunCd5=A,1,S
T(3,5)


#FunCd=A,1,S
E(0,2)
E(0,@FirstIn,0)

! #BUF1=X,11,S
! C(3,#CustNo,0,HIDE(#BTN1),SHOW(#BTN1))
! T(2,#FunCd1+#CustNo+#FacmNo)

! #BIND1=X,1,S
! BIND(#BTN1,click, {cmd=CHAIN; ntxcd=L1108; ntxbuf=#BUF1})

<include src="COM.icf::RPTFG.dec"/>

</sub>


!---------- TXFORM = L1908 ----------
<sub type="FORM" name="L1908">

!#SCRPRT=_,_,S
!C(5,#TXFORM,L1908,$,S)

#SCRTIL=_,_,S
T(3,[L1908] 申請不列印書面通知書)

!------ TOM(HEAD)   Variables (FOR OUTPUT SCREEN) ------
!------ TOM(OCCURS) Variables (FOR OUTPUT SCREEN) ------
#OCustNo=A,7,L
T(2,#CustNo)

#OFacmNo=A,3,L
T(2,#FacmNo)

#OCustName=X,100,L
T(2,#CustName)

#LOOP=X,1,S
E(0,1)ASGN(LOOP$)

!戶號
#OOCustNo=A,7,L

! 額度號碼
#OOFacmNo=A,3,L

! 不列印記號-書面
#OOPaper=X,1,S
#PaperLb=X,6,L
SWITCH(#OOPaper,N,T(3,不寄送),
                ,T(3,寄送))
! 不列印記號-簡訊
#OOMsg=X,1,S
#MsgLb=X,6,L
SWITCH(#OOMsg,N,T(3,不發送),
                ,T(3,發送))
! 不列印記號-EMail
#OOEMail=X,1,S
#EMailLb=X,6,L
SWITCH(#OOEMail,N,T(3,不發送),
                ,T(3,發送))

#OOFormNo=X,10,S

#OOFormName=X,40,S

#OOApplyDt=D,7,L

#OOLastUpdateEmpNo=X,6,S

#OOLastUpdateEmpNoName=X,50,S

#OOLastUpdate_Combine=X,57,L
T(2,#OOLastUpdateEmpNo+ +#OOLastUpdateEmpNoName)

#OOLastUpdate=X,10,L

!修改
#OOBUF1=X,11,S
T(2,#FunCd2+#OOCustNo+#OOFacmNo)
#OOBUF2=X,11,S
T(2,#FunCd3+#OOCustNo+#OOFacmNo)
#OOBTN1=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L1108; ntxbuf=#OOBUF1})
#OOBTN2=X,1,S
BIND(grid,{ cmd=CHAIN; ntxcd=L1108; ntxbuf=#OOBUF2})
</sub>



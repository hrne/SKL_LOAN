!---------- BASIC LABEL Variables ----------
<sub type="SYS">
</sub>

<sub type="SYS" name="label">
!#TXCD
!T(3,L420C)

![資料表頭(HEADER)長度]
#INQHD=n,3,S
E(0,0)ASGN(INQHD$)

![每筆資料明細(OCCURS)長度]
#INQLEN=n,3,S
E(0,90)ASGN(INQLEN$)

![多筆查詢之一個畫面有N筆資料]
#INQREC=n,2,S
E(0,40)ASGN(INQREC$)

![畫面顯示的明細間的高度]
#LOOPH=n,3,S
E(0,1)ASGN(LOOPHEIGHT$)

![每張印錄單列印的筆數]
#INQPRT=n,2,S
E(0,40)ASGN(INQPRT$)

</sub>

<sub type="DC" name="BFHEAD">
<include src="COM.icf::BFHEAD.dec"/>
!起始交易記號  0:起始交易,1:非起始交易
#ORGFG=_,_,S
E(0,0)
</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L420C" layout="cols=1;screen.width=[130,870];printer.width=[20,80];order=1">
[
["[L420C]@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@虛擬轉暫收@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@"],
]
#CHAIN=A,1,S
T(4,CHAIN$) 

#NTXBUF=X,23,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@AcDate,#NTXBUF,1,7)
T(1,@BatchNo,#NTXBUF,8,6)
T(1,@DetailSeq,#NTXBUF,14,6)
T(1,@ReconCode,#NTXBUF,20,3)


! 戶號
#AcDate=D,7,S

! 批號
#BatchNo=X,6,S

! 批號
#DetailSeq=A,6,S

![對帳類別]
#ReconCode=X,3,S


![顯示方式]
#RPTFG=A,1,S
E(0,0)

</sub>

!---------- 輸出共用變數宣告區 ----------
<sub type="RTN" name="AFXMT">
<include src="PRT.icf::PRTHEAD.rtn"/>
</sub>

!------------ TXFORM = L420C -----------
<sub type="FORM" name="L420C">
</sub>

!------ 上行電文 TEXT ------
<sub type="TIM">
#AcDate
#BatchNo
#DetailSeq
#ReconCode
#END
</sub>

!------ 下行電文TOM ------
<sub type="TOM">
<include src="COM.icf::TRCTL.tom"/>
TXFORM=L420C
^
</sub>


!---------- 單據輸出組合 ----------
<sub type="SELECT">
<include src="PRT.icf::UPD01.sel"/>
</sub>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L4606" layout="cols=1;screen.width=[150,850];printer.width=[20,80];order=1">
[
["[L4606]@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@火險佣金作業"],
[#<->#,"@@@@@@選擇檔案","#BUTTON_FLD"],
[#<->#,"#FdInsuEndMonth+火險到期年月",#InsuEndMonth],
[#<->#,"#FdFlagA+作業選項","PC上傳媒體檔轉入佣金媒體檔"],
[#<->#,"@","火險佣金發放報表"],
[#<->#,"@","產生下傳媒體"],
]

#FdInsuEndMonth=X,2,L
T(3,*)
#FdFlagA=X,2,L

#CHAIN=A,1,S
T(4,CHAIN$) 

![一般數值 multiple:true 可上船多個檔案]
#BUTTON_FLD=X,1,O,ui:button;value:瀏覽;enable:1;tabbable:1;file:true;multiple:false;fieldName:#FILENA;width:600

#BIND_ABA=X,1,S
BIND(#BUTTON_FLD,click)

#FILENA=x,1000,S


#FORE_MONTH=D,8,S
D(7,3,#SYSDATE,1,0)

#COLDIS=A,1,S
T(1,@InsuEndMonth,#FORE_MONTH,2,5)
T(2,@FlagA,Y)
T(2,@FlagB,Y)
T(2,@FlagC,Y)

#NTXBUF=X,8,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@InsuEndMonth,#NTXBUF,1,1)
INVOKEJS(SHOW,p1,02,02,1)

! 火險到期年月
#InsuEndMonth=A,5,I
@A(YM,1,#InsuEndMonth)

#FlagA=X,1,S
HELP(Y: ;N: )
! 查詢選項

#FlagB=X,1,S
HELP(Y: ;N: )
! 查詢選項

#FlagC=X,1,S
HELP(Y: ;N: )



#RPTFG=A,1,S

</sub>


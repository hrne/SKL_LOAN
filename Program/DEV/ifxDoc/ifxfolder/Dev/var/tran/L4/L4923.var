! Help
<include src="HELPRIM.icf::HELPRIM.rtn"/>
! 授權狀態
#CDDEF01=x,50,S
T(3,CdCode.AuthStatusCode)
#AuthStatusCodeHelp=x,1024,S

!RepayCode 繳款方式
#CDDEF02=x,20,S
T(3,CdCode.RepayCode)

#RepayCodeHelp=x,1024,S


!BankDeductCd 銀行代碼
#CDDEF03=x,25,S
T(3,CdCode.BankCdAppl)

#BankDeductCdHelp=x,1024,S 

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF01,#CDDEF02,#CDDEF03)
R(1,XXR99)
CALL(#HelpProc) 
T(2,@AuthStatusCodeHelp,#HelpDesc1)
T(2,@RepayCodeHelp,#HelpDesc2)
T(2,@BankDeductCdHelp,#HelpDesc3)

#CHAIN=A,1,S
T(4,CHAIN$)
T(2,@RimTxCode,#TXCD) 
E(0,@RimFKey,#FKEY)

#NTXBUF=X,34,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
C(3,#CHAIN,1,T(1,@CustNo,#NTXBUF,1,7),$)
C(3,#CHAIN,1,T(1,@FacmNo,#NTXBUF,8,3),$)



#FdMain=X,2,L
T(3,*) 

![借款人戶號]
!借款人戶號, 統一編號擇一輸入
#FdCustNo=X,2,L

#CustNo=A,7,L

#SendL2r07B=X,1,S
C(3,#CustNo,0,S,$)
T(3,@RimCustId, )
E(0,@RimCustNo,#CustNo)
RESET_RIM(#SendL2r07B,L2R07)
S(L2R07,1,#RimTxCode,#RimCustId,#RimCustNo)
R(1,L2R07)

#CustNoX=x,2.50,L
C(3,#CustNo,0,S,T(2,#L2r07CustName))

![額度編號]
#FdCustId=X,2,L

#FacmNo=A,3,L

#CHAINxmt=X,1,S
XMT()




<include src="ROM.icf::L0Rim.dec"/>
<include src="ROM.icf::L2Rim.dec"/>

!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L4966" layout="cols=1;screen.width=[130,1100];printer.width=[20,20];">
[
["[L4966]                                 火險佣金資料查詢"],
["#FdMain+查詢條件","@(借款人統編、借款人戶號、業務員統編、業務員員編，需擇一輸入)"],
["#FdCustId+借款人統編",#CustId],
["#FdCustNo+借款人戶號",#CustNo],
["#FdEmpId+業務員統編",#EmpId],
["#FdFireOfficer+業務員員編",#FireOfficer],
]

#FdMain=X,2,L
#FdCustId=X,2,L
#FdCustNo=X,2,L
#FdEmpId=X,2,L
#FdFireOfficer=X,2,L

!---------- AP Button AREA ----------
![Buttom資料查詢]
#BUF1=X,42,S
!---------- AP TEXT AREA Variables ----------

#CHAIN=A,1,S
T(4,CHAIN$)

#NTXBUF=X,13,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
!T(1,@SearchFlag,#NTXBUF,1,1)
!T(1,@CustNo,#NTXBUF,2,7)
!T(1,@FacmNo,#NTXBUF,9,3)


#CustId=X,10,I
@A(ID_UNINO,0,#CustId)

#CustNo=A,7,I
C(4,#CustId,$,S)

#EmpId=X,10,I
C(4,#CustId,$,S)
C(3,#CustNo,0,$,S)
@A(ID_UNINO,0,#EmpId)

#FireOfficer=X,6,I
C(4,#CustId,$,S)
C(3,#CustNo,0,$,S)
C(4,#EmpId,$,S)
@
C(4,#FireOfficer,V(P,借款人統編、借款人戶號、業務員統編、業務員原編擇一輸入),$)

#RPTFG=A,1,S

</sub>


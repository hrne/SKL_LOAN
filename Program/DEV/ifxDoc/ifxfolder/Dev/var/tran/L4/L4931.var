!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="L4931" layout="cols=3;screen.width=[130,870];printer.width=[20,80];order=1">
[
["[L4931]@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@個別利率調整作業"],
[#<->#,"@@@@@個金/企金",#CustType,#CustTypeX],
[#<->#,"@@@@@作業項目",#TxKind,#TxKindX],
[#<->#,"@@@@@@@@@註記",#AdjCode,#AdjCodeX],
[#<->#,"@@@@@@@@@狀態",#InqCode,#InqCodeX],
[#<->#,"@@@@@調整日期",#AdjDate],
[#<->#,"@@@@@逾期期數",#OvduTerm],
[#<->#,"@@@@@戶號",#CustNo],
]

! Help
<include src="HELPRIM.icf::HELPRIM.rtn"/>

![戶別]
#CDDEF01=x,26,S
T(3,CdCode.EntCode2)

#EntCode2Help=x,1024,S 

![作業項目]
#CDDEF02=x,26,S
T(3,CdCode.TxKind)

#TxKindHelp=x,1024,S 

![戶別]
#CDDEF03=x,26,S
T(3,CdCode.EntCode)

#EntCodeHelp=x,1024,S 

#HelpRim=X,1,S
RESET_RIM(#HelpRim,XXR99)
S(XXR99,01,#CDDEF01,#CDDEF02,#CDDEF03)
R(1,XXR99)
CALL(#HelpProc) 
T(2,@EntCode2Help,#HelpDesc1)
T(2,@TxKindHelp,#HelpDesc2)  
T(2,@EntCodeHelp,#HelpDesc3) 

#AdjCodeHelp=x,120,S
!T(3,1:按合約自動調整;2:按地區別自動調整;3:人工調整-未調整;4:人工調整-待輸入;5:人工調整-已調整;9:檢核有誤;A:全部)

#InqCodeHelp=x,120,S
T(3,0:要處理;9:待處理;A:全部)

#CHAIN=A,1,S
T(4,CHAIN$) 
IF(#CHAIN!=1,V(P,此為連動交易，請從交易:L4031進入),$)

#NTXBUF=X,50,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@CustType,#NTXBUF,1,1)
T(1,@TxKind,#NTXBUF,2,1)
T(1,@AdjCode,#NTXBUF,3,2)
T(1,@AdjCodeX,#NTXBUF,5,30)
T(1,@InqCode,#NTXBUF,35,1)
T(1,@AdjDate,#NTXBUF,36,7)
T(1,@RPTFG,#NTXBUF,43,1)

#ColDispaly=A,1,S
C(3,#CHAIN,1,$,S)

! 戶別
#CustType=A,1,I
C(3,#CHAIN,0,$,s)
HELP(#EntCode2Help)
@V(H,#EntCode2Help)
#CustTypeX=X,30,L
T(3,)T(H,#CustType,#EntCode2Help)
!SWITCH(#CustType,1,T(3,個金),2,T(3,企金（含企金自然人）),T(3,))

! 作業項目
#TxKind=m,2,I
C(3,#CHAIN,0,$,s)
HELP(#TxKindHelp)
@V(H,#TxKindHelp)

#TxKindX=X,28,L
T(3,)T(H,#TxKind,#TxKindHelp)
!SWITCH(#TxKind,1,T(2,定期機動調整),
!                 2,T(2,指數型利率調整),
!                 3,T(2,機動利率調整),
!                 4,T(2,員工利率調整),
!                 5,T(2,按商品別調整),T(3,))

#AdjCode=X,2,I
C(3,#CHAIN,0,$,s) 
!HELP(#AdjCodeHelp)
!@V(H,#AdjCodeHelp)


#AdjCodeX=X,30,L


! [#<->#,"@是否輸入利率",#InqCode,#InqCodeX],
! 查詢區分
#InqCode=X,1,I
C(3,#CHAIN,0,$,s)
HELP(#InqCodeHelp)
@V(H,#InqCodeHelp)

#InqCodeX=X,14,L
T(3,)T(H,#InqCode,#InqCodeHelp)


! 利率區分
#RateCode=A,1,S


#RateCodeX=X,11,L

! 年月
#AdjDate=D,7,I
C(3,#CHAIN,0,$,s)
E(0,#SYSDATE)


#OvduTerm=A,2,I
C(3,#CHAIN,0,$,s)

#CustNo=A,7,I
C(3,#CHAIN,0,$,s)



#RPTFG=A,1,S
!C(3,#InqCode,2,E(0,2),$)
!C(3,#InqCode,2,s,$)
!SWITCH(#AdjCode,1,E(0,5),
!                2,E(0,6),
!                3,C(3,#TxKind,3,E(0,7),E(0,3)),
!                9,E(0,2),
!                  E(0,4))



! 現在利率
#NowRate=m,2.4,L
! 生效日
#NowEffectDate=D,7,L


#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

</sub>


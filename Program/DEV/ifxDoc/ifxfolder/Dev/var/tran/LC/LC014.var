!---------- AP Button AREA ----------
![Buttom資料查詢]
!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>

#FdTranNo=X,2,L
#FdCustNo=X,2,L
#FdMrKey=X,2,L
#FdDesc=X,2,L
#FdNote=X,2,L

#CHAIN=A,1,S
T(4,CHAIN$)
IF(#CHAIN!=1,V(P,此為連動交易，請從入口交易進入),$)



#NTXBUF=X,300,S
C(3,#CHAIN,1,T(4,NTXBUF$),S)
T(1,@TranNo,#NTXBUF,1,5)
T(1,@CustNo,#NTXBUF,6,7)
T(1,@FacmNo,#NTXBUF,13,3)
T(1,@BormNo,#NTXBUF,16,3)
T(1,@MrKey,#NTXBUF,19,100)
T(1,@Note,#NTXBUF,119,100)

#BufLC104=X,300,S
T(2,#TranNo+#CustNo+#FacmNo+#BormNo+#MrKey+#Note+LC014)

#Button=x,100,S
T(2,{cmd=CHAIN; ntxcd=LC104; ntxbuf=#BufLC104})ASGN(BTN_NEW1$)
K(MODIFYBUTTON,1,LC104 上傳附件)

!交易代號
#TranNo=X,5,L

!戶號
#CustNo=A,7,L
#FacmNo=A,3,L
#BormNo=A,3,L

#MrKey=x,100,L

#Note=x,100,L

#MRKEY=_,_,S
C(3,#CustNo,0,S,T(2,#CustNo+-+#FacmNo+-+#BormNo))

#RPTFG=A,1,S

#CHAINXMT=X,1,S
C(3,#CHAIN,1,E(0,@CHAIN,0)XMT(),$)

</sub>


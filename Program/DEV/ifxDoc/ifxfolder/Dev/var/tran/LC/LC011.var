!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[LC011]                                 交易明細查詢"],
["*@會計日期",#iEntdy],
["@@使用單位",#iBrNo],
["@@經@@@@辦",#iTlrNo],
["@@交易代碼",#iTranNo],
["*@交易型態",#iStatus,#iStatusX],
]
!---------- AP Button AREA ----------


!---------- AP TEXT AREA Variables ----------
#CHAIN=A,1,S
T(4,CHAIN$)
T(2,@iTlrNo,#TLRNO)
#NTXBUF=X,3,S

#iEntdy=D,7,I
E(0,#SYSDATE)
D(8)
@V(7)A(DATE,0,#iEntdy)
V(5,0010101,#SYSDATE)

#iBrNo=X,4,L
T(2,#KINBR) 

#iTlrNo=X,6,I


!#SendL6R17=X,1,S
!C(4,#iTlrNo,S,$)
!E(0,@RimFuncCode,5)
!T(2,@RimTxCode,#TXCD)
!T(2,@RimEmployeeNo,#iTlrNo)
!RESET_RIM(#SendL6R17,L6R17)
!S(L6R17,1,#RimFuncCode,#RimTxCode,#RimEmployeeNo)
!R(1,L6R17)


!#iTlrItem=X,40,L
!C(4,#iTlrNo,T(2,),T(2,#L6R17Fullname))


!交易代碼
#iTranNo=X,5,I

!狀態
#iStatus=A,1,I
E(0,0)
HELP(#iStatusH)
@V(H,#iStatusH)

#iStatusX=X,20,L
T(H,#iStatus,#iStatusH)

#RPTFG=A,1,S
E(0,0)

<include src="ROM.icf::L0Rim.dec"/>
<include src="ROM.icf::L6R17Rim.dec"/>
<include src="ROM.icf::L6R17.dec"/>
</sub>


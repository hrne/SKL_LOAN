!---------- 交易畫面及個別交易變數宣告 ----------
<sub type="DC" name="p1" layout="cols=1;screen.width=[150,850];printer.width=[20,20];">
[
["[L9715]                                 業務專辦照顧十八個月明細表"],

["#R1+@滯繳期數",#UNPAY_TERM_ST,"~",#UNPAY_TERM_ED],
["#R2+@滯繳日數",#UNPAY_DAY_ST,"~",#UNPAY_DAY_ED],
["#R3+@基礎日期",#FUND_DAY],
["#R4+@撥款日期",#APPRO_DAY,"@起"],
]
!---------- AP Button AREA ----------
![Buttom資料查詢]
! 功能 必須輸入(1: 新增;2: 修改;4: 刪除;5: 查詢)
#FUNCIND=_,_,S
E(0,5)
<include src="COM.icf::CheckAuth.rtn"/>

! 必輸欄位
##loop{times:4,i:1}
#R{i}=X,1,L
INVOKEJS(SHOW,#R{i},0)
K(LIGHT,#R{i},TXTRED,0)
##end

! 永遠為必輸欄位
#ALWAYS_HIGHLIGHT=@,1,S
T(3,@R1, )
T(3,@R2, )
T(3,@R3,*)
T(3,@R4, )

!---------- AP TEXT AREA Variables ----------
#CHAIN=A,1,S
T(4,CHAIN$)

#INIT=X,1,S
CALL(#ALWAYS_HIGHLIGHT)


#DT1=D,8,S
T(4,DATE$)

#DT2=D,7,S
T(1,#DT1,2,7)

!滯繳期數
!起
#UNPAY_TERM_ST=A,2,I 

!止
#UNPAY_TERM_ED=A,2,I
C(3,#UNPAY_TERM_ST,0,S,$) 
@V(5,#UNPAY_TERM_ST,99)

!滯繳日數
!起
#UNPAY_DAY_ST=A,3,I
C(3,#UNPAY_TERM_ST,0,$,S) 
@V(2,0)

!止
#UNPAY_DAY_ED=A,3,I
C(3,#UNPAY_TERM_ST,0,$,S) 
@V(5,#UNPAY_DAY_ST,999)

!基礎日期
#FUND_DAY=D,7,I
C(4,#FUND_DAY,E(0,#DT2),$)
@V(7)A(DATE,0,#FUND_DAY)

#FUND_FIRST_DAY=D,7,S
E(0,#FUND_DAY/100)
E(0,#FUND_FIRST_DAY*100)
E(0,#FUND_FIRST_DAY+1)

#TMP_APPRO_DAY=D,8,S
D(7,3,#FUND_FIRST_DAY,17,0)

!撥款日期
#APPRO_DAY=D,7,L
E(0,#TMP_APPRO_DAY)

</sub>


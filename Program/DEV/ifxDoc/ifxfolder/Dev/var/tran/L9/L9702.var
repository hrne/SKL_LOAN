!---------- AP Button AREA ----------
![Buttom資料查詢]
! 功能 必須輸入(1: 新增;2: 修改;4: 刪除;5: 查詢)
#FUNCIND=_,_,S
E(0,5)
<include src="COM.icf::CheckAuth.rtn"/>

! 必輸欄位
##loop{times:2,i:1}
#R{i}=X,1,L
INVOKEJS(SHOW,#R{i},0)
K(LIGHT,#R{i},TXTRED,0)
##end

! 永遠為必輸欄位
#ALWAYS_HIGHLIGHT=@,1,S
T(3,@R1,*)
T(3,@R2,*)



!---------- AP TEXT AREA Variables ----------
<include src="HELPRIM.icf::HELPRIM.rtn"/>

!ReportKind
#CDCODEReportKind=x,20,S
T(3,CdCode.ReportKind)

#ReportKindHelpRim=X,1,S
RESET_RIM(#ReportKindHelpRim,XXR99)
S(XXR99,01,#CDCODEReportKind)
R(1,XXR99)
CALL(#HelpProc)
T(2,@ReportKindHelp,#HelpDesc1)

#ReportKindHelp=x,200,S

#CHAIN=A,1,S
T(4,CHAIN$)

#INIT=X,1,S
CALL(#ALWAYS_HIGHLIGHT)

#DT1=D,8,S
T(4,DATE$)

#DT2=D,7,S
T(1,#DT1,2,7)

#DT_YM=A,5,S
T(1,#DT1,2,5)

!會計日期 必須輸入，不可大於本營業日
#ACCTDATE_ST=D,7,I
T(2,#DT_YM+01)
@V(7)
V(5,0,#DT2)
A(DATE,0,#ACCTDATE_ST)

#ACCTDATE_ED=D,7,I
T(2,#DT2)
@V(7)
V(5,#ACCTDATE_ST,#DT2)
A(DATE,0,#ACCTDATE_ED)

!報表種類
#REPORT_TYPE=A,1,I
E(0,1)
HELP(#ReportKindHelp)
@T(3,@REPORT_TYPEX,)
V(H,#ReportKindHelp)

#REPORT_TYPEX=X,22,L
T(H,#REPORT_TYPE,#ReportKindHelp)

</sub>

